    SYNTAX INTEGER
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "[FW:E2080.AC.1:5.52.0]"
    ::= { bridgeWirelessScanEntry 3 }


bridgeWirelessScanMode OBJECT-TYPE
    SYNTAX INTEGER {
		b-only(1),
		g-only(2),
		mixed-b-g(3),
		a-only(4)
    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "Show AP's IEEE 802.11x compatibility mode.
		[FW:E2080.AC.1:5.52.0]"
    ::= { bridgeWirelessScanEntry 4 }

bridgeWirelessScanSecurity OBJECT-TYPE
    SYNTAX INTEGER {
    		none(0),
			wep(1),
			wpa(2)
		}
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "[FW:E2080.AC.1:5.52.0]"
    ::= { bridgeWirelessScanEntry 5 }


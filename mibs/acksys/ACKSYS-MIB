    SYNTAX INTEGER {charTime(1),millisecond(2)}
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	    "
		Unit of send trigger
		
		[FW:E2080.AC.1-Serial:5.10.0]
	    "
    ::= { serialSendTriggerFrameDelay  3 }


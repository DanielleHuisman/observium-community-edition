	SYNTAX  INTEGER {
			up(1),       -- ready to pass packets
            down(2)
		}
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION "MAX-ACCESS POINT's connection status.
		[FW:E2080.AC.1:5.52.0]
		"
	::= { bridge-mode 1 }


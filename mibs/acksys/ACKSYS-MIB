	SYNTAX DisableEnable
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION "Show if the rule is enabled or not.
		[FW:E2080.AC.1:5.52.0]"
	::= { apClientFilteringEntry 4 }




    SYNTAX INTEGER {enable(2),disable(1)}
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	    "
		Enable / disable this sendtrigger
		
		[FW:E2080.AC.1-Serial:5.10.0]
	    "
    ::= { serialSendTriggerFrameDelay  1 }


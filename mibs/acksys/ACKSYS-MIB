tcpRawClientConnectionTimeout OBJECT-TYPE
    SYNTAX INTEGER 
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	    "
			How long before we give up trying to call a server. 0 means indefinitely.
			[FW:E2080.AC.1-Serial:5.10.0]
	    "
    ::= { serialServiceTcpRawPortClient  4 }

tcpRawClientPollPeriode OBJECT-TYPE
    SYNTAX INTEGER 
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	    "
			How long we wait between two scans of the servers list (in case no server is available).
			[FW:E2080.AC.1-Serial:5.10.0]
	    "
    ::= { serialServiceTcpRawPortClient  5 }


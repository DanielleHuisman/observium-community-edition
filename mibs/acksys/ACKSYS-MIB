	statusPhyCellSimIMSI OBJECT-TYPE
		SYNTAX DisplayString MAX-ACCESS read-only STATUS current
		DESCRIPTION "Subscriber identity (IMSI)
		[FW:E2148.AC.1:3.16.1.1]"
		::= { statusPhyCellEntry 10 }

	statusPhyCellAttachMode OBJECT-TYPE
		SYNTAX CellAttachMode MAX-ACCESS read-only STATUS current
		DESCRIPTION "Network registration state, see 3GPP TS27.007 +CREG
		[FW:E2148.AC.1:3.16.1.1]"

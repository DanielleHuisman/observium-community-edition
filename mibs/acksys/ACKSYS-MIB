		SYNTAX INTEGER
		MAX-ACCESS read-only STATUS current
		DESCRIPTION "gpio index"
		::= { gpioInEntry 1 }

	gpioInState OBJECT-TYPE
		SYNTAX INTEGER
		MAX-ACCESS read-only STATUS current
		DESCRIPTION "gpio index"
		::= { gpioInEntry 2 }

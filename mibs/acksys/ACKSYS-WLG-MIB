--acksys group
acksysProductID OBJECT-TYPE
    SYNTAX  INTEGER {
                wlg-link(1),      
                wlg-aboard-n(2),
				wlg-link-v2(3),
				wlg-aboard-n-v2(4)
            }
    MAX-ACCESS  read-only
	STATUS  current
    DESCRIPTION
            "Return the product Id code
			"
    ::= { acksys 3 }

--network-product

--Wireless Interface group

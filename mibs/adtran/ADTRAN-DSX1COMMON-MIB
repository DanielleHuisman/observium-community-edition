        adDSX1CommonStatusLoopStatus OBJECT-TYPE
        SYNTAX INTEGER {
                        na(1),
                        ok(2),
                        los(3),
                        lof(4),
                        ais(5),
                        rai(6),
                        bpv(7)
        }
        MAX-ACCESS    read-only
        STATUS  current
        DESCRIPTION
            "Loop Status
                         1 - N/A
                         2 - O.K.
                         3 - LOS
                         4 - LOF
                         5 - AIS
                         6 - RAI
                         7 - BPV"
     ::= { adDSX1CommonStatusEntry 1 }


    adGenFrGroup        OBJECT IDENTIFIER ::= { adGenFrameRelayMIBObjects 1 }
    adGenFrLink         OBJECT IDENTIFIER ::= { adGenFrameRelayMIBObjects 2 }
    adGenFrPVC          OBJECT IDENTIFIER ::= { adGenFrameRelayMIBObjects 3 }
    adGenFrSlot         OBJECT IDENTIFIER ::= { adGenFrameRelayMIBObjects 4 }

    adGenFrAlarmsPrefix OBJECT IDENTIFIER ::= {adGenFrameRelay 2 }

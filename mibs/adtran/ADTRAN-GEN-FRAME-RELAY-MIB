       REVISION "201003240000Z"    -- 3/24/2010 00:00GMT
       DESCRIPTION
          ""
         ::= { adGenFrameRelayID 1 }

    adGenFrameRelayMIBObjects      OBJECT IDENTIFIER  ::= { adGenFrameRelay 1 }

    -- ******************************************************************

      ::= { adGenMuxPProtGroupLastCreateErrorTable 1 }

   AdGenMuxPProtGroupLastCreateErrorEntry ::=
      SEQUENCE {
         adGenMuxPProtGroupLastCreateError
            DisplayString
      }


   AdGenMuxPMultiProtGroupLastCreateErrorEntry ::=
      SEQUENCE {
         adGenMuxPMultiProtGroupLastCreateError
            DisplayString
      }

   adGenMuxPMultiProtGroupLastCreateError OBJECT-TYPE
      SYNTAX DisplayString
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION

         --#GENERIC      6
         --#CATEGORY     "Error Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
      ::= { adGenMuxPAlarmEvents 2 }

   adGenMuxPUPMClear  NOTIFICATION-TYPE
      OBJECTS {
                adTrapInformSeqNum,
                sysName,
                adGenSlotInfoIndex,
                adTAeSCUTrapAlarmLevel,
                adGenMuxPMappingType
              }

         --#SEVERITY     CRITICAL
         --#GENERIC      6
         --#CATEGORY     "Error Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
      ::= { adGenMuxPAlarmEvents 1 }

   adGenMuxPLFDSet  NOTIFICATION-TYPE

         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
         ::= { adGenVdsl2VturRemoteTraps 4 }

         adGenVdsl2VturESRemoteThreshTrap       NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       ifIndex,
                       adGenVdsl2VturPerfCurr15MinEs,
                       adGenVdsl2VturThresh15MinEs
                     }
             STATUS  current
             DESCRIPTION
                 "Errored Second 15-minute interval threshold reached."
         --#TYPE         "WARNING - VTUR 15-Minute ES Threshold crossed"
         --#SUMMARY      "VTUR 15-Minute ES Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, remote: %d, margin: %d, threshold: %d"
         --#ARGUMENTS    {1,2,3,4}
         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
         ::= { adGenVdsl2VturRemoteTraps 5 }

         adGenVdsl2VturSESRemoteThreshTrap       NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       ifIndex,
                       adGenVdsl2VturPerfCurr15MinSes,
                       adGenVdsl2VturThresh15MinSes
                     }
             STATUS  current
             DESCRIPTION
                 "Severely Errored Second 15-minute interval threshold
                 reached."
         --#TYPE         "WARNING - VTUR 15-Minute SES Threshold crossed"
         --#SUMMARY      "VTUR 15-Minute SES Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, remote: %d, margin: %d, threshold: %d"
         --#ARGUMENTS    {1,2,3,4}
         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
         ::= { adGenVdsl2VturRemoteTraps 6 }

         adGenVdsl2VturUASRemoteThreshTrap       NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       ifIndex,
                       adGenVdsl2VturPerfCurr15MinUas,
                       adGenVdsl2VturThresh15MinUas
                     }
             STATUS  current
             DESCRIPTION
                 "Unavailable Second 15-minute interval threshold
                 reached."
         --#TYPE         "WARNING - VTUR 15-Minute UAS Threshold crossed"
         --#SUMMARY      "VTUR 15-Minute UAS Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, remote: %d, margin: %d, threshold: %d"

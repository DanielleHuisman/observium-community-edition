         adGenVdsl2VtucLprsThreshTrap      NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       adGenSlotInfoIndex,
                       adGenPortTrapIdentifier,
                       adGenVdsl2VtucPerfCurr15MinLprs,
                       adGenVdsl2VtucThresh15MinLprs
                     }
             STATUS  current
             DESCRIPTION
                 "Loss of Power 15-minute interval threshold reached."
         --#TYPE         "WARNING - VTUC 15-Minute LPRS Threshold crossed"
         --#SUMMARY      "VTUC 15-Minute LPRS Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, slot: %d, port: %d, lprs: %d, threshold: %d"
         --#ARGUMENTS    {1,2,3,4,5}
         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
         ::= { adGenVdsl2VtucTraps 5 }

         adGenVdsl2VtucESThreshTrap       NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       adGenSlotInfoIndex,
                       adGenPortTrapIdentifier,
                       adGenVdsl2VtucPerfCurr15MinEs,
                       adGenVdsl2VtucThresh15MinEs
                     }
             STATUS  current
             DESCRIPTION
                 "Errored Second 15-minute interval threshold reached."
         --#TYPE         "WARNING - VTUC 15-Minute ES Threshold crossed"
         --#SUMMARY      "VTUC 15-Minute ES Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, slot: %d, port: %d, es: %d, threshold: %d"
         --#ARGUMENTS    {1,2,3,4,5}
         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"

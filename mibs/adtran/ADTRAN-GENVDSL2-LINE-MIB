         -- ========= PM Group (adGenVdsl2VturPerfDataTable) =================
         adGenVdsl2VturPerfDataTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VturPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtur.
                 VDSL2 physical interfaces are
                 those ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2PM 2 }

         adGenVdsl2VturPerfDataEntry       OBJECT-TYPE
             SYNTAX          AdGenVdsl2VturPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in adGenVdsl2VturPerfDataTable."


             INDEX { ifIndex }
         ::= { adGenVdsl2VturPerfDataTable 1 }

         AdGenVdsl2VturPerfDataEntry ::=
             SEQUENCE {
                 adGenVdsl2VturPerfLofs                    Counter32,
                 adGenVdsl2VturPerfLoss                    Counter32,
                 adGenVdsl2VturPerfLprs                    Counter32,
                 adGenVdsl2VturPerfEs                      Counter32,
                 adGenVdsl2VturPerfSes                     Counter32,
                 adGenVdsl2VturPerfUas                     Counter32,
                 adGenVdsl2VturPerfFecs                    Counter32,
                 adGenVdsl2VturPerfCrc                     Counter32,
                 adGenVdsl2VturPerfFec                     Counter32,
                 adGenVdsl2VturPerfValidIntervals          Integer32,
                 adGenVdsl2VturPerfInvalidIntervals        Integer32,
                 adGenVdsl2VturPerfCurr15MinTimeElapsed    Gauge32,
                 adGenVdsl2VturPerfCurr15MinLofs           Counter32,
                 adGenVdsl2VturPerfCurr15MinLoss           Counter32,
                 adGenVdsl2VturPerfCurr15MinLprs           Counter32,
                 adGenVdsl2VturPerfCurr15MinEs             Counter32,
                 adGenVdsl2VturPerfCurr15MinSes            Counter32,
                 adGenVdsl2VturPerfCurr15MinUas            Counter32,
                 adGenVdsl2VturPerfCurr15MinFecs           Counter32,
                 adGenVdsl2VturPerfCurr15MinCrc            Counter32,
                 adGenVdsl2VturPerfCurr15MinFec            Counter32,
                 adGenVdsl2VturPerfCurr1DayTimeElapsed     Gauge32,
                 adGenVdsl2VturPerfCurr1DayLofs            Counter32,
                 adGenVdsl2VturPerfCurr1DayLoss            Counter32,
                 adGenVdsl2VturPerfCurr1DayLprs            Counter32,
                 adGenVdsl2VturPerfCurr1DayEs              Counter32,
                 adGenVdsl2VturPerfCurr1DaySes             Counter32,
                 adGenVdsl2VturPerfCurr1DayUas             Counter32,
                 adGenVdsl2VturPerfCurr1DayFecs            Counter32,
                 adGenVdsl2VturPerfCurr1DayCrc             Counter32,
                 adGenVdsl2VturPerfCurr1DayFec             Counter32,
                 adGenVdsl2VturPerfTcTxUnits               Counter32,
                 adGenVdsl2VturPerfTcTxDataUnits           Counter32,
                 adGenVdsl2VturPerfTcTxDataOctets          Counter32,
                 adGenVdsl2VturPerfTcTxIdleUnits           Counter32,
                 adGenVdsl2VturPerfTcRxUnits               Counter32,
                 adGenVdsl2VturPerfTcRxDataUnits           Counter32,
                 adGenVdsl2VturPerfTcRxDataOctets          Counter32,
                 adGenVdsl2VturPerfTcRxIdleUnits           Counter32,
                 adGenVdsl2VturPerfTcRxErroredUnits        Counter32,
                 adGenVdsl2VturPerfSraDownshifts           Counter32,
                 adGenVdsl2VturPerfSraUpshifts             Counter32,
                 adGenVdsl2VturPerfCurr15MinSraDownshifts  Counter32,
                 adGenVdsl2VturPerfCurr15MinSraUpshifts    Counter32,
                 adGenVdsl2VturPerfCurr15MinSraRateMax     Gauge32,
                 adGenVdsl2VturPerfCurr15MinSraRateMin     Gauge32,
                 adGenVdsl2VturPerfCurr1DaySraDownshifts   Counter32,
                 adGenVdsl2VturPerfCurr1DaySraUpshifts     Counter32,
                 adGenVdsl2VturPerfCurr1DaySraRateMax      Gauge32,
                 adGenVdsl2VturPerfCurr1DaySraRateMin      Gauge32,
                 adGenVdsl2VturPerfRtxMinEftr              Gauge32,
                 adGenVdsl2VturPerfRtxLeftrs               Counter32,
                 adGenVdsl2VturPerfCurr15MinRtxMinEftr     Gauge32,
                 adGenVdsl2VturPerfCurr15MinRtxLeftrs      Counter32,
                 adGenVdsl2VturPerfCurr1DayRtxMinEftr      Gauge32,
                 adGenVdsl2VturPerfCurr1DayRtxLeftrs       Counter32
              }

         adGenVdsl2VturPerfLofs OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of Loss of Framing failures since
                 agent reset."
         ::= { adGenVdsl2VturPerfDataEntry 1 }

         adGenVdsl2VturPerfLoss  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of Loss of Signal failures since
                 agent reset."
         ::= { adGenVdsl2VturPerfDataEntry 2 }

        adGenVdsl2VturPerfLprs  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of Loss of Power failures since
                 agent reset."
         ::= { adGenVdsl2VturPerfDataEntry 3 }

         adGenVdsl2VturPerfEs  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Errored Seconds since the unit was last reset.
                 An Errored Second is a one-second interval containing one
                 or more CRC anomalies, or one or more LOS or LOF defects."
             REFERENCE    "T1E1.4/2000-009R3, Part 1, common spec"
         ::= { adGenVdsl2VturPerfDataEntry 4 }

         adGenVdsl2VturPerfSes  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Severely Errored Seconds since the unit was last
                  reset."
         ::= { adGenVdsl2VturPerfDataEntry 5 }

         adGenVdsl2VturPerfUas  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Unavailable Seconds since the unit was last reset."
         ::= { adGenVdsl2VturPerfDataEntry 6 }

         adGenVdsl2VturPerfFecs  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of FEC Seconds since agent reset."
         ::= { adGenVdsl2VturPerfDataEntry 7 }

         adGenVdsl2VturPerfCrc  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of CRC anomolies since agent reset."
         ::= { adGenVdsl2VturPerfDataEntry 8 }

         adGenVdsl2VturPerfFec  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of FEC anomolies since agent reset."
         ::= { adGenVdsl2VturPerfDataEntry 9 }

         -- general 15 min interval information
         --
         adGenVdsl2VturPerfValidIntervals OBJECT-TYPE
             SYNTAX      Integer32(0..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The number of previous 15-minute intervals in the
                 interval table for which data was collected.  Given
                 that <n> is the maximum # of intervals supported.
                 The value will be <n> unless the measurement was
                 (re-)started within the last (<n>*15) minutes, in which
                 case the value will be the number of complete 15
                 minute intervals for which the agent has at least
                 some data. In certain cases (e.g., in the case
                 where the agent is a proxy) it is possible that some
                 intervals are unavailable.  In this case, this
                 interval is the maximum interval number for
                 which data is available."
         ::= { adGenVdsl2VturPerfDataEntry 10 }

         adGenVdsl2VturPerfInvalidIntervals OBJECT-TYPE
             SYNTAX      Integer32(0..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The number of intervals in the range from
                 0 to the value of adGenVdsl2VturPerfValidIntervals
                 for which no data is available. This object
                 will typically be zero except in cases where
                 the data for some intervals are not available
                 (e.g., in proxy situations)."
         ::= { adGenVdsl2VturPerfDataEntry 11 }

         adGenVdsl2VturPerfCurr15MinTimeElapsed OBJECT-TYPE
             SYNTAX      Gauge32(0..899)
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Total elapsed seconds in this interval."
         ::= { adGenVdsl2VturPerfDataEntry 12 }

         adGenVdsl2VturPerfCurr15MinLofs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the current 15 minute interval
                 when there was Loss of Framing."
         ::= { adGenVdsl2VturPerfDataEntry 13 }

         adGenVdsl2VturPerfCurr15MinLoss  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"

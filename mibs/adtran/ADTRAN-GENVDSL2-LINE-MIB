             SYNTAX  INTEGER {
                 ndr (0),
                 inp (1)
             }
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "Selects the US latency path 1 channel framing parameter
                 policy during initialization. The options are to
                 maximize net data rate (ndr) or impulse noise
                 protection (inp).
                "
         ::= { adGenVdsl2LineConfProfileEntry 47 }

         adGenVdsl2ConfDsLp1MaxPayloadRate OBJECT-TYPE
             SYNTAX             Unsigned32 (0..350000000)
             UNITS              "bps"
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                 "Latency Path 1 downstream maximum payload data rate."
         ::= { adGenVdsl2LineConfProfileEntry 48 }


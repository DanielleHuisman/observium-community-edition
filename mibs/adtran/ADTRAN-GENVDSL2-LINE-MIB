         ::= { adGenVdsl2VturChanEntry 7 }


         -- ******************************************************************
         -- Group: PM
         --   The PM Group consists of the tables of entries necessary
         --   to monitor the performance and interval statistics for the
         --   VDSL2 physical and channel interfaces.
         -- ******************************************************************

         -- ========= PM Group (adGenVdsl2VtucPerfDataTable) =================
         adGenVdsl2VtucPerfDataTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VtucPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtuc.
                 VDSL2 physical interfaces are
                 those ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2PM 1}

         adGenVdsl2VtucPerfDataEntry       OBJECT-TYPE
             SYNTAX          AdGenVdsl2VtucPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in adGenVdsl2VtucPerfDataTable."


             INDEX { ifIndex }
         ::= { adGenVdsl2VtucPerfDataTable 1 }

         AdGenVdsl2VtucPerfDataEntry ::=
             SEQUENCE {
                 adGenVdsl2VtucPerfLofs                    Counter32,
                 adGenVdsl2VtucPerfLoss                    Counter32,
                 adGenVdsl2VtucPerfLols                    Counter32,
                 adGenVdsl2VtucPerfLprs                    Counter32,
                 adGenVdsl2VtucPerfEs                      Counter32,
                 adGenVdsl2VtucPerfInits                   Counter32,
                 adGenVdsl2VtucPerfSes                     Counter32,
                 adGenVdsl2VtucPerfUas                     Counter32,
                 adGenVdsl2VtucPerfFecs                    Counter32,
                 adGenVdsl2VtucPerfCrc                     Counter32,
                 adGenVdsl2VtucPerfFec                     Counter32,
                 adGenVdsl2VtucPerfValidIntervals          Integer32,
                 adGenVdsl2VtucPerfInvalidIntervals        Integer32,
                 adGenVdsl2VtucPerfCurr15MinTimeElapsed    Gauge32,
                 adGenVdsl2VtucPerfCurr15MinLofs           Counter32,
                 adGenVdsl2VtucPerfCurr15MinLoss           Counter32,
                 adGenVdsl2VtucPerfCurr15MinLols           Counter32,
                 adGenVdsl2VtucPerfCurr15MinLprs           Counter32,
                 adGenVdsl2VtucPerfCurr15MinEs             Counter32,
                 adGenVdsl2VtucPerfCurr15MinInits          Counter32,
                 adGenVdsl2VtucPerfCurr15MinSes            Counter32,
                 adGenVdsl2VtucPerfCurr15MinUas            Counter32,
                 adGenVdsl2VtucPerfCurr15MinFecs           Counter32,
                 adGenVdsl2VtucPerfCurr15MinCrc            Counter32,
                 adGenVdsl2VtucPerfCurr15MinFec            Counter32,
                 adGenVdsl2VtucPerfCurr1DayTimeElapsed     Gauge32,
                 adGenVdsl2VtucPerfCurr1DayLofs            Counter32,
                 adGenVdsl2VtucPerfCurr1DayLoss            Counter32,
                 adGenVdsl2VtucPerfCurr1DayLols            Counter32,
                 adGenVdsl2VtucPerfCurr1DayLprs            Counter32,
                 adGenVdsl2VtucPerfCurr1DayEs              Counter32,
                 adGenVdsl2VtucPerfCurr1DayInits           Counter32,
                 adGenVdsl2VtucPerfCurr1DaySes             Counter32,
                 adGenVdsl2VtucPerfCurr1DayUas             Counter32,
                 adGenVdsl2VtucPerfCurr1DayFecs            Counter32,
                 adGenVdsl2VtucPerfCurr1DayCrc             Counter32,
                 adGenVdsl2VtucPerfCurr1DayFec             Counter32,
                 adGenVdsl2VtucPerfTcTxUnits               Counter32,
                 adGenVdsl2VtucPerfTcTxDataUnits           Counter32,
                 adGenVdsl2VtucPerfTcTxDataOctets          Counter32,
                 adGenVdsl2VtucPerfTcTxIdleUnits           Counter32,
                 adGenVdsl2VtucPerfTcRxUnits               Counter32,
                 adGenVdsl2VtucPerfTcRxDataUnits           Counter32,
                 adGenVdsl2VtucPerfTcRxDataOctets          Counter32,
                 adGenVdsl2VtucPerfTcRxIdleUnits           Counter32,
                 adGenVdsl2VtucPerfTcRxErroredUnits        Counter32,
                 adGenVdsl2VtucPerfSraDownshifts           Counter32,
                 adGenVdsl2VtucPerfSraUpshifts             Counter32,
                 adGenVdsl2VtucPerfCurr15MinSraDownshifts  Counter32,
                 adGenVdsl2VtucPerfCurr15MinSraUpshifts    Counter32,
                 adGenVdsl2VtucPerfCurr15MinSraRateMax     Gauge32,
                 adGenVdsl2VtucPerfCurr15MinSraRateMin     Gauge32,
                 adGenVdsl2VtucPerfCurr1DaySraDownshifts   Counter32,
                 adGenVdsl2VtucPerfCurr1DaySraUpshifts     Counter32,
                 adGenVdsl2VtucPerfCurr1DaySraRateMax      Gauge32,
                 adGenVdsl2VtucPerfCurr1DaySraRateMin      Gauge32,
                 adGenVdsl2VtucPerfRtxMinEftr              Gauge32,
                 adGenVdsl2VtucPerfRtxLeftrs               Counter32,
                 adGenVdsl2VtucPerfCurr15MinRtxMinEftr     Gauge32,
                 adGenVdsl2VtucPerfCurr15MinRtxLeftrs      Counter32,
                 adGenVdsl2VtucPerfCurr1DayRtxMinEftr      Gauge32,
                 adGenVdsl2VtucPerfCurr1DayRtxLeftrs       Counter32
             }

         adGenVdsl2VtucPerfLofs OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of Loss of Framing failures since
                 agent reset."
         ::= { adGenVdsl2VtucPerfDataEntry 1 }

         adGenVdsl2VtucPerfLoss  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current


             DESCRIPTION
                 "Count of the number of Loss of Signal failures since
                 agent reset."
         ::= { adGenVdsl2VtucPerfDataEntry 2 }

         adGenVdsl2VtucPerfLols  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current


             DESCRIPTION
                 "Count of the number of Loss of Link failures since
                 agent reset."
         ::= { adGenVdsl2VtucPerfDataEntry 3 }

        adGenVdsl2VtucPerfLprs  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of Loss of Power failures since
                 agent reset."
         ::= { adGenVdsl2VtucPerfDataEntry 4 }

         adGenVdsl2VtucPerfEs  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Errored Seconds since the unit was last reset.
                 An Errored Second is a one-second interval containing one
                 or more CRC anomalies, or one or more LOS or LOF defects."
             REFERENCE    "T1E1.4/2000-009R3, Part 1, common spec"
         ::= { adGenVdsl2VtucPerfDataEntry 5 }

         adGenVdsl2VtucPerfInits OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the line initialization attempts since
                 agent reset. Includes both successful and failed
                 attempts."
         ::= { adGenVdsl2VtucPerfDataEntry 6 }

         adGenVdsl2VtucPerfSes  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Severely Errored Seconds since the unit was last
                  reset."
         ::= { adGenVdsl2VtucPerfDataEntry 7 }

         adGenVdsl2VtucPerfUas  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Unavailable Seconds since the unit was last
                  reset."
         ::= { adGenVdsl2VtucPerfDataEntry 8 }

         adGenVdsl2VtucPerfFecs  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of FEC Seconds since agent
                 reset."
         ::= { adGenVdsl2VtucPerfDataEntry 9 }

         adGenVdsl2VtucPerfCrc  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of CRC anomolies since agent

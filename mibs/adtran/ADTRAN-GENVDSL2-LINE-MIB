         -- ========= PM Group (adGenVdsl2VtucIntervalTable) =================
         adGenVdsl2VtucIntervalTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VtucIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtuc
                 performance data collection interval.
                 VDSL2 physical interfaces are
                 those ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2PM 3 }

         adGenVdsl2VtucIntervalEntry   OBJECT-TYPE
             SYNTAX          AdGenVdsl2VtucIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2VtucIntervalTable."
             INDEX           { ifIndex, adGenVdsl2VtucIntervalNumber }
         ::= { adGenVdsl2VtucIntervalTable 1 }

         AdGenVdsl2VtucIntervalEntry ::=
             SEQUENCE {
                 adGenVdsl2VtucIntervalNumber          Integer32,
                 adGenVdsl2VtucIntervalLofs            Counter32,
                 adGenVdsl2VtucIntervalLoss            Counter32,
                 adGenVdsl2VtucIntervalLols            Counter32,
                 adGenVdsl2VtucIntervalLprs            Counter32,
                 adGenVdsl2VtucIntervalES              Counter32,
                 adGenVdsl2VtucIntervalInits           Counter32,
                 adGenVdsl2VtucIntervalValidData         INTEGER,
                 adGenVdsl2VtucIntervalSES             Counter32,
                 adGenVdsl2VtucIntervalUAS             Counter32,
                 adGenVdsl2VtucIntervalFECs            Counter32,
                 adGenVdsl2VtucIntervalFEC             Counter32,
                 adGenVdsl2VtucIntervalCRC             Counter32,
                 adGenVdsl2VtucIntervalSraDownshifts   Counter32,
                 adGenVdsl2VtucIntervalSraUpshifts     Counter32,
                 adGenVdsl2VtucIntervalSraRateMax        Gauge32,
                 adGenVdsl2VtucIntervalSraRateMin        Gauge32,
                 adGenVdsl2VtucIntervalRtxMinEftr        Gauge32,
                 adGenVdsl2VtucIntervalRtxLeftrs       Counter32
             }

         adGenVdsl2VtucIntervalNumber OBJECT-TYPE
             SYNTAX      Integer32(1..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Performance Data Interval number 1 is the
                 the most recent previous interval; interval
                 96 is 24 hours ago.  Intervals 2..96 are
                 optional."
         ::= { adGenVdsl2VtucIntervalEntry 1 }

         adGenVdsl2VtucIntervalLofs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval when there was Loss
                 of Framing."
         ::= { adGenVdsl2VtucIntervalEntry 2 }

         adGenVdsl2VtucIntervalLoss  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval when there was Loss
                  of Signal."
         ::= { adGenVdsl2VtucIntervalEntry 3 }

         adGenVdsl2VtucIntervalLols  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval when there was Loss of Link."
         ::= { adGenVdsl2VtucIntervalEntry 4 }

         adGenVdsl2VtucIntervalLprs  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval when there was Loss
                  of Power."
         ::= { adGenVdsl2VtucIntervalEntry 5 }

         adGenVdsl2VtucIntervalES OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Errored Seconds in the interval."
         ::= { adGenVdsl2VtucIntervalEntry 6 }

         adGenVdsl2VtucIntervalInits  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the line initialization attempts
                 during the interval. Includes both successful
                 and failed attempts."
         ::= { adGenVdsl2VtucIntervalEntry 7 }

         adGenVdsl2VtucIntervalValidData OBJECT-TYPE
             SYNTAX      INTEGER {
                 valid (1),            -- no channels exist
                 notValid (2)          -- fast channel exists only
             }
             MAX-ACCESS read-only
             STATUS current
             DESCRIPTION
                 "This variable indicates if the data for this
                 interval is valid."
         ::= { adGenVdsl2VtucIntervalEntry 8 }

         adGenVdsl2VtucIntervalSES OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Severely Errored Seconds in the interval."
         ::= { adGenVdsl2VtucIntervalEntry 9 }

         adGenVdsl2VtucIntervalUAS OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Unavailable Seconds in the interval."
         ::= { adGenVdsl2VtucIntervalEntry 10 }

         adGenVdsl2VtucIntervalFECs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval during which at
                 least one forward error correction event took
                 place."
         ::= { adGenVdsl2VtucIntervalEntry 11 }

         adGenVdsl2VtucIntervalFEC OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of forward error correction events that
                 took place in the interval."
         ::= { adGenVdsl2VtucIntervalEntry 12 }

         adGenVdsl2VtucIntervalCRC OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of CRC anomolies that took place in the interval."
         ::= { adGenVdsl2VtucIntervalEntry 13 }

         adGenVdsl2VtucIntervalSraDownshifts OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "downshifts"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of SRA payload rate downshifts since last
                 successful train in the specified 15-min interval"
         ::= { adGenVdsl2VtucIntervalEntry 14 }

         adGenVdsl2VtucIntervalSraUpshifts OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "upshifts"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of SRA payload rate upshifts since last
                 successful train in the specified 15-min interval"
         ::= { adGenVdsl2VtucIntervalEntry 15 }

         adGenVdsl2VtucIntervalSraRateMax OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Max payload rate in the specified 15-min interval
                 since last successful train."
         ::= { adGenVdsl2VtucIntervalEntry 16 }

         adGenVdsl2VtucIntervalSraRateMin OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Min payload rate in the specified 15-min interval
                 since last successful train."
         ::= { adGenVdsl2VtucIntervalEntry 17 }

         adGenVdsl2VtucIntervalRtxMinEftr OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Retransmission parameter defined as the minimum of
                  the error-free throughput rate observed in the specified
                  15-min interval."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.4.3"
         ::= { adGenVdsl2VtucIntervalEntry 18 }

         adGenVdsl2VtucIntervalRtxLeftrs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of low error-free throughput rate defect
                  seconds in the specified 15-min interval."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.4.1"
         ::= { adGenVdsl2VtucIntervalEntry 19 }


         -- ========= PM Group (adGenVdsl2VturIntervalTable) =================
         adGenVdsl2VturIntervalTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VturIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtur
                 performance data collection 1 Day interval.
                 VDSL2 physical interfaces are those
                 ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2PM 4 }

         adGenVdsl2VturIntervalEntry   OBJECT-TYPE
             SYNTAX          AdGenVdsl2VturIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2VturIntervalTable."
             INDEX           { ifIndex, adGenVdsl2VturIntervalNumber }
         ::= { adGenVdsl2VturIntervalTable 1 }

         AdGenVdsl2VturIntervalEntry ::=
             SEQUENCE {
                 adGenVdsl2VturIntervalNumber          Integer32,
                 adGenVdsl2VturIntervalLofs            Counter32,
                 adGenVdsl2VturIntervalLoss            Counter32,
                 adGenVdsl2VturIntervalLprs            Counter32,
                 adGenVdsl2VturIntervalES              Counter32,
                 adGenVdsl2VturIntervalValidData         INTEGER,
                 adGenVdsl2VturIntervalSES             Counter32,
                 adGenVdsl2VturIntervalUAS             Counter32,
                 adGenVdsl2VturIntervalFECs            Counter32,
                 adGenVdsl2VturIntervalFEC             Counter32,
                 adGenVdsl2VturIntervalCRC             Counter32,
                 adGenVdsl2VturIntervalSraDownshifts   Counter32,
                 adGenVdsl2VturIntervalSraUpshifts     Counter32,
                 adGenVdsl2VturIntervalSraRateMax        Gauge32,
                 adGenVdsl2VturIntervalSraRateMin        Gauge32,
                 adGenVdsl2VturIntervalRtxMinEftr        Gauge32,
                 adGenVdsl2VturIntervalRtxLeftrs       Counter32
             }

         adGenVdsl2VturIntervalNumber OBJECT-TYPE
             SYNTAX      Integer32(1..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Performance Data Interval number 1 is the
                 the most recent previous interval; interval
                 96 is 24 hours ago.  Intervals 2..96 are
                 optional."

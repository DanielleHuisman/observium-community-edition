                 adGenVdsl2ConfAmRadioFreqMask1              INTEGER,
                 adGenVdsl2ConfAmRadioFreqMask2              INTEGER,
                 adGenVdsl2ConfAmRadioFreqMask3              INTEGER,
                 adGenVdsl2ConfAmRadioFreqMask4              INTEGER,
                 adGenVdsl2ConfAmRadioFreqMask5              INTEGER,

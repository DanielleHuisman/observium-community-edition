             INDEX           { ifIndex, adGenVdsl2VturChannelNumber }
         ::= { adGenVdsl2VturChanTable 1 }

         AdGenVdsl2VturChanEntry ::=
             SEQUENCE {
                 adGenVdsl2VturChannelNumber           Integer32,
                 adGenVdsl2VturChanInterleaveDelay     Gauge32,
                 adGenVdsl2VturChanCurrTxRate          Gauge32,
                 adGenVdsl2VturChanPrevTxRate          Gauge32,
                 adGenVdsl2VturChanCrcBlockLength      Gauge32,
                 adGenVdsl2VturChanINP                 Gauge32,
                 adGenVdsl2VturChanINPRein             Gauge32
             }

         adGenVdsl2VturChannelNumber OBJECT-TYPE
             SYNTAX      Integer32(1..4)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "VDSL2 logical channel. Channel values are 1 - 4."
         ::= { adGenVdsl2VturChanEntry 1 }

         adGenVdsl2VturChanInterleaveDelay OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "0.1 milli-seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Interleave Delay for this channel.

                 Interleave delay applies only to the
                 interleave channel and defines the mapping
                 (relative spacing) between subsequent input
                 bytes at the interleaver input and their placement
                 in the bit stream at the interleaver output.
                 Larger numbers provide greater separation between
                 consecutive input bytes in the output bit stream
                 allowing for improved impulse noise immunity at
                 the expense of payload latency.

                 In the case where the ifType is Fast(125), use


                 noSuchObject."
         ::= { adGenVdsl2VturChanEntry 2 }


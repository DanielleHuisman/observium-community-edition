DEFINITIONS ::= BEGIN

    IMPORTS
        Integer32,
        Unsigned32,
        Counter32,
        Gauge32,
        NOTIFICATION-TYPE,
        OBJECT-TYPE,
        enterprises,
         MODULE-IDENTITY
           FROM SNMPv2-SMI
         OBJECT-GROUP
             FROM SNMPv2-CONF
        ifIndex
           FROM IF-MIB
        RowStatus,
        DisplayString,
        TEXTUAL-CONVENTION,
        TimeStamp
          FROM SNMPv2-TC
        sysName
            FROM SNMPv2-MIB
-- from [RFC2579]
        adGenPortTrapIdentifier
            FROM ADTRAN-GENPORT-MIB
        adTrapInformSeqNum
            FROM ADTRAN-GENTRAPINFORM-MIB
	adGenSlotInfoIndex
            FROM ADTRAN-GENSLOT-MIB
        adGenVdsl2,
--	adtran,
--	adProducts,
        adGenVdsl2ID
            FROM ADTRAN-SHARED-VDSL2-MIB
        ;

adGenVdsl2MIB MODULE-IDENTITY
   LAST-UPDATED "202005110000Z"
   ORGANIZATION "Adtran, Inc."
   CONTACT-INFO "http://www.adtran.com"
   DESCRIPTION

                 adGenVdsl2ConfUsLp0CustomRtxLeftrThresh     INTEGER,

                 adGenVdsl2ConfDsLp0RtxSetting               INTEGER,
                 adGenVdsl2ConfDsLp0CustomRtxMaxNdr          Unsigned32,
                 adGenVdsl2ConfDsLp0CustomRtxMinDelay        INTEGER,
                 adGenVdsl2ConfDsLp0CustomRtxMaxDelay        INTEGER,
                 adGenVdsl2ConfDsLp0CustomRtxInpMinShine     INTEGER,
                 adGenVdsl2ConfDsLp0CustomRtxInpMinRein      INTEGER,
                 adGenVdsl2ConfDsLp0CustomRtxIatRein         INTEGER,

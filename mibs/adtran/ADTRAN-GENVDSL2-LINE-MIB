         adGenVdsl2ConfDsLp1MaxDelay OBJECT-TYPE
             SYNTAX             Unsigned32 (0..63)
             UNITS              "msec"
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "Latency Path 1 downstream maximum Interleave Delay.
                 This parameter is the maximum one-way interleaving delay
                 in milli-seconds introduced by the PMS-TC between the
                 alfa and the beta reference points, in the direction of
                 the latency path.  A delay of 0 indicates no maximum delay
                 restrictions are imposed.
                "
         ::= { adGenVdsl2LineConfProfileEntry 50 }

         adGenVdsl2ConfDsLp1MinProtection OBJECT-TYPE
             SYNTAX             Unsigned32 (0..32)
             UNITS              "0.5 Sym"
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "Latency Path 1 downstream Minimum Impulse Noise Protection.
                 This parameter specifies the minimum impulse noise
                 protection for the latency path in 0.5 fractions of DMT
                 symbols.  A value of 0 indicates no minimum bound.
                "
         ::= { adGenVdsl2LineConfProfileEntry 51 }

         adGenVdsl2ConfDsLp1RaRatio OBJECT-TYPE
             SYNTAX             Integer32 (0..100)
             UNITS              "%"
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "This parameter (expressed in %) specifies the ratio that
                 should be taken into account for the latency path when
                 performing rate adaptation in the direction of the
                 channel. The ratio is defined as a percentage in the
                 0 to 100 range.  The sum of all active latency paths
                 in a given direction ratios shall add up to 100%.
                "
         ::= { adGenVdsl2LineConfProfileEntry 52 }


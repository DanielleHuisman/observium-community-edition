                 adGenVdsl2ConfDsLp1CustomRtxMaxDelay        INTEGER,
                 adGenVdsl2ConfDsLp1CustomRtxInpMinShine     INTEGER,
                 adGenVdsl2ConfDsLp1CustomRtxInpMinRein      INTEGER,
                 adGenVdsl2ConfDsLp1CustomRtxIatRein         INTEGER,

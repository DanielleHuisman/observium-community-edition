         ::= { adGenVdsl2VturPerfDataEntry 18 }

         adGenVdsl2VturPerfCurr15MinFecs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of FEC Seconds in the current 15 minute interval."
         ::= { adGenVdsl2VturPerfDataEntry 19 }

         adGenVdsl2VturPerfCurr15MinCrc OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of CRC anomolies in the current 15 minute interval."
         ::= { adGenVdsl2VturPerfDataEntry 20 }

         adGenVdsl2VturPerfCurr15MinFec OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of FEC anomolies in the current 15 minute interval."
         ::= { adGenVdsl2VturPerfDataEntry 21 }

         adGenVdsl2VturPerfCurr1DayTimeElapsed OBJECT-TYPE
             SYNTAX      Gauge32(0..86399)
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Number of seconds that have elapsed since the
                 beginning of the current 1-day interval."
         ::= { adGenVdsl2VturPerfDataEntry 22 }

         adGenVdsl2VturPerfCurr1DayLofs  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the number of seconds when there was Loss
                 of Framing during the current day as measured by
                 adGenVdsl2VturPerfCurr1DayTimeElapsed."
         ::= { adGenVdsl2VturPerfDataEntry 23 }


         -- ========= PM Group (adGenVdsl2Vtuc1DayIntervalTable) =================
         adGenVdsl2Vtuc1DayIntervalTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2Vtuc1DayIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtuc
                 performance data collection 1 day interval.
                 VDSL2 physical interfaces are
                 those ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2PM 9 }

         adGenVdsl2Vtuc1DayIntervalEntry   OBJECT-TYPE
             SYNTAX          AdGenVdsl2Vtuc1DayIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2Vtuc1DayIntervalTable."
             INDEX           { ifIndex, adGenVdsl2Vtuc1DayIntervalNumber }
         ::= { adGenVdsl2Vtuc1DayIntervalTable 1 }

         AdGenVdsl2Vtuc1DayIntervalEntry ::=
             SEQUENCE {
                 adGenVdsl2Vtuc1DayIntervalNumber         Integer32,
                 adGenVdsl2Vtuc1DayIntervalLofs           Counter32,
                 adGenVdsl2Vtuc1DayIntervalLoss           Counter32,
                 adGenVdsl2Vtuc1DayIntervalLols           Counter32,
                 adGenVdsl2Vtuc1DayIntervalLprs           Counter32,
                 adGenVdsl2Vtuc1DayIntervalES             Counter32,
                 adGenVdsl2Vtuc1DayIntervalInits          Counter32,
                 adGenVdsl2Vtuc1DayIntervalValidData        INTEGER,
                 adGenVdsl2Vtuc1DayIntervalSES            Counter32,
                 adGenVdsl2Vtuc1DayIntervalUAS            Counter32,
                 adGenVdsl2Vtuc1DayIntervalFECs           Counter32,
                 adGenVdsl2Vtuc1DayIntervalFEC            Counter32,
                 adGenVdsl2Vtuc1DayIntervalCRC            Counter32,
                 adGenVdsl2Vtuc1DayIntervalSraDownshifts  Counter32,
                 adGenVdsl2Vtuc1DayIntervalSraUpshifts    Counter32,
                 adGenVdsl2Vtuc1DayIntervalSraRateMax       Gauge32,
                 adGenVdsl2Vtuc1DayIntervalSraRateMin       Gauge32,
                 adGenVdsl2Vtuc1DayIntervalRtxMinEftr       Gauge32,
                 adGenVdsl2Vtuc1DayIntervalRtxLeftrs      Counter32
             }

         adGenVdsl2Vtuc1DayIntervalNumber OBJECT-TYPE
             SYNTAX      Integer32(1..7)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Performance Data Interval number 1 is the
                 the most recent previous 1 day interval;
                 interval 7 is 7 Days ago.  Intervals 1..7 are
                 optional."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 1 }

         adGenVdsl2Vtuc1DayIntervalLofs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval when there was Loss
                 of Framing."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 2 }

         adGenVdsl2Vtuc1DayIntervalLoss  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the 1 Day interval when there was Loss
                  of Signal."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 3 }

         adGenVdsl2Vtuc1DayIntervalLols  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the 1 Day interval when there was
                  Loss of Link."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 4 }

         adGenVdsl2Vtuc1DayIntervalLprs  OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the 1 Day interval when there was Loss
                  of Power."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 5 }

         adGenVdsl2Vtuc1DayIntervalES OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Errored Seconds in the interval."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 6 }

         adGenVdsl2Vtuc1DayIntervalInits  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of the line initialization attempts
                 during the 1 Day interval. Includes both successful
                 and failed attempts."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 7 }


         adGenVdsl2Vtuc1DayIntervalValidData OBJECT-TYPE
             SYNTAX      INTEGER {
                 valid (1),            -- no channels exist
                 notValid (2)          -- fast channel exists only
             }
             MAX-ACCESS read-only
             STATUS current
             DESCRIPTION
                 "This variable indicates if the data for this 1 Day
                 interval is valid."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 8 }

         adGenVdsl2Vtuc1DayIntervalSES OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Severely Errored Seconds in the 1 Day interval."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 9 }

         adGenVdsl2Vtuc1DayIntervalUAS OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Unavailable Seconds in the 1 Day interval."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 10 }

         adGenVdsl2Vtuc1DayIntervalFECs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval during which at
                 least one forward error correction event took
                 place."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 11 }

         adGenVdsl2Vtuc1DayIntervalFEC OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of forward error correction events that
                 took place in the 1 Day interval."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 12 }

         adGenVdsl2Vtuc1DayIntervalCRC OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of CRC anomolies that took place in the 1 Day interval."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 13 }

         adGenVdsl2Vtuc1DayIntervalSraDownshifts OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "downshifts"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of SRA payload rate downshifts since last
                 successful train in the specified 24-hr interval"
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 14 }

         adGenVdsl2Vtuc1DayIntervalSraUpshifts OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "upshifts"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of SRA payload rate upshifts since last
                 successful train in the specified 24-hr interval"
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 15 }

         adGenVdsl2Vtuc1DayIntervalSraRateMax OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Max payload rate in the specified 24-hr interval
                 since last successful train."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 16 }

         adGenVdsl2Vtuc1DayIntervalSraRateMin OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Min payload rate in the specified 24-hr interval
                 since last successful train."
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 17 }

         adGenVdsl2Vtuc1DayIntervalRtxMinEftr OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Retransmission parameter defined as the minimum of
                  the error-free throughput rate observed in the specified
                  24-hr interval."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.4.3"
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 18 }


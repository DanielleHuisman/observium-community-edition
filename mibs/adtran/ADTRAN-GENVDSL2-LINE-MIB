             DEFVAL { 0 }
         ::= { adGenVdsl2LineConfProfileEntry 112 }

         adGenVdsl2ConfAmRadioFreqMask4 OBJECT-TYPE
             SYNTAX             INTEGER (0|540..1700)
             UNITS              "kHz"
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                 "AM radio station 4 center frequency in kHz. Carrier
                 frequencies around this center frequency will be masked
                 off. The value 0 will disable the masking of the carrier
                 frequencies around radio station 4 center frequency."
             DEFVAL { 0 }
         ::= { adGenVdsl2LineConfProfileEntry 113 }

         adGenVdsl2ConfAmRadioFreqMask5 OBJECT-TYPE
             SYNTAX             INTEGER (0|540..1700)
             UNITS              "kHz"
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                 "AM radio station 5 center frequency in kHz. Carrier
                 frequencies around this center frequency will be masked
                 off. The value 0 will disable the masking of the carrier
                 frequencies around radio station 5 center frequency."
             DEFVAL { 0 }
         ::= { adGenVdsl2LineConfProfileEntry 114 }

         adGenVdsl2ConfAmRadioFreqMask6 OBJECT-TYPE
             SYNTAX             INTEGER (0|540..1700)
             UNITS              "kHz"
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                 "AM radio station 6 center frequency in kHz. Carrier
                 frequencies around this center frequency will be masked
                 off. The value 0 will disable the masking of the carrier
                 frequencies around radio station 6 center frequency."
             DEFVAL { 0 }
         ::= { adGenVdsl2LineConfProfileEntry 115 }

         adGenVdsl2ConfMemorySplitRatio OBJECT-TYPE
             SYNTAX  INTEGER {
                 oneToOne  (1),       -- DS:US interleaver memory allocation ratio of 1:1
                 twoToOne  (2),       -- DS:US interleaver memory allocation ratio of 2:1
                 threeToOne(3),       -- DS:US interleaver memory allocation ratio of 3:1
                 fourToOne (4),       -- DS:US interleaver memory allocation ratio of 4:1
                 fiveToOne (5)        -- DS:US interleaver memory allocation ratio of 5:1
             }
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                 "This parameter defines the ratio of how the interleaver memory
		  is allocated between the downstream and upstream directions.
		  For example a ratio of 2:1 will assign twice as much interleaver
		  memory to the downstream path than the upstream."
             DEFVAL { twoToOne }
         ::= { adGenVdsl2LineConfProfileEntry 116 }

         adGenVdsl2ConfRateAdaptConfigMode OBJECT-TYPE
             SYNTAX  INTEGER {
                 manual  (1),       -- user inputs for shift margins and times are used for SRA.
                 auto    (2),       -- automatic settings based on target margin and fixed times are used for SRA.
             }
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                 "This parameter determines how the upshift and downshift margins and times
                  are configured when the rate mode is set to adaptAtRuntime."
             DEFVAL { manual }
         ::= { adGenVdsl2LineConfProfileEntry 117 }

         -- ============ Prov Group (Alarm Profile) ==========================
         adGenVdsl2LineAlarmConfProfileTable    OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2LineAlarmConfProfileEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table contains information on the VDSL2 line
                 configuration.  One entry in this table reflects a
                 profile defined by a manager which can be used to
                 configure the modem for a physical line"
         ::= { adGenVdsl2Prov 2 }

         adGenVdsl2LineAlarmConfProfileEntry    OBJECT-TYPE
             SYNTAX          AdGenVdsl2LineAlarmConfProfileEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "Each entry consists of a list of parameters that
                 represents the configuration of an VDSL2 modem.

                 `Static' profiles -  profiles are automaticly
		 created or destroyed as VDSL2 physical lines
		 are discovered and removed by the system.
		 The name of the profile will be equivalent to
                 the decimal value of the line's interface index.
                "
             INDEX { IMPLIED adGenVdsl2LineAlarmConfProfileName }

         ::= { adGenVdsl2LineAlarmConfProfileTable 1}

         AdGenVdsl2LineAlarmConfProfileEntry ::=
             SEQUENCE {
                 adGenVdsl2LineAlarmConfProfileName          DisplayString,
                 adGenVdsl2VtucThreshSnrMgn                  Integer32,
                 adGenVdsl2VtucThresh15MinLofs               Gauge32,
                 adGenVdsl2VtucThresh15MinLoss               Gauge32,
                 adGenVdsl2VtucThresh15MinLols               Gauge32,
                 adGenVdsl2VtucThresh15MinLprs               Gauge32,
                 adGenVdsl2VtucThresh15MinEs                 Gauge32,
                 adGenVdsl2VtucThresh15MinSes                Gauge32,
                 adGenVdsl2VtucThresh15MinUas                Gauge32,
                 adGenVdsl2VturThreshSnrMgn                  Integer32,
                 adGenVdsl2VturThresh15MinLofs               Gauge32,
                 adGenVdsl2VturThresh15MinLoss               Gauge32,
                 adGenVdsl2VturThresh15MinLprs               Gauge32,
                 adGenVdsl2VturThresh15MinEs                 Gauge32,
                 adGenVdsl2VturThresh15MinSes                Gauge32,
                 adGenVdsl2VturThresh15MinUas                Gauge32,
                 adGenVdsl2LineAlarmConfProfileRowStatus     RowStatus,
                 adGenVdsl2ThreshUsLp0RateUp                 Gauge32,
                 adGenVdsl2ThreshUsLp0RateDown               Gauge32,
                 adGenVdsl2ThreshDsLp0RateUp                 Gauge32,
                 adGenVdsl2ThreshDsLp0RateDown               Gauge32,
                 adGenVdsl2ThreshUsLp1RateUp                 Gauge32,
                 adGenVdsl2ThreshUsLp1RateDown               Gauge32,
                 adGenVdsl2ThreshDsLp1RateUp                 Gauge32,
                 adGenVdsl2ThreshDsLp1RateDown               Gauge32
             }

         adGenVdsl2LineAlarmConfProfileName    OBJECT-TYPE
             SYNTAX          DisplayString (SIZE (1..10))
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This object is used by the line configuration table
                 in order to identify a row of this table.

                 This value cannot be user-specified and is not dynamic.

                 There is a one-to-one relationship between each line and
                 its profile.  The profile name algorithmicly represents
		 the Line's ifIndex.

                 Therefore, the profile's name is a decimalized string
                 of the ifIndex that is fixed-length (10) with
                 leading zero(s).  For example, the profile name for
                 ifIndex which equals '15' will be '0000000015'.
                "
         ::= { adGenVdsl2LineAlarmConfProfileEntry 1}

         adGenVdsl2VtucThreshSnrMgn OBJECT-TYPE
             SYNTAX      Integer32(0..99)
             UNITS       "dB"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "The SNR margin on the VDSL2 interface below which
                 the SNMP agent sends an adGenVdsl2USPerfSNRThresTrap.
                 One trap will be sent per interval per interface.
                 A value of '0' will disable the trap.
                "
         ::= { adGenVdsl2LineAlarmConfProfileEntry 2}

         adGenVdsl2VtucThresh15MinLofs OBJECT-TYPE
             SYNTAX      Gauge32(0..900)
             UNITS       "seconds"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "The number of Loss of Frame Seconds
                 encountered by an VDSL2 interface within any given 15
                 minutes performance data collection period, which
                 causes the SNMP agent to send an
                 adGenVdsl2VtucPerfLofsThreshTrap.
                 One trap will be sent per interval per interface.
                 A value of `0' will disable the trap.
                "
         ::= { adGenVdsl2LineAlarmConfProfileEntry 3}

         adGenVdsl2VtucThresh15MinLoss  OBJECT-TYPE
             SYNTAX      Gauge32(0..900)
             UNITS       "seconds"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "The number of Loss of Signal Seconds
                 encountered by VDSL2 interface within any given 15
                 minutes performance data collection period, which
                 causes the SNMP agent to send an
                 adGenVdsl2VtucPerfLossThreshTrap.
                 One trap will be sent per interval per interface.
                 A value of `0' will disable the trap.
                "
         ::= { adGenVdsl2LineAlarmConfProfileEntry 4}

         adGenVdsl2VtucThresh15MinLols  OBJECT-TYPE
             SYNTAX      Gauge32(0..900)
             UNITS       "seconds"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "The number of Loss of Link Seconds
                 encountered by VDSL2 interface within any given 15
                 minutes performance data collection period, which
                 causes the SNMP agent to send an
                 adGenVdsl2VtucPerfLolsThreshTrap.
                 One trap will be sent per interval per interface.
                 A value of `0' will disable the trap.
                "
         ::= { adGenVdsl2LineAlarmConfProfileEntry 5}

         adGenVdsl2VtucThresh15MinLprs  OBJECT-TYPE
             SYNTAX      Gauge32(0..900)
             UNITS       "seconds"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "The number of Loss of Power Seconds
                 encountered by an VDSL2 interface within any given 15
                 minutes performance data collection period, which
                 causes the SNMP agent to send an
                 adGenVdsl2VtucPerfLprsThreshTrap.
                 One trap will be sent per interval per interface.
                 A value of `0' will disable the trap.

         -- ========= PM Group (adGenVdsl2VtucChanIntervalTable) =============
         adGenVdsl2VtucChan1DayIntervalTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VtucChan1DayIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtuc channel's
                 performance data daily collection intervals.
                 VDSL2 channel interfaces are those ifEntries
                 where ifType is equal to Interleave(124)
                 or Fast(125)."
         ::= { adGenVdsl2PM 11 }

         adGenVdsl2VtucChan1DayIntervalEntry   OBJECT-TYPE
             SYNTAX          AdGenVdsl2VtucChan1DayIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2Vtuc1DayIntervalTable."
             INDEX           { ifIndex, adGenVdsl2Vtuc1DayChanNum ,adGenVdsl2VtucChan1DayIntervalNumber }
         ::= { adGenVdsl2VtucChan1DayIntervalTable 1 }

         AdGenVdsl2VtucChan1DayIntervalEntry ::=
             SEQUENCE {
                 adGenVdsl2Vtuc1DayChanNum                          Integer32,
                 adGenVdsl2VtucChan1DayIntervalNumber               Integer32,
                 adGenVdsl2VtucChan1DayIntervalReceivedBlks         Counter32,
                 adGenVdsl2VtucChan1DayIntervalTransmittedBlks      Counter32,
                 adGenVdsl2VtucChan1DayIntervalCorrectedBlks        Counter32,
                 adGenVdsl2VtucChan1DayIntervalUncorrectBlks        Counter32,
                 adGenVdsl2VtucChan1DayIntervalValidData            INTEGER,
                 adGenVdsl2VtucChan1DayIntervalRtxUncorrectedDtu    Counter32,
                 adGenVdsl2VtucChan1DayIntervalRtxCorrectedDtu      Counter32,
                 adGenVdsl2VtucChan1DayIntervalRtxRetransmittedDtu  Counter32
             }

         adGenVdsl2Vtuc1DayChanNum OBJECT-TYPE
             SYNTAX      Integer32(1..4)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "VDSL2 logical channel. Channel values are 1 - 4."
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 1 }

         adGenVdsl2VtucChan1DayIntervalNumber OBJECT-TYPE
             SYNTAX      Integer32(1..7)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Performance Data Interval number 1 is the
                 the most recent previous interval; interval
                 7 is 1 week ago.  Intervals 2..7 are
                 optional."
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 2 }

         adGenVdsl2VtucChan1DayIntervalReceivedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks received on this channel
                 during this interval."
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 3 }

         adGenVdsl2VtucChan1DayIntervalTransmittedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks transmitted on this
                 channel during this interval."
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 4 }

         adGenVdsl2VtucChan1DayIntervalCorrectedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with errors that were
                 corrected on this channel during this interval."
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 5 }

         adGenVdsl2VtucChan1DayIntervalUncorrectBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with uncorrectable
                 errors on this channel during this interval."
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 6 }

         adGenVdsl2VtucChan1DayIntervalValidData OBJECT-TYPE
             SYNTAX      INTEGER {
                 valid (1),
                 notValid (2)
             }
             MAX-ACCESS read-only
             STATUS current
             DESCRIPTION
                 "This variable indicates if the data for this
                 interval is valid."
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 7 }

         adGenVdsl2VtucChan1DayIntervalRtxUncorrectedDtu OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "dtus"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of DTUs in the specified 24-hr interval that have been
                  detected in error and that have not been corrected by one or
                  more retransmissions within the maximum delay constraint."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 12"
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 8 }

         adGenVdsl2VtucChan1DayIntervalRtxCorrectedDtu OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "dtus"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of DTUs in the specified 24-hr interval that have been
                  detected in error and that have been successfully corrected
                  by a retransmission within the maximum delay constraint."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 12"
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 9 }

         adGenVdsl2VtucChan1DayIntervalRtxRetransmittedDtu OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "dtus"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of DTUs in the specified 24-hr interval that have been
                  retransmitted."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 12"
         ::= { adGenVdsl2VtucChan1DayIntervalEntry 10 }



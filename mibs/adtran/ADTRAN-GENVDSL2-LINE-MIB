         -- ******************************************************************
         -- Group: ExtConfig
         --   The ExtConfig Group consists of the tables of entries
         --   necessary to maintain the extended VDSL2 line configuration.
         --   These entries are items that must be tracked per ifIndex
         --   (ie port) and not based on the profiles that could possibly
         --   be dynamically assigned.
         -- ******************************************************************

         adGenVdsl2ConfProfileExtTable   OBJECT-TYPE
         SYNTAX          SEQUENCE OF AdGenVdsl2ConfProfileExtEntry
         MAX-ACCESS      not-accessible
         STATUS          current
         DESCRIPTION
            "This table contains VDSL management profile configuration
             information not defined in the adGenVdsl2LineConfProfileTable
             from the VDSL-LINE-MIB.
            "
         ::= { adGenVdsl2ExtConfig 1 }

         adGenVdsl2ConfProfileExtEntry   OBJECT-TYPE
         SYNTAX          AdGenVdsl2ConfProfileExtEntry
         MAX-ACCESS      not-accessible
         STATUS          current
         DESCRIPTION
             "An entry extends the adslLineConfProfileEntry
              defined in VDSL-LINE-MIB.  Each entry corresponds
              to an VDSL line profile."
         INDEX { ifIndex }
         ::= { adGenVdsl2ConfProfileExtTable 1 }

         AdGenVdsl2ConfProfileExtEntry ::=
         SEQUENCE {
             adGenVdsl2LineManagementProfileNameApplied  DisplayString,
             adGenVdsl2LineManagementProfileIndexApplied Unsigned32,
             adGenVdsl2LineCircuitID                     DisplayString,
             adGenVdsl2LineResetCounters                 INTEGER,
             adGenVdsl2LineRetrainRequest                INTEGER
         }

         adGenVdsl2LineManagementProfileNameApplied OBJECT-TYPE
             SYNTAX  DisplayString --(SIZE (1..32))
             MAX-ACCESS  read-write
             STATUS  current
             DESCRIPTION
                 "Management profile name applied to the VDSL Port profile."
         ::= { adGenVdsl2ConfProfileExtEntry 1 }

         adGenVdsl2LineManagementProfileIndexApplied OBJECT-TYPE
             SYNTAX             Unsigned32
             MAX-ACCESS  read-write
             STATUS  current
             DESCRIPTION
                 "Management profile name applied to the VDSL Port profile."
         ::= { adGenVdsl2ConfProfileExtEntry 2 }

         adGenVdsl2LineCircuitID OBJECT-TYPE
             SYNTAX  DisplayString --(SIZE (1..32))
             MAX-ACCESS  read-write
             STATUS  current
             DESCRIPTION
                 "Circuit ID assigned to VDSL Port."
         ::= { adGenVdsl2ConfProfileExtEntry 3 }

         adGenVdsl2LineResetCounters OBJECT-TYPE
            SYNTAX      INTEGER
            {
                 all(1),              -- Reset both PM history and current counters
                 pmhistory(2),        -- Reset PM history counters
                 current(3),          -- Reset current counters
                 retrainne(4),        -- Retrain near-end counters
                 retrainfe(5),        -- Retrain far-end counters
                 rolling(6)           -- Reset rolling counters
            }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
              "Selection of counter types to reset/clear for the indexed interface.
               The counter types are performance monitoring intervals (PM history)
               which typically consist of 15 minute/24 hour interval data,
               current counters, near-end retrain counters, far-end retrain
               counters, and rolling counters. A read will always return all(1)."
            ::= { adGenVdsl2ConfProfileExtEntry 4 }

         adGenVdsl2LineRetrainRequest OBJECT-TYPE
            SYNTAX      INTEGER
            {
                 full(1),             -- request a full retrain/init
                 fast(2)              -- request a fast retrain/init
            }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
              "Request the selected line to perform a full retrain/init or a
               fast retrain/init. A read will always return full(1)."
            ::= { adGenVdsl2ConfProfileExtEntry 5 }


   -- ============ Prov Group (Band Profile) ===========================
         adGenVdsl2BandConfProfileTable    OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2BandConfProfileEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This table contains information on the VDSL2 Band
                 configuration.  One entry in this table reflects band
                 profile defined by a manager which can be used to
                 configure the VDSL2 bands for a given
                 adGenVdsl2LineConfProfileEntry."
         ::= { adGenVdsl2Prov 4 }

         adGenVdsl2BandConfProfileEntry    OBJECT-TYPE
             SYNTAX          AdGenVdsl2BandConfProfileEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "Each entry consists of a list of parameters that
                 represents the configuration of an VDSL2 modem
                 band parameters.  These entries extend that of the
                 adGenVdsl2LineConfProfileTable by band.
                "
             INDEX { adGenVdsl2LineConfProfileName, adGenVdsl2BandNumber }
         ::= { adGenVdsl2BandConfProfileTable 1}

         AdGenVdsl2BandConfProfileEntry ::=
             SEQUENCE {
                 adGenVdsl2BandNumber                        Integer32,
                 adGenVdsl2BandConfUsCustomPboCableA         Unsigned32,
                 adGenVdsl2BandConfUsCustomPboCableB         Unsigned32
             }

   adGenVdsl2BandNumber OBJECT-TYPE
       SYNTAX      Integer32 (1..4)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "VDSL2 band number. Band values are 1 - 5 corresponding
            to upstream bands U0..U4 and downstream bands D1..D5."
   ::= { adGenVdsl2BandConfProfileEntry 1 }

   adGenVdsl2BandConfUsCustomPboCableA OBJECT-TYPE
       SYNTAX             Unsigned32 (4000..8095)
       UNITS              "0.01 dBm/Hz"
       MAX-ACCESS         read-create
       STATUS             current
       DESCRIPTION
           "Upstream power back-off band cable A parameter.
            This parameter defines the UPBO reference PSD used
            to compute the upstream power backoff for each
            upstream band except US0. A UPBOPSD defined for each
            band shall consist of two parameters [A, B]. The UPBO
            reference PSD at the frequency f expressed in MHz shall
            be equal to -A - B*sqrt(f).  Parameter A ranges from
            40 dBm/Hz to 80.95 dBm/Hz in steps of 0.01 dBm/Hz. A special
            case of A=40.00 dBm/Hz and B=0.00 dBm/Hz disables UPBO in
            the respective band. The first entry U0 must be disabled
	    since it is not supported in the standard yet."
   ::= { adGenVdsl2BandConfProfileEntry 2 }

   adGenVdsl2BandConfUsCustomPboCableB OBJECT-TYPE
       SYNTAX             Unsigned32 (0..4095)
       UNITS              "0.01 dBm/Hz"
       MAX-ACCESS         read-create
       STATUS             current
       DESCRIPTION
           "Upstream power back-off band cable B parameter.
            This parameter defines the UPBO reference PSD used
            to compute the upstream power backoff for each
            upstream band except US0. A UPBOPSD defined for each
            band shall consist of two parameters [A, B]. The UPBO
            reference PSD at the frequency f expressed in MHz shall
            be equal to -A - B*sqrt(f).  Parameter B ranges from
            0 dBm/Hz to 40.95 dBm/Hz in steps of 0.01 dBm/Hz. A special
            case of A=40.00 dBm/Hz and B=0.00 dBm/Hz disables UPBO in
            the respective band. The first entry U0 must be disabled
	    since it is not supported in the standard yet."
   ::= { adGenVdsl2BandConfProfileEntry 3 }

   -- ============ Prov Group (Alarm Provisioning) ===========================
         adGenVdsl2AlarmSlotProvTable    OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2AlarmSlotProvEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This table sets alarm severity for all VDSL2 interfaces."
         ::= { adGenVdsl2Prov 5 }

         adGenVdsl2AlarmSlotProvEntry    OBJECT-TYPE
             SYNTAX          AdGenVdsl2AlarmSlotProvEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                ""
             INDEX { adGenSlotInfoIndex }
         ::= { adGenVdsl2AlarmSlotProvTable 1}

         AdGenVdsl2AlarmSlotProvEntry ::=
             SEQUENCE {
                 adGenVdsl2AlmSlotLinkdownSeverity               INTEGER
         }

         adGenVdsl2AlmSlotLinkdownSeverity OBJECT-TYPE
         SYNTAX INTEGER {
                                info(2),
                                alert(3),
                                minor(4),
                                major(5),
                                critical(6)
                        }
    	MAX-ACCESS read-write
    	STATUS current
    	DESCRIPTION
        	"Sets the Linkdown alarm severity (affects all VDSL2 Ports)"
    	DEFVAL    { major }
        ::= { adGenVdsl2AlarmSlotProvEntry 1 }

   -- ============ Status Group (Band Status) ===========================
         adGenVdsl2BandStatusTable    OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2BandStatusEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This table contains per band status information for VDSL2
                 configuration.  One entry in this table reflects band
                 profile defined by a manager which can be used to
                 configure the VDSL2 bands for a given
                 adGenVdsl2LineConfProfileEntry."
         ::= { adGenVdsl2Status 6 }

         adGenVdsl2BandStatusEntry    OBJECT-TYPE
             SYNTAX          AdGenVdsl2BandStatusEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "Each entry consists of a list of band status items.
                "
             INDEX {  ifIndex, adGenVdsl2BandNumber }
         ::= { adGenVdsl2BandStatusTable 1}

         AdGenVdsl2BandStatusEntry ::=
             SEQUENCE {
                 adGenVdsl2BandStatusNumber                  Integer32,
                 adGenVdsl2BandStatusUsStartCarrier          Unsigned32,
                 adGenVdsl2BandStatusUsStopCarrier           Unsigned32,
                 adGenVdsl2BandStatusUsMargin                Integer32,
                 adGenVdsl2BandStatusUsLatn                  Integer32,
                 adGenVdsl2BandStatusUsSatn                  Integer32,

                 adGenVdsl2BandStatusDsStartCarrier          Unsigned32,
                 adGenVdsl2BandStatusDsStopCarrier           Unsigned32,
                 adGenVdsl2BandStatusDsMargin                Integer32,
                 adGenVdsl2BandStatusDsLatn                  Integer32,
                 adGenVdsl2BandStatusDsSatn                  Integer32
             }

   adGenVdsl2BandStatusNumber OBJECT-TYPE
       SYNTAX      Integer32 (1..4)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "VDSL2 band number. Band values are 1 - 5 corresponding
            to upstream bands U0..U4 and downstream bands D1..D5."
   ::= { adGenVdsl2BandStatusEntry 1 }


   adGenVdsl2BandStatusUsStartCarrier OBJECT-TYPE
       SYNTAX      Unsigned32 (0..8192)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The starting carrier of the VDSL2 upstream band at 4.3125KHz
            spacing"
   ::= { adGenVdsl2BandStatusEntry 2 }

   adGenVdsl2BandStatusUsStopCarrier OBJECT-TYPE
       SYNTAX      Unsigned32 (0..8192)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The ending carrier of the VDSL2 upstream band at 4.3125KHz
            spacing"
   ::= { adGenVdsl2BandStatusEntry 3 }

   adGenVdsl2BandStatusUsMargin OBJECT-TYPE
       SYNTAX      Integer32 (-641..630)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream Signal-to-Noise Ratio Margin per Band. This parameter
            is defined by usable band. The upstream signal to noise ratio
            margin per band is the maximum increase in dB of the noise
            power received at the VTU-C such that the BER requirements are met
            for all bearer channels of the band. The upstream SNR margin per
            band ranges from -64.1 dB to +63.0 dB with 0.1 dB steps. A value of
            -64.1 indicates out of range.
           "
   ::= { adGenVdsl2BandStatusEntry 4 }


   adGenVdsl2BandStatusUsLatn OBJECT-TYPE
       SYNTAX      Integer32 (0..1270)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream Line Attenuation per Band. This parameter is defined per
            usable bands. It is the measured difference in the total power
            transmitted in this band and the total power received over all
            subcarriers of this band during initialization. The upstream
            line attenuation ranges per band from 0 to +127 dB with 0.1 dB
            steps.
           "
   ::= { adGenVdsl2BandStatusEntry 5 }


   adGenVdsl2BandStatusUsSatn OBJECT-TYPE
       SYNTAX      Integer32 (0..1270)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream Signal Attenuation per Band. This parameter is defined per
            usable bands. It is the measured difference in the total power
            transmitted in this band and the total power received over all
            subcarriers of this band during showtime. The upstream line
            attenuation ranges per band from 0 to +127 dB with 0.1 dB steps.
            During showtime, only a subset of the subcarriers may be transmitted
            as compared to initialization.  Therefore, the upstream Signal
            attenuation may be significantly lower than the downstream Line
            attenuation.
           "
   ::= { adGenVdsl2BandStatusEntry 6 }

   adGenVdsl2BandStatusDsStartCarrier OBJECT-TYPE
       SYNTAX      Unsigned32 (0..8192)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The starting carrier of the VDSL2 downstream band at 4.3125KHz
            spacing"
   ::= { adGenVdsl2BandStatusEntry 7 }

   adGenVdsl2BandStatusDsStopCarrier OBJECT-TYPE
       SYNTAX      Unsigned32 (0..8192)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The ending carrier of the VDSL2 downstream band at 4.3125KHz
            spacing"
   ::= { adGenVdsl2BandStatusEntry 8 }

   adGenVdsl2BandStatusDsMargin OBJECT-TYPE
       SYNTAX      Integer32 (-641..630)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Downstream Signal-to-Noise Ratio Margin per Band. This parameter
            is defined by usable band. The downstream signal to noise ratio
            margin per band is the maximum increase in dB of the noise
            power received at the VTU-R such that the BER requirements are met
            for all bearer channels of the band. The downstream SNR margin per
            band ranges from -64.1 dB to +63.0 dB with 0.1 dB steps. A value of
            -64.1 indicates out of range.
           "
   ::= { adGenVdsl2BandStatusEntry 9 }


   adGenVdsl2BandStatusDsLatn OBJECT-TYPE
       SYNTAX      Integer32 (0..1270)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Downstream Line Attenuation per Band. This parameter is defined per
            usable bands. It is the measured difference in the total power
            transmitted in this band and the total power received over all
            subcarriers of this band during initialization. The downstream
            line attenuation ranges per band from 0 to +127 dB with 0.1 dB
            steps.
           "
   ::= { adGenVdsl2BandStatusEntry 10 }


   adGenVdsl2BandStatusDsSatn OBJECT-TYPE
       SYNTAX      Integer32 (0..1270)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Downstream Signal Attenuation per Band. This parameter is defined per
            usable bands. It is the measured difference in the total power
            transmitted in this band and the total power received over all
            subcarriers of this band during showtime. The downstream line
            attenuation ranges per band from 0 to +127 dB with 0.1 dB steps.
            During showtime, only a subset of the subcarriers may be transmitted
            as compared to initialization.  Therefore, the downstream Signal
            attenuation may be significantly lower than the downstream Line
            attenuation.
           "
   ::= { adGenVdsl2BandStatusEntry 11 }


   -- ============ Status Group (Sub-Carrier Status) ========================
         adGenVdsl2ScStatusTable    OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2ScStatusEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This table contains per band status information for VDSL2
                 configuration.  One entry in this table reflects band
                 profile defined by a manager which can be used to
                 configure the VDSL2 bands for a given
                 adGenVdsl2LineConfProfileEntry."
         ::= { adGenVdsl2Status 7 }

         adGenVdsl2ScStatusEntry    OBJECT-TYPE
             SYNTAX          AdGenVdsl2ScStatusEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "Each entry consists of a list of band status items.
                "
             INDEX {  ifIndex, adGenVdsl2ScStatusNumber }
         ::= { adGenVdsl2ScStatusTable 1}

         AdGenVdsl2ScStatusEntry ::=
             SEQUENCE {
                 adGenVdsl2ScStatusNumber                  Integer32,

                 adGenVdsl2ScStatusUsBits                  Integer32,
                 adGenVdsl2ScStatusUsGain                  Integer32,
                 adGenVdsl2ScStatusUsSnr                   Integer32,

                 adGenVdsl2ScStatusDsBits                  Integer32,
                 adGenVdsl2ScStatusDsGain                  Integer32,
                 adGenVdsl2ScStatusDsSnr                   Integer32,

                 adGenVdsl2ScStatusUsQln                   Integer32,
                 adGenVdsl2ScStatusDsQln                   Integer32,
                 adGenVdsl2ScStatusUsHlog                  Integer32,
                 adGenVdsl2ScStatusDsHlog                  Integer32
             }

   adGenVdsl2ScStatusNumber OBJECT-TYPE
       SYNTAX      Integer32 (1..8192)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "DMT subcarrier number. There are 4096 possible DMT sub-carriers
            spaced by 4.3125 KHz in the upstream and downstream directions.
            These carriers are typically broken up into non-overlapping
            groups called bands. For this reason, not all carriers will be
            used in each direction.
           "
   ::= { adGenVdsl2ScStatusEntry 1 }

   adGenVdsl2ScStatusUsBits OBJECT-TYPE
       SYNTAX      Integer32 (0..15)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Bits allocated per subcarrier in the upstream direction.
            Value ranges from 0 to 15.  A zero value indicates
            that no bits are allocated the upstream subcarrier.
           "
   ::= { adGenVdsl2ScStatusEntry 2 }

   adGenVdsl2ScStatusUsGain OBJECT-TYPE
       SYNTAX      Integer32 (0..4095)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream gains allocation per subcarrier. It is an array of integer
            values in the range of 0 to 4095 for each subcarrier.
            The gain value is represented as a multiple of 1/512 on
            linear scale.  The value of zero indicates carrier is not used.
            Min. value: 20*log(1/512) = -54.2 dB
            Max. value: 20*log(4095/512) = 18.0 dB
           "
   ::= { adGenVdsl2ScStatusEntry 3 }

   adGenVdsl2ScStatusUsSnr OBJECT-TYPE
       SYNTAX      Integer32 (0..955)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream signal to noise (SNR) per sub-carrier number. The SNR
            ranges from 0 to +95.0 dB with 0.1 dB steps. A special value of
            955 indicates that no measurement could be done for the
            subcarrier because it is out of the passband or that the noise
            PSD is out of range to be represented.
           "
   ::= { adGenVdsl2ScStatusEntry 4 }

   adGenVdsl2ScStatusDsBits OBJECT-TYPE
       SYNTAX      Integer32 (0..15)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Bits allocated per subcarrier in the downstream direction.
            Value ranges from 0 to 15.  A zero value indicates
            that no bits are allocated the downstream subcarrier.
           "
   ::= { adGenVdsl2ScStatusEntry 5 }

   adGenVdsl2ScStatusDsGain OBJECT-TYPE
       SYNTAX      Integer32 (0..4095)
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Downstream gains allocation per subcarrier. It is an array of
            integer values in the range of 0 to 4095 for each subcarrier.
            The gain value is represented as a multiple of 1/512 on
            linear scale.  The value of zero indicates carrier is not used.
            Min. value: 20*log(1/512) = -54.2 dB
            Max. value: 20*log(4095/512) = 18.0 dB
           "
   ::= { adGenVdsl2ScStatusEntry 6 }

   adGenVdsl2ScStatusDsSnr OBJECT-TYPE
       SYNTAX      Integer32 (0..955)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Downstream signal to noise (SNR) per sub-carrier number. The SNR
            ranges from 0 to +95.0 dB with 0.1 dB steps. A special value of
            955 indicates that no measurement could be done for the
            subcarrier because it is out of the passband or that the noise
            PSD is out of range to be represented.
           "
   ::= { adGenVdsl2ScStatusEntry 7 }


   adGenVdsl2ScStatusUsQln OBJECT-TYPE
       SYNTAX      Integer32 (-1505..-230)
       UNITS       "0.1 dBm/Hz"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream quiet line noise (QLN) per sub-carrier number. The QLN
            ranges from -150.5 to -23.0 dBm/Hz with 0.1 dB steps. A special
            value of -1505 indicates that no measurement could be done for the
            subcarrier because it is out of the passband or that the noise
            PSD is out of range to be represented."
   ::= { adGenVdsl2ScStatusEntry 8 }

   adGenVdsl2ScStatusDsQln OBJECT-TYPE
       SYNTAX      Integer32 (-1505..-230)
       UNITS       "0.1 dBm/Hz"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream quiet line noise (QLN) per sub-carrier number. The QLN
            ranges from -150.5 to -23.0 dBm/Hz with 0.1 dB steps. A special
            value of -1505 indicates that no measurement could be done for the
            subcarrier because it is out of the passband or that the noise
            PSD is out of range to be represented."
   ::= { adGenVdsl2ScStatusEntry 9 }

   adGenVdsl2ScStatusUsHlog OBJECT-TYPE
       SYNTAX      Integer32 (-963..60)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Upstream channel transfer function (HLOG) per sub-carrier number. The HLOG
            ranges from -96.3 to +6.0 dB with 0.1 dB steps. A special value of
            -963 indicates that no measurement could be done for the
            subcarrier because it is out of the passband or that the noise
            PSD is out of range to be represented."
   ::= { adGenVdsl2ScStatusEntry 10 }

   adGenVdsl2ScStatusDsHlog OBJECT-TYPE
       SYNTAX      Integer32 (-963..60)
       UNITS       "0.1 dB"
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "Downstream channel transfer function (HLOG) per sub-carrier number. The HLOG
            ranges from -96.3 to +6.0 dB with 0.1 dB steps. A special value of
            -963 indicates that no measurement could be done for the
            subcarrier because it is out of the passband or that the noise
            PSD is out of range to be represented."
   ::= { adGenVdsl2ScStatusEntry 11 }

--==================================================================================================================
-- Bulk VDSL Tlv reserve instance
--==================================================================================================================

    adGenVdsl2ReserveInstanceBulkVDSLTlvTable
     OBJECT-TYPE
       SYNTAX      SEQUENCE OF AdGenVdsl2ReserveInstanceBulkVDSLTlvEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "Table contains instances that are created when there is a request to reserve a Bulk VDSL instance"
       ::= { adGenVdsl2Status 8 }

    adGenVdsl2ReserveInstanceBulkVDSLTlvEntry OBJECT-TYPE
       SYNTAX      AdGenVdsl2ReserveInstanceBulkVDSLTlvEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "Index is the slot number."
       INDEX { adGenSlotInfoIndex }
       ::= { adGenVdsl2ReserveInstanceBulkVDSLTlvTable 1 }

    AdGenVdsl2ReserveInstanceBulkVDSLTlvEntry ::=
       SEQUENCE {
           adGenVdsl2ReserveInstanceBulkVDSLSlotInstance           Integer32
       }

    adGenVdsl2ReserveInstanceBulkVDSLSlotInstance      OBJECT-TYPE
       SYNTAX                    		Integer32
       MAX-ACCESS                		read-only
       STATUS                    		current
       DESCRIPTION
           "Used to index the Tlv table; each time this is called by slot it
            creates an instance for that viewer"
        ::= { adGenVdsl2ReserveInstanceBulkVDSLTlvEntry 1 }


--==================================================================================================================
-- The Bulk VDSL Tlv Table
--==================================================================================================================
    adGenVdsl2BulkVDSLTlvTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF AdGenVdsl2BulkVDSLTlvEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "Table contains filters that can be applied on the requested instances"
       ::= { adGenVdsl2Status 9 }

    adGenVdsl2BulkVDSLTlvEntry OBJECT-TYPE
       SYNTAX      AdGenVdsl2BulkVDSLTlvEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "Indexes are the slot and instance number"
       INDEX { adGenSlotInfoIndex, adGenVdsl2BulkVDSLTlvInstance }
       ::= { adGenVdsl2BulkVDSLTlvTable 1 }

    AdGenVdsl2BulkVDSLTlvEntry ::=
       SEQUENCE {
       	   adGenVdsl2BulkVDSLTlvInstance       Integer32,
       	   adGenVdsl2BulkVDSLTlvType           INTEGER,
       	   adGenVdsl2BulkVDSLTlvPort           Unsigned32,
       	   adGenVdsl2BulkVDLSTlvCreate         INTEGER
       }

    adGenVdsl2BulkVDSLTlvInstance      	OBJECT-TYPE
       SYNTAX                    		Integer32
       MAX-ACCESS                		not-accessible
       STATUS                    		current
       DESCRIPTION
           "Used to index the table by TLV Instance number."
        ::= { adGenVdsl2BulkVDSLTlvEntry 1 }

    adGenVdsl2BulkVDSLTlvType      OBJECT-TYPE
       SYNTAX                      INTEGER {
                                                bat(1),
                                                snr(2),
                                                qln(3),
                                                hlog(4)
                                           }
       MAX-ACCESS                  read-write
       STATUS                      current
       DESCRIPTION
            "Used to set the type of VDSL info to return in the VDSL TLV instance:
            1 - Bit Allocation Table (BAT), 2 - Signal Noise Ratio (SNR),
            3 - Quiet Line Noise (QLN), 4 - Channel Transfer Function (HLOG)"
       DEFVAL { bat }
        ::= { adGenVdsl2BulkVDSLTlvEntry 2 }

    adGenVdsl2BulkVDSLTlvPort      OBJECT-TYPE
       SYNTAX                      Unsigned32
       MAX-ACCESS                  read-write
       STATUS                      current
       DESCRIPTION
           "Used to set the VDSL port to get the info for VDSL TLV instance"

       DEFVAL { 1 }
        ::= { adGenVdsl2BulkVDSLTlvEntry 3 }

    adGenVdsl2BulkVDLSTlvCreate    OBJECT-TYPE
       SYNTAX INTEGER              {
       								 updateinstance(1)
       							   }
       MAX-ACCESS                  read-write
       STATUS                      current
       DESCRIPTION
           "Set this OID to 1 to update the instance with Bulk VDSL info depending on type set.
            A get on this OID always returns a 1."

        ::= { adGenVdsl2BulkVDSLTlvEntry 4 }

-- ***************************************************************************
-- Trap definitions
-- ***************************************************************************

  -- ============ Traps (VTUC)  ===========================
  adGenVdsl2VtucTraps OBJECT IDENTIFIER ::= { adGenVdsl2VtucTrapPrefix 0 }

         adGenVdsl2VtucSnrMgnThreshTrap      NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       adGenSlotInfoIndex,
                       adGenPortTrapIdentifier,
                       adGenVdsl2CurrUSSnrMgn,
                       adGenVdsl2VtucThreshSnrMgn
                     }
             STATUS  current
             DESCRIPTION
                 "SNR Margin threshold reached."
         --#TYPE         "WARNING - VTUC SNR Margin Threshold crossed"
         --#SUMMARY      "VTUC SNR Margin Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, slot: %d, port: %d, margin: %d, threshold: %d"
         --#ARGUMENTS    {1,2,3,4,5}
         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
         ::= { adGenVdsl2VtucTraps 1 }

         adGenVdsl2VtucLofsThreshTrap      NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       adGenSlotInfoIndex,
                       adGenPortTrapIdentifier,
                       adGenVdsl2VtucPerfCurr15MinLofs,
                       adGenVdsl2VtucThresh15MinLofs
                     }
             STATUS  current
             DESCRIPTION
                 "Loss of Framing 15-minute interval threshold reached."
         --#TYPE         "WARNING - VTUC 15-Minute LOFS Threshold crossed"
         --#SUMMARY      "VTUC 15-Minute LOFS Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, slot: %d, port: %d, lofs: %d, threshold: %d"
         --#ARGUMENTS    {1,2,3,4,5}
         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
         ::= { adGenVdsl2VtucTraps 2 }

         adGenVdsl2VtucLossThreshTrap      NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       adGenSlotInfoIndex,
                       adGenPortTrapIdentifier,
                       adGenVdsl2VtucPerfCurr15MinLoss,
                       adGenVdsl2VtucThresh15MinLoss
                     }
             STATUS  current
             DESCRIPTION
                 "Loss of Signal 15-minute interval threshold reached."
         --#TYPE         "WARNING - VTUC 15-Minute LOSS Threshold crossed"
         --#SUMMARY      "VTUC 15-Minute LOSS Threshold Alarm"
         --#SUMMARY      "detected at shelf: %s, slot: %d, port: %d, loss: %d, threshold: %d"
         --#ARGUMENTS    {1,2,3,4,5}
         --#SEVERITY     WARNING
         --#GENERIC      6
         --#CATEGORY     "Threshold Events"
         --#SOURCE_ID    "A"
         --#TIMEINDEX    0
         ::= { adGenVdsl2VtucTraps 3 }

         adGenVdsl2VtucLolsThreshTrap      NOTIFICATION-TYPE
             OBJECTS {
                       adTrapInformSeqNum,
                       sysName,
                       adGenSlotInfoIndex,
                       adGenPortTrapIdentifier,
                       adGenVdsl2VtucPerfCurr15MinLols,
                       adGenVdsl2VtucThresh15MinLols
                     }
             STATUS  current
             DESCRIPTION
                 "Lols of Signal 15-minute interval threshold reached."
         --#TYPE         "WARNING - VTUC 15-Minute LOLS Threshold crossed"
         --#SUMMARY      "VTUC 15-Minute LOLS Threshold Alarm"

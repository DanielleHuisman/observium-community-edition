         adGenVdsl2ConfHamBandNotches OBJECT-TYPE
             SYNTAX             Unsigned32
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "A bit-map of transmit power spectral density mask code, used
                 to avoid interference with HAM (Handheld Amateur Radio)
                 radio bands by introducing power control (notching) in one
                 or more of these bands. A bit set indicates notching is enabled.

                     Bit Assignments:
                     Bit 00:    Hamband 1800
                     Bit 01:    Hamband 3500
                     Bit 02:    Hamband 7000
                     Bit 03:    Hamband 10100
                     Bit 04:    Hamband 14000
                     Bit 05:    Hamband 18068
                     Bit 06:    Hamband 21000
                     Bit 07:    Hamband 24890
                     Bit 08:    Hamband 28000
                "
         ::= { adGenVdsl2LineConfProfileEntry 23 }

         adGenVdsl2ConfUsNomAggTxPwr OBJECT-TYPE
             SYNTAX             Integer32 (0..255)
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "The maximum nominal aggregate transmit power in the upstream
                 direction during initialization and Showtime.  It ranges from
                 0 to 255 units of 0.1 dBm (physical values are 0 to 25.5 dBm).
                "
         ::= { adGenVdsl2LineConfProfileEntry 24 }

         adGenVdsl2ConfDsNomAggTxPwr OBJECT-TYPE
             SYNTAX             Integer32
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "The maximum nominal aggregate transmit power in the
                 downstream direction during initialization and Showtime.  It
                 ranges from 0 to 255 units of 0.1 dBm (physical values are 0
                 to 25.5 dBm).
                "
         ::= { adGenVdsl2LineConfProfileEntry 25 }

         adGenVdsl2ConfUsMaxNomTxPsd OBJECT-TYPE
             SYNTAX             Integer32 (-600..300)
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "This parameter represents the maximum nominal transmit PSD in
                 the upstream direction during initialization and showtime
                 (in dBm/Hz). It ranges from -60 to -30 dBm/Hz, with 0.1 dB steps
                 (-600..-300).
                "
         ::= { adGenVdsl2LineConfProfileEntry 26 }


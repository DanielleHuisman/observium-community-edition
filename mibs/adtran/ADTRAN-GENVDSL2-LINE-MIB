         -- ========= PM Group (adGenVdsl2VturChanPerfDataTable) =============
         adGenVdsl2VturChanPerfDataTable       OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VturChanPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtur channel.
                 VDSL2 channel interfaces are those ifEntries
                 where ifType is equal to Interleave(124)
                 or Fast(125)."
         ::= { adGenVdsl2PM 6 }

         adGenVdsl2VturChanPerfDataEntry       OBJECT-TYPE
             SYNTAX          AdGenVdsl2VturChanPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in adGenVdsl2VturChanPerfDataTable."
             INDEX           { ifIndex, adGenVdsl2VturChanNumber }
         ::= { adGenVdsl2VturChanPerfDataTable 1 }

         AdGenVdsl2VturChanPerfDataEntry ::=
          SEQUENCE {
              adGenVdsl2VturChanNumber                            Integer32,
              adGenVdsl2VturChanReceivedBlks                      Counter32,
              adGenVdsl2VturChanTransmittedBlks                   Counter32,
              adGenVdsl2VturChanCorrectedBlks                     Counter32,
              adGenVdsl2VturChanUncorrectBlks                     Counter32,
              adGenVdsl2VturChanPerfValidIntervals                Integer32,
              adGenVdsl2VturChanPerfInvalidIntervals              Integer32,
              adGenVdsl2VturChanPerfCurr15MinTimeElapsed          Gauge32,
              adGenVdsl2VturChanPerfCurr15MinReceivedBlks         Counter32,
              adGenVdsl2VturChanPerfCurr15MinTransmittedBlks      Counter32,
              adGenVdsl2VturChanPerfCurr15MinCorrectedBlks        Counter32,
              adGenVdsl2VturChanPerfCurr15MinUncorrectBlks        Counter32,
              adGenVdsl2VturChanPerfCurr1DayTimeElapsed           Gauge32,
              adGenVdsl2VturChanPerfCurr1DayReceivedBlks          Counter32,
              adGenVdsl2VturChanPerfCurr1DayTransmittedBlks       Counter32,
              adGenVdsl2VturChanPerfCurr1DayCorrectedBlks         Counter32,
              adGenVdsl2VturChanPerfCurr1DayUncorrectBlks         Counter32,
              adGenVdsl2VturChanPerfTcTxUnits                     Counter32,
              adGenVdsl2VturChanPerfTcTxDataUnits                 Counter32,
              adGenVdsl2VturChanPerfTcTxDataOctets                Counter32,
              adGenVdsl2VturChanPerfTcTxIdleUnits                 Counter32,
              adGenVdsl2VturChanPerfTcRxUnits                     Counter32,
              adGenVdsl2VturChanPerfTcRxDataUnits                 Counter32,
              adGenVdsl2VturChanPerfTcRxDataOctets                Counter32,
              adGenVdsl2VturChanPerfTcRxIdleUnits                 Counter32,
              adGenVdsl2VturChanPerfTcRxErroredUnits              Counter32,
              adGenVdsl2VturChanPerfRtxUncorrectedDtu             Counter32,
              adGenVdsl2VturChanPerfRtxCorrectedDtu               Counter32,
              adGenVdsl2VturChanPerfRtxRetransmittedDtu           Counter32,
              adGenVdsl2VturChanPerfCurr15MinRtxUncorrectedDtu    Counter32,
              adGenVdsl2VturChanPerfCurr15MinRtxCorrectedDtu      Counter32,
              adGenVdsl2VturChanPerfCurr15MinRtxRetransmittedDtu  Counter32,
              adGenVdsl2VturChanPerfCurr1DayRtxUncorrectedDtu     Counter32,
              adGenVdsl2VturChanPerfCurr1DayRtxCorrectedDtu       Counter32,
              adGenVdsl2VturChanPerfCurr1DayRtxRetransmittedDtu   Counter32
          }

         adGenVdsl2VturChanNumber OBJECT-TYPE
             SYNTAX      Integer32(1..4)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "VDSL2 logical channel. Channel values are 1 - 4."
         ::= { adGenVdsl2VturChanPerfDataEntry 1 }

         adGenVdsl2VturChanReceivedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks received on this channel
                 since agent reset."
         ::= { adGenVdsl2VturChanPerfDataEntry 2 }

         adGenVdsl2VturChanTransmittedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks transmitted on this
                 channel since agent reset."
         ::= { adGenVdsl2VturChanPerfDataEntry 3 }

         adGenVdsl2VturChanCorrectedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with errors that were
                 corrected since agent reset.  These blocks are passed
                 on as good data."
         ::= { adGenVdsl2VturChanPerfDataEntry 4 }

         adGenVdsl2VturChanUncorrectBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with uncorrectable
                 errors since agent reset."
         ::= { adGenVdsl2VturChanPerfDataEntry 5 }

         adGenVdsl2VturChanPerfValidIntervals OBJECT-TYPE
             SYNTAX      Integer32(0..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The number of previous 15-minute intervals in the
                 interval table for which data was collected.  Given
                 that <n> is the maximum # of intervals supported.
                 The value will be <n> unless the measurement was
                 (re-)started within the last (<n>*15) minutes, in which
                 case the value will be the number of complete 15
                 minute intervals for which the agent has at least
                 some data. In certain cases (e.g., in the case
                 where the agent is a proxy) it is possible that some
                 intervals are unavailable.  In this case, this
                 interval is the maximum interval number for
                 which data is available."
         ::= { adGenVdsl2VturChanPerfDataEntry 6 }

         adGenVdsl2VturChanPerfInvalidIntervals OBJECT-TYPE
             SYNTAX      Integer32(0..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The number of intervals in the range from
                 0 to the value of adGenVdsl2VturChanPerfValidIntervals
                 for which no data is available. This object
                 will typically be zero except in cases where
                 the data for some intervals are not available
                 (e.g., in proxy situations)."
         ::= { adGenVdsl2VturChanPerfDataEntry 7 }

         adGenVdsl2VturChanPerfCurr15MinTimeElapsed OBJECT-TYPE
             SYNTAX      Gauge32(0..899)
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Total elapsed seconds in this interval.
                 A full interval is 900 seconds."
         ::= { adGenVdsl2VturChanPerfDataEntry 8 }

         adGenVdsl2VturChanPerfCurr15MinReceivedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks received on this
                 channel within the current 15 minute interval."
         ::= { adGenVdsl2VturChanPerfDataEntry 9 }

         adGenVdsl2VturChanPerfCurr15MinTransmittedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks transmitted on this
                 channel within the current 15 minute interval."
         ::= { adGenVdsl2VturChanPerfDataEntry 10 }

         adGenVdsl2VturChanPerfCurr15MinCorrectedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with errors that were
                 corrected on this channel within the current 15 minute
                 interval."
         ::= { adGenVdsl2VturChanPerfDataEntry 11 }

         adGenVdsl2VturChanPerfCurr15MinUncorrectBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with uncorrectable
                 errors on this channel within the current 15 minute
                 interval."
         ::= { adGenVdsl2VturChanPerfDataEntry 12 }

         adGenVdsl2VturChanPerfCurr1DayTimeElapsed OBJECT-TYPE
             SYNTAX      Gauge32(0..86399)
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Number of seconds that have elapsed since the
                 beginning of the current 1-day interval."
         ::= { adGenVdsl2VturChanPerfDataEntry 13 }

         adGenVdsl2VturChanPerfCurr1DayReceivedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks received on this
                 channel during the current day as measured by
                 adGenVdsl2VturChanPerfCurr1DayTimeElapsed."
         ::= { adGenVdsl2VturChanPerfDataEntry 14 }

         adGenVdsl2VturChanPerfCurr1DayTransmittedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks transmitted on this
                 channel during the current day as measured by
                 adGenVdsl2VturChanPerfCurr1DayTimeElapsed."

         ::= { adGenVdsl2VturChanPerfDataEntry 15 }

         adGenVdsl2VturChanPerfCurr1DayCorrectedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with errors that were
                 corrected on this channel during the current day as
                 measured by adGenVdsl2VturChanPerfCurr1DayTimeElapsed."
         ::= { adGenVdsl2VturChanPerfDataEntry 16}

         adGenVdsl2VturChanPerfCurr1DayUncorrectBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with uncorrectable
                 errors on this channel during the current day as
                 measured by adGenVdsl2VturChanPerfCurr1DayTimeElapsed."
         ::= { adGenVdsl2VturChanPerfDataEntry 17 }

         adGenVdsl2VturChanPerfTcTxUnits OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "pkts/cells"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION

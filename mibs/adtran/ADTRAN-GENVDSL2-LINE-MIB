         -- ========= PM Group (adGenVdsl2VtucChanIntervalTable) =============
         adGenVdsl2VtucChanIntervalTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VtucChanIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtuc channel's
                 performance data collection interval.
                 VDSL2 channel interfaces are those ifEntries
                 where ifType is equal to Interleave(124)
                 or Fast(125)."
         ::= { adGenVdsl2PM 7 }

         adGenVdsl2VtucChanIntervalEntry   OBJECT-TYPE
             SYNTAX          AdGenVdsl2VtucChanIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2VtucIntervalTable."
             INDEX           { ifIndex, adGenVdsl2VtucChanNum ,adGenVdsl2VtucChanIntervalNumber }
         ::= { adGenVdsl2VtucChanIntervalTable 1 }

         AdGenVdsl2VtucChanIntervalEntry ::=
             SEQUENCE {
                 adGenVdsl2VtucChanNum                          Integer32,
                 adGenVdsl2VtucChanIntervalNumber               Integer32,
                 adGenVdsl2VtucChanIntervalReceivedBlks         Counter32,
                 adGenVdsl2VtucChanIntervalTransmittedBlks      Counter32,
                 adGenVdsl2VtucChanIntervalCorrectedBlks        Counter32,
                 adGenVdsl2VtucChanIntervalUncorrectBlks        Counter32,
                 adGenVdsl2VtucChanIntervalValidData            INTEGER,
                 adGenVdsl2VtucChanIntervalRtxUncorrectedDtu    Counter32,
                 adGenVdsl2VtucChanIntervalRtxCorrectedDtu      Counter32,
                 adGenVdsl2VtucChanIntervalRtxRetransmittedDtu  Counter32
             }


         adGenVdsl2VtucChanNum OBJECT-TYPE
             SYNTAX      Integer32(1..4)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "VDSL2 logical channel. Channel values are 1 - 4."
         ::= { adGenVdsl2VtucChanIntervalEntry 1 }

         adGenVdsl2VtucChanIntervalNumber OBJECT-TYPE
             SYNTAX      Integer32(1..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Performance Data Interval number 1 is the
                 the most recent previous interval; interval
                 96 is 24 hours ago.  Intervals 2..96 are
                 optional."
         ::= { adGenVdsl2VtucChanIntervalEntry 2 }

         adGenVdsl2VtucChanIntervalReceivedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks received on this channel
                 during this interval."
         ::= { adGenVdsl2VtucChanIntervalEntry 3 }

         adGenVdsl2VtucChanIntervalTransmittedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks transmitted on this
                 channel during this interval."
         ::= { adGenVdsl2VtucChanIntervalEntry 4 }

         adGenVdsl2VtucChanIntervalCorrectedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with errors that were
                 corrected on this channel during this interval."
         ::= { adGenVdsl2VtucChanIntervalEntry 5 }

         adGenVdsl2VtucChanIntervalUncorrectBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with uncorrectable
                 errors on this channel during this interval."
         ::= { adGenVdsl2VtucChanIntervalEntry 6 }

         adGenVdsl2VtucChanIntervalValidData OBJECT-TYPE
             SYNTAX      INTEGER {
                 valid (1),
                 notValid (2)
             }
             MAX-ACCESS read-only
             STATUS current
             DESCRIPTION
                 "This variable indicates if the data for this
                 interval is valid."
         ::= { adGenVdsl2VtucChanIntervalEntry 7 }

         adGenVdsl2VtucChanIntervalRtxUncorrectedDtu OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "dtus"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of DTUs in the specified 15-min interval that have been
                  detected in error and that have not been corrected by one or
                  more retransmissions within the maximum delay constraint."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 12"
         ::= { adGenVdsl2VtucChanIntervalEntry 8 }

         adGenVdsl2VtucChanIntervalRtxCorrectedDtu OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "dtus"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of DTUs in the specified 15-min interval that have been
                  detected in error and that have been successfully corrected
                  by a retransmission within the maximum delay constraint."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 12"
         ::= { adGenVdsl2VtucChanIntervalEntry 9 }

         adGenVdsl2VtucChanIntervalRtxRetransmittedDtu OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "dtus"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of DTUs in the specified 15-min interval that have been
                  retransmitted."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 12"
         ::= { adGenVdsl2VtucChanIntervalEntry 10 }


         -- ========= PM Group (adGenVdsl2VtucChanIntervalTable) =============

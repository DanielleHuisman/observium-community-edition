                 adGenVdsl2ConfMemorySplitRatio              INTEGER,
                 adGenVdsl2ConfRateAdaptConfigMode           INTEGER
         }

         adGenVdsl2LineConfProfileName    OBJECT-TYPE
             SYNTAX          DisplayString (SIZE (1..10))
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This object is used by the line configuration table
                 in order to identify a row of this table.

                 This value cannot be user-specified and is not dynamic.

                 There is a one-to-one relationship between each line and
                 its profile.  The profile name algorithmicly represents
		 the Line's ifIndex.

                 Therefore, the profile's name is a decimalized string
                 of the ifIndex that is fixed-length (10) with
                 leading zero(s).  For example, the profile name for
                 ifIndex which equals '15' will be '0000000015'.
                "
         ::= { adGenVdsl2LineConfProfileEntry 1 }

         adGenVdsl2ConfDsRateMode OBJECT-TYPE
             SYNTAX    INTEGER {
                 fixed (1),              -- no rate adaptation
                 adaptAtStartup (2),     -- perform rate adaptation
                                         -- only at initialization
                 adaptAtRuntime (3)      -- perform rate adaptation at
                                         -- any time
             }
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "Defines what form of downstream rate adaptation is
                 configured on this modem.
                "
         ::= { adGenVdsl2LineConfProfileEntry 2 }

         adGenVdsl2ConfDsTargetSnrMgn OBJECT-TYPE
             SYNTAX      Integer32 (0..310)
             UNITS       "tenth dB"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "Configured Target Signal/Noise Margin.  This is the noise
                 margin the modem must achieve with the target BER (10-7)
                 or better to successfully complete initialization.  The
                 noise margin ranges from 0 to 31 dB with 0.1 dB steps
                 (0..310). This margin should be above the minimum margin
                 and below the maximum margin.
                "
         ::= { adGenVdsl2LineConfProfileEntry 3 }

         adGenVdsl2ConfDsMaxSnrMgn OBJECT-TYPE
             SYNTAX      Integer32 (0..310)
             UNITS       "tenth dB"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "Configured Maximum acceptable Signal/Noise Margin.
                 If the Noise Margin is above this the modem should
                 attempt to reduce its power output to optimize its
                 operation.  The noise margin ranges from 0 to 31 dB
                 with 0.1 dB steps (0..310). This margin should be
                 above the target and minimum margins.
                "
         ::= { adGenVdsl2LineConfProfileEntry 4 }

         adGenVdsl2ConfDsMinSnrMgn  OBJECT-TYPE
             SYNTAX      Integer32 (0..310)
             UNITS       "tenth dB"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "Configured Minimum acceptable Signal/Noise Margin.
                 If the noise margin falls below this level, the modem
                 should attempt to increase its power output.  If that
                 is not possible the modem will attempt to re-initialize
                 or shut down. The noise margin ranges from 0 to 31 dB
                 with 0.1 dB steps (0..310).  This margin should be
                 below the target and maximum margins.
                "
         ::= { adGenVdsl2LineConfProfileEntry 5 }

         adGenVdsl2ConfUsTargetSnrMgn OBJECT-TYPE
             SYNTAX      Integer32 (0..310)
             UNITS       "tenth dB"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "Configured Target Signal/Noise Margin.  This is the noise
                 margin the modem must achieve with the target BER (10-7)
                 or better to successfully complete initialization.  The
                 noise margin ranges from 0 to 31 dB with 0.1 dB steps
                 (0..310). This margin should be above the minimum margin
                 and below the maximum margin.
                "
         ::= { adGenVdsl2LineConfProfileEntry 6 }

         adGenVdsl2ConfUsMaxSnrMgn OBJECT-TYPE
             SYNTAX      Integer32 (0..310)
             UNITS       "tenth dB"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "Configured Maximum acceptable Signal/Noise Margin.
                 If the Noise Margin is above this the modem should
                 attempt to reduce its power output to optimize its
                 operation.  The noise margin ranges from 0 to 31 dB
                 with 0.1 dB steps (0..310). This margin should be
                 above the target and minimum margins.
                "
         ::= { adGenVdsl2LineConfProfileEntry 7 }

         adGenVdsl2ConfUsMinSnrMgn  OBJECT-TYPE
             SYNTAX      Integer32 (0..310)
             UNITS       "tenth dB"
             MAX-ACCESS  read-create
             STATUS      current
             DESCRIPTION
                "Configured Minimum acceptable Signal/Noise Margin.
                 If the noise margin falls below this level, the modem
                 should attempt to increase its power output.  If that
                 is not possible the modem will attempt to re-initialize
                 or shut down. The noise margin ranges from 0 to 31 dB
                 with 0.1 dB steps (0..310).  This margin should be
                 below the target and maximum margins.
                "
         ::= { adGenVdsl2LineConfProfileEntry 8 }

         adGenVdsl2LineConfProfileRowStatus OBJECT-TYPE
             SYNTAX          RowStatus
             MAX-ACCESS      read-only
             STATUS          current
             DESCRIPTION
                "This object is used to create a new row or modify or
                 delete an existing row in this table.

                 A profile activated by setting this object to
                 `active'.  When `active' is set, the system
                 will validate the profile.

                 Before a profile can be deleted or taken out of
                 service, (by setting this object to `destroy' or
                 `outOfService') it must be first unreferenced
                 from all associated lines.

                 If the implementator of this MIB has chosen not
                 to implement `dynamic assignment' of profiles, this
                 object's MIN-ACCESS is read-only and its value
                 is always to be `active'.
                "
         ::= { adGenVdsl2LineConfProfileEntry 9 }


         adGenVdsl2ConfServiceMode OBJECT-TYPE
             SYNTAX             INTEGER {
                 multiMode        (1),  -- adapt to the ATUR's mode,
                                        -- assuming T1.413, G.dmt, or G.lite
                                        -- **NOTE**  For adsl2+ line cards,
                                        -- this is adsl2+ multimode
                 t1413            (2),  -- T1.413 Mode
                 gDMT             (3),  -- G.dmt Mode
                 gLite            (4),  -- G.lite Mode
                 g9923            (5),  -- ADSL2 Mode
                 g9924            (6),  -- ADSL2-lite Mode
                 g9925            (7),  -- ADSL2+ Mode
                 readsl           (8),  -- READSL Mode
                 adsl1MultiMode   (9),  -- adsl legacy multimode, to be used
                                        -- by adsl2+ line cards to enable
                                        -- multimode equivalent to that of
                                        -- adsl line cards
                 g9925AnxM        (10), -- ADSL2+ Annex M Mode
                 g9931            (11), -- VDSL1 Mode
                 g9932            (12), -- VDSL2 Mode
                 vdsl2MultiMode   (13), -- Multimode including VDSL2
                 vdsl2PtmFallback (14), -- PTM Fallback mode
                 tseMultimode     (255) -- Multimode according to DMT TSE enables
               }
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "The configuration parameter sets the modem service mode."
         ::= { adGenVdsl2LineConfProfileEntry 10 }

         adGenVdsl2ConfMultiModeDmtTse OBJECT-TYPE
             SYNTAX  Unsigned32
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "Multimode DMT Transmission System Enabling (XTSE).  A list of the
                 different modulation types enabled in this profile for multimode.
                 It is coded in a bit-map representation with 1 or more bits set.
                 A bit set to  '1' means that the VTUs may train in the respective
                 mode for the DSL line.  A bit set to '0' means that the VTUs cannot
                 train in the respective mode for the line.  All 'reserved' bits
                 should be set to '0'.

                     Bit Assignments:
                     Bit 00:         T1E1 ADSL Issue 2
                     Bit 01:         Full Rate ADSL Over POTS
                     Bit 02:         Full Rate ADSL Over ISDN
                     Bit 03:         Full Rate ADSL in TCM-ISDN
                     Bit 04:         Full Rate SSDSL in ISDN Binder
                     Bit 05:         All Digital ADSL
                     Bit 06:         Lite ADSL Over POTS/ISDN
                     Bit 07:         Lite ADSL in TCM-ISDN
                     Bit 08:         ADSL2 Over POTS
                     Bit 09:         ADSL2 Over ISDN
                     Bit 10:         All Digital ADSL2 (POTS)
                     Bit 11:         All Digital ADSL2 (ISDN)
                     Bit 12:         Double Upstream ADSL2
                     Bit 13:         RE-ADSL2 Over POTS
                     Bit 14:         ADSL2 Lite Over POTS
                     Bit 15:         All Digital ADSL2 Lite
                     Bit 16:         ADSL2+ over POTS
                     Bit 17:         ADSL2+ over ISDN
                     Bit 18:         All Digital ADSL2+ (POTS)
                     Bit 19:         All Digital ADSL2+ (ISDN)
                     Bit 20:         Double Upstream ADSL2+
                     Bit 21:         VDSL1
                     Bit 22:         VDSL2 Annex A
                     Bit 23:         VDSL2 Annex B
                     Bit 24:         VDSL2 Annex C
                "
         ::= { adGenVdsl2LineConfProfileEntry 11 }

         adGenVdsl2ConfBandProfiles OBJECT-TYPE
             SYNTAX             Unsigned32
             MAX-ACCESS         read-create
             STATUS             current
             DESCRIPTION
                "Bit-map of ITU-T G.993.2 implementation profiles supported by
                 an VTU-x and can be provisioned on the VDSL2 line.

                     Bit Assignments:
                     Bit 00:         Profile 8A
                     Bit 01:         Profile 8B
                     Bit 02:         Profile 8C
                     Bit 03:         Profile 8D
                     Bit 04:         Profile 12A
                     Bit 05:         Profile 12B
                     Bit 06:         Profile 17A
                     Bit 07:         Profile 30A
                     Bit 08:         Profile 17B (Deprecated)
                     Bit 09:         Profile 35B
               "
         ::= { adGenVdsl2LineConfProfileEntry 12 }


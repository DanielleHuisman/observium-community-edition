         -- ========== Prov Group (RestoreDefaultTable) =======================
         adGenVdsl2RestoreDefaultTable    OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2RestoreDefaultEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table contains items to restore line and/or
                  alarm provisioning to their default values."
         ::= { adGenVdsl2Prov 3 }

         adGenVdsl2RestoreDefaultEntry    OBJECT-TYPE
             SYNTAX          AdGenVdsl2RestoreDefaultEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "For later"
             INDEX { adGenSlotInfoIndex }

         ::= { adGenVdsl2RestoreDefaultTable 1}

         AdGenVdsl2RestoreDefaultEntry ::=
             SEQUENCE {
             adGenVdsl2ConfRestoreDefaults    INTEGER
             }

         adGenVdsl2ConfRestoreDefaults    OBJECT-TYPE
             SYNTAX      INTEGER {
                 restoreProvisioning(1),  -- restore only line provisiong
                 restoreAlarms(2),        -- restore alarm provisioning
                 restoreAll(3)            -- restore all provisiong
             }
             MAX-ACCESS      read-write
             STATUS          current
             DESCRIPTION
                 "This object is used to restore the set of provisioning
                  indicated.  A read will always return a value of 1."

         ::= { adGenVdsl2RestoreDefaultEntry 1}



         -- ******************************************************************
         -- Group: Status
         --   The Status Group consists of the tables of entries
         --   to determine modem status and inventory of parameters
         --   specific to the overall line, and current physical Vtur/Vtuc
         --   attributes.
         -- ******************************************************************

         -- ============ Status Group (LineTable) ============================
         adGenVdsl2LineTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2LineEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This table includes common attributes describing
                 both ends of the line.  It is required for all VDSL2
                 physical interfaces.  VDSL2 physical interfaces are
                 those ifEntries where ifType is equal to vdsl(97).
                "
         ::= { adGenVdsl2Status 1 }

         adGenVdsl2LineEntry   OBJECT-TYPE
             SYNTAX          AdGenVdsl2LineEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in adGenVdsl2LineTable."
             INDEX           { ifIndex }
         ::= { adGenVdsl2LineTable 1 }

         AdGenVdsl2LineEntry ::=
             SEQUENCE {
                 adGenVdsl2LineCoding                       INTEGER,
                 adGenVdsl2LinePortServiceState             INTEGER,
                 adGenVdsl2LineStatus                       INTEGER,
                 adGenVdsl2LineUpTime                       Gauge32,

                 adGenVdsl2LineCurrTransSysMode             INTEGER,
                 adGenVdsl2LineCurrBandProfile              INTEGER,
                 adGenVdsl2LineCurrEstimatedLength          Gauge32,
                 adGenVdsl2LineCurrTpsTcFramingMode         INTEGER

--               Loop diagnostics for future implementaion
--               adGenVdsl2LineLoopDiagMode                 INTEGER,
--               adGenVdsl2LineLoopDiagStatus               INTEGER
             }

         adGenVdsl2LineCoding OBJECT-TYPE
             SYNTAX  INTEGER {
                 other(1),-- none of the following
                 dmt (2), -- Discrete MultiTone
                 cap (3), -- Carrierless Amplitude & Phase modulation
                 qam (4)  -- QuadrVture Amplitude Modulation
             }
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Specifies the VDSL2 coding type used on this
                 line."
         ::= { adGenVdsl2LineEntry 1 }

        adGenVdsl2LinePortServiceState OBJECT-TYPE
             SYNTAX  INTEGER {
                 is(1),          -- In Service
                 oosUas(2),      -- Out of Service, Unassigned
                 oosMA(3)        -- Out of Service, Maintenance mode
             }
             MAX-ACCESS  read-write
             STATUS  current
             DESCRIPTION
                 "VDSL2 port service state."
         ::= { adGenVdsl2LineEntry 3 }

        adGenVdsl2LineStatus OBJECT-TYPE
             SYNTAX  INTEGER {
                 portDown(1),
                 portTraining(2),
                 showtime(3)
             }
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Indicates current state of the dsl line."
         ::= { adGenVdsl2LineEntry 6 }

        adGenVdsl2LineUpTime OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Indicates the amount of time the line has
                  been in showtime."
         ::= { adGenVdsl2LineEntry 7 }

        adGenVdsl2LineCurrTransSysMode OBJECT-TYPE
             SYNTAX  INTEGER {
                 unknown(0),
                 multimode(1),
                 t1413(2),
                 g9921a(3),
                 g9921b(4),
                 g9921c(5),
                 g9921h(6),
                 g9921i(7),
                 g9922(8),
                 g9922c(9),
                 g9923a(10),
                 g9923b(11),
                 g9923i(12),
                 g9923j(13),
                 g9923m(14),
                 g9923l(15),
                 g9924a(16),
                 g9924i(17),
                 g9925a(18),
                 g9925b(19),
                 g9925i(20),
                 g9925j(21),
                 g9925m(22),
                 g9931(23),
                 g9932a(24),
                 g9932b(25),
                 g9932c(26),
                 g9935(27),
                 g9932y(28)
             }
        MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "A report of the current operating xDSL line transmission mode.
                       offline     (0)      -- Unknow mode (not connected)
                       multiMode   (1)      -- Automatic mode selection
                       t1413       (2),     -- T1.413 Mode
                       g9921a      (3),     -- Full Rate ADSL Over POTS
                       g9921b      (4),     -- Full Rate ADSL Over ISDN
                       g9921c      (5),     -- Full Rate ADSL in TCM-ISDN
                       g9921h      (6),     -- Full Rate SSDSL in ISDN Binder
                       g9921i      (7),     -- All Digital ADSL
                       g9922       (8),     -- ADSL Lite Over POTS/ISDN
                       g9922c      (9),     -- ADSL Lite in TCM-ISDN
                       g9923a      (10),    -- ADSL2 Over POTS
                       g9923b      (11),    -- ADSL2 Over POTS
                       g9923i      (12),    -- ADSL2 Over ISDN
                       g9923j      (13),    -- All Digital ADSL2 Over POTS
                       g9923m      (14),    --Double Upstream ADLS2
                       g9923l      (15),    -- RE-ADSL2 over POTS
                       g9924a      (16),    -- ADSL2 Lite Over POTS
                       g9924i      (17),    -- All Digital ADSL2 Lite
                       g9925a      (18),    -- ADSL2+ Over POTS
                       g9925b      (19),    -- ADSL2+ Over ISDN
                       g9925i      (20),    -- All Digital ADSL2+ Over POTS
                       g9925j      (21),    -- All Digital ADSL2+ Over POTS
                       g9925m      (22),    -- Double Upstream ADSL2+
                       g9931       (23),    -- VDSL1
                       g9932a      (24),    -- VDSL2 Annex A
                       g9932b      (25),    -- VDSL2 Annex B
                       g9932c      (26),    -- VDSL2 Annex C
                       g9935       (27),    -- VDSL2 Vectoring
                       g9932y      (28),    -- VDSL2 Annex Y, Bi-Directional Vectoring Friendly
                       g9932q      (29)     -- VDSL2 Annex Q, Profile 35B (35Mhz VDSL2)
                 "
         ::= { adGenVdsl2LineEntry 8 }

        adGenVdsl2LineCurrBandProfile OBJECT-TYPE
             SYNTAX  INTEGER {
                 unknown(0),
                 g99328a(1),
                 g99328b(2),
                 g99328c(3),
                 g99328d(4),
                 g993212a(5),
                 g993212b(6),
                 g993217a(7),
                 g993230a(8),
                 g993217b(9),
                 g993235b(10)
             }
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The current VDSL2 band profile in use."
         ::= { adGenVdsl2LineEntry 9 }

        adGenVdsl2LineCurrEstimatedLength OBJECT-TYPE    -- Subbu ??
             SYNTAX      Gauge32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 ""
         ::= { adGenVdsl2LineEntry 10 }

        adGenVdsl2LineCurrTpsTcFramingMode OBJECT-TYPE
             SYNTAX  INTEGER {
                 auto(0),
                 atm(1),
                 efm(2),
                 hdlc(3)
             }
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Parameter that indicates the application-specific
                  Transport Protocol Specific Transmission
                  Convergence (TPS-TC) type."
         ::= { adGenVdsl2LineEntry 11 }


         -- ============ Status Group (VtucPhysTable) ========================
         adGenVdsl2VtucPhysTable       OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VtucPhysEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtuc.
                 Each row contains the Physical Layer Parameters
                 table for that Vtuc. VDSL2 physical interfaces are
                 those ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2Status 2 }

         adGenVdsl2VtucPhysEntry       OBJECT-TYPE
             SYNTAX          AdGenVdsl2VtucPhysEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2VtucPhysTable."
             INDEX           { ifIndex }
         ::= { adGenVdsl2VtucPhysTable 1 }

         AdGenVdsl2VtucPhysEntry ::=
             SEQUENCE {
                 adGenVdsl2VtucInvSerialNumber         DisplayString,
                 adGenVdsl2VtucInvVendorID             DisplayString,
                 adGenVdsl2VtucInvVersionNumber        DisplayString,
                 adGenVdsl2CurrUSSnrMgn                Integer32,
                 adGenVdsl2CurrUSAtn                   Integer32,
                 adGenVdsl2VtucCurrStatus              Unsigned32,
                 adGenVdsl2VtucCurrOutputPwr           Integer32,
                 adGenVdsl2VtucCurrAttainableRate      Gauge32,
                 adGenVdsl2VtucCurrTxRate              Gauge32,
                 adGenVdsl2VtucPrevTxRate              Gauge32,
                 adGenVdsl2VtucActTxPsd                Integer32,
                 adGenVdsl2VtucCurrTpsTcStatus         INTEGER,
                 adGenVdsl2VtucCurrTxLineRate          Gauge32,
                 adGenVdsl2VtucPrevTxLineRate          Gauge32,
                 adGenVdsl2VtucCircuitProviderCode     DisplayString,
                 adGenVdsl2VtucCurrTxRateMax           Gauge32,
                 adGenVdsl2VtucCurrTxRateMin           Gauge32,
                 adGenVdsl2VtucLastSraDownshift        TimeStamp,
                 adGenVdsl2VtucLastSraUpshift          TimeStamp,
                 adGenVdsl2VtucRtxUsed                 INTEGER,
                 adGenVdsl2VtucRtxEtr                  Gauge32
             }

         -- inventory group
         -- These items should describe the lowest level identifiable
         -- component, be it a stand-alone modem, a card in a rack,
         -- a child-board, etc.
         --
         adGenVdsl2VtucInvSerialNumber OBJECT-TYPE
             SYNTAX      DisplayString --(SIZE (0..32))
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The vendor specific string that identifies the
                 vendor equipment."
         ::= { adGenVdsl2VtucPhysEntry 1 }

         adGenVdsl2VtucInvVendorID OBJECT-TYPE
             SYNTAX      DisplayString --(SIZE (0..16))
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The vendor ID code is a copy of the binary
                 vendor identification field defined by the
                 PHY[10] and expressed as readable characters."

             REFERENCE "ANSI T1.413[10]"
         ::= { adGenVdsl2VtucPhysEntry 2 }

         adGenVdsl2VtucInvVersionNumber OBJECT-TYPE
             SYNTAX      DisplayString --(SIZE (0..16))
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The vendor specific version number sent by this ATU
                 as part of the initialization messages.  It is a copy
                 of the binary version number field defined by the
                 PHY[10] and expressed as readable characters."
             REFERENCE "ANSI T1.413[10]"
         ::= { adGenVdsl2VtucPhysEntry 3 }

         -- current status group
         --
         adGenVdsl2CurrUSSnrMgn OBJECT-TYPE
             SYNTAX      Integer32 (-640..640)
             UNITS       "tenth dB"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Noise Margin as seen by this ATU with respect to its
                 received signal in tenth dB."

         ::= { adGenVdsl2VtucPhysEntry 4 }

         adGenVdsl2CurrUSAtn OBJECT-TYPE
             SYNTAX      Integer32(0..630)
             UNITS       "tenth dB"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Measured difference in the total power transmitted by
                 the peer ATU and the total power received by this ATU."
         ::= { adGenVdsl2VtucPhysEntry 5 }

        adGenVdsl2VtucCurrStatus OBJECT-TYPE
             SYNTAX      Unsigned32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Indicates current state of the Vtuc line.  This is a
                 bit-map of possible conditions.  The various bit
                 positions are:

                        Bit 00 :        There no defects on the line

                        Bit 01 :        Vtuc failure due to not
                                        receiving valid frame.

                        Bit 02 :        Vtuc failure due to not
                                        receiving signal.

                        Bit 03 :        Vtuc failure due to loss of
                                        power.
                                        Note: the Agent may still
                                        function.

                        Bit 04 :        Loss of Signal Quality is
                                        declared when the Noise Margin
                                        falls below the Minimum Noise
                                        Margin, or the bit-error-rate
                                        exceeds 10^-7.

                        Bit 05 :        Vtuc failure due to inability
                                        to link with Vtur.

                        Bit 06 :        Vtuc failure during
                                        initialization due to bit
                                        errors corrupting startup
                                        exchange data.

                        Bit 07 :        Vtuc failure during
                                        initialization due to peer
                                        ATU not able to support
                                        requested configuration

                        Bit 08 :        Vtuc failure during
                                        initialization due to
                                        incompatible protocol used by
                                        the peer ATU.

                        Bit 09 :        Vtuc failure during
                                        initialization due to no
                                        activation sequence detected
                                        from peer ATU.

                 This is intended to supplement ifOperStatus."
         ::= { adGenVdsl2VtucPhysEntry 6 }

         adGenVdsl2VtucCurrOutputPwr OBJECT-TYPE
             SYNTAX      Integer32 (-310..310)
             UNITS       "tenth dBm"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Measured total output power transmitted by this ATU.
                 This is the measurement that was reported during
                 the last activation sequence."
         ::= { adGenVdsl2VtucPhysEntry 7 }

         adGenVdsl2VtucCurrAttainableRate OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Indicates the maximum currently attainable data rate
                 by the ATU.  This value will be equal or greater than
                 the current line rate."
         ::= { adGenVdsl2VtucPhysEntry 8 }

         adGenVdsl2VtucCurrTxRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The payload rate of the last successful train."
         ::= { adGenVdsl2VtucPhysEntry 9 }

         adGenVdsl2VtucPrevTxRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The payload rate of the previous successful train."
         ::= { adGenVdsl2VtucPhysEntry 10 }

         adGenVdsl2VtucActTxPsd  OBJECT-TYPE
             SYNTAX      Integer32 (-900..0 | 2147483647)
             UNITS       "0.1 dB"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Actual Power Spectrum Density (PSD) transmitted.  The average
                  transmit PSD over the sub-carriers.
                  It ranges from -900 to 0 units of 0.1 dB
                  (Physical values are -90 to 0 dBm/Hz).
                  A value of 0x7FFFFFFF (2147483647) indicates the measurement
                  is out of range to be represented."
         ::= { adGenVdsl2VtucPhysEntry 11 }

         adGenVdsl2VtucCurrTpsTcStatus  OBJECT-TYPE
             SYNTAX INTEGER {
                 noDefect(0),
                 outOfSync(1)
                 }
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "Attributes with this syntax are status parameters that
                 reflect the failure status for Transmission Convergence (TC)
                 layer of a given data layer interface (data path over a VDSL2
                 /ADSL/ADSL2 or ADSL2+ link)."
         ::= { adGenVdsl2VtucPhysEntry 12 }

         adGenVdsl2VtucCurrTxLineRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The line rate, including all payload and
                 overhead rates."
         ::= { adGenVdsl2VtucPhysEntry 13 }

         adGenVdsl2VtucPrevTxLineRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The line rate, including all payload and
                 overhead rates, of the previous successful train."
         ::= { adGenVdsl2VtucPhysEntry 14 }

         adGenVdsl2VtucCircuitProviderCode  OBJECT-TYPE
             SYNTAX      DisplayString
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns local circuit provider code"
         ::= { adGenVdsl2VtucPhysEntry 15 }

         adGenVdsl2VtucCurrTxRateMax  OBJECT-TYPE
             SYNTAX      Gauge32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns max payload rate since last successful
                 train."
         ::= { adGenVdsl2VtucPhysEntry 16 }

         adGenVdsl2VtucCurrTxRateMin  OBJECT-TYPE
             SYNTAX      Gauge32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns min payload rate since last successful
                 train."
         ::= { adGenVdsl2VtucPhysEntry 17 }

         adGenVdsl2VtucLastSraDownshift  OBJECT-TYPE
             SYNTAX      TimeStamp
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns system uptime of last SRA downshift
                 since last successful train."
         ::= { adGenVdsl2VtucPhysEntry 18 }

         adGenVdsl2VtucLastSraUpshift  OBJECT-TYPE
             SYNTAX      TimeStamp
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns system uptime of last SRA upshift
                 since last successful train."
         ::= { adGenVdsl2VtucPhysEntry 19 }

         adGenVdsl2VtucRtxUsed  OBJECT-TYPE
             SYNTAX  INTEGER {
                 rtxInUse                            (1),
                 rtxNotInUseForbidden                (2),
                 rtxNotInUseNotSupportedXTUC         (3),
                 rtxNotInUseNotSupportedXTUR         (4),
                 rtxNotInUseNotSupportedXTUCAndXTUR  (5)
             }
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Specifies whether retransmission is active in the
                  transmit direction."
             REFERENCE "Recommendation ITU-T G.997.1, Clause 7.5.1.38"
         ::= { adGenVdsl2VtucPhysEntry 20 }

         adGenVdsl2VtucRtxEtr  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Expected throughput rate calculated from combined effect of
                  impulse noise protection against worst-case REIN and worst-case
                  SHINE and overhead from correction of stationary noise errors."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.2.1"
         ::= { adGenVdsl2VtucPhysEntry 21 }


         -- ============ Status Group (VturPhysTable) ========================
         adGenVdsl2VturPhysTable        OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VturPhysEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtur.
                 Each row contains the Physical Layer Parameters
                 table for that Vtur. VDSL2 physical interfaces are
                 those ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2Status 3 }

         adGenVdsl2VturPhysEntry       OBJECT-TYPE
             SYNTAX          AdGenVdsl2VturPhysEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2VturPhysTable."
             INDEX           { ifIndex }
         ::= { adGenVdsl2VturPhysTable 1 }

         AdGenVdsl2VturPhysEntry ::=
             SEQUENCE {
                 adGenVdsl2VturInvSerialNumber         DisplayString,
                 adGenVdsl2VturInvVendorID             DisplayString,
                 adGenVdsl2VturInvVersionNumber        DisplayString,
                 adGenVdsl2CurrDSSnrMgn                Integer32,
                 adGenVdsl2CurrDSAtn                   Integer32,
                 adGenVdsl2VturCurrStatus              Unsigned32,
                 adGenVdsl2VturCurrOutputPwr           Integer32,
                 adGenVdsl2VturCurrAttainableRate      Gauge32,
                 adGenVdsl2VturCurrTxRate              Gauge32,
                 adGenVdsl2VturPrevTxRate              Gauge32,
                 adGenVdsl2VturActTxPsd                Integer32,
                 adGenVdsl2VturCurrTxLineRate          Gauge32,
                 adGenVdsl2VturPrevTxLineRate          Gauge32,
                 adGenVdsl2VturCircuitProviderCode     DisplayString,
                 adGenVdsl2VturCircuitCapabilities     OCTET STRING,
                 adGenVdsl2VturCurrTxRateMax           Gauge32,
                 adGenVdsl2VturCurrTxRateMin           Gauge32,
                 adGenVdsl2VturLastSraDownshift        TimeStamp,
                 adGenVdsl2VturLastSraUpshift          TimeStamp,
                 adGenVdsl2VturRtxUsed                 INTEGER,
                 adGenVdsl2VturRtxEtr                  Gauge32
             }

         -- inventory group
         --
         adGenVdsl2VturInvSerialNumber OBJECT-TYPE
             SYNTAX      DisplayString --(SIZE (0..32))
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The vendor specific string that identifies the
                 vendor equipment."
         ::= { adGenVdsl2VturPhysEntry 1 }

         adGenVdsl2VturInvVendorID OBJECT-TYPE
             SYNTAX      DisplayString --(SIZE (0..16))
             MAX-ACCESS  read-only


             STATUS      current
             DESCRIPTION
                 "The vendor ID code is a copy of the binary
                 vendor identification field defined by the
                 PHY[10] and expressed as readable characters."
             REFERENCE "ANSI T1.413"
         ::= { adGenVdsl2VturPhysEntry 2 }

         adGenVdsl2VturInvVersionNumber OBJECT-TYPE
             SYNTAX      DisplayString --(SIZE (0..16))
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The vendor specific version number sent by this ATU
                 as part of the initialization messages.  It is a copy
                 of the binary version number field defined by the
                 PHY[10] and expressed as readable characters."
             REFERENCE "ANSI T1.413"
         ::= { adGenVdsl2VturPhysEntry 3 }

         -- current status group
         --
         adGenVdsl2CurrDSSnrMgn OBJECT-TYPE
             SYNTAX      Integer32 (-640..640)
             UNITS       "tenth dB"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Noise Margin as seen by this ATU with respect to its
                 received signal in tenth dB."
         ::= { adGenVdsl2VturPhysEntry 4 }

         adGenVdsl2CurrDSAtn OBJECT-TYPE
             SYNTAX      Integer32(0..630)
             UNITS       "tenth dB"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Measured difference in the total power transmitted by
                 the peer ATU and the total power received by this ATU."
         ::= { adGenVdsl2VturPhysEntry 5 }

        adGenVdsl2VturCurrStatus OBJECT-TYPE
             SYNTAX     Unsigned32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Indicates current state of the Vtur line.  This is a
                 bit-map of possible conditions.  Due to the isolation
                 of the Vtur when line problems occur, many state
                 conditions like loss of power, loss of quality signal,
                 and initialization errors,  can not be determined.
                 While trouble shooting Vtur, also use object,
                 adGenVdsl2VtucCurrStatus.  The various bit positions are:

                        Bit 00 :        There no defects on the line

                        Bit 01 :        Vtur failure due to not
                                        receiving valid frame.

                        Bit 02 :        Vtur failure due to not
                                        receiving signal.

                        Bit 03 :        Vtur failure due to loss of
                                        power.
                                        Note: the Agent may still
                                        function.

                        Bit 04 :        Loss of Signal Quality is
                                        declared when the Noise Margin
                                        falls below the Minimum Noise
                                        Margin, or the bit-error-rate
                                        exceeds 10^-7.


                 This is intended to supplement ifOperStatus."
         ::= { adGenVdsl2VturPhysEntry 6 }

         adGenVdsl2VturCurrOutputPwr OBJECT-TYPE
             SYNTAX      Integer32 (-310..310)
             UNITS       "tenth dBm"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Measured total output power transmitted by this ATU.
                 This is the measurement that was reported during
                 the last activation sequence."
         ::= { adGenVdsl2VturPhysEntry 7 }

         adGenVdsl2VturCurrAttainableRate OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Indicates the maximum currently attainable data rate
                 by the ATU.  This value will be equal or greater than
                 the current line rate."
         ::= { adGenVdsl2VturPhysEntry 8 }


         adGenVdsl2VturCurrTxRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The payload rate of the last successful train."
         ::= { adGenVdsl2VturPhysEntry 9 }

         adGenVdsl2VturPrevTxRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The payload rate of the previous successful train."
         ::= { adGenVdsl2VturPhysEntry 10 }

         adGenVdsl2VturActTxPsd  OBJECT-TYPE
             SYNTAX      Integer32 (-900..0 | 2147483647)
             UNITS       "0.1 dB"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Actual Power Spectrum Density (PSD) transmitted.  The average
                  transmit PSD over the sub-carriers.
                  It ranges from -900 to 0 units of 0.1 dB
                  (Physical values are -90 to 0 dBm/Hz).
                  A value of 0x7FFFFFFF (2147483647) indicates the measurement
                  is out of range to be represented."
         ::= { adGenVdsl2VturPhysEntry 11 }

         adGenVdsl2VturCurrTxLineRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The line rate, including all payload and
                 overhead rates."
         ::= { adGenVdsl2VturPhysEntry 12 }

         adGenVdsl2VturPrevTxLineRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The line rate, including all payload and
                 overhead rates, of the previous successful train."
         ::= { adGenVdsl2VturPhysEntry 13 }

         adGenVdsl2VturCircuitProviderCode  OBJECT-TYPE
             SYNTAX      DisplayString
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns remote circuit provider code"
         ::= { adGenVdsl2VturPhysEntry 14 }

         adGenVdsl2VturCircuitCapabilities  OBJECT-TYPE
             SYNTAX      OCTET STRING (SIZE (8))
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns remote circuit capabilities in bit masks with following order:
                 0 - Disabled, 1 - Enabled
                 Byte Order of octet string:
                 Byte 0. Byte 1. Byte 2. Byte 3. Byte 4. Byte 5. Byte 6. Byte 7

                 Byte 0: Bit 2 - G992.1 Annex A (Full Rate ADSL over POTS)
                         Bit 4 - G992.1 Annex B (Full Rate ADSL over ISDN)
                         Bit 6 - G992.1 Annex C (Full Rate ADSL in TCM-ISDN)
                 Byte 1: Bit 0 - G992.2 Annex A/B (Lite ADSL Over POTS/ISDN)
                         Bit 2 - G992.2 Annex C (Lite ADSL in TCM-ISDN)
                 Byte 2: Bit 2 - G992.3 Annex A/L (ADSL2 over POTS)
                         Bit 4 - G992.3 Annex B   (ADSL2 over ISDN)
                 Byte 3: Bit 0 - G992.4 Annex A   (ADSL2 Lite Over POTS)
                         Bit 4 - G992.3 Annex I   (All Digital ADSL2 (POTS))
                         Bit 6 - G992.3 Annex J   (All Digital ADSL2 (ISDN))
                 Byte 4: Bit 0 - G992.4 Annex I   (All Digital ADSL2 Lite)
                         Bit 2 - G992.3 Annex L Mode 1 Wide Upstream (RE-ADSL2 Over POTS)
                         Bit 6 - G992.3 Annex M   (Double Upstream ADSL2)
                 Byte 5: Bit 0 - G992.5 Annex A   (ADSL2+ over POTS)
                         Bit 2 - G992.5 Annex B   (ADSL2+ Over ISDN)
                         Bit 6 - G992.5 Annex I   (All Digital ADSL2+ (POTS))
                 Byte 6: Bit 0 - G992.5 Annex J   (All Digital ADSL2+ (ISDN))
                         Bit 2 - G992.5 Annex M   (Double Upstream ADSL2+)
                 Byte 7: Bit 0 - VDSL2 (North America)
                         Bit 1 - VDSL2 (Europe)
                         Bit 2 - VDSL2 (Japan)

                 "
         ::= { adGenVdsl2VturPhysEntry 15 }

         adGenVdsl2VturCurrTxRateMax  OBJECT-TYPE
             SYNTAX      Gauge32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns max payload rate since last successful
                 train."

         ::= { adGenVdsl2VturPhysEntry 16 }

         adGenVdsl2VturCurrTxRateMin  OBJECT-TYPE
             SYNTAX      Gauge32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns min payload rate since last successful
                 train."

         ::= { adGenVdsl2VturPhysEntry 17 }

         adGenVdsl2VturLastSraDownshift  OBJECT-TYPE
             SYNTAX      TimeStamp
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns system uptime of last SRA downshift
                 since last successful train."

         ::= { adGenVdsl2VturPhysEntry 18 }

         adGenVdsl2VturLastSraUpshift  OBJECT-TYPE
             SYNTAX      TimeStamp
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Returns system uptime of last SRA upshift
                 since last successful train."

         ::= { adGenVdsl2VturPhysEntry 19 }

         adGenVdsl2VturRtxUsed  OBJECT-TYPE
             SYNTAX  INTEGER {
                 rtxInUse                            (1),
                 rtxNotInUseForbidden                (2),
                 rtxNotInUseNotSupportedXTUC         (3),
                 rtxNotInUseNotSupportedXTUR         (4),
                 rtxNotInUseNotSupportedXTUCAndXTUR  (5)
             }
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Specifies whether retransmission is active in the
                  transmit direction."
             REFERENCE "Recommendation ITU-T G.997.1, Clause 7.5.1.38"
         ::= { adGenVdsl2VturPhysEntry 20 }

         adGenVdsl2VturRtxEtr  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Expected throughput rate calculated from combined effect of
                  impulse noise protection against worst-case REIN and worst-case
                  SHINE and overhead from correction of stationary noise errors."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.2.1"
         ::= { adGenVdsl2VturPhysEntry 21 }


         -- ============ Status Group (VtucChanTable) ========================
         adGenVdsl2VtucChanTable       OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VtucChanEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                "This table provides one row for each Vtuc channel.
                 VDSL2 channel interfaces are those ifEntries
                 where ifType is equal to channel.
                "
         ::= { adGenVdsl2Status 4 }

         adGenVdsl2VtucChanEntry       OBJECT-TYPE
             SYNTAX          AdGenVdsl2VtucChanEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2VtucChanTable."
             INDEX           { ifIndex, adGenVdsl2VtucChannelNumber}
         ::= { adGenVdsl2VtucChanTable 1 }

         AdGenVdsl2VtucChanEntry ::=
             SEQUENCE {
                 adGenVdsl2VtucChannelNumber           Integer32,
                 adGenVdsl2VtucChanInterleaveDelay     Gauge32,
                 adGenVdsl2VtucChanCurrTxRate          Gauge32,
                 adGenVdsl2VtucChanPrevTxRate          Gauge32,
                 adGenVdsl2VtucChanCrcBlockLength      Gauge32,
                 adGenVdsl2VtucChanINP                 Gauge32,
                 adGenVdsl2VtucChanINPRein             Gauge32
             }

         adGenVdsl2VtucChannelNumber OBJECT-TYPE
             SYNTAX      Integer32(1..4)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "VDSL2 logical channel. Channel values are 1 - 4."
         ::= { adGenVdsl2VtucChanEntry 1 }

         adGenVdsl2VtucChanInterleaveDelay OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "0.1 milli-seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Interleave Delay for this channel.

                 Interleave delay applies only to the
                 interleave channel and defines the mapping
                 (relative spacing) between subsequent input
                 bytes at the interleaver input and their placement
                 in the bit stream at the interleaver output.

                 Larger numbers provide greater separation between
                 consecutive input bytes in the output bit stream
                 allowing for improved impulse noise immunity at
                 the expense of payload latency.

                 In the case where the ifType is Fast(125), use
                 noSuchObject."
         ::= { adGenVdsl2VtucChanEntry 2 }

         adGenVdsl2VtucChanCurrTxRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Actual transmit rate on this channel."
         ::= { adGenVdsl2VtucChanEntry 3 }

         adGenVdsl2VtucChanPrevTxRate  OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The rate at the time of the last
                 adGenVdsl2VtucRateChangeTrap event. It is also set at
                 initialization to prevent a trap being sent.
                 Rate changes less than adGenVdsl2VtucThresh(*)RateDown
                 or less than adGenVdsl2VtucThresh(*)RateUp will not
                 cause a trap or cause this object to change.
                 (*) == Fast or Interleave.
                 See AdGenVdsl2LineAlarmConfProfileEntry."
         ::= { adGenVdsl2VtucChanEntry 4 }

         adGenVdsl2VtucChanCrcBlockLength OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS        "byte"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Indicates the length of the channel data-block
                 on which the CRC operates.  Refer to Line Code
                 Specific MIBs, [11] and [12] for more
                 information."
         ::= { adGenVdsl2VtucChanEntry 5 }

         adGenVdsl2VtucChanINP OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "0.1 dmt symbols"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Current impulse noise protection on this channel,
                 measured in increments of 0.1 DMT symbols."
         ::= { adGenVdsl2VtucChanEntry 6 }

         adGenVdsl2VtucChanINPRein OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "0.1 dmt symbols"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Current impulse noise protection against REIN on this
                 channel, measured in increments of 0.1 DMT symbols."
         ::= { adGenVdsl2VtucChanEntry 7 }


         -- ============ Status Group (VturChanTable) ========================

             STATUS      current
             DESCRIPTION
                 "Count of low error-free throughput rate defect
                  seconds in the specified 24-hr interval."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.4.1"
         ::= { adGenVdsl2Vtuc1DayIntervalEntry 19 }


         -- ========= PM Group (adGenVdsl2Vtur1DayIntervalTable) =================
         adGenVdsl2Vtur1DayIntervalTable   OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2Vtur1DayIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtur
                 performance data collection 1 Day interval.
                 VDSL2 physical interfaces are those
                 ifEntries where ifType is equal to vdsl(97)."
         ::= { adGenVdsl2PM 10 }

         adGenVdsl2Vtur1DayIntervalEntry   OBJECT-TYPE
             SYNTAX          AdGenVdsl2Vtur1DayIntervalEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in the adGenVdsl2Vtur1DayIntervalTable."
             INDEX           { ifIndex, adGenVdsl2Vtur1DayIntervalNumber }
         ::= { adGenVdsl2Vtur1DayIntervalTable 1 }

         AdGenVdsl2Vtur1DayIntervalEntry ::=
             SEQUENCE {
                 adGenVdsl2Vtur1DayIntervalNumber         Integer32,
                 adGenVdsl2Vtur1DayIntervalLofs           Counter32,
                 adGenVdsl2Vtur1DayIntervalLoss           Counter32,
                 adGenVdsl2Vtur1DayIntervalLprs           Counter32,
                 adGenVdsl2Vtur1DayIntervalES             Counter32,
                 adGenVdsl2Vtur1DayIntervalValidData        INTEGER,
                 adGenVdsl2Vtur1DayIntervalSES            Counter32,
                 adGenVdsl2Vtur1DayIntervalUAS            Counter32,
                 adGenVdsl2Vtur1DayIntervalFECs           Counter32,
                 adGenVdsl2Vtur1DayIntervalFEC            Counter32,
                 adGenVdsl2Vtur1DayIntervalCRC            Counter32,
                 adGenVdsl2Vtur1DayIntervalSraDownshifts  Counter32,
                 adGenVdsl2Vtur1DayIntervalSraUpshifts    Counter32,
                 adGenVdsl2Vtur1DayIntervalSraRateMax       Gauge32,
                 adGenVdsl2Vtur1DayIntervalSraRateMin       Gauge32,
                 adGenVdsl2Vtur1DayIntervalRtxMinEftr       Gauge32,
                 adGenVdsl2Vtur1DayIntervalRtxLeftrs      Counter32
             }

         adGenVdsl2Vtur1DayIntervalNumber OBJECT-TYPE
             SYNTAX      Integer32(1..7)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Performance Data Interval number 1 is the
                 the most recent previous 1 day interval;
                 interval 7 is 7 Days ago.  Intervals 1..7 are
                 optional."

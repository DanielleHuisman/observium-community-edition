             STATUS      current
             DESCRIPTION
                 "Count of Errored Seconds in the interval."
         ::= { adGenVdsl2VturIntervalEntry 5 }

         adGenVdsl2VturIntervalValidData OBJECT-TYPE
             SYNTAX      INTEGER {
                 valid (1),
                 notValid (2)
             }
             MAX-ACCESS read-only
             STATUS current
             DESCRIPTION
                 "This variable indicates if the data for this
                  interval is valid."
         ::= { adGenVdsl2VturIntervalEntry 6 }

         adGenVdsl2VturIntervalSES OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Severely Errored Seconds in the interval."
         ::= { adGenVdsl2VturIntervalEntry 7 }


             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of Unavailable Seconds in the interval."
         ::= { adGenVdsl2VturIntervalEntry 8 }

         adGenVdsl2VturIntervalFECs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of seconds in the interval during which at
                 least one forward error correction event took
                 place."
         ::= { adGenVdsl2VturIntervalEntry 9 }

         adGenVdsl2VturIntervalFEC OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of forward error correction events that
                 took place in the interval."
         ::= { adGenVdsl2VturIntervalEntry 10 }

         adGenVdsl2VturIntervalCRC OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of CRC anomolies that took place in
                  the interval."
         ::= { adGenVdsl2VturIntervalEntry 11 }

         adGenVdsl2VturIntervalSraDownshifts OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "downshifts"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of SRA payload rate downshifts since last
                 successful train in the specified 15-min interval"
         ::= { adGenVdsl2VturIntervalEntry 12 }

         adGenVdsl2VturIntervalSraUpshifts OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "upshifts"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of SRA payload rate upshifts since last
                 successful train in the specified 15-min interval"
         ::= { adGenVdsl2VturIntervalEntry 13 }

         adGenVdsl2VturIntervalSraRateMax OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Max payload rate in the specified 15-min interval
                 since last successful train."
         ::= { adGenVdsl2VturIntervalEntry 14 }

         adGenVdsl2VturIntervalSraRateMin OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Min payload rate in the specified 15-min interval
                 since last successful train."
         ::= { adGenVdsl2VturIntervalEntry 15 }

         adGenVdsl2VturIntervalRtxMinEftr OBJECT-TYPE
             SYNTAX      Gauge32
             UNITS       "bps"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Retransmission parameter defined as the minimum of
                  the error-free throughput rate observed in the specified
                  15-min interval."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.4.3"
         ::= { adGenVdsl2VturIntervalEntry 16 }

         adGenVdsl2VturIntervalRtxLeftrs OBJECT-TYPE
             SYNTAX      Counter32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of low error-free throughput rate defect
                  seconds in the specified 15-min interval."
             REFERENCE "Recommendation ITU-T G.998.4, Clause 11.4.1"
         ::= { adGenVdsl2VturIntervalEntry 17 }


         -- ========= PM Group (adGenVdsl2VtucChanPerfDataTable) =============
         adGenVdsl2VtucChanPerfDataTable       OBJECT-TYPE
             SYNTAX          SEQUENCE OF AdGenVdsl2VtucChanPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION
                 "This table provides one row for each Vtuc channel.
                 VDSL2 channel interfaces can range from 1 -4"
         ::= { adGenVdsl2PM 5 }

         adGenVdsl2VtucChanPerfDataEntry       OBJECT-TYPE

             SYNTAX          AdGenVdsl2VtucChanPerfDataEntry
             MAX-ACCESS      not-accessible
             STATUS          current
             DESCRIPTION     "An entry in adGenVdsl2VtucChanPerfDataTable."
             INDEX           { ifIndex, adGenVdsl2VtucChanNumber }
         ::= { adGenVdsl2VtucChanPerfDataTable 1 }

         AdGenVdsl2VtucChanPerfDataEntry ::=
          SEQUENCE {
              adGenVdsl2VtucChanNumber                            Integer32,
              adGenVdsl2VtucChanReceivedBlks                      Counter32,
              adGenVdsl2VtucChanTransmittedBlks                   Counter32,
              adGenVdsl2VtucChanCorrectedBlks                     Counter32,
              adGenVdsl2VtucChanUncorrectBlks                     Counter32,
              adGenVdsl2VtucChanPerfValidIntervals                Integer32,
              adGenVdsl2VtucChanPerfInvalidIntervals              Integer32,
              adGenVdsl2VtucChanPerfCurr15MinTimeElapsed          Gauge32,
              adGenVdsl2VtucChanPerfCurr15MinReceivedBlks         Counter32,
              adGenVdsl2VtucChanPerfCurr15MinTransmittedBlks      Counter32,
              adGenVdsl2VtucChanPerfCurr15MinCorrectedBlks        Counter32,
              adGenVdsl2VtucChanPerfCurr15MinUncorrectBlks        Counter32,
              adGenVdsl2VtucChanPerfCurr1DayTimeElapsed           Gauge32,
              adGenVdsl2VtucChanPerfCurr1DayReceivedBlks          Counter32,
              adGenVdsl2VtucChanPerfCurr1DayTransmittedBlks       Counter32,
              adGenVdsl2VtucChanPerfCurr1DayCorrectedBlks         Counter32,
              adGenVdsl2VtucChanPerfCurr1DayUncorrectBlks         Counter32,
              adGenVdsl2VtucChanPerfTcTxUnits                     Counter32,
              adGenVdsl2VtucChanPerfTcTxDataUnits                 Counter32,
              adGenVdsl2VtucChanPerfTcTxDataOctets                Counter32,
              adGenVdsl2VtucChanPerfTcTxIdleUnits                 Counter32,
              adGenVdsl2VtucChanPerfTcRxUnits                     Counter32,
              adGenVdsl2VtucChanPerfTcRxDataUnits                 Counter32,
              adGenVdsl2VtucChanPerfTcRxDataOctets                Counter32,
              adGenVdsl2VtucChanPerfTcRxIdleUnits                 Counter32,
              adGenVdsl2VtucChanPerfTcRxErroredUnits              Counter32,
              adGenVdsl2VtucChanPerfRtxUncorrectedDtu             Counter32,
              adGenVdsl2VtucChanPerfRtxCorrectedDtu               Counter32,
              adGenVdsl2VtucChanPerfRtxRetransmittedDtu           Counter32,
              adGenVdsl2VtucChanPerfCurr15MinRtxUncorrectedDtu    Counter32,
              adGenVdsl2VtucChanPerfCurr15MinRtxCorrectedDtu      Counter32,
              adGenVdsl2VtucChanPerfCurr15MinRtxRetransmittedDtu  Counter32,
              adGenVdsl2VtucChanPerfCurr1DayRtxUncorrectedDtu     Counter32,
              adGenVdsl2VtucChanPerfCurr1DayRtxCorrectedDtu       Counter32,
              adGenVdsl2VtucChanPerfCurr1DayRtxRetransmittedDtu   Counter32
          }

         adGenVdsl2VtucChanNumber OBJECT-TYPE
             SYNTAX      Integer32(1..4)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "VDSL2 logical channel. Channel values are 1 - 4."
         ::= { adGenVdsl2VtucChanPerfDataEntry 1 }


         adGenVdsl2VtucChanReceivedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks received on this channel
                 since agent reset."
         ::= { adGenVdsl2VtucChanPerfDataEntry 2 }

         adGenVdsl2VtucChanTransmittedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks transmitted on this
                 channel since agent reset."
         ::= { adGenVdsl2VtucChanPerfDataEntry 3 }

         adGenVdsl2VtucChanCorrectedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with errors that were
                 corrected since agent reset.  These blocks are passed
                 on as good data."
         ::= { adGenVdsl2VtucChanPerfDataEntry 4 }

         adGenVdsl2VtucChanUncorrectBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with uncorrectable
                 errors since agent reset."
         ::= { adGenVdsl2VtucChanPerfDataEntry 5 }

         adGenVdsl2VtucChanPerfValidIntervals OBJECT-TYPE
             SYNTAX      Integer32(0..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The number of previous 15-minute intervals in the
                 interval table for which data was collected.  Given
                 that <n> is the maximum # of intervals supported.
                 The value will be <n> unless the measurement was
                 (re-)started within the last (<n>*15) minutes, in which
                 case the value will be the number of complete 15
                 minute intervals for which the agent has at least
                 some data. In certain cases (e.g., in the case
                 where the agent is a proxy) it is possible that some
                 intervals are unavailable.  In this case, this
                 interval is the maximum interval number for
                 which data is available."
         ::= { adGenVdsl2VtucChanPerfDataEntry 6 }

         adGenVdsl2VtucChanPerfInvalidIntervals OBJECT-TYPE
             SYNTAX      Integer32(0..96)
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The number of intervals in the range from
                 0 to the value of adGenVdsl2VtucChanPerfValidIntervals
                 for which no data is available. This object
                 will typically be zero except in cases where
                 the data for some intervals are not available
                 (e.g., in proxy situations)."
         ::= { adGenVdsl2VtucChanPerfDataEntry 7 }

         adGenVdsl2VtucChanPerfCurr15MinTimeElapsed OBJECT-TYPE
             SYNTAX      Gauge32(0..899)
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Total elapsed seconds in this interval."
         ::= { adGenVdsl2VtucChanPerfDataEntry 8}

         adGenVdsl2VtucChanPerfCurr15MinReceivedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks received on this channel
                 within the current 15 minute interval."
         ::= { adGenVdsl2VtucChanPerfDataEntry 9 }


         adGenVdsl2VtucChanPerfCurr15MinTransmittedBlks OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all encoded blocks transmitted on this
                 channel within the current 15 minute interval."
         ::= { adGenVdsl2VtucChanPerfDataEntry 10 }

         adGenVdsl2VtucChanPerfCurr15MinCorrectedBlks  OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "Count of all blocks received with errors that were

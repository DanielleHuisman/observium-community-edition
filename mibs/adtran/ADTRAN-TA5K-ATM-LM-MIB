        SYNTAX  INTEGER {
                                loop1(1),
                                loop2(2),
                                loop3(3),
                                loop4(4),
                                loop5(5),
                                loop6(6),
                                loop7(7),
                                loop8(8),
                                loop9(9),
                                loop10(10),
                                loop11(11),
                                loop12(12),
                                loop13(13),
                                loop14(14),
                                loop15(15),
                                loop16(16),
                                loop17(17),
                                loop18(18),
                                loop19(19),
                                loop20(20),
                                loop21(21),
                                loop22(22),
                                loop23(23),
                                loop24(24),
                                loop25(25),
                                loop26(26),
                                loop27(27),
                                loop28(28),
                                loop29(29),
                                loop30(30),
                                loop31(31),
                                loop32(32),
                                none(255)
                        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "LoopA Timing Source."
        ::= { adGenTa5kAtmLMLoopTimingEntry 1 }

    adGenTa5kAtmLMLoopTimingB OBJECT-TYPE
        SYNTAX  INTEGER {
                                loop1(1),
                                loop2(2),
                                loop3(3),
                                loop4(4),
                                loop5(5),
                                loop6(6),
                                loop7(7),
                                loop8(8),
                                loop9(9),
                                loop10(10),
                                loop11(11),
                                loop12(12),

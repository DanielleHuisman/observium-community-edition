                                 cLI(5)
                              }
               MAX-ACCESS  read-write
               STATUS  current
               DESCRIPTION
                  "Admin port communication mode."
               ::= { adTAeSCUAdminPort 1 }

            adTAeSCUAdminPortModeOpti OBJECT-TYPE
               SYNTAX INTEGER {
                                 menus(1),
                                 tl1(2),
                                 terminalServer(3)
                              }
               MAX-ACCESS  read-write
               STATUS  current
               DESCRIPTION

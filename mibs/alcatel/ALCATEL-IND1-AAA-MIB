        aaaRedirectServerRowStatus,
             -- Pause timer
        aaaRedirectPauseTimerConfig,
             -- Port Bounce
        aaaPortBounceConfig,
             -- Port Bounce slot/port
        aaaPortBounceStatus,
        aaaBYODWhiteListIPAddress,
        aaaBYODWhiteListIPMask,
        aaaBYODWhiteListRowStatus  
  
     }
     STATUS  current
     DESCRIPTION
        "Collection of Redirect Objects for Management."
     ::= { alcatelIND1AAAMIBGroups 12 }		
       

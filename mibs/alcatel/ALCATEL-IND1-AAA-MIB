--
-- AAA-HIC Trap description
--

alaAaaTrapsDesc  OBJECT IDENTIFIER ::= { alaAaaTraps 1 }
alaAaaTrapsDescRoot  OBJECT IDENTIFIER ::= { alaAaaTrapsDesc 0 }
alaAaaTrapsObj  OBJECT IDENTIFIER ::= { alaAaaTraps 2 }

    aaaHicServerTrap  NOTIFICATION-TYPE
        OBJECTS {
	    aaaHSvrIpAddress,
            aaaHSvrRole,
            aaaHSvrName		
	}
	STATUS  current
        DESCRIPTION
	    "AAA HIC server down Trap is sent when the HIC server is down."
	::= {alaAaaTrapsDescRoot 1 }

    aaaHicServerChangeTrap  NOTIFICATION-TYPE   
         OBJECTS {   
             aaaHSvrIpAddress,   
             aaaHSvrCurrIpAddress   
         }   
         STATUS  current   
         DESCRIPTION   
             "AAA HIC server Change Trap is sent when the active HIC server is changed from one server to primary to back ot back to primary."   
         ::= {alaAaaTrapsDescRoot 2 } 

    aaaHicServerUpTrap NOTIFICATION-TYPE   
         OBJECTS {   
             aaaHSvrIpAddress,
             aaaHSvrRole,
             aaaHSvrName
         }   
         STATUS  current   
         DESCRIPTION   
             "AAA HIC server Up trap is send when at least one of the eserver cam UP."   
         ::= {alaAaaTrapsDescRoot 3 } 

--
-- Object used in the trap
--

        aaaHSvrIpAddress       OBJECT-TYPE
            SYNTAX               IpAddress
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "HIC/Rem/WebDL server's IP address."
            ::= { alaAaaTrapsObj 1 }

       aaaHSvrCurrIpAddress       OBJECT-TYPE   
             SYNTAX               IpAddress   
             MAX-ACCESS           accessible-for-notify   
             STATUS               current   
             DESCRIPTION          "Current active HIC server's IP address."   
             ::= { alaAaaTrapsObj 2 }   

        aaaHSvrRole     OBJECT-TYPE
            SYNTAX               INTEGER
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "HIC server's Role."
            ::= { alaAaaTrapsObj 3 }

        aaaHSvrName       OBJECT-TYPE
            SYNTAX               DisplayString
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "HIC server's Name."
            ::= { alaAaaTrapsObj 4 }

--  AAA authentication TRAPS
    alaAaaAuthTrapsDesc  OBJECT IDENTIFIER ::= { alaAaaTraps 3 }
    alaAaaAuthTrapsDescRoot  OBJECT IDENTIFIER ::= { alaAaaAuthTrapsDesc 0 }
    alaAaaAuthTrapsObj  OBJECT IDENTIFIER ::= { alaAaaTraps 4 }
    aaaAuthenticationFailureTrap NOTIFICATION-TYPE 
    OBJECTS  {
               aaaAuthSysName,
               aaaAuthIpAddress,
               aaaAuthPort,
               aaaAuthUserName,
               aaaAuthType,
               aaaAuthFailureReason
             }
    STATUS        current
    DESCRIPTION
       "This trap shall be raised, when ever
        user authentication gets failed."
    ::= { alaAaaAuthTrapsDescRoot 1 }
--
-- Object used in the trap
--
       aaaAuthSysName       OBJECT-TYPE
            SYNTAX               DisplayString
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "System name"
            ::= { alaAaaAuthTrapsObj 1 }
       aaaAuthIpAddress       OBJECT-TYPE   
             SYNTAX               IpAddress   
             MAX-ACCESS           accessible-for-notify   
             STATUS               current   
             DESCRIPTION         "This is the IP-address with which authentication request is sent. It is the IP-Address of the switch "   
             ::= { alaAaaAuthTrapsObj 2 }   
       aaaAuthPort     OBJECT-TYPE
            SYNTAX               INTEGER
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "This the port-ID, which is filled in Auth-request."
            ::= { alaAaaAuthTrapsObj 3 }
       aaaAuthUserName       OBJECT-TYPE
            SYNTAX               DisplayString
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "user-name string."
            ::= { alaAaaAuthTrapsObj 4 }
       aaaAuthType       OBJECT-TYPE
            SYNTAX               DisplayString
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "Type of authentication which took place."
            ::= { alaAaaAuthTrapsObj 5 }
       aaaAuthFailureReason       OBJECT-TYPE
            SYNTAX               INTEGER
            MAX-ACCESS           accessible-for-notify
            STATUS               current
            DESCRIPTION          "Failure reason for which authentication failed."
            ::= { alaAaaAuthTrapsObj 6 }

 --  AAA RADIUS SERVER STATUS TRAPS
      alaAaaRadiusTrapsDesc  OBJECT IDENTIFIER ::= { alaAaaTraps 5 }
      alaAaaRadiusTrapsDescRoot  OBJECT IDENTIFIER ::= { alaAaaRadiusTrapsDesc 0 }
      aaaRadiusServerUpTrap NOTIFICATION-TYPE
      OBJECTS  {
                aaasIpAddress,
                aaasIpAddress2
               }
      STATUS        current
      DESCRIPTION
           "AAA RADIUS server Up trap is sent when the server is reachable."
      ::= { alaAaaRadiusTrapsDescRoot 1 }

      aaaRadiusServerDownTrap NOTIFICATION-TYPE
      OBJECTS  {
                aaasIpAddress,
                aaasIpAddress2 
               }
      STATUS        current
      DESCRIPTION
            "AAA RADIUS server Down trap is sent when the server is unreachable."
      ::= { alaAaaRadiusTrapsDescRoot 2 }


--
-- Compliance Statements
--

    alcatelIND1AAAMIBCompliance MODULE-COMPLIANCE
        STATUS  current
        DESCRIPTION
            "Compliance statement for
             Authentication, Authorization, and Accounting (AAA) Subsystem."
        MODULE  -- this module

            MANDATORY-GROUPS
            {
                aaaServerMIBGroup,
                aaaAuthAcctGroup,
                aaaUserMIBGroup,
                aaaHicGroup,
                aaaVlanGroup,
                aaaUNPMacRangeRuleGroup,
                aaaUNPMacRuleGroup,

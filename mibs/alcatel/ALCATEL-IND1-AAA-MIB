-- AAA Redirect URL config MIB --

        aaaRedirectUrlConfigTable    OBJECT-TYPE
                SYNTAX                       SEQUENCE OF AaaRedirectURLEntry
                MAX-ACCESS                   not-accessible
                STATUS                       current
                DESCRIPTION
                "This table is used to configure redirect URLs for the redirection to happen."
                ::= { aaaRedirectConfig  2 }

        aaaRedirectURLEntry          OBJECT-TYPE
                SYNTAX                       AaaRedirectURLEntry
                MAX-ACCESS                   not-accessible
                STATUS                       current
                DESCRIPTION
                "Redirect URL configuration.Maximum 5 entries can be allowed"
                INDEX { aaaRedirectServerUrlName }
                ::= { aaaRedirectUrlConfigTable 1 }

        AaaRedirectURLEntry          ::= SEQUENCE
        {
                 aaaRedirectServerUrlName    SnmpAdminString,
                 aaaRedirectServerUrl        SnmpAdminString,
                 aaaRedirectServerRowStatus  RowStatus
        }

        aaaRedirectServerUrlName     OBJECT-TYPE
                SYNTAX                       SnmpAdminString ( SIZE( 1 .. 31 ) )
                MAX-ACCESS                   not-accessible
                STATUS                       current
                DESCRIPTION
                "The name of the redirect URL.At maximum 5 redirect URLS can be allowed per switch."
                ::= { aaaRedirectURLEntry 1 }

        aaaRedirectServerUrl         OBJECT-TYPE
                SYNTAX                       SnmpAdminString ( SIZE( 1 .. 31 ) )
                MAX-ACCESS                   read-write
                STATUS                       current
                DESCRIPTION
                "The URL where the redirect should happen. At maximum 5 URLS can be allowed per switch."
                ::= { aaaRedirectURLEntry 2 }

        aaaRedirectServerRowStatus  OBJECT-TYPE
                SYNTAX                      RowStatus
                MAX-ACCESS                  read-create
                STATUS                      current
                DESCRIPTION
                "The RowStatus of the table entry."
                ::= { aaaRedirectURLEntry 3 }

-- END: Redirect URL config MIB --


                ::= { aaaRedirectConfig 4 }

        aaaPortBounceInterfaceEntry  OBJECT-TYPE
                SYNTAX                     AaaPortBounceInterfaceEntry
                MAX-ACCESS                 not-accessible
                STATUS                     current
                DESCRIPTION
                "Entries for aaa port bounce slot/port status."
                INDEX { ifIndex }
                ::= { aaaPortBounceInterfaceTable 1 }

        AaaPortBounceInterfaceEntry ::= SEQUENCE
        {
                aaaPortBouncePortSlot Integer32,
                aaaPortBounceIF       Integer32,
                aaaPortBounceStatus   INTEGER
        }

        aaaPortBouncePortSlot  OBJECT-TYPE
                SYNTAX                 Integer32
                MAX-ACCESS             read-only
                STATUS                 current
                DESCRIPTION

-- START: AAA REDIRECT CONFIG-RELATED MIB---------------------

aaaRedirectConfig   OBJECT IDENTIFIER ::= { alcatelIND1AAAMIBObjects 16 }

-- Redirect server config MIB --

        aaaRedirectServerTable OBJECT-TYPE
                SYNTAX           SEQUENCE OF AaaRedirectServerEntry
                MAX-ACCESS       not-accessible
                STATUS           current
                DESCRIPTION
                "This table is used to configure ClearPass redirect server name with its Ip address and url list."
                ::= { aaaRedirectConfig  1 }

        aaaRedirectServerEntry   OBJECT-TYPE
                SYNTAX                   AaaRedirectServerEntry
                MAX-ACCESS               not-accessible
                STATUS                   current
                DESCRIPTION
                "Redirect server configuration."
                INDEX  { aaaRedirectServerName }
                ::= { aaaRedirectServerTable 1 }

        AaaRedirectServerEntry   ::= SEQUENCE
        {
                aaaRedirectServerName        SnmpAdminString,
                aaaRedirectServerIpAddress   IpAddress,
                aaaRedirectServerUrl1        SnmpAdminString,
                aaaRedirectServerUrl2        SnmpAdminString,
                aaaRedirectServerUrl3        SnmpAdminString,
                aaaRedirectServerUrl4        SnmpAdminString,
                aaaRedirectServerUrl5        SnmpAdminString,
                aaaRedirectSvrConfigRowStatus  RowStatus 
        }

        aaaRedirectServerName   OBJECT-TYPE
                SYNTAX                  SnmpAdminString ( SIZE( 1 .. 31 ) )

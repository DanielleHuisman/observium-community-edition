 --    Current Rate Limit Table On Dot1x Port MIB
 
     alaDot1xCrlUnpTable OBJECT-TYPE
         SYNTAX        SEQUENCE OF AlaDot1xCrlUnpEntry
         MAX-ACCESS    not-accessible
         STATUS        current
         DESCRIPTION
             "Current Rate Limit Table On 8021x Port"
         ::= { alaIND1Dot1XMIBObjects 13 }
 
  alaDot1xCrlUnpEntry OBJECT-TYPE
        SYNTAX        AlaDot1xCrlUnpEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "Current Rate Limit Table entry on a 8021X port."
        INDEX {alaDot1xCrlIfIndex }
        ::= { alaDot1xCrlUnpTable 1 }

  AlaDot1xCrlUnpEntry ::= SEQUENCE
        {
            alaDot1xCrlIfIndex 	         InterfaceIndex,
            alaDot1xCrlIngBw              Integer32,
            alaDot1xCrlEgrBw              Integer32,
            alaDot1xCrlIngTypeFlag        INTEGER,
            alaDot1xCrlEgrTypeFlag        INTEGER,
            alaDot1xCrlDefDepth           Integer32,
            alaDot1xCrlIngProfile         SnmpAdminString,
            alaDot1xCrlEgrProfile         SnmpAdminString
        }

  alaDot1xCrlIfIndex OBJECT-TYPE
  SYNTAX      InterfaceIndex 
  MAX-ACCESS  not-accessible 
  STATUS      current
  DESCRIPTION
       "The ifIndex of the port."
      ::= { alaDot1xCrlUnpEntry 1 }

  alaDot1xCrlIngBw  OBJECT-TYPE
          SYNTAX      Integer32 (-1 .. 10485760)
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                   "Current Ingress BW active on 8021x Port in Kbps.
                    Default value is -1 which means no Ingress Bandwidth exist on port."
         DEFVAL        { -1 }
         ::= { alaDot1xCrlUnpEntry 2 }

  alaDot1xCrlEgrBw  OBJECT-TYPE
          SYNTAX      Integer32 (-1 .. 10485760)
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                   "Current Egress BW active on 8021x Port in Kbps.
                    Default value is -1 which means no Egress Bandwidth exist on port."
         DEFVAL        { -1 }
         ::= { alaDot1xCrlUnpEntry 3 }

  alaDot1xCrlIngTypeFlag  OBJECT-TYPE
          SYNTAX      INTEGER
                {
                        notapplicable(0),
                        unp(1),
                        qos(2)
                }
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                   "Application responsible for setting Ingress BW on port.
                    Default is - which means no application has set BW on port."
         DEFVAL        { 0 }
         ::= { alaDot1xCrlUnpEntry 4 }

  alaDot1xCrlEgrTypeFlag  OBJECT-TYPE
          SYNTAX      INTEGER
                {
                        notapplicable(0),
                        unp(1),
                        qos(2)
                }
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                   "Application responsible for setting Egress BW on port.
                    Default is - which means no application has set BW on port."
		    DEFVAL        { 0 }
         ::= { alaDot1xCrlUnpEntry 5 }

  alaDot1xCrlDefDepth  OBJECT-TYPE
          SYNTAX      Integer32 (-1 .. 131072)
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                   "Latest Default Depth applied on port.
                    Default depth is -1 which means no depth is configured. BW is programmed with default optimal depth."
         DEFVAL        { -1 }
         ::= { alaDot1xCrlUnpEntry 6 }

  alaDot1xCrlIngProfile  OBJECT-TYPE
          SYNTAX      SnmpAdminString ( SIZE( 1 .. 32 ) )
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                   "The name of the UNP Profile Name responsible for setting current Ingress
                   Bandwidth limitation on port. Applicable only if current bandwidth set is
                   due to UNP classification on port."
         ::= { alaDot1xCrlUnpEntry 7 }

  alaDot1xCrlEgrProfile  OBJECT-TYPE
          SYNTAX      SnmpAdminString ( SIZE( 1 .. 32 ) )
          MAX-ACCESS  read-only
          STATUS      current
          DESCRIPTION
                   "The name of the UNP Profile Name responsible for setting current Egress
                   Bandwidth limitation on port. Applicable only if current bandwidth set is
                   due to UNP classification on port"
         ::= { alaDot1xCrlUnpEntry 8 }


   alaByodClientMacAddress  MacAddress,
   alaByodClientDeviceType  ALADot1xDeviceType,
   alaByodPreviousUNP       SnmpAdminString, 
   alaByodNewUNP            SnmpAdminString,
   alaByodCOAStatus         SnmpAdminString
 }


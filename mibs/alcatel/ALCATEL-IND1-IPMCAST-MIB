    ::= { alaIpMcastMIBGroups 7 }

alaIpMcastMIBBoundaryIfGroup OBJECT-GROUP
    OBJECTS { alaIpMcastBoundaryStatus,
              alaIpMcastBoundaryStorageType,
              alaIpMcastBoundaryDroppedMcastOctets,

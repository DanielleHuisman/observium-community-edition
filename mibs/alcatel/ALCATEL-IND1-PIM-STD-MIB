    SYNTAX     AlaPimNbrSecAddressEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
            "An entry (conceptual row) in the alaPimNbrSecAddressTable."
    INDEX      { alaPimNbrSecAddressIfIndex,
                 alaPimNbrSecAddressType,
                 alaPimNbrSecAddressPrimary,
                 alaPimNbrSecAddress }

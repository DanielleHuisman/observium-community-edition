        }

   iphelperDhcpSourceFilterVlanNumber  OBJECT-TYPE
           SYNTAX  Integer32 (1..4094)
           MAX-ACCESS not-accessible
           STATUS  current
           DESCRIPTION
              "The VLAN number identifying this instance. Valid
               range from 1 to 4094."
    ::= { iphelperDhcpSourceFilterVlanEntry 1 }

   iphelperDhcpSourceFilterVlanFilteringStatus OBJECT-TYPE
           SYNTAX  INTEGER {
                enabled(1),
                disabled(2)
            }
           MAX-ACCESS  read-write
           STATUS  current
           DESCRIPTION
                "The DHCP Snooping IP Source filtering  status. Default value
                 is disabled. Once enabled, only the incoming traffic with the proper
                 client IP address, MAC address , port and vlan will be allowed."
                 DEFVAL { disabled }
    ::= { iphelperDhcpSourceFilterVlanEntry 2 }


   iphelperDhcpSnoopingOption82FormatASCIIConfigurableEntry OBJECT-TYPE
	  SYNTAX    IphelperDhcpSnoopingOption82FormatASCIIConfigurableEntry
	  MAX-ACCESS  not-accessible
	  STATUS current
	  DESCRIPTION
		"An ascii field entry for Circuit ID or Remote ID."
	  INDEX { iphelperDhcpSnoopingOption82FormatASCIIConfigurableIndex }
   ::= { iphelperDhcpSnoopingOption82FormatASCIIConfigurableTable 1 }

   IphelperDhcpSnoopingOption82FormatASCIIConfigurableEntry ::= SEQUENCE {
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableIndex  	IphelperOption82CircuitOrRemoteId,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField1	IphelpereOption82ASCIIFieldType,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField1StrVal SnmpAdminString,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField2	IphelpereOption82ASCIIFieldType,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField2StrVal SnmpAdminString,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField3	IphelpereOption82ASCIIFieldType,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField3StrVal SnmpAdminString,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField4	IphelpereOption82ASCIIFieldType,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField4StrVal SnmpAdminString,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField5	IphelpereOption82ASCIIFieldType,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableField5StrVal SnmpAdminString,
		iphelperDhcpSnoopingOption82FormatASCIIConfigurableDelimiter	SnmpAdminString,

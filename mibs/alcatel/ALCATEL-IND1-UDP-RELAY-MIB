    ::= { iphelperDhcpSnoopingBindingEntry 7 }

    iphelperDhcpSnoopingBindingRemoteFlag  OBJECT-TYPE
        SYNTAX  INTEGER {
           remote(1),
           local(2)
         }
         MAX-ACCESS      read-only
         STATUS current
         DESCRIPTION

   ipv6helperDhcpSnoopingPortEntry  OBJECT-TYPE
	SYNTAX  Ipv6helperDhcpSnoopingPortEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"A LDRA/DHCP Snooping Port entry."
	INDEX { ipv6helperDhcpSnoopingPortIfIndex }
   ::= { ipv6helperDhcpSnoopingPortTable 1 }
   Ipv6helperDhcpSnoopingPortEntry ::= SEQUENCE {
	ipv6helperDhcpSnoopingPortIfIndex
		InterfaceIndex,
	ipv6helperDhcpSnoopingPortTrustMode
		INTEGER,
        ipv6helperSnoopingClientViolation
                Counter32,
        ipv6helperSnoopingServerViolation
                Counter32,
        ipv6helperSnoopingBindingViolation
                Counter32,
        ipv6helperSnoopingInterfaceidViolation
                Counter32,
		ipv6helperSnoopingPortSourceFilterStatus
				INTEGER
   }
   ipv6helperDhcpSnoopingPortIfIndex  OBJECT-TYPE
	    SYNTAX  InterfaceIndex
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		 "The ifIndex subindex identifying this instance."
   ::= { ipv6helperDhcpSnoopingPortEntry 1 }
   ipv6helperDhcpSnoopingPortTrustMode OBJECT-TYPE
	    SYNTAX  INTEGER {
	    	blocked(1),
		clientOnlyUntrusted(2),
		clientOnlyTrusted(3),
	    	trusted(4)
	    }
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"The LDRA/DHCPv6 Snooping's VLAN port's trust mode. Default value
	  	 is clientOnlyUntrusted, which means once the VLAN is enabled for DHCPv6
		 snooping, the vlan ports only allow DHCPv6 client packets. Blocked
		 means all DHCPv6 traffic is block on the port. clientOnlyTrusted means 
                 DHCPV6 client and Relay-Foward msg from another relay agent is allowed.
                 Trusted means all DHCPv6 traffic is allowed on the port."
		 DEFVAL { clientOnlyUntrusted }
   ::= { ipv6helperDhcpSnoopingPortEntry 2 }
   ipv6helperSnoopingClientViolation OBJECT-TYPE
        SYNTAX  Counter32
        MAX-ACCESS      read-only
        STATUS current
        DESCRIPTION
              "This keeps track of the number client violation packets dropped."
   ::= { ipv6helperDhcpSnoopingPortEntry 3}
   ipv6helperSnoopingServerViolation OBJECT-TYPE
        SYNTAX  Counter32
        MAX-ACCESS      read-only
        STATUS current
        DESCRIPTION
              "This keeps track of the number of server violation packets received/dropped , server 
                packet received on client port."
   ::= { ipv6helperDhcpSnoopingPortEntry 4}
   ipv6helperSnoopingBindingViolation OBJECT-TYPE
        SYNTAX  Counter32
        MAX-ACCESS      read-only
        STATUS current
        DESCRIPTION
              "This keeps track of the number of packets dropped due to mismacthed binding entry
                with client infomration."
   ::= { ipv6helperDhcpSnoopingPortEntry 5}
   ipv6helperSnoopingInterfaceidViolation OBJECT-TYPE
        SYNTAX  Counter32
        MAX-ACCESS      read-only
        STATUS current
        DESCRIPTION
              "This keeps track of the number of packets dropped due to invalid interface id value
               from DHCP server in Relay Agent Information option in the DHCP packet."
   ::= { ipv6helperDhcpSnoopingPortEntry 6}

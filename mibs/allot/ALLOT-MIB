--This module

alCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION
                "The compliance statement for SNMP entities which
                implement this MIB"

        MODULE ALLOT-MIB
        MANDATORY-GROUPS {
                        alGenericGroup,
                        alProvisioningGroup,
                        alStatisticsGroup,
                        alAlertsGroup,
                        alEventsGroup,
                        alLoadGroup }

        -- 1.3.6.1.4.1.2603.6.2.1
        ::= { alCompls 1 }


-- RADIUS
                        serviceProfileDoesNotExist (200),           -- Service Profile does not exist
						tooManyVlansPerService     (201),           -- Too many VLAN IDs defined for the service
						tooManyVlansPerSU          (202),           -- Too many VLAN IDs on one SU

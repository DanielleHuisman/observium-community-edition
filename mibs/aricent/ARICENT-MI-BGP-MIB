        InetAddressType,
    fsMIBgp4mpePathAttrPeer
        InetAddress,
    fsMIBgp4mpeRfdRtFom
        Integer32,
    fsMIBgp4mpeRfdRtLastUpdtTime
        Integer32,
    fsMIBgp4mpeRfdRtState
        INTEGER,
    fsMIBgp4mpeRfdRtStatus
        INTEGER,
    fsMIBgp4mpeRfdRtFlapCount
        Integer32,
    fsMIBgp4mpeRfdRtFlapTime
        Integer32,
    fsMIBgp4mpeRfdRtReuseTime
        Integer32
    }


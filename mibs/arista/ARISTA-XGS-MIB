    OBJECTS {
        aristaXgsQueueWatermarkMaxCellsUsed,
        aristaXgsQueueWatermarkCellSize,
        aristaXgsQueueWatermarkLastResetTime,
        aristaXgsIfTxIpV4L3UcOk,
        aristaXgsIfTxIpV6L3UcOk,
        aristaXgsIfTxIpV4L3McOk,
        aristaXgsIfTxIpV6L3McOk,

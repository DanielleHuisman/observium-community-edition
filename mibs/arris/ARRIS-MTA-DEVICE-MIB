arrisMtaDevRTPTxQueueSize OBJECT-TYPE
     SYNTAX INTEGER(2..20)
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION 
         "Sets the RTP transmit queue size."
     DEFVAL { 20 }
     ::= { arrisMtaDevOperationalSetup 3 }

arrisMtaDevEchoCancellerTailLength OBJECT-TYPE
     SYNTAX INTEGER {
         eightMs(1),
         thirtyTwoMs(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION 

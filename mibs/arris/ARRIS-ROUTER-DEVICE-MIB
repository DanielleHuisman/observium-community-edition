    SYNTAX      INTEGER {
			wpsUnknown(-1),
			wpsInitialState(0),
			wpsAssociatedStarted(1),
			wpsM2Sent(2),
			wpsM7Sent(3),
			wpsTimedOut(4),
			wpsMsgDone(5),
			wpsSuccessful(6),
			wpsMsgExchangeErr(7),    
			wpsPushButtonOverlap(8),
			wpsAssociating(9),    
			wpsPushButtonFindAP(10)    
			}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION  "Current status of the WPS subsystem."
	::= { arrisRouterWPS50Cfg 8 }
 

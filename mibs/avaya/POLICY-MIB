            DESCRIPTION
                "Logical NOT of the tcp source port settings."
            DEFVAL { disable }
            ::= { ipPolicyRuleEntry 27 }
        
        ipPolicyRuleL4DestPortNot OBJECT-TYPE
            SYNTAX INTEGER
                {
                disable(1),
                not-destination-port(2)
                }

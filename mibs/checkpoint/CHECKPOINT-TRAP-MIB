        chkpntTrapNet	      	OBJECT IDENTIFIER ::= { chkpntTrap 1 } 
		chkpntTrapDisk		OBJECT IDENTIFIER ::= { chkpntTrap 2 } 
        chkpntTrapCPU		OBJECT IDENTIFIER ::= { chkpntTrap 3 } 
        chkpntTrapMemory		OBJECT IDENTIFIER ::= { chkpntTrap 4 }

    MODULE          -- this module
    MANDATORY-GROUPS {
                        ciscoWirelessHotspotConfigGroup2,
                        ciscoWirelessHotspotConfigGroup3
                    }
    ::= { ciscoWirelessHotspotCompliances 2 }


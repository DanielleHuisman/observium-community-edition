    --swACLIpRulePort OBJECT-TYPE	
     --   SYNTAX  PortList
     --   MAX-ACCESS  read-create
      --  STATUS  current
      --  DESCRIPTION
       --     ""
       -- ::= { swACLIpRuleEntry 20 } 
        

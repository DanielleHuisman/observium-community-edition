    swL2QOSMgmt					OBJECT IDENTIFIER ::= { swL2MgmtMIB 6 } 
-- be excluded by ACL.mib

    swL2PortSecurityMgmt		OBJECT IDENTIFIER ::= { swL2MgmtMIB 7 } 
    

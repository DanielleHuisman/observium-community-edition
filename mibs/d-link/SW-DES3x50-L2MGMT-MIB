	swL2UnitMaxSupportedUnits OBJECT-TYPE
	  SYNTAX  INTEGER (0..65535)
	  MAX-ACCESS  read-only
	  STATUS  current
	  DESCRIPTION
	      "Maximum number of units are supported in the system." 
	  ::= { swL2UnitMgmt 1 }
	    
	swL2UnitNumOfUnit OBJECT-TYPE
	  SYNTAX  INTEGER (0..65535)
	  MAX-ACCESS  read-only
	  STATUS  current
	  DESCRIPTION
	      "The current number of units."
	  ::= { swL2UnitMgmt 2 }
	  
	swL2UnitMgmtTable OBJECT-TYPE
	  SYNTAX  SEQUENCE OF SwL2UnitMgmtEntry
	  MAX-ACCESS  not-accessible
	  STATUS  current
	  DESCRIPTION
	      "This table contains the unit information."
	  ::= { swL2UnitMgmt 3 }
	  
	swL2UnitMgmtEntry OBJECT-TYPE
	 SYNTAX  SwL2UnitMgmtEntry
	 MAX-ACCESS  not-accessible
	 STATUS  current
	 DESCRIPTION
	     "A list of management information for each unit in the system."
	 INDEX  { swL2UnitMgmtId }
	 ::= { swL2UnitMgmtTable 1 }
	
	SwL2UnitMgmtEntry ::= 
		SEQUENCE {
			swL2UnitMgmtId
				INTEGER,
			swL2UnitMgmtMacAddr
				MacAddress,
			swL2UnitMgmtStartPort
				INTEGER,
			swL2UnitMgmtPortRange
				INTEGER,
			swL2UnitMgmtCtrlMode
				INTEGER,
			swL2UnitMgmtCurrentMode
				INTEGER,
			swL2UnitMgmtVersion
				DisplayString,
			swL2UnitMgmtRPSStatus
				INTEGER,
			swL2UnitMgmtModelName
				DisplayString
	   }
	
	swL2UnitMgmtId OBJECT-TYPE
	   SYNTAX  INTEGER (1..12)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	       "This object indicates the specific entry in the stacking/chassis 
	       table."
	   ::= { swL2UnitMgmtEntry 1 }
	
	swL2UnitMgmtMacAddr OBJECT-TYPE
	   SYNTAX  MacAddress
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	       "The Mac address of this unit."
	   ::= { swL2UnitMgmtEntry 2 }
	
	swL2UnitMgmtStartPort OBJECT-TYPE
	   SYNTAX  INTEGER (1..65535)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	       "This object indicates the start port of this unit."
	   ::= { swL2UnitMgmtEntry 3 }
	
	swL2UnitMgmtPortRange OBJECT-TYPE
	   SYNTAX  INTEGER (0..65535)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	       "This object indicates the total ports of this unit." 
	   ::= { swL2UnitMgmtEntry 4 }
	   
	swL2UnitMgmtCtrlMode OBJECT-TYPE
	   SYNTAX  INTEGER {
	               other(1),
	               auto(2),
	               stand-alone(3),
	               master(4),
	               slave(5)
	           }
	   MAX-ACCESS  read-write
	   STATUS  current
	   DESCRIPTION
	       "This object indicates the stack mode that user configed for 
	       this unit.  The object only can be configed when the device is
	       stand alone.
	       
	       other(1) - this entry is currently in use but the conditions 
	           under which it will remain so are different from each of the
	           following values.
	       auto(2) - the system will auto assign this stack role of this 
	           unit to be stand-alone(3), master(4), or slave(5).    
	       stand-alone(3) - the unit is forced to stand alone.
	       master(4) - the unit is forced to master. If this unit is 
	           seleted to be master, it can modify the configuration of the
	           stacking system.
	       slave(5) - the unit is forced to slave. If this unit is 
	           seleted to be slave, it only can view the configuration of 
	           the stacking system." 
	   ::= { swL2UnitMgmtEntry 5 }
	   
	swL2UnitMgmtCurrentMode OBJECT-TYPE
	   SYNTAX  INTEGER {
	               other(1),
	               auto(2),
	               stand-alone(3),
	               master(4),
	               slave(5)
	           }
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	       "The current stack role of this unit." 
	   ::= { swL2UnitMgmtEntry 6 }
	   
	swL2UnitMgmtVersion OBJECT-TYPE
	   SYNTAX  DisplayString (SIZE(0..8))
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	        "This object indicates the firmware version of this stacking unit." 
	    ::= { swL2UnitMgmtEntry 7 }

	swL2UnitMgmtRPSStatus OBJECT-TYPE
		SYNTAX  INTEGER {
					other(1),
					notSupport(2),
					notPresent(3),
					inUsed(4),
					present(5)
				}
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"This object indicates the status of RPS of this stacking unit."
		::= { swL2UnitMgmtEntry 8 }

	swL2UnitMgmtModelName OBJECT-TYPE
		SYNTAX DisplayString (SIZE(0..32))
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"This object indicates the model name of this stacking unit."
		::= { swL2UnitMgmtEntry 9 }


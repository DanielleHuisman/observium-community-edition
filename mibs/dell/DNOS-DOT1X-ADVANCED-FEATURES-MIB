IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
    Unsigned32, TimeTicks               FROM SNMPv2-SMI
    ieee8021XPaePortNumber              FROM IEEE8021X-PAE-MIB
    TEXTUAL-CONVENTION,
    RowStatus,MacAddress,DateAndTime    FROM SNMPv2-TC
    DisplayString                       FROM RFC1213-MIB
    dnOS                            FROM DELL-REF-MIB;
    

    fastPathdot1xAdvanced MODULE-IDENTITY
           LAST-UPDATED "201806150000Z" -- 15 June 2018 12:00:00 GMT
           ORGANIZATION "Dell EMC"
           CONTACT-INFO ""
           
     DESCRIPTION
          "The Dell Networking Private MIB for Dot1x Advanced Features"

      -- Revision history.
     REVISION

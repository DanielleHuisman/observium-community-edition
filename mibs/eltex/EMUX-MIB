	E1StatistcsEntry ::=
    SEQUENCE {
			e1StChIndex 	Integer32,
			e1rxOkCnt		Counter32,
			e1rxNOS			Counter32,
			e1rxAIS			Counter32,
			e1rxAZS			Counter32,
			e1rxLOS			Counter32,
			e1rxRAI			Counter32,
			e1rxPRBS			Counter32,
			e1rxTest			Counter32,

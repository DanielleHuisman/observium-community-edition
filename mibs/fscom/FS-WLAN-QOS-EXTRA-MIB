			SYNTAX				FSAPdefaultStatusMappingEntry
			MAX-ACCESS			not-accessible
			STATUS				current
			DESCRIPTION
				" "
            INDEX   {
                   fsApMacAddr
                   }
		::= { fsPrivMappingAPstatusTable 1 }


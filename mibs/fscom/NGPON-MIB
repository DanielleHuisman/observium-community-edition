    SYNTAX         INTEGER(0..1)
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "demoLineHguFlowCtrlSwitch:
         0  disable(default)
         1  enable"
    ::= { demoLineHGUEntry 2 }


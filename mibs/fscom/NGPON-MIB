    ::= { ontBaseInfoEntry 11 }    

ontOfflineReason  OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..255))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
		"ont off line reason. 
		0:	NONE                    Reason get fail                     
		1:	DEACTIVATION            ONT DEACTIVED                       ONT
		2:	ACK_TIMEOUT             ONT acknowledge time out            ONT
		3:	SFI                     Signal fail of ONT                  ONT
		4:	TIWI                    Transmission interface warning      ONT
		5:	PASSWORD_AUTHENTICATION ONT password authentication fail    ONT
		6:	LOSI                    Loss of signal for ONT              ONT
		7:	LOS                     Loss of signal for OLT              OLT
		8:	LOKI                    Loss of key synch with ONT          ONT
		9:	RERANGE_FAILURE         ONT re-ranging fail                 ONT
		10:	LOFI                    Loss of frame of ONT                ONT
		11:	POWER_OFF               ONT power off                       ONT
		12:	UNKNOWN                 UNKNOWN                                
		13:	LOAMI                   Loss of PLOAM for ONT               ONTPLOAM
		"
    ::= { ontBaseInfoEntry 12 }    

ontOnOfflineTime   OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..255))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "ont on/off line time."
    ::= { ontBaseInfoEntry 13 }    

ontUpDownTime   OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..255))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "ont up/down time."
    ::= { ontBaseInfoEntry 14 }    


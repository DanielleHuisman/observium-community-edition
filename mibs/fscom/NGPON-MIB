        SYNTAX      OntFindListEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "entry of ont find list table"
        INDEX { spSlotIndex, spPportIndex, ontFindListListId }
        ::= { ontFindListTable 1 }


            ontRXPowerI       OCTET STRING,
            ontTXPowerI       OCTET STRING,
            ontBiasCurrentI   OCTET STRING,
            ontTemperatureI   OCTET STRING,
            ontRxCatvPowerI   OCTET STRING,
            ontTxCatvPowerI   OCTET STRING
        }


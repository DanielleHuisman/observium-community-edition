        OBJECTS { gponTrapSlotId, gponTrapPortId, gponTrapOntId, gponTrapOntEthportId, gponTrapStatus }
        STATUS  current
        DESCRIPTION
            "ont ethernet port status trap."
             --onttrap
        ::= { ontPortTrap 1 }


    SYNTAX           OCTET STRING (SIZE(0..255))
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
        "ont ethernet port speed."
        -- 123   
        -- 17,18,19 
    ::= { ontEthportStateEntry 7 }    


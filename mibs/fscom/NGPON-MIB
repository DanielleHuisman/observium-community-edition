    SYNTAX          INTEGER(0..5)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "demoLineHguDsIgmpControlType:
         0  transparent(default)
         1  strip outer tag
         2  add tag
         3  replace tag
         4  replace only vid
         5  add only vid"
    ::= { demoLineHGUEntry 6 }


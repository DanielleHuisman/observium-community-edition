        SYNTAX      INTEGER(1..2)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "ONU告警使能，1：关闭   2：开启"
        ::= { nscrtvOnuOpAlarmConfigEntry 2 }  

    nscrtvOnuThresholdTempHighAlarm      OBJECT-TYPE
        SYNTAX      INTEGER 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            " onu温度高告警门限值，最大取值：+128℃, 以16位带符号二进制数表示，单位为1/256摄氏度。表示范围为-128℃～+128℃"
        ::= { nscrtvOnuOpAlarmConfigEntry 3 }
    
    nscrtvOnuThresholdTempLowAlarm       OBJECT-TYPE
        SYNTAX      INTEGER 

    ::= { h3cPortalPktStatistic 7 }

h3cPortalPktStaAckChallengeSuccess OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the total number of the response to challenge packet of
        portal server including error code 0 in the packet, that is
        challenge is success."
    ::= { h3cPortalPktStatistic 8 }

h3cPortalPktStaAckChallengeReject OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the total number of the response to challenge packet of
        portal server including error code 1 in the packet, that is
        challenge is rejected by server."
    ::= { h3cPortalPktStatistic 9 }

h3cPortalPktStaAckChallengeEstablish OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the total number of the response to challenge packet of
        portal server including error code 2 in the packet, that is the
        link is alreadys established."
    ::= { h3cPortalPktStatistic 10 }

h3cPortalPktStaAckChallengeBusy OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the total number of the response to challenge packet of
        portal server including error code 3 in the packet, that is the
        system is busy."
    ::= { h3cPortalPktStatistic 11 }

h3cPortalPktStaAckChallengeAuthFail OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the total number of the response to challenge packet of
        portal server including error code 4 in the packet, that is
        authentication fail."
    ::= { h3cPortalPktStatistic 12 }

h3cPortalExtConfig OBJECT IDENTIFIER ::= { h3cPortal 6 }

--
-- PORTAL MAX USER NUMBER
--

h3cPortalExtMaxUserNumber OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify the maximum number of online users."
    ::= { h3cPortalExtConfig 1 }

--
-- PORTAL CURRENT USER NUMBER
--

h3cPortalExtCurrentUserNumber OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The current number of online portal users."
    ::= { h3cPortalExtConfig 2 }

--
-- PORTAL STATUS
--

h3cPortalExtStatus OBJECT-TYPE
    SYNTAX      INTEGER
    {
         enabled(1),
         disabled(2)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It indicates the portal status.
         enabled:
            Portal is enabled on one or more interfaces.
         disabled:
            Portal is not enabled on any interface."
    ::= { h3cPortalExtConfig 3 }

h3cPortalExtTables OBJECT IDENTIFIER ::= { h3cPortal 7 }

--
-- PORTAL server table
--
h3cPortalExtSrvTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF H3cPortalExtSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines the name of the portal server.  It can be created
        and destroyed by MIB."
    ::= { h3cPortalExtTables 1 }

h3cPortalExtSrvEntry OBJECT-TYPE
    SYNTAX      H3cPortalExtSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for each name and URL of the portal server.
        It is indexed by using the object h3cPortalExtSrvName."
    INDEX
        {
            h3cPortalExtSrvName
        }
    ::= { h3cPortalExtSrvTable 1 }


H3cPortalExtSrvEntry ::= SEQUENCE
    {
        h3cPortalExtSrvName          OCTET STRING,
        h3cPortalExtSrvIPAddrType    InetAddressType,
        h3cPortalExtSrvIP            InetAddress,
        h3cPortalExtSrvPort          Integer32,
        h3cPortalExtSrvRowStatus     RowStatus
    }

h3cPortalExtSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table h3cPortalExtSrvTable.  It shows
        the name of the portal server."
    ::= { h3cPortalExtSrvEntry 1 }

h3cPortalExtSrvIPAddrType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The type of the portal server IP address."
    ::={ h3cPortalExtSrvEntry 2 }

h3cPortalExtSrvIP OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the IP address of the portal server."
    ::= { h3cPortalExtSrvEntry 3 }

h3cPortalExtSrvPort OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "It shows the UDP port of the portal server.
        The port will be used when clients send packets to the portal server.
        Default value is 50100.
        It is not configurable when the portal server has been enabled."
    ::= { h3cPortalExtSrvEntry 4 }

h3cPortalExtSrvRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo and
        Destroy operations.  To create a new row, the portal server must be
        specified.
        To destroy an existing row, the portal server MUST NOT be referenced by
        an interface."
    ::= { h3cPortalExtSrvEntry 5 }

--
-- PORTAL web server table
--
h3cPortalExtWebSrvTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF H3cPortalExtWebSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines the name of the portal Web server.  It can be created
         and destroyed by MIB."
    ::= { h3cPortalExtTables 2 }

h3cPortalExtWebSrvEntry OBJECT-TYPE
    SYNTAX      H3cPortalExtWebSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for each name and portal Web server.
        It is indexed by using the object h3cPortalExtWebSrvName."
    INDEX
        {
            h3cPortalExtWebSrvName
        }
    ::= { h3cPortalExtWebSrvTable 1 }


H3cPortalExtWebSrvEntry ::= SEQUENCE
    {
        h3cPortalExtWebSrvName         OCTET STRING,
        h3cPortalExtWebSrvUrl          OCTET STRING,
        h3cPortalExtWebSrvRowStatus    RowStatus
    }

h3cPortalExtWebSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table h3cPortalExtWebSrvTable.  It shows
        the name of the portal Web server."
    ::= { h3cPortalExtWebSrvEntry 1 }

h3cPortalExtWebSrvUrl OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the URL to the portal Web server.
        Users which have not logged on to portal will be redirected to the
        portal server according to h3cPortalServerIfUrl.
        The value is consisted of protocol, IP address and relative path.
        For example: http://10.1.1.1/portal."
    ::= { h3cPortalExtWebSrvEntry 2 }

h3cPortalExtWebSrvRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo and
        Destroy operations.  To create a new row, the portal server must be specified.
        To destroy an existing row, the portal Web server MUST NOT be referenced by an
        interface."
    ::= { h3cPortalExtWebSrvEntry 3 }

h3cPortalExtMTSrvTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF H3cPortalExtMTSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes the configuration of the MAC Trigger server."
    ::= { h3cPortalExtTables 3 }

h3cPortalExtMTSrvEntry OBJECT-TYPE
    SYNTAX      H3cPortalExtMTSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in the table for the MAC Trigger server.  It is indexed by using the object Index."
    INDEX
        {
            h3cPortalExtMTSrvName
        }
    ::= { h3cPortalExtMTSrvTable 1 }

H3cPortalExtMTSrvEntry ::= SEQUENCE
    {
        h3cPortalExtMTSrvName          OCTET STRING,
        h3cPortalExtMTSrvIPAddrType    InetAddressType,
        h3cPortalExtMTSrvIP            InetAddress,
        h3cPortalExtMTSrvPort          Integer32,
        h3cPortalExtMTDetctFlowPeriod  Integer32,
        h3cPortalExtMTThresholdFlow    Unsigned32,
        h3cPortalExtMTSrvRowStatus     RowStatus
    }

h3cPortalExtMTSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table h3cPortalExtMTSrvTable.  It shows
         the name of the MAC Trigger server."
    ::= { h3cPortalExtMTSrvEntry 1 }

h3cPortalExtMTSrvIPAddrType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The type of the MAC Trigger server IP address."
    ::={ h3cPortalExtMTSrvEntry 2 }

h3cPortalExtMTSrvIP OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the IP address of the MAC Trigger server."
    ::= { h3cPortalExtMTSrvEntry 3 }

h3cPortalExtMTSrvPort OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the UDP port of the MAC Trigger server.
        The port will be used when clients send packets to the MAC Trigger server.
        Default value is 50100."
    DEFVAL { 50100 }
    ::= { h3cPortalExtMTSrvEntry 4 }

h3cPortalExtMTDetctFlowPeriod  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the user flow detection interval (in seconds) when
        MAC Trigger and Portal are enabled.
        Default value is 300."
    DEFVAL { 300 }
    ::= { h3cPortalExtMTSrvEntry 5 }

h3cPortalExtMTThresholdFlow  OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "bytes"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the user traffic threshold (in bytes) that triggers authentication
        when MAC Trigger and Portal are enabled.
        Default value is 0."
    DEFVAL { 0 }
    ::= { h3cPortalExtMTSrvEntry 6 }

h3cPortalExtMTSrvRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo and
        Destroy operations."
    ::= { h3cPortalExtMTSrvEntry 7 }
--
-- PORTAL extend if table
--
h3cPortalExtIfConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF H3cPortalExtIfConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes portal information on interfaces."
    ::= { h3cPortalExtTables 4 }

h3cPortalExtIfConfigEntry OBJECT-TYPE
    SYNTAX      H3cPortalExtIfConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for an interface on which
        portal has been enabled.
        It is indexed by using the object ifIndex."
    INDEX
        {
            h3cPortalExtIfIndex
        }
    ::= { h3cPortalExtIfConfigTable 1 }

H3cPortalExtIfConfigEntry ::= SEQUENCE
    {
        h3cPortalExtIfIndex            InterfaceIndex,
        h3cPortalExtIfWebSrvName       OCTET STRING,
        h3cPortalExtIfDomainName       OCTET STRING,
        h3cPortalExtIfAuthMethod       H3cPortalAuthMethod,
        h3cPortalExtIfMTSrvName        OCTET STRING,
        h3cPortalExtIfMaxUser          Unsigned32
    }

h3cPortalExtIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the interface index and the index of h3cPortalExtIfConfigTable."
    ::= { h3cPortalExtIfConfigEntry 1 }

h3cPortalExtIfWebSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The Portal Web server name must exist.  It references
        h3cPortalExtWebSrvName."
    ::= { h3cPortalExtIfConfigEntry 2 }

h3cPortalExtIfDomainName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Authentication domain."
    ::= { h3cPortalExtIfConfigEntry 3 }

h3cPortalExtIfAuthMethod OBJECT-TYPE
    SYNTAX      H3cPortalAuthMethod
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify the portal authentication method."
    ::= { h3cPortalExtIfConfigEntry 4 }

h3cPortalExtIfMTSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The MAC trigger server name must exist.  It references h3cPortalExtMTSrvName."
    ::= { h3cPortalExtIfConfigEntry 5 }

h3cPortalExtIfMaxUser OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Maximum number of portal users."
    ::= { h3cPortalExtIfConfigEntry 6 }


			 SYNTAX 		 RowStatus
			 MAX-ACCESS 	 read-write
			 STATUS 		 current
			 DESCRIPTION
				 "The row status of this table entry."
			 ::= { hmSec2FwL3PfInEntry 11 }


hmSec2FwL3PfInLogNonMatching   OBJECT-TYPE
		     SYNTAX			 INTEGER {
							 enable(1),
							 disable(2)
					  }
			 MAX-ACCESS 	 read-write
			 STATUS 		 current
			 DESCRIPTION
				 "Log incoming packets not matching any rule."
			 DEFVAL  { disable }
			 ::= { hmSec2FirewallL3PfIncomingGroup 2 }


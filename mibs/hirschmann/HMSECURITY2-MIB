		     SYNTAX			 INTEGER {
							 enable(1),
							 disable(2),
							 logAndTrap(3)
					  }
			 MAX-ACCESS 	 read-write
			 STATUS 		 current
			 DESCRIPTION
				 "Enables or disables logging for packets matching this rule."
			 DEFVAL  { disable }                
			 ::= { hmSec2FwSnmpEntry 5 }


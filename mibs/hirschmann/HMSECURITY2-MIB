		   	 SYNTAX			 INTEGER {
							 accept(1),
							 drop(2),
							 reject(3)
					  }
			 MAX-ACCESS 	 read-write
			 STATUS 		 current
			 DESCRIPTION
				 "Action to be performed if the firewall rule matches."
			 DEFVAL  { accept }                
			 ::= { hmSec2FwSnmpEntry 4 }


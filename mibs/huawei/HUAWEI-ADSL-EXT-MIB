           "
        REFERENCE    "ITU-T G.997.1, paragraph 7.3.2.15"
        DEFVAL       { 0 }
        ::= { hwadslLineExtConfProfileCfgEntry 51 } 
            
    hwadslLineExtConfProfMinInpReinRtxDs  OBJECT-TYPE

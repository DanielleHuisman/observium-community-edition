			     8   testing;
			     9   activated;
			     10  activating; 
			     11  deactivated;
			     18  localloopback;
			     19  remoteloopback;
			     20  blocked;  
			     37  utopialoopback;
			     38  afeloopback; 
			     39  hybridloopback; 
			     255 unknown;

                -- 1.3.6.1.4.1.2011.6.25.1.12.1.1
                hw1588ACRMasterEntry OBJECT-TYPE
                        SYNTAX Hw1588ACRMasterEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "1588 ACR clock manage table.
                                The index of this entry is master index.
                                "
                        INDEX { hw1588ACRMasterIndex }
                        ::= { hw1588ACRMasterTable 1 }


                Hw1588ACRMasterEntry ::=
                        SEQUENCE {
                                hw1588ACRMasterIndex
                                        Integer32,
                                hw1588ACRMasterIPAddr
                                        IpAddress,
                                hw1588ACRMasterNegotiate
                                        INTEGER,
                                hw1588ACRRowStatus
                                        RowStatus
                         }

                -- 1.3.6.1.4.1.2011.6.25.1.12.1.1.1

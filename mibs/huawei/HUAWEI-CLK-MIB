                                BC:
                                Default: 1/8s(7)
                                Options:
                                    1. 1/16s  (6)  - indicates that the time interval is 1/16s
                                    2. 1/8s   (7)  - indicates that the time interval is 1/8s
                                    3. 1/4s   (8)  - indicates that the time interval is 1/4s
                                    4. 1/2s   (9)  - indicates that the time interval is 1/2s
                                    5. 1s     (10) - indicates that the time interval is 1s
                                    6. 2s     (11) - indicates that the time interval is 2s
                                    7. 4s     (12) - indicates that the time interval is 4s
                                    8. 8s     (13) - indicates that the time interval is 8s
                                    9. 16s    (14) - indicates that the time interval is 16s  
									
                                T-BC:
                                    1/8s   (7)  - indicates that the time interval is 1/8s
                                "
                        ::= { hwOntClkProfilePortInfoEntry 6 }

                -- 1.3.6.1.4.1.2011.6.25.2.2.1.7
                hwOntClkProfOntPortPtpSyncInterval OBJECT-TYPE
                        SYNTAX Integer32 (2..11)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Interval for sending sync packets.
                                BC:
                                Default: 1/16s(6)
                                Options:
                                    1. 1/256s (2)  - indicates that the time interval is 1/256s
                                    2. 1/128s (3)  - indicates that the time interval is 1/128s
                                    3. 1/64s  (4)  - indicates that the time interval is 1/64s
                                    4. 1/32s  (5)  - indicates that the time interval is 1/32s
                                    5. 1/16s  (6)  - indicates that the time interval is 1/16s
                                    6. 1/8s   (7)  - indicates that the time interval is 1/8s
                                    7. 1/4s   (8)  - indicates that the time interval is 1/4s
                                    8. 1/2s   (9)  - indicates that the time interval is 1/24s
                                    9. 1s     (10) - indicates that the time interval is 1s
                                    10.2s     (11) - indicates that the time interval is 2s   
				
                                T-BC:					
                                    1/16s  (6)  - indicates that the time interval is 1/16s
                                "
                        ::= { hwOntClkProfilePortInfoEntry 7 }

                -- 1.3.6.1.4.1.2011.6.25.2.2.1.8
                hwOntClkProfOntPortPtpReqInterval OBJECT-TYPE
                        SYNTAX Integer32 (6..14)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Interval for sending req packets.
                                BC:
                                Default: 1/8s(7)
	                            Options:
	                                1. 1/16s  (6)  - indicates that the time interval is 1/16s
	                                3. 1/8s   (7)  - indicates that the time interval is 1/8s
	                                3. 1/4s   (8)  - indicates that the time interval is 1/4s
	                                4. 1/2s   (9)  - indicates that the time interval is 1/2s
	                                5. 1s     (10) - indicates that the time interval is 1s
	                                6. 2s     (11) - indicates that the time interval is 2s
	                                7. 4s     (12) - indicates that the time interval is 4s
	                                8. 8s     (13) - indicates that the time interval is 8s
	                                9. 16s    (14) - indicates that the time interval is 16s    
	                            
                                T-BC:
                                    1/16s  (6)  - indicates that the time interval is 1/16s
                                "
                        ::= { hwOntClkProfilePortInfoEntry 8 }

                -- 1.3.6.1.4.1.2011.6.25.2.2.1.9

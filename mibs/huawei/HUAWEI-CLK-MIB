                                "
                        ::= { hwClkMibObjects 4 }


                -- 1.3.6.1.4.1.2011.6.25.1.4.1
                hwClockOutPortManageEntry OBJECT-TYPE
                        SYNTAX HwClockOutPortManageEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "Clock output port management table.
                                This table is used to enable or disable the output
                                port and query the status of the output port. The index of this entry is
                                hwClkOutputPostion, indicating the ifIndex of physical position of the clock source.
                                "
                        INDEX { hwClkOutputPostion }
                        ::= { hwClockOutPortManageTable 1 }


                HwClockOutPortManageEntry ::=
                        SEQUENCE {
                                hwClkOutputPostion
                                        Integer32,
                                hwClkOutPortEnable
                                        INTEGER,
                                hwSsmQlOutPortEnable
                                        INTEGER
                                 }

                -- 1.3.6.1.4.1.2011.6.25.1.4.1.1
                hwClkOutputPostion OBJECT-TYPE
                        SYNTAX Integer32
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The index of physical position of the clock source.
                                The highest byte is useless.
                                The second byte describes the frame ID.
                                The third byte describes the slot ID.
                                The lowest byte describes the port ID.
                                The following shows how to use parameter during the get or set operation:
                                0x 00, FF,00,00  Frame ID;     0~255
                                0x 00, 00,FF,00  Slot  ID;     0~255
                                0x 00, 00,00,FF  Port  ID;     0~255
                                "
                        ::= { hwClockOutPortManageEntry 1 }


                -- 1.3.6.1.4.1.2011.6.25.1.4.1.2
                hwClkOutPortEnable OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                enable(0),
                                disable(1)
                                }
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Clock output port switch.
                                Options:
                                1. enable(0)  - indicates that clock source output of this BITS port is enabled
                                2. disable(1) - indicates that clock source output of this BITS port is disabled
                                "
                        ::= { hwClockOutPortManageEntry 2 }


                -- 1.3.6.1.4.1.2011.6.25.1.4.1.3
                hwSsmQlOutPortEnable OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                enable(1),
                                disable(2)
                                }
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "SSM QL output mode.
                                Options:
                                1.enable(1)  - indicates that SSM QL output of the port is enabled
                                2.disable(2) - indicates that SSM QL output of the port is disabled
                                "
                        ::= { hwClockOutPortManageEntry 3 }



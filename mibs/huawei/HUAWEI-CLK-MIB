                -- 1.3.6.1.4.1.2011.6.25.1.2
                hwClockOutManageTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF HwClockOutManageEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "Clock Out Management Table.
                                This table is used to configure and query the priority of the clock source,
                                and query the index of the currently-output clock source, and the working mode (the output mode)
                                of the current clock.The indexes of this table are hwClkInIndex and hwClkOutIndex.
                                hwClkInIndex comes from hwClockInManageTable and is the index of the input clock source.
                                hwClkOutIndex is the type of the output clock source.
                                The output clock source can be of the SDH, TDM, 8K, or 19M type.
                                "
                        ::= { hwClkMibObjects 2 }


                -- 1.3.6.1.4.1.2011.6.25.1.2.1
                hwClockOutManageEntry OBJECT-TYPE
                        SYNTAX HwClockOutManageEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "Clock Out Management Table.
                                This table is used to configure and query the priority of the clock source,
                                and query the index of the currently-output clock source, and the working mode (the output mode)
                                of the current clock.The indexes of this entry are hwClkInIndex and hwClkOutIndex.
                                hwClkInIndex comes from hwClockInManageTable and is the index of the input clock source.
                                hwClkOutIndex is the type of the output clock source.
                                The output clock source can be of the SDH, TDM, 8K, or 19M type.
                                "
                        INDEX { hwClkInIndex, hwClkOutIndex }
                        ::= { hwClockOutManageTable 1 }


                HwClockOutManageEntry ::=
                        SEQUENCE {
                                hwClkOutIndex
                                        Integer32,
                                hwClkOutType
                                        INTEGER,
                                hwClkOutPriority
                                        Integer32,
                                hwClkOutWorkMode
                                        INTEGER,
                                hwClkOutOperStatus
                                        TruthValue
                         }

                -- 1.3.6.1.4.1.2011.6.25.1.2.1.1
                hwClkOutIndex OBJECT-TYPE
                        SYNTAX Integer32 (1..7)
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "Index of clock out table. Its meaning is the same as hwClkOutType.
                                The clock out index must be 8k in UA system and must be SDH or
                                TDM in MD/MA system during the setting of priority for a clock source.
                                Range: 1 - 7
                                "
                        ::= { hwClockOutManageEntry 1 }


                -- 1.3.6.1.4.1.2011.6.25.1.2.1.2
                hwClkOutType OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                sdh(1),
                                tdm(2),
                                system(3),
                                clk19M(4),
                                systemObsolete(5),
                                external(6),
                                external1(7)
                                }
                        MAX-ACCESS read-only
                        STATUS current
                        DESCRIPTION
                                "Clock out type.
                                Currently, there are seven types: SDH, TDM, system, 19 MHzm, systemObsolete, external and external1.
                                Options:
                                1.sdh(1)                -   indicates that output clock type is SDH
                                2.tdm(2)                -   indicates that output clock type is TDM
                                3.system(3)             -   indicates that output clock type is system
                                4.clk19M(4)             -   indicates that output clock type is clk19M
                                5.systemObsolete(5)     -   indicates that output clock type is systemObsolete
                                6.external(6)           -   indicates that output clock type is external
                                7.external1(7)          -   indicates that output clock type is external1
                                "
                        ::= { hwClockOutManageEntry 2 }


                -- 1.3.6.1.4.1.2011.6.25.1.2.1.3
                hwClkOutPriority OBJECT-TYPE
                        SYNTAX Integer32 (0..9 | 255)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "Clock source priority.
                                The priority ranges from 0 to 9 in a descending order. If the value is 255 in the get operation,
                                it indicates that the priority of the clock source isn't configured.
                                "
                        DEFVAL { 255 }
                        ::= { hwClockOutManageEntry 3 }


                -- 1.3.6.1.4.1.2011.6.25.1.2.1.4

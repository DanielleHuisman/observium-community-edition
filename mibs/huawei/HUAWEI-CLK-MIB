                -- 1.3.6.1.4.1.2011.6.25.2.3.1.4
                hwOntObjectClkWorkMode OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                  trace(1), 
                                  surge(2), 
                                  keep(3), 
                                  snap(4), 
                                  thirdsurge(5),
                                  invalid(-1)
                                }
                        MAX-ACCESS read-only
                        STATUS current
                        DESCRIPTION
                                  "Clock out working mode.
                                   Options:
                                      1.trace(1)       - indicates that the clock is working in tracing mode.
                                      2.surge(2)       - indicates that the clock is working in surge mode.
                                      3.keep(3)        - indicates that the clock is working in keeping mode.
                                      4.snap(4)        - indicates that the clock is working in snapping mode.
                                      5.thirdsurge(5)  - indicates that the clock is working in thirdsurge mode.
                                      6.invalid(-1)    - indicates that it is not be queried.
                                  "
                        ::= { hwOntClkGlobalInfoEntry 4 }

                -- 1.3.6.1.4.1.2011.6.25.2.3.1.5
                hwOntObjectClkLockState OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                  locked(1),
                                  unlocked(2),
                                  invalid(-1)
                                }
                        MAX-ACCESS read-only
                        STATUS current
                        DESCRIPTION
                                  "Clock lock state.
                                   Options:
                                      1.locked(1)   - indicates that the clock is locked.
                                      2.unlocked(2) - indicates that the clock is unlocked.
                                      3.invalid(-1) - indicates that it is not be queried.
                                  "
                        ::= { hwOntClkGlobalInfoEntry 5 }

                -- 1.3.6.1.4.1.2011.6.25.2.3.1.6
                hwOntObjectClkSrcState OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                  none(1), 
                                  normal(2),
                                  failure(3),
                                  failureLoss(4),
                                  failureEFD(5),
                                  invalid(-1)
                                }
                        MAX-ACCESS read-only
                        STATUS current
                        DESCRIPTION
                                  "Clock source state.
                                   Options:
                                      1.none(1)        - indicates that the clock source index isn't configured.
                                      2.normal(2)      - indicates that the state of the clock source is active.
                                      3.failure(3)     - indicates that the state of the clock source fails and this leaf is not used on the OLT.
                                      4.failureLoss(4) - indicates that the clock source fails due to signal loss.
                                      5.failureEFD(5)  - indicates that the clock source fails due to frequency offset.
                                      6.invalid(-1)    - indicates that it is not be queried.
                                   "
                        ::= { hwOntClkGlobalInfoEntry 6 }

                -- 1.3.6.1.4.1.2011.6.25.2.3.1.7
                hwOntObjectClkSrcQL OBJECT-TYPE
                        SYNTAX INTEGER
                               {
                                qlPRC(2),
                                qlSsuA(4), 
                                qlSsuB(8), 
                                qlSEC(11), 
                                qlDNU(15), 
                                qlCLEAR(255), 
                                invalid(-1)
                               }
                        MAX-ACCESS read-only
                        STATUS current
                        DESCRIPTION
                                "Clock source quality level:
                                 Options:
                                 1. qlPRC(2)     -  indicates that primary reference clock.
                                 2. qlSsuA(4)    -  indicates that the clock signal comes from class I or class.
                                                    V sub-clock prescribed on G.812 protocol.
                                 3. qlSsuB(8)    -  indicates that the clock signal comes from class VI sub-clock.
                                                    prescribed on G.812 protocol.
                                 4. qlSEC(11)    -  indicates that the clock signal comes from SEC(SDH Equipment.
                                                    Clock) prescribed on G.813 protocol.
                                 5. qlDNU(15)    -  indicates that the clock signal can't be used for synchronization.
                                 6. qlCLEAR(255) -  indicates that it is a wrong clock quality level.
                                 7. invalid(-1)  -  indicates that it is not be queried.
                                "
                        ::= { hwOntClkGlobalInfoEntry 7 }


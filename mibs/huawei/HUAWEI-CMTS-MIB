                Options:
                1. hz200K(1)  - 200KHz
                2. hz300K(2)  - 300KHz
                3. hz400K(3)  - 400KHz
                Default:hz400K(3)
                "
         ::= { hwCmtsFmChannelEntry 3 }    
         
   hwCmtsFmChannelTxPower OBJECT-TYPE
            SYNTAX Integer32
            UNITS  "0.1dBmV"
            MAX-ACCESS read-write
            STATUS obsolete
            DESCRIPTION
                "Indicates the transmit power of FM channel. 
                Value range: 250..600.
                Default:450
                "
         ::= { hwCmtsFmChannelEntry 4 }    
         

            ::= { hwDatacomm 134 }  

                EnabledStatus ::= TEXTUAL-CONVENTION
                STATUS    current
                DESCRIPTION
                "A simple status value for the object."
                SYNTAX   INTEGER 
                { 
                    enabled(1),
                    disabled(2)
                }
     

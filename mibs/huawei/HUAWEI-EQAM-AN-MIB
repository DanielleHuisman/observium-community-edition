                  INTEGER,
                hwDocsQamInputTSStateActive
                  INTEGER,
                hwDocsQamInputTSStateSwitchoverReason
                  INTEGER
                }
        
        -- 1.3.6.1.4.1.2011.6.187.20.1
        hwDocsQamInputTSStateSwitchOver        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  switchover(1),
                  none(255)
                  }
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the opeartion for swiching over, the default value is none(255).
                 Options: 
                 1. switchover(1)   -switch over the input TS.
                 2. none(255)       -no operation.
                "
            ::= { hwDocsQamInputTSStateEntry 1 }
      
        -- 1.3.6.1.4.1.2011.6.187.20.2
        hwDocsQamInputTSStateActive        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  primary(1),
                  secondary(2),
                  none(255)
                  }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the active state of input TS.
                 Options: 
                 1. primary(1)           -indicates the active state of the input TS is primary.
                 2. secondary(2)         -indicates the active state of the input TS is secondary.
                 3. none(255)            -indicates it is not a redundancy input TS.
                "
            ::= { hwDocsQamInputTSStateEntry 2 }  
                                    
        -- 1.3.6.1.4.1.2011.6.187.20.3
        hwDocsQamInputTSStateSwitchoverReason        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  manual(1),
                  nobitrate(2),
                  nosignal(3),
                  patmissing(4),
                  pmtmissing(5),
                  none(255)
                  }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the protection switching cause, the default value is none(255).
                 Options: 
                 1. manual(1)       -indicates the protection switching cause is manual switching.
                 2. nobitrate(2)    -indicates switching is triggered because no bit rate.
                 3. nosignal(3)     -indicates switching is triggered because no signals. 
                 4. patmissing(4)   -indicates switching is triggered because of PAT missing.
                 5. pmtmissing(5)   -indicates switching is triggered because of PMT missing.
                 6. none(255)       -indicates no switching occurs.
                "
            ::= { hwDocsQamInputTSStateEntry 3 }
            

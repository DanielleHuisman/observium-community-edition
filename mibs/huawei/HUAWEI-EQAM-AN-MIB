    -- the traps VbOids
    hwDocsQamNotificationObject OBJECT IDENTIFIER ::= { hwEqamAn 24 }
    
    hwDocsQamNotificationProgramID OBJECT-TYPE
        SYNTAX Integer32(1..65535)
        MAX-ACCESS  accessible-for-notify
        STATUS current
        DESCRIPTION
            "Program ID."
        ::= { hwDocsQamNotificationObject 1 }

    hwDocsQamNotificationPID OBJECT-TYPE
        SYNTAX Integer32(0..8190)
        MAX-ACCESS  accessible-for-notify
        STATUS current
        DESCRIPTION
            "PID."
        ::= { hwDocsQamNotificationObject 2 }
        
    hwDocsQamNotificationPSIType        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  pat(1),
                  cat(2),
                  pmt(3),
                  nit(4),
                  sdt(5),
                  eit(6),
                  tdttot(7)
                  }
            MAX-ACCESS  accessible-for-notify
            STATUS      current
            DESCRIPTION
                "Indicates the program stream type.
                 Options: 
                 1. pat(1)      --Program Association Table
                 2. cat(2)      --Conditional Access Table
                 3. pmt(3)      --Program Map Table
                 4. nit(4)      --Network Information Table
                 5. sdt(5)      --Service Description Table
                 6. eit(6)      --Event Information Table
                 7. tdttot(7)   --Time and Date Table/Time Offset Table
                "
            ::= { hwDocsQamNotificationObject 3 }
            
    hwDocsQamNotificationProgramName        OBJECT-TYPE
            SYNTAX       OCTET STRING (SIZE (0..63))
            MAX-ACCESS   accessible-for-notify
            STATUS       current
            DESCRIPTION
              "Indicate the program name.
              "
            ::= { hwDocsQamNotificationObject 4 } 
        

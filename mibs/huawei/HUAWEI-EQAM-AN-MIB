		-- 1.3.6.1.4.1.2011.6.187.84
        hwDocsQamScsEcmgEcmTable    OBJECT-TYPE
            SYNTAX          SEQUENCE OF HwDocsQamScsEcmgEcmEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the EQAM SCS ECMG ECM table.
                 The index of this table is hwDocsQamSCSName, hwDocsQamScsEcmgChannelID and hwDocsQamScsEcmgStreamID.
                "
            ::= { hwEqamAn 84 }
    
        -- 1.3.6.1.4.1.2011.6.187.84.1
        hwDocsQamScsEcmgEcmEntry    OBJECT-TYPE
            SYNTAX          HwDocsQamScsEcmgEcmEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the EQAM SCS ECMG ECM table.
                 The index of this entry is hwDocsQamSCSName, hwDocsQamScsEcmgChannelID and hwDocsQamScsEcmgStreamID.
                "
            INDEX   
                { 
                hwDocsQamSCSName,
				hwDocsQamScsEcmgChannelID,
				hwDocsQamScsEcmgStreamID
                }
            ::= { hwDocsQamScsEcmgEcmTable  1 }
        
        HwDocsQamScsEcmgEcmEntry ::=
            SEQUENCE 
                { 
                hwDocsQamScsEcmgStreamID
                  Unsigned32,
                hwDocsQamScsEcmgECMID
                  Unsigned32,
                hwDocsQamScsEcmgName
                  OCTET STRING,
                hwDocsQamScsEcmgEcmSuperCasID
                  Unsigned32,
                hwDocsQamScsEcmgEcmAccessCriteria
                  OCTET STRING,
                hwDocsQamScsEcmgEcmACTransferMode
                  Unsigned32
                }

        -- 1.3.6.1.4.1.2011.6.187.84.1.1
	hwDocsQamScsEcmgStreamID        OBJECT-TYPE
            SYNTAX      Unsigned32 (0..4294967295)
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "Indicates the stream ID of ECMG.
                "
            ::= { hwDocsQamScsEcmgEcmEntry 1 }
			
		-- 1.3.6.1.4.1.2011.6.187.84.1.2
        hwDocsQamScsEcmgECMID        OBJECT-TYPE
            SYNTAX      Unsigned32 (0..4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ECM ID.
                "
			DEFVAL { 4294967295 }
            ::= { hwDocsQamScsEcmgEcmEntry 2 }
			
		-- 1.3.6.1.4.1.2011.6.187.84.1.3   
        hwDocsQamScsEcmgName        OBJECT-TYPE
            SYNTAX       OCTET STRING (SIZE (0..31))
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION
              "Indicates the ECMG name. The default value is null string.
              "
            ::= { hwDocsQamScsEcmgEcmEntry 3 }
			
		-- 1.3.6.1.4.1.2011.6.187.84.1.4
        hwDocsQamScsEcmgEcmSuperCasID        OBJECT-TYPE
            SYNTAX      Unsigned32 (0..4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the super conditional access system ID of ECMG session. The default value is 0.
                "
			DEFVAL { 0 }
            ::= { hwDocsQamScsEcmgEcmEntry 4 }
			
		-- 1.3.6.1.4.1.2011.6.187.84.1.5   
        hwDocsQamScsEcmgEcmAccessCriteria        OBJECT-TYPE
            SYNTAX       OCTET STRING (SIZE (0..127))
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION
              "Indicates the ECMG access criteria. The default value is null string.
              "
            ::= { hwDocsQamScsEcmgEcmEntry 5 }
			
		-- 1.3.6.1.4.1.2011.6.187.84.1.6
        hwDocsQamScsEcmgEcmACTransferMode        OBJECT-TYPE
            SYNTAX      Unsigned32 (0..1)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the send mode of ECMG AC.
                "
			DEFVAL { 0 }
            ::= { hwDocsQamScsEcmgEcmEntry 6 }

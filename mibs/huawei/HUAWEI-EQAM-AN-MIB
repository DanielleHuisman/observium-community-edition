        -- 1.3.6.1.4.1.2011.6.187.2.1
        hwDocsQamVideoProfileEntry    OBJECT-TYPE
            SYNTAX          HwDocsQamVideoProfileEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the video profile table used to manage information including video mapping.
                 The index of this entry is hwDocsQamVideoProfileName.
                "
            INDEX   
                { 
                hwDocsQamVideoProfileName
                }
            ::= { hwDocsQamVideoProfileTable  1 }
        
        HwDocsQamVideoProfileEntry ::=
            SEQUENCE 
                { 
                hwDocsQamVideoProfileName
                  OCTET STRING,
                hwDocsQamVideoProfileRowStatus
                  RowStatus   
                }
        
        -- 1.3.6.1.4.1.2011.6.187.2.1.1   
        hwDocsQamVideoProfileName        OBJECT-TYPE
            SYNTAX       OCTET STRING (SIZE (1..15))
            MAX-ACCESS   not-accessible
            STATUS       current
            DESCRIPTION
              "Indicates the video profile name. 
              "
            ::= { hwDocsQamVideoProfileEntry 1}
          
        -- 1.3.6.1.4.1.2011.6.187.2.1.2 
        hwDocsQamVideoProfileRowStatus        OBJECT-TYPE
            SYNTAX       RowStatus
            MAX-ACCESS   read-create
            STATUS       current
            DESCRIPTION
                "Row status, used to query, create, modify, and delete a video profile table. 
                 It supports the active(1), createAndGo(4), and destroy(6) operations.
                 During query, the value of this leaf is always active(1).
                 During deletion, specify the value of this leaf to destroy(6).
                 During creation, specify the value of this leaf to createAndGo(4).
                 To modify the parameters of a video profile table, this leaf need not be issued together.
                 Options:
                 1. active(1)      -indicates the query operation
                 2. createAndGo(4) -indicates the creating operation
                 3. destroy(6)     -indicates the deleting operation
                "
            ::= { hwDocsQamVideoProfileEntry 2}
          

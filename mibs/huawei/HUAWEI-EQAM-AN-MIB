                "   
            ::= { hwDocsQamInputPIDETR290DetailsEntry 2 }
            
        -- 1.3.6.1.4.1.2011.6.187.56.1.3
        hwDocsQamInputPIDETR290DetailsPID        OBJECT-TYPE
            SYNTAX      Unsigned32(0..8191|4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the PID value of an input TS.                
                "   
            ::= { hwDocsQamInputPIDETR290DetailsEntry 3 }
            
        -- 1.3.6.1.4.1.2011.6.187.56.1.4
        hwDocsQamInputPIDETR290DetailsTableID        OBJECT-TYPE
            SYNTAX      Unsigned32(0..255|4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the PID table ID of an input TS.
                "   
            ::= { hwDocsQamInputPIDETR290DetailsEntry 4 }
            
        -- 1.3.6.1.4.1.2011.6.187.56.1.5
        hwDocsQamInputPIDETR290MonitoringState        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  ok(1),
                  alarm(2),
                  unknown(3),
                  disable(4)
                  }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ETR290 monitoring status of the PID of an input TS. 
                 Options:
                 1. ok(1)          --No error
                 2. alarm(2)       --Have error
                 3. unknown(3)     --Monitoring
                 4. disable(4)     --ETR290 monitor disable
                 The default value is unknown(3).
                "   
            ::= { hwDocsQamInputPIDETR290DetailsEntry 5 }
            
        -- 1.3.6.1.4.1.2011.6.187.56.1.6
        hwDocsQamInputPIDETR290MonitoringLastErrorIndication        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  now(1),
                  never(2),
                  last(3)
                  }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the last error indication of ETR290 monitoring for the PID of an input TS.
                 Options:
                 1. now(1)      --Now
                 2. never(2)    --No error
                 3. last(3)     --Last error
                 4. The default value is never(2).
                "   
            ::= { hwDocsQamInputPIDETR290DetailsEntry 6 }
            
        -- 1.3.6.1.4.1.2011.6.187.56.1.7
        hwDocsQamInputPIDETR290MonitoringLastErrorOccurTime        OBJECT-TYPE
            SYNTAX      TimeTicks
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the difference between the time when ETR290 monitoring of the PID of an input TS has an error and the current time. 
                The default value is 0.
                "   
            ::= { hwDocsQamInputPIDETR290DetailsEntry 7 }
            
        -- 1.3.6.1.4.1.2011.6.187.56.1.8
        hwDocsQamInputPIDETR290MonitoringCurrentErrorCount        OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION

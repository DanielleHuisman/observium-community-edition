            SYNTAX          HwDocsQamInputTSEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the input TS table.
                 The index of this entry is hwDocsQamInputTSIndex.
                "
            INDEX
                { 
                hwDocsQamInputTSIndex
                }
            ::= { hwDocsQamInputTSTable  1 }  
            
        HwDocsQamInputTSEntry ::=
            SEQUENCE 
                { 
                hwDocsQamInputTSIndex
                  Unsigned32,
                hwDocsQamInputTSIpAddressType
                  InetAddressType,
                hwDocsQamInputTSMulticastIpAddress
                  InetAddress,
                hwDocsQamInputTSSourceIpAddress
                  InetAddress,
                hwDocsQamInputTSSecondaryMulticastIpAddress
                  InetAddress,
                hwDocsQamInputTSSecondarySourceIpAddress
                  InetAddress,
                hwDocsQamInputTSState
                  INTEGER,
                hwDocsQamInputTSTraffic
                  Unsigned32,
                hwDocsQamInputTSDiscontinuityCounter
                  Counter64,
                hwDocsQamInputTSEPGInput
                  TruthValue,
                hwDocsQamInputTSJitter
                  Integer32,
                hwDocsQamInputTSRTPSequenceErrorCounter
                  Counter64, 
                hwDocsQamInputTSRedundancyMode 
                  INTEGER, 
                hwDocsQamInputTSUdpPort
                  InetPortNumber,
                hwDocsQamInputTSSecondaryUdpPort
                  InetPortNumber,
                hwDocsQamInputTSMode
                  INTEGER, 
                hwDocsQamInputTSTransportStreamID
                  Unsigned32, 
                hwDocsQamInputTSRedundancyTriggers
                  BITS,
                hwDocsQamInputTSRedundancyTriggersDefault
                  TruthValue,
                hwDocsQamInputTSRowStatus
                  RowStatus
                }          
                

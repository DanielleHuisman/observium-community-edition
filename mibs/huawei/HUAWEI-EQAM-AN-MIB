-- Textual Conventions

HwEqamPsiPolicyDvbDomain ::= TEXTUAL-CONVENTION
     STATUS      current
     DESCRIPTION
         "PSI/SI output policy for DVB domain.
          'stop' indicates PSI/SI output is disabled.
          'play' indicates PSI/SI will be enabled for playout.
         "
     SYNTAX      INTEGER 
           {
           stop(1),
           play(2)
           }
           
HwEqamPsiPolicyChannnel ::= TEXTUAL-CONVENTION
      STATUS      current
      DESCRIPTION
         "PSI/SI output policy for channel.
          'stop' indicates PSI/SI output is disabled.
          'play' indicates PSI/SI will be enabled for playout.
          'default' indicates the output policy inherits from the DVB domain.
         "
     SYNTAX      INTEGER 
           {
           stop(1),
           play(2),
           default(3)
           }  
                      
HwEqamInputPIDType ::= TEXTUAL-CONVENTION
     STATUS      current
     DESCRIPTION
         "Indicates the input PID type. 
         Options:
         pat(1)      --Program Association Table
         pmt(2)      --Program Map Table
         cat(3)      --Conditional Access Table
         nit(4)      --Network Information Table
         sdt(5)      --Service Description Table
         eit(6)      --Event Information Table
         tdttot(7)   --Time and Date Table/Time Offset Table
         ecm(8)      --Entitlement Control Message
         emm(9)      --Entitlement Management Message
         pcr(10)     --Program Clock Reference
         null(11)    --NULL
         media(12)   --Media
         unknown(255)--Unkonwn
         "                              
     SYNTAX      INTEGER 
           {
           pat(1),      
           pmt(2),               
           cat(3),      
           nit(4),      
           sdt(5),      
           eit(6),      
           tdttot(7),   
           ecm(8),      
           emm(9),      
           pcr(10),     
           null(11),    
           media(12),   
           unknown(255)
           }                  
--  Node definitions

        -- 1.3.6.1.4.1.2011.6.187.1
        hwDocsQamVideoGlobalConfigObject   OBJECT IDENTIFIER ::= { hwEqamAn 1 }
        
        -- 1.3.6.1.4.1.2011.6.187.1.1
        hwDocsQamVideoTimeOutInterval  OBJECT-TYPE
            SYNTAX      Integer32 (1..6000)
            UNITS       "second"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the video timeout interval.
                 Default: 60
                "
            ::= { hwDocsQamVideoGlobalConfigObject 1 }

        -- 1.3.6.1.4.1.2011.6.187.1.2
        hwDocsQamAutoConfigIppoolAddressType  OBJECT-TYPE
            SYNTAX      InetAddressType
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the address type of the address pool. Only IPv4 addresses are supported.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 2 }
                  
        -- 1.3.6.1.4.1.2011.6.187.1.3
        hwDocsQamAutoConfigIppoolStartAddress  OBJECT-TYPE
            SYNTAX      InetAddress
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the start address of the address pool. 
                 To delete an IP address pool, bind hwDocsQamAutoConfigIppoolStartAddress and hwDocsQamAutoConfigIppoolEndAddress together and set their values to 0xff 0xff 0xff 0xff.
                 The value of hwDocsQamAutoConfigIppoolStartAddress or hwDocsQamAutoConfigIppoolEndAddress must be set to a valid unicast IP address in other scenarios.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 3 }
      
        -- 1.3.6.1.4.1.2011.6.187.1.4
        hwDocsQamAutoConfigIppoolEndAddress  OBJECT-TYPE
            SYNTAX      InetAddress
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the end address of the address pool.
                 To delete an IP address pool, bind hwDocsQamAutoConfigIppoolStartAddress and hwDocsQamAutoConfigIppoolEndAddress together and set their values to 0xff 0xff 0xff 0xff.
                 The value of hwDocsQamAutoConfigIppoolStartAddress or hwDocsQamAutoConfigIppoolEndAddress must be set to a valid unicast IP address in other scenarios.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 4 }
      

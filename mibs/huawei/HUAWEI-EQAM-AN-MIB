        -- 1.3.6.1.4.1.2011.6.187.83.1.8
        hwDocsQamScsEcmgState        OBJECT-TYPE
            SYNTAX      INTEGER
				{
                  connecting(1),
                  connected(2),
                  notopen(3),
                  settingup(4),
                  open(5),
                  error(6)
                }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the state of ECMG session. The default value is connecting.
				 Options: 
                 1. connecting(1) - The ECMG is connecting.
                 2. connected(2) - The ECMG is connected.
				 3. notopen(3) - The ECMG is not open.
                 4. settingup(4) - The ECMG is setting up.
				 5. open(5) - The ECMG is open.
                 6. error(6) - The ECMG is error.
                "
            DEFVAL { connecting }
            ::= { hwDocsQamScsEcmgInfoEntry 8 }	
			

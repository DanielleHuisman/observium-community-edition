        HwDocsQamInputTSETR290DetailsEntry ::=
            SEQUENCE 
                {
                hwDocsQamInputTSETR290MonitoringType 
                  INTEGER, 
                hwDocsQamInputTSETR290MonitoringState  
                  INTEGER,
                hwDocsQamInputTSETR290MonitoringLastErrorIndication
                  INTEGER,
                hwDocsQamInputTSETR290MonitoringLastErrorOccurTime
                  TimeTicks,
                hwDocsQamInputTSETR290MonitoringCurrentErrorCount
                  Counter64,
                hwDocsQamInputTSETR290MonitoringTotalErrorCount
                  Counter64
               }
                                 
        -- 1.3.6.1.4.1.2011.6.187.55.1.1
        hwDocsQamInputTSETR290MonitoringType        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  tsSyncLoss(1),
                  syncByteError(2),
                  patError(3),
                  ccError(4),
                  pmtError(5),
                  pidError(6),
                  transportError(7),
                  crcError(8),
                  pcrError(9),
                  pcrAccuracyError(10),
                  catError(11)
                  }
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "Indicates the ETR290 monitoring type of an input TS.
                 Options:
                 1. tsSyncLoss(1)           --TS sync loss
                 2. syncByteError(2)        --Sync byte error
                 3. patError(3)             --PAT error
                 4. ccError(4)              --Continuity count error
                 5. pmtError(5)             --PMT error
                 6. pidError(6)             --PID error
                 7. transportError(7)       --Transport error
                 8. crcError(8)             --CRC error
                 9. pcrError(9)             --PCR error
                 10. pcrAccuracyError(10)   --PCR accuracy error
                 11. catError(11)           --CAT error
                "   
            ::= { hwDocsQamInputTSETR290DetailsEntry 1 }
            
        -- 1.3.6.1.4.1.2011.6.187.55.1.2
        hwDocsQamInputTSETR290MonitoringState        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  ok(1),
                  alarm(2),
                  unknown(3),
                  disable(4)
                  }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the ETR290 monitoring status of an input TS.
                 Options:
                 1. ok(1)          --No error
                 2. alarm(2)       --Have error
                 3. unknown(3)     --Monitoring
                 4. disable(4)     --ETR290 monitor disable
                 The default value is unknown(3).
                "   
            ::= { hwDocsQamInputTSETR290DetailsEntry 2 }
            
        -- 1.3.6.1.4.1.2011.6.187.55.1.3
        hwDocsQamInputTSETR290MonitoringLastErrorIndication        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  now(1),
                  never(2),
                  last(3)
                  }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the last error indication of ETR290 monitoring for an input TS.
                 Options:
                 1. now(1)      --Now
                 2. never(2)    --No error
                 3. last(3)     --Last error
                 4. The default value is never(2).
                "   
            ::= { hwDocsQamInputTSETR290DetailsEntry 3 }
            
        -- 1.3.6.1.4.1.2011.6.187.55.1.4
        hwDocsQamInputTSETR290MonitoringLastErrorOccurTime        OBJECT-TYPE
            SYNTAX      TimeTicks
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the difference between the time when ETR290 monitoring of an input TS has an error and the current time.  
                The default value is 0.
                "   
            ::= { hwDocsQamInputTSETR290DetailsEntry 4 }
            
        -- 1.3.6.1.4.1.2011.6.187.55.1.5
        hwDocsQamInputTSETR290MonitoringCurrentErrorCount        OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the error count of the current probe in ETR290 monitoring for an input TS.                
                "   
            ::= { hwDocsQamInputTSETR290DetailsEntry 5 }
            
        -- 1.3.6.1.4.1.2011.6.187.55.1.6
        hwDocsQamInputTSETR290MonitoringTotalErrorCount        OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Indicates the total error count in ETR290 monitoring for an input TS.
                "   
            ::= { hwDocsQamInputTSETR290DetailsEntry 6 }
        
        -- 1.3.6.1.4.1.2011.6.187.56
        hwDocsQamInputPIDETR290DetailsTable    OBJECT-TYPE
            SYNTAX          SEQUENCE OF HwDocsQamInputPIDETR290DetailsEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the detailed ETR290 monitoring information table for the PID of an input TS.
                 The indexes of this table are hwDocsQamInputTSIndex, hwDocsQamInputTSETR290MonitoringType, hwDocsQamInputPIDETR290DetailsPIDIndex 
                 and hwDocsQamInputPIDETR290DetailsTableIndex.
                "
            ::= { hwEqamAn 56 }
                        
        -- 1.3.6.1.4.1.2011.6.187.56.1
        hwDocsQamInputPIDETR290DetailsEntry    OBJECT-TYPE
            SYNTAX          HwDocsQamInputPIDETR290DetailsEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the detailed ETR290 monitoring information table for the PID of an input TS.
                 The indexes of this entry are hwDocsQamInputTSIndex, hwDocsQamInputTSETR290MonitoringType, hwDocsQamInputPIDETR290DetailsPIDIndex 
                 and hwDocsQamInputPIDETR290DetailsTableIndex.
                "
            INDEX   
                { 
                hwDocsQamInputTSIndex,
                hwDocsQamInputTSETR290MonitoringType,
                hwDocsQamInputPIDETR290DetailsPIDIndex,
                hwDocsQamInputPIDETR290DetailsTableIndex
                }
            ::= { hwDocsQamInputPIDETR290DetailsTable 1 }
            
        HwDocsQamInputPIDETR290DetailsEntry ::=
            SEQUENCE 
                {
                hwDocsQamInputPIDETR290DetailsPIDIndex 
                  Unsigned32, 
                hwDocsQamInputPIDETR290DetailsTableIndex  
                  Unsigned32,
                hwDocsQamInputPIDETR290DetailsPID
                  Unsigned32,
                hwDocsQamInputPIDETR290DetailsTableID
                  Unsigned32,
                hwDocsQamInputPIDETR290MonitoringState
                  INTEGER,
                hwDocsQamInputPIDETR290MonitoringLastErrorIndication
                  INTEGER,
                hwDocsQamInputPIDETR290MonitoringLastErrorOccurTime
                  TimeTicks,
                hwDocsQamInputPIDETR290MonitoringCurrentErrorCount
                  Counter64,
                hwDocsQamInputPIDETR290MonitoringCurrentRecv
                  TruthValue,
                hwDocsQamInputPIDETR290MonitoringRecvCount
                  Counter64,
                hwDocsQamInputPIDETR290MonitoringPCRDiscontinuity
                  Unsigned32,
                hwDocsQamInputPIDETR290MonitoringPCRDiscontinuityMax
                  Unsigned32,
                hwDocsQamInputPIDETR290MonitoringPCRAccuracyJitter
                  Unsigned32,
                hwDocsQamInputPIDETR290MonitoringPCRAccuracyJitterMax
                  Unsigned32
               }
                                 
        -- 1.3.6.1.4.1.2011.6.187.56.1.1
        hwDocsQamInputPIDETR290DetailsPIDIndex        OBJECT-TYPE
            SYNTAX      Unsigned32(1..4294967295)
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "Indicates the PID index of an input TS.
                "   
            ::= { hwDocsQamInputPIDETR290DetailsEntry 1 }
        
        -- 1.3.6.1.4.1.2011.6.187.56.1.2

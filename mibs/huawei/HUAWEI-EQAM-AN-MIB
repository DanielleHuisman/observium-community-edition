        -- 1.3.6.1.4.1.2011.6.187.39.1.2
        hwDocsQamInputTSPIDList        OBJECT-TYPE
            SYNTAX       OCTET STRING (SIZE (0..1024))
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION
                "Format: 2 bytes in network byte order,
                 which indicates a PID. A maximum of 512 PIDs are supported.                  
                 a 2-byte bit-field value defined as follows:
                 Bit 0:xx
                 Bit 1:xx.
               "
            ::= { hwDocsQamInputTSPIDEntry 2 }
        
        -- 1.3.6.1.4.1.2011.6.187.39.1.3
        hwDocsQamInputTSPIDStateMask        OBJECT-TYPE
            SYNTAX       OCTET STRING (SIZE (0..1024))
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION
                "Indicates the PID list status mask of the input TS. The default value is an empty character string, indicating no PID list status mask.
                Format: 2 bytes in network byte order, which indicates a PID list status mask. A maximum of 512 PID list status masks are supported. 
                a 2-byte bit-field value defined as follows:
                 Bit 0:notScambled
                 Bit 1:evenScambled
                 Bit 2:oddScambled
                 Bit 3:pcrreferenced
                 Bit 4:ccerror.
                "
            ::= { hwDocsQamInputTSPIDEntry 3 }    
            

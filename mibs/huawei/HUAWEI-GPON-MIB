   hwGponDevicePortidMapObjectPortTypeIndex  OBJECT-TYPE
       SYNTAX     INTEGER {
            t1 (13),
            e1 (14),
            fe (24),
            ge (34),
            vdsl (37),
            moca (46),
            eth (47),
            iphost (50),
            tdmvcl (253),
            invalidValue(-1)
            }
       MAX-ACCESS   not-accessible
       STATUS      current

    hwIpDslamTraps OBJECT IDENTIFIER ::= { hwIpDslamSecurity 27 }

    hwIpDslamCommonTraps OBJECT IDENTIFIER ::= { hwIpDslamTraps 1 }

    hwIpDslamCommonTrapsPrefix OBJECT IDENTIFIER ::= { hwIpDslamCommonTraps 0 }


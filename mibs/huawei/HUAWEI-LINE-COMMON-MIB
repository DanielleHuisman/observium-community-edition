        hwOpticsDdmInfoExModuleType OBJECT-TYPE
            SYNTAX      INTEGER {
                gpon(1),
                epon(2),
                epon10g(3),
                gpon10g(4),
                wdmpon(5),
                hybridpon(6),
                gponepon(7),
                unknown(8),
                xgspon(9),
                gponandxgpon(10),
                gponandxgspon(11),
                twdmpon(15),				
                invalid(-1)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Options:
                 1. gpon(1)            - GPON
                 2. epon(2)            - EPON
                 3. epon10g(3)         - 10G EPON
                 4. gpon10g(4)         - 10G GPON
                 5. wdmpon(5)          - WDM PON
                 6. hybridpon(6)       - hybrid PON
                 7. gponepon(7)        - GPON/EPON
                 8. unknown(8)         - Unknown
                 9. xgspon(9)          - XGS-PON
                 10.gponandxgpon(10)   - GPON+XG-PON
                 11.gponandxgspon(11)  - GPON+XGS-PON
                 12.twdmpon(15)        - TWDM-PON
                 13.invalid(-1)        - Indicates that the query fails or no information is detected
                "
            ::= { hwOpticsDdmInfoExEntry  30 }

        hwOpticsDdmInfoExSubType OBJECT-TYPE
            SYNTAX      INTEGER {
                classbplus(1),
                classcplus(2),
                classb(3),
                classcplusplus(4),
                classd(5),
                n2a(33),
                n2b(34),
                n1(35),
                e1(36),
                e2a(37),
                e2b(38),
                n1n2ae1e2a(39),

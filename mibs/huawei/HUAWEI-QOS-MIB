            FROM SNMPv2-TC
        hwFrameIndex, hwSlotIndex, hwPortIndex
            FROM HUAWEI-DEVICE-MIB
        hwAclNumGroupAclNum     
            FROM HUAWEI-DSLAM-ACL-MIB;
              

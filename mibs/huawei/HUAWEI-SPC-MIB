			huaweiUtility
				FROM HUAWEI-MIB
			Integer32, IpAddress, OBJECT-TYPE, NOTIFICATION-TYPE, MODULE-IDENTITY, Unsigned32
				FROM SNMPv2-SMI
			RowStatus
				FROM SNMPv2-TC;
		



				hwEndtCESFrame
					Integer32,
				hwEndCESSlot
					Integer32,
				hwEndCESPort
					Integer32,
				hwEndUpPv8Frame
					Integer32,
				hwEndUpPv8Slot
					Integer32,
				hwEndUpPv8Port
					Integer32,
				hwEndDownPv8Frame
					Integer32,
				hwEndDownPv8Slot
					Integer32,
				hwEndDownPv8Port
					Integer32,
				hwEndRspFrame
					Integer32,
				hwEndRspSlot
					Integer32,
				hwEndRspPort
					Integer32,
                hwIpSpcAttributeLocalIPAddr	
                    IpAddress,
	  	        hwIpSpcAttributeLocalPort	
	  	  	        Integer32,
				hwIpSpcAttributeRemoteIPAddr	
					IpAddress,
				hwIpSpcAttributeRemotePort	
					Integer32,

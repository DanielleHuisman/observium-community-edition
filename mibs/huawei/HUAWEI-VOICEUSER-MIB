          ::= { hwPstnCTPConfigEntry 16 } 
          
        hwPstnCTPDspInputGain OBJECT-TYPE
          SYNTAX      Integer32(1..16)
          MAX-ACCESS  read-create
          STATUS      current
          DESCRIPTION
              "
              Dsp input gain.
              1:    -10db
              2:    -9db
              3:    -8db
              4:    -7db
              5:    -6db
              6:    -5db
              7:    -4db
              8:    -3db
              9:    -2db
              10:   -1db
              11:    0db
              12:    1db
              13:    2db
              14:    3db
              15:    4db
              16:    5db
              Unit: db
              "
              DEFVAL      { 11 }
          ::= { hwPstnCTPConfigEntry 17 } 
          
        hwPstnCTPDspOutputGain OBJECT-TYPE
          SYNTAX      Integer32(1..21)

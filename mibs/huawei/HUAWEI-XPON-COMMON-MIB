            SEQUENCE { 
                     hwXponOpticalUseType                       INTEGER,
                     hwXponOpticalGradeType                     INTEGER,
                     hwXponOpticalType                          INTEGER,                     
                     hwXponOpticalSubType                       INTEGER,

            ::= { hwXponOntFileTransmitionEntry  4 }
            
        hwXponOntTransmittingFree OBJECT-TYPE
            SYNTAX      INTEGER
                {
                clear(1),
                invalid(-1)
                }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION

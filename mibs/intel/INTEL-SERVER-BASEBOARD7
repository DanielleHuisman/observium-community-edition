    OBJECTS { 
        eventDescription,
        systemManagementInfoOverallStatusHealth
    }
    STATUS      current
    DESCRIPTION "The overall server health is now Critical."
    --Novell NMS trap annotation
    --TYPE       "Server Health Critical"
    --SEVERITY   CRITICAL 
    --SUMMARY    "The overall health of the server is critical."
    --CATEGORY   "Error Alarms"
    --STATE      FAILED 
    --STATUS     MANDATORY 

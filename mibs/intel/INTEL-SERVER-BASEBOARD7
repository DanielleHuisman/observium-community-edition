    OBJECTS { 
        eventDescription,
        systemManagementInfoOverallStatusHealth
    }
    STATUS      current
    DESCRIPTION "The overall server health is now Degraded."
    --Novell NMS trap annotation
    --TYPE       "Server Health Non-Critical"
    --SEVERITY   MAJOR 
    --SUMMARY    "The overall health of the server is non-critical."
    --CATEGORY   "Error Alarms"
    --STATE      DEGRADED 
    --STATUS     MANDATORY         

    OBJECTS { 
        eventDescription,
        eventSeverity
    }
    STATUS      current
    DESCRIPTION "An intrusion to the chassis has been detected."
    --Novell NMS trap annotation
    --TYPE       "Server Chassis Intrusion Detected"
    --SEVERITY   MINOR 
    --SUMMARY    "An intrusion into the chassis has been detected."
    --CATEGORY   "Error Alarms"
    --STATE      OPERATIONAL
    --STATUS     OPTIONAL 
    ::= { chassisEvents 1 }

chassisIntrusionClearedEvent                NOTIFICATION-TYPE
    OBJECTS { 
        eventDescription,
        eventSeverity
    }
    STATUS      current
    DESCRIPTION "The chassis intrusion is no longer detected."
    --Novell NMS trap annotation
    --TYPE       "Server Chassis Intrusion Cleared"
    --SEVERITY   INFORMATIONAL
    --SUMMARY    "The intrusion into the chassis has been cleared."

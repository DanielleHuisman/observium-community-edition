coolingDeviceUpperNonRecoverableEvent       NOTIFICATION-TYPE
    OBJECTS { 
        eventDescription,
        coolingDeviceStatus,
        coolingDeviceDescription,
        coolingDeviceCIMDeviceId
    }
    STATUS      current
    DESCRIPTION "Current reading exceeds upper non-recoverable threshold.  Status for this
sensor is non-recoverable."
    --Novell NMS trap annotation
    --TYPE       "Server Cooling Upper Non-Recoverable"
    --SEVERITY   CRITICAL 
    --SUMMARY    "Cooling device %s exceeded upper non-recoverable threshold."
    --ARGUMENTS  {3}
    --CATEGORY   "Threshold Alarms"
    --STATE      FAILED  
    --STATUS     OPTIONAL    
    ::= { thermalEvents 10 }


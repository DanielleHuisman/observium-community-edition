    OBJECTS { 
        eventDescription,
        systemManagementInfoOverallStatusHealth
    }
    STATUS      current
    DESCRIPTION "The overall server health is now unknown."
    --Novell NMS trap annotation
    --TYPE       "Server Health Unknown"
    --SEVERITY   INFORMATIONAL 
    --SUMMARY    "The overall health of the server is unknown."
    --CATEGORY   "Error Alarms"
    --STATE      OPERATIONAL
    --STATUS     MANDATORY 
    ::= { systemManagementSWEvents 2 }


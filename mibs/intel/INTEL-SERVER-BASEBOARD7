    OBJECTS { 
        eventDescription,
        systemManagementInfoOverallStatusHealth
    }
    STATUS      current
    DESCRIPTION "The overall server health is now OK."
    --Novell NMS trap annotation
    --TYPE       "Server Health OK"
    --SEVERITY   INFORMATIONAL 
    --SUMMARY    "The overall health of the server is OK."
    --CATEGORY   "Error Alarms"
    --STATE      OPERATIONAL
    --STATUS     MANDATORY         
    ::= { systemManagementSWEvents 3 }


-- System Management SW Events
-------------------------------------------------------------------------------
systemHealthOtherEvent                      NOTIFICATION-TYPE
    OBJECTS { 
        eventDescription,
        systemManagementInfoOverallStatusHealth 
    }
    STATUS      current
    DESCRIPTION "The overall server health is now a non-standard value of other."
    --Novell NMS trap annotation
    --TYPE       "Server Health Other"
    --SEVERITY   INFORMATIONAL 
    --SUMMARY    "The overall health of the server is other."
    --CATEGORY   "Error Alarms"
    --STATE      OPERATIONAL
    --STATUS     MANDATORY 
    ::= { systemManagementSWEvents 1 }


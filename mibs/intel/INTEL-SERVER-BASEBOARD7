INTEL-SERVER-BASEBOARD7 DEFINITIONS ::= BEGIN


IMPORTS
      MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, enterprises, Counter32, Integer32
        FROM SNMPv2-SMI

    DisplayString, TEXTUAL-CONVENTION     
        FROM SNMPv2-TC;

intel                           OBJECT IDENTIFIER ::= { enterprises 343 }
products                        OBJECT IDENTIFIER ::= { intel 2 }
server-management               OBJECT IDENTIFIER ::= { products 10 }
software                        OBJECT IDENTIFIER ::= { server-management 3 }
baseboardGroup7                 OBJECT IDENTIFIER ::= { software 5 }
systemManagementSoftwareGroup   OBJECT IDENTIFIER ::= { baseboardGroup7  100 }
chassisInformationGroup         OBJECT IDENTIFIER ::= { baseboardGroup7  200 }
processorGroup                  OBJECT IDENTIFIER ::= { baseboardGroup7  300 }
powerGroup                      OBJECT IDENTIFIER ::= { baseboardGroup7  400 }
memoryGroup                     OBJECT IDENTIFIER ::= { baseboardGroup7  500 }
thermalGroup                    OBJECT IDENTIFIER ::= { baseboardGroup7  600 }
driveSlotGroup                  OBJECT IDENTIFIER ::= { baseboardGroup7  700 }
miscellaneousGroup              OBJECT IDENTIFIER ::= { baseboardGroup7  800 }
baseboard7EventGroup            OBJECT IDENTIFIER ::= { baseboardGroup7  1000 }

--*****************************************************************************
--
-- MODULE IDENTITY AND REVISION GROUP
--
--*****************************************************************************

intelServerBaseboard7       MODULE-IDENTITY
    LAST-UPDATED    "201209151700Z" -- coordinated universal time UTC format is YYMMDDHHmmZ
    ORGANIZATION    "Intel, Server Management Software"
    CONTACT-INFO    " "
    DESCRIPTION     "This SNMP MIB module supports the Intel Server Baseboard SNMP subagent
                    for monitoring baseboard components, providing SNMP access to GET information
                    for baseboard components, SET sensor thresholds and support events through
                    the NOTIFICATION-TYPE construct.

                    Version:  2.0   9/15/2012 
 
                    Intel copyright information 2012/2013"
                    ::= { baseboardGroup7 1 }


IntelStatus                     ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The status used for the overall health of the server."
    SYNTAX      INTEGER {
					unavailable(1),
                    healthy(2),
                    warning(3),
                    critical(4)}

IntelFeatureStatus              ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The status of a particular feature."
    SYNTAX      INTEGER {
                    other(1), 
                    unknown(2),  
                    disabled(3),               
                    enabled(4),           
                    notImplemented(5) }      

IntelRedundancyStatus           ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The status of the redundancy capability if enabled on the server."
    SYNTAX      INTEGER {
                    other(1), 
                    unknown(2),  
                    full(3),               
                    degraded(4),           
                    lost(5),                
                    notRedundant(6),        
                    redundancyOffline(7),
	       			notApplicable(8) }
 
                            
OneBasedIndex                   ::= Integer32(1..2147483647)


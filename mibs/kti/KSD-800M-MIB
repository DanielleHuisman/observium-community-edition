KSD-800M-MIB DEFINITIONS ::= BEGIN

IMPORTS
        enterprises, OBJECT-TYPE, NetworkAddress, IpAddress, Counter,
        Gauge, TimeTicks
                FROM RFC1155-SMI
        DisplayString, PhysAddress
                FROM RFC1213-MIB;   

KTI OBJECT IDENTIFIER ::= { enterprises 867 }

KSD-800M OBJECT IDENTIFIER ::= { KTI 50 }

Device_CPU		OBJECT IDENTIFIER ::= { KSD-800M 1 }

PortStatus		OBJECT IDENTIFIER ::= { KSD-800M 4 }

VlanStatus		OBJECT IDENTIFIER ::= { KSD-800M 5 }

QosStatus		OBJECT IDENTIFIER ::= { KSD-800M 6 }

WDTStatus		OBJECT IDENTIFIER ::= { KSD-800M 8 }


-- object types

-- the slot2and3cpumodule group

softwareVer OBJECT-TYPE
        SYNTAX  OCTET STRING
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The version of the device firmware"
        ::= { Device_CPU 1 }

mibFileVer OBJECT-TYPE
        SYNTAX  OCTET STRING
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The version of the MIB file supported"
        ::= { Device_CPU 2 }

--Login_Trap_Message1 OBJECT-TYPE

             INTEGER,
        protectedPortenable
             INTEGER
      }
       
    protectedindex    OBJECT-TYPE
       SYNTAX    INTEGER
       ACCESS    read-only
       STATUS    mandatory
       DESCRIPTION
          " 
          "
       ::= { protectedPortEntry 1 }   
       
    protectedPortenable    OBJECT-TYPE
       SYNTAX    INTEGER
        {
          enable(1),
          disable(2)
        }
       ACCESS    read-write
       STATUS    mandatory
       DESCRIPTION
          " A switch to select whether the protected Port of Port is Enable(1) or Disable(2).
          "
       ::= { protectedPortEntry 2 }  

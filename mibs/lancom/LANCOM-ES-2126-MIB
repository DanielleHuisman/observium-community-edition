    SYNTAX      ES2126TrapHostEntry
    MAX-ACCESS      not-accessible
    STATUS      current
    DESCRIPTION
        "This is the Entry Information for trap host."
    INDEX {es2126TrapHostIndex}
    ::= { es2126TrapHostTable 1 }


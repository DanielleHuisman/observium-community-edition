    ::= { es2126pConfigFile 3 }

es2126pDoImportConfig OBJECT-TYPE
    SYNTAX      INTEGER(1..3)
    MAX-ACCESS      read-write
    STATUS      current
    DESCRIPTION
        "Do import-config.
        Get this OID always return zero.
            1   -   Import start section.
            2   -   Import user-conf section.
            3   -   Import start and overwrite current ip."
    ::= { es2126pConfigFile 4 }


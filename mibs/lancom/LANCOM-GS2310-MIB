gs2310TrapEvent                                             OBJECT IDENTIFIER ::= { gs2310Trap 1 }

gs2310Emergency NOTIFICATION-TYPE
  OBJECTS   { gs2310Information }
  STATUS        current
    DESCRIPTION
        "The severity level is Emergency. The system is unusable."
    ::= { gs2310TrapEvent 1 }


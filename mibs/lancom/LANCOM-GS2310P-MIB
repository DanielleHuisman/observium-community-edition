   gs2310pIGMPSnoopingSSMIndex          Integer32,
   gs2310pIGMPSnoopingSSMVLANID         Integer32,
   gs2310pIGMPSnoopingSSMGroup          DisplayString,
   gs2310pIGMPSnoopingSSMPort           Integer32,
   gs2310pIGMPSnoopingSSMMode           DisplayString,
   gs2310pIGMPSnoopingSSMSourceAddress  DisplayString,
   gs2310pIGMPSnoopingSSMType           DisplayString

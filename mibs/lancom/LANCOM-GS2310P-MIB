   gs2310pNASSwitchStatusPortState		DisplayString,
   gs2310pNASSwitchStatusLastSource		DisplayString,
   gs2310pNASSwitchStatusLastID			DisplayString,
   gs2310pNASSwitchStatusQoSClass		DisplayString,
   gs2310pNASSwitchStatusPortVlanID		DisplayString
}
   

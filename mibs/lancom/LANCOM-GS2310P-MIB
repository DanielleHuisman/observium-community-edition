   gs2310pMLDSnoopingVLANRV        	Integer32,
   gs2310pMLDSnoopingVLANQI      	    Integer32,
   gs2310pMLDSnoopingVLANQRI   	    Integer32,
   gs2310pMLDSnoopingVLANLLQI   	    Integer32,
   gs2310pMLDSnoopingVLANURI    	    Integer32
}


   gs2310pplusQosDSCPList       Integer32,
   gs2310pplusQosDSCP           DisplayString,
   gs2310pplusQosDSCPTrust      INTEGER,
   gs2310pplusQosDSCPQosClass	  Integer32,
   gs2310pplusQosDSCPDPL	      INTEGER
}


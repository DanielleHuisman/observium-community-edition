    	SYNTAX 	AliasInGroupEntry
    	MAX-ACCESS not-accessible
    	STATUS current
    	DESCRIPTION
		"Each entry contains the alias used by group. "
    	INDEX { idGroupName, idAliasName }
    	::= { aliasInGroupTable 1 }
    

    ::= {siuInfoEntry 6}    

siuWorkingStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				on(1),
    				off(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "siu卡工作状态。(1 工作，2未工作)"
    ::= {siuInfoEntry 7}

siuAlarmStatus  OBJECT-TYPE
    SYNTAX  TruthValue
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源告警状态。(1 有告警, 2 无告警)"
    ::= {siuInfoEntry 8}     
    
   
-- siuInfoTable end



-- subSlotTable  接口属性表

subSlotTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF subSlotEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "端口属性表。"
    ::= {mpSysSubSlotMib 1}

subSlotEntry  OBJECT-TYPE
    SYNTAX  subSlotEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "端口属性表条目。"
    INDEX   {subSlotParentIndex,subSlotIndex}
    ::= {subSlotTable 1}

subSlotParentIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "子插槽所在的父插槽号。"
    ::= {subSlotEntry 1}
    
subSlotIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "子插槽号。"
    ::= {subSlotEntry 2}
    
subSlotCardType  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "子插槽上插的板卡类型。"
    ::= {subSlotEntry 3}

subSlotPortNumber  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "子插槽上的端口数目"
    ::= {subSlotEntry 4}

subSlotOnlineStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				online(1),
    				offline(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "子插槽物理在线状态。(1在线 2不在线)"
    ::= {subSlotEntry 5}
    
-- subSlotTable  end


-- portInfoTable  端口信息表

portInfoTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF portInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "端口属性表。"
    ::= {mpSysPortMib 1}

portInfoEntry  OBJECT-TYPE
    SYNTAX  portInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "端口属性表条目。"
    INDEX   {portSlotIndex,portSubSlotIndex,portIndex}
    ::= {portInfoTable 1}

portSlotIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "端口所在板卡的索引，即插槽号绝对值。"
    ::= {portInfoEntry 1}
    
portSubSlotIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "端口所在子插槽板卡的索引。"
    ::= {portInfoEntry 2}
    
portIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "端口索引。"
    ::= {portInfoEntry 3}
    
portType  OBJECT-TYPE
    SYNTAX  INTEGER{
    				copper(1),
    				fiber(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "端口类型。1 电口 2光口"
    ::= {portInfoEntry 4}

portLinkStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				up(1),
    				down(2),
    				loop(3)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "端口状态。(1 up 2 down 3 loop)"
    ::= {portInfoEntry 5}
    
portOnlineStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				online(1),
    				offline(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "端口物理在线状态。(1在线 2不在线)"
    ::= {portInfoEntry 6}

-- portInfoTable  end


-- powerInfoTable   电源表

powerInfoTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF powerInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "电源表。"
    ::= {mpSysPowerMib 1}

powerInfoEntry  OBJECT-TYPE
    SYNTAX  powerInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "电源表条目。"
    INDEX   {powerIndex}
    ::= {powerInfoTable 1}

powerIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源索引，即插槽号绝对值。"
    ::= {powerInfoEntry 1}

powerType  OBJECT-TYPE
    SYNTAX  INTEGER{
            alternating-non-intelligent(1),
            direct-non-intelligent(2),
            alternating-intelligent(3),
            direct-intelligent(4)
            }
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源类型。 (1交流非智能 2直流非智能 3交流智能 4直流智能)"
    ::= {powerInfoEntry 2}

powerDescription  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..128))
    ACCESS  read-write
    STATUS  current
    DESCRIPTION
            "电源描述，最大长度128个字节"
    ::= {powerInfoEntry 3}

powerSerialNumber  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源序列号，最大长度32个字节。"
    ::= {powerInfoEntry 4}

powerCMMSoftwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源CMM软件版本，最大长度32个字节。"
    ::= {powerInfoEntry 5}
    
powerCMMHardwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源CMM硬件版本，最大长度32个字节。"
    ::= {powerInfoEntry 6}

powerCMMMonitorVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源Monitor版本，最大长度32个字节。"
    ::= {powerInfoEntry 7}    
    
powerOnlineStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				online(1),
            offline(2)
            }
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源物理在线状态。(1 在线, 2 不在线)"
    ::= {powerInfoEntry 8}    

powerWorkingStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				on(1),
            off(2)
            }
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源工作状态。(1 工作, 2 未工作)"
    ::= {powerInfoEntry 9}    
    
powerAlarmStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
						normal(1),
						alarm(2)
            }
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源告警状态。(1 正常, 2 告警)"
    ::= {powerInfoEntry 10}        
    
powerVoltageInput  OBJECT-TYPE
    SYNTAX  INTEGER
    UNITS   "mV"
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源输入电压。(单位：mv)"
    ::= {powerInfoEntry 11}               
    
powerVoltageOutput  OBJECT-TYPE
    SYNTAX  INTEGER
    UNITS   "mV"
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源输出电压。(单位：mv)"
    ::= {powerInfoEntry 12}     
                 
powerCurrentInput  OBJECT-TYPE
    SYNTAX  INTEGER
    UNITS   "mA"
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源输出电流。(单位：mA)"
    ::= {powerInfoEntry 13}    
                  
powerCurrentOutput OBJECT-TYPE
    SYNTAX  INTEGER
		UNITS   "mA"
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "电源输出电流。(单位：mA)"
    ::= {powerInfoEntry 14}               
           
-- powerInfoTable end


-- fanCardInfoTable   风扇卡表

fanCardInfoTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF fanCardInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "风扇卡表。"
    ::= {mpSysFanCardMib 1}


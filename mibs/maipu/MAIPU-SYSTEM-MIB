    SYNTAX  INTEGER
    UNITS   "mA"
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "系统电流。(单位mA)"
    ::= {mpSysInfoMib 2}
	

    
--mpSysInfo end



--mpSysWorkMod 系统工作模式
		
SysWorkMod  OBJECT-TYPE
    SYNTAX  INTEGER {
				   modeInvalid(1),
				   modeBigTable(2),
                              modeSmallTable(3),
                              modeBh01Standard(4),
                              modeBh01Mix(5)   
            }


    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "系统当前工作模式。(1系统工作模式还没确定 2带TCAM的4代板卡 3不带TCAM的4代板卡 4全是3代板卡 5 三、四代板卡混合)"
    ::= {mpSysWorkModMib 1}
	

    
--mpSysWorkMod end



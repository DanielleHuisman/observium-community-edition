MAIPU-SYSTEM-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, enterprises, Counter32, Gauge32,
          Integer32, Unsigned32, TimeTicks,IpAddress,Counter64
            FROM SNMPv2-SMI
        TEXTUAL-CONVENTION, TruthValue, MacAddress,
          RowStatus, DisplayString, DateAndTime
            FROM SNMPv2-TC
        MODULE-COMPLIANCE, OBJECT-GROUP
            FROM SNMPv2-CONF
        mpMgmt
            FROM MAIPU-SMI;
            
mpSystemMib 							OBJECT IDENTIFIER ::= {mpMgmt 600}


mpSysInfoMib							OBJECT IDENTIFIER ::= {mpSystemMib 1}
mpSysMpuMib								OBJECT IDENTIFIER ::= {mpSystemMib 2}
mpSysUnitMib							OBJECT IDENTIFIER ::= {mpSystemMib 3}
mpSysSiuMib								OBJECT IDENTIFIER ::= {mpSystemMib 4}
mpSysSubSlotMib							OBJECT IDENTIFIER ::= {mpSystemMib 5}
mpSysPortMib    						OBJECT IDENTIFIER ::= {mpSystemMib 6}
mpSysPowerMib 							OBJECT IDENTIFIER ::= {mpSystemMib 7}
mpSysFanCardMib 						OBJECT IDENTIFIER ::= {mpSystemMib 8}
mpSysCpuMib 							OBJECT IDENTIFIER ::= {mpSystemMib 9}
mpSysMemoryMib 							OBJECT IDENTIFIER ::= {mpSystemMib 10}
mpSysWorkModMib                         OBJECT IDENTIFIER ::= {mpSystemMib 11}





--mpuInfoTable MPU卡信息表

mpuInfoTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF mpuInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "MPU卡信息表。"
    ::= {mpSysMpuMib 1}

mpuInfoEntry  OBJECT-TYPE
    SYNTAX  mpuInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "MPU卡信息表条目。"
    INDEX   {mpuIndex}
    ::= {mpuInfoTable 1}

mpuIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡索引，即插槽号绝对值。"
    ::= {mpuInfoEntry 1}

mpuType  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡类型。"
    ::= {mpuInfoEntry 2}
    
mpuDescription  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..128))
    ACCESS  read-write
    STATUS  current
    DESCRIPTION
            "MPU卡描述，最大长度128个字节。"
    ::= {mpuInfoEntry 3}

mpuSerialNumber  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡序列号，最大长度32个字节。"
    ::= {mpuInfoEntry 4}

mpuSoftwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡软件版本。最大长度32个字节。"
    ::= {mpuInfoEntry 5}

mpuHardwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..255))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡硬件版本，最大长度255个字节。"
    ::= {mpuInfoEntry 6}

mpuFPGAVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..255))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡FPGA版本，最大长度255个字节。"
    ::= {mpuInfoEntry 7}

           
mpuMonitorVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡Monitor版本，最大长度32个字节。"
    ::= {mpuInfoEntry 8}    

mpuCMMSoftwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡CMM软件版本，最大长度32个字节。"
    ::= {mpuInfoEntry 9}
    
mpuCMMHardwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡CMM硬件版本，最大长度32个字节。"
    ::= {mpuInfoEntry 10}
    
mpuCMMMonitorVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡CMMMonitor版本，最大长度32个字节。"
    ::= {mpuInfoEntry 11}    
    
mpuFlashTotalBytes  OBJECT-TYPE
    SYNTAX  Gauge32
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡flash总字节数(单位字节)。"
    ::= {mpuInfoEntry 12}    
    
mpuFlashLeftBytes  OBJECT-TYPE
    SYNTAX  Gauge32
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡flash剩余字节数(单位字节)。"
    ::= {mpuInfoEntry 13}        
    
mpuWorkingMode OBJECT-TYPE
    SYNTAX  INTEGER{
    				master(1),
    				slave(2),
    				doubleRouter(3)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡工作模式。(1主卡 2从卡 3双路由器)"
    ::= {mpuInfoEntry 14}    

mpuOnlineStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				online(1),
    				offline(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡物理在线状态。(1在线 2不在线)"
    ::= {mpuInfoEntry 15}    
        
mpuWorkingStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				on(1),
    				off(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡工作状态。(1 工作，2未工作)"
    ::= {mpuInfoEntry 16}
        
mpuPowerStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				on(1),
    				off(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡加电状态。(1已加电 2未加电)"
    ::= {mpuInfoEntry 17}        

mpuSynchronizationStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				batchSync(1),
    				realtimeSync(2),
    				noSlave(3),
    				abnormal(4)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡同步状态。(1完全同步(批量同步) 2实时同步 3从卡空缺 4异常)"
    ::= {mpuInfoEntry 18}
    
mpuCFStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				online(1),
    				offline(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPUCF卡在位状态。(1在位2不在位)"
    ::= {mpuInfoEntry 19}
    
mpuTemperature  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡温度。(单位 摄氏度)"
    ::= {mpuInfoEntry 20}    
    
mpuSubSlotNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "MPU卡上的子槽位数。"
    ::= {mpuInfoEntry 21}    
   
--mpuInfoTable end

-- unitInfoTable LPU与SFU信息表

unitInfoTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF unitInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "LPU与SFU信息表。"
    ::= {mpSysUnitMib 1}

unitInfoEntry  OBJECT-TYPE
    SYNTAX  unitInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "LPU与SFU信息表条目。"
    INDEX   {unitIndex}
    ::= {unitInfoTable 1}

unitIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "LPU或SFU卡索引，即插槽号绝对值。"
    ::= {unitInfoEntry 1}

unitType  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡类型。629477376 sfu-a,629547009 sfu-b,591949824 sfu-c,591953921 sfu-d,629227544 24GET(四代),629227800 24GEF(四代),612451120 24GET+24GEF(四代),629227568 48GET(四代),629227824 48GEF(四代),629354754 2XGE(四代),629354756 4XGE(四代),629354760 8XGE(四代),591481112 24GEFHC,591480856 24GETHC,592206616 16GET8CGEH,591481136 48GEFHC,591480880 48GETHC,592210714 24GEF2XGEH,591630336 NTA"
    ::= {unitInfoEntry 2}
    
unitDescription  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..128))
    ACCESS  read-write
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡描述，最大长度128个字节。"
    ::= {unitInfoEntry 3}

unitSerialNumber  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡序列号，最大长度32个字节。"
    ::= {unitInfoEntry 4}

unitSoftwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡软件版本。最大长度32个字节。"
    ::= {unitInfoEntry 5}

unitHardwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..255))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡硬件版本，最大长度255个字节。"
    ::= {unitInfoEntry 6}

unitFPGAVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..255))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡FPGA版本，最大长度255个字节。"
    ::= {unitInfoEntry 7}

unitCMMSoftwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡CMM软件版本，最大长度32个字节。"
    ::= {unitInfoEntry 8}
    
unitCMMHardwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡CMM更件版本，最大长度32个字节。"
    ::= {unitInfoEntry 9}      
         
unitCMMMonitorVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "LPU或SFU卡CMMMonitor版本，最大长度32个字节。"
    ::= {unitInfoEntry 10}
    
unitOnlineStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				online(1),
    				offline(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡在线状态。(1在线 2不在线)"
    ::= {unitInfoEntry 11}    
        
unitWorkingStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				on(1),
    				off(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡工作状态。(1 工作，2未工作)"
    ::= {unitInfoEntry 12}
    
unitPowerStatus OBJECT-TYPE
    SYNTAX  INTEGER{
    				on(1),
    				off(2)
    				}
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "lpu或sfu卡加电状态。(1已加电 2未加电)"
    ::= {unitInfoEntry 13}   
             
        
-- lpuInfoTable end


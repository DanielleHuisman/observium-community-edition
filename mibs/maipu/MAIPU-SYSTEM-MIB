    ::= {fanCardInfoEntry 4}
    
fanCardCMMHardwareVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇卡CMM硬件版本，最大长度32个字节。"
    ::= {fanCardInfoEntry 5}

fanCardCMMMonitorVersion  OBJECT-TYPE
    SYNTAX  DISPLASTRING(SIZE (0..31))
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇卡CMMMonitor版本，最大长度32个字节。"
    ::= {fanCardInfoEntry 6}       
    
fanCardOnlineStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				online(1),
            offline(2)
            }
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇卡物理在线状态。(1 在线, 2 不在线)"
    ::= {fanCardInfoEntry 7}    

fanCardWorkingStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
    				on(1),
            off(2)
            }
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇卡工作状态。(1 工作, 2 未工作)"
    ::= {fanCardInfoEntry 8}    
    
fanCardAlarmStatus  OBJECT-TYPE
    SYNTAX  INTEGER{
						normal(1),
						alarm(2)
            }
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇卡告警状态。(1 正常, 2 告警)"
    ::= {fanCardInfoEntry 9}   

fanCardGrps  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇卡的风扇组数。"
    ::= {fanCardInfoEntry 10}

fanCardNumPerGrp  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇卡每组风扇的个数。"
    ::= {fanCardInfoEntry 11}


-- fanCardInfoTable  end

-- fanInfoTable   风扇表

fanInfoTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF fanInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "风扇表。"
    ::= {mpSysFanCardMib 2}

fanInfoEntry  OBJECT-TYPE
    SYNTAX  fanInfoEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "风扇表条目。"
    INDEX   {fanCardIndex,fanIndex}
    ::= {fanInfoTable 1}

fanCardIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇所在风扇卡索引，即插槽号绝对值。"
    ::= {fanInfoEntry 1}

fanIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇索引，从第0组第0个风扇开始。"
    ::= {fanInfoEntry 2}


fanStatus  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇状态息，1 OK,2 FAIL"
    ::= {fanInfoEntry 3}

fanSpeed  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "风扇速度，0代表最低速,7代表最高速。"
    ::= {fanInfoEntry 4}



-- fanInfoTable  end




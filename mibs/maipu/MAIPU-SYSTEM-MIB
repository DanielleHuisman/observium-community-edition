-- memoryTable  内存表

memoryTable    OBJECT-TYPE
    SYNTAX  SEQUENCE OF memoryEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "内存表。"
    ::= {mpSysMemoryMib 1}

memoryEntry  OBJECT-TYPE
    SYNTAX  memoryEntry
    ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "内存表条目。"
    INDEX   {memIndex}
    ::= {memoryTable 1}

memIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "内存索引。"
    ::= {memoryEntry 1}


memBytesFree  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "空闲内存字节数。"
    ::= {memoryEntry 2}
    
memBlocksFree  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "空闲内存块数。"
    ::= {memoryEntry 3}

memAvgBlockSizeFree  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "空闲的内存块平均大小。"
    ::= {memoryEntry 4}
        
memMaxBlockSizeFree  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "最大空闲内存块。"
    ::= {memoryEntry 5}

memBytesAlloc  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "已分配内存字节数。"
    ::= {memoryEntry 6}

memBlocksAlloc  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "已分配内存块数。"
    ::= {memoryEntry 7}

memAvgBlockSizeAlloc  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "已分配的平均内存块大小。"
    ::= {memoryEntry 8}

memTotalBytes  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "内存总数。"
    ::= {memoryEntry 9}

memUtilization  OBJECT-TYPE
    SYNTAX  INTEGER(0..100)
    UNITS   "%"
    ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "内存使用率。(单位%)"
    ::= {memoryEntry 10}
    
-- memoryTable  end



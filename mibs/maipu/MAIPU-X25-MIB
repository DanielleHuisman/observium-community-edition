MAIPU-X25-MIB DEFINITIONS ::= BEGIN

IMPORTS
    RowStatus, DisplayString
                                                 	FROM SNMPv2-TC
    MODULE-IDENTITY, OBJECT-TYPE, Counter32, IpAddress
						FROM SNMPv2-SMI
    mpMgmt                              	FROM MAIPU-SMI;

mpX25Mib MODULE-IDENTITY
    LAST-UPDATED "0102260000Z"
    ORGANIZATION "Maipu DataComm"
    CONTACT-INFO
	"   Maipu DataComm
		Customer Service

		Postal: Consulate Drive
			Chengdu, Sichuan  610041
			China

		Tel: 86-028-5161006

		E-mail: office@maipu.com"
    DESCRIPTION
		"The assignments for Maipu router's MIB of X.25 infomation!"
    ::= {mpMgmt 7 }

    x25IfMib     OBJECT IDENTIFIER ::= { mpX25Mib 1 }
    x25MapMib    OBJECT IDENTIFIER ::= { mpX25Mib 2 }
    x25VcMib      OBJECT IDENTIFIER ::= { mpX25Mib 3 }
    x25RouteMib          OBJECT IDENTIFIER ::= { mpX25Mib 4 }
    x25AgentMib          OBJECT IDENTIFIER ::= { mpX25Mib 5 }

--x25IfMib	OBJECT-IDENTITY
--	STATUS	current
--	DESCRIPTION
--		"X.25协议第三层."
--	::= { mpX25Mib 1 }
--
--x25MapMib	OBJECT-IDENTITY
--	STATUS	current
--	DESCRIPTION
--		"X.25协议地址映射表."
--	::= { mpX25Mib 2 }
--
--x25VcMib	OBJECT-IDENTITY
--	STATUS	current
--	DESCRIPTION
--		"X.25协议虚电路."
--	::= { mpX25Mib 3 }
	
--x25RouteMib	OBJECT-IDENTITY
--	STATUS	current
--	DESCRIPTION
--		"X.25路由表."
--	::= { mpX25Mib 4 }

--x25AgentMib	OBJECT-IDENTITY
--	STATUS	current
--	DESCRIPTION
--		"X.25代理，该模块实现TCP之上的应用层数据与X.25分组层数据之间的相互转换。"
--	::= { mpX25Mib 5 }

--//******** if configuration *******//
x25ConfTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF X25ConfEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"X.25协议在接口上的配置表。"
	::= { x25IfMib 1 }
	
x25ConfEntry 	OBJECT-TYPE
	SYNTAX	X25ConfEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"X.25协议在接口上的配置表条目。"
	INDEX { x25ConfIfIndex }
	::= { x25ConfTable 1 }	
	
X25ConfEntry ::=
	SEQUENCE {
		x25ConfIfIndex
			INTEGER,
		x25ConfMode
			INTEGER,
		x25ConfStat
			INTEGER,
		x25ConfAddress
			OCTET STRING,
		x25ConfHtc
			INTEGER,
		x25ConfLtc
			INTEGER,
		x25ConfHoldQueue
			INTEGER,
		x25ConfIdle
			INTEGER,
		x25ConfIps
			INTEGER,
		x25ConfOps
			INTEGER,
		x25ConfModulo
			INTEGER,
		x25ConfNvc
			INTEGER,
		x25ConfWin
			INTEGER,
		x25ConfWout
			INTEGER,
		x25ConfT20
			INTEGER,
		x25ConfT21
			INTEGER,
		x25ConfT22
			INTEGER,
		x25ConfT23
			INTEGER,
		x25ConfHic
			INTEGER,
		x25ConfHoc
			INTEGER
				}
		
x25ConfIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"接口上的X.25协议配置表索引，与ifIndex相同。"
	::= { x25ConfEntry 1 }
	
x25ConfMode	OBJECT-TYPE
	SYNTAX	INTEGER {
				dte(1),
				dce(2)
					}
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25接口模式。"
	DEFVAL { dte }
	::= { x25ConfEntry 2 }

x25ConfState	OBJECT-TYPE
	SYNTAX	INTEGER {
				r0(0),
				r1(1),
				r23(2)
					}
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"X.25分组层状态：r0表示分组层处于初始化状态；r1表示分组层处于就绪状态；r23表示分组层处于重新启动状态。"
	DEFVAL { dte }
	::= { x25ConfEntry 3 }
	
x25ConfAddress	OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1..14))
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议地址，采用X.121格式，即最多15位的数字字符串。"
	::= { x25ConfEntry 4 }
	
x25ConfHtc	OBJECT-TYPE
	SYNTAX	INTEGER (1..1024)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议最高双向虚电路。"
	::= { x25ConfEntry 5 }
	
x25ConfLtc	OBJECT-TYPE
	SYNTAX	INTEGER (1..1024)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议最低双向虚电路。"
	::= { x25ConfEntry 6 }
	
x25ConfHoldQueue	OBJECT-TYPE
	SYNTAX	INTEGER (0..9999)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议虚电路分组保持队列长度，即当一条虚电路不能发送数据时能保持的最大分组个数。"
	::= { x25ConfEntry 7 }
	
x25ConfIdle	OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议封装虚电路空闲时间，使虚电路一段时间不工作后，软件能清除一个SVC。"
	::= { x25ConfEntry 8 }
	
x25ConfIps	OBJECT-TYPE
	SYNTAX	INTEGER (16..4096)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议最大输入分组大小。"
	DEFVAL { 128 }
	::= { x25ConfEntry 9 }
	
x25ConfOps	OBJECT-TYPE
	SYNTAX	INTEGER (16..4096)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议最大输出分组大小。"
	::= { x25ConfEntry 10 }
	
x25ConfModulo	OBJECT-TYPE
	SYNTAX	INTEGER {
				modulo8(1),
				modulo128(2)
					}
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议工作模式。"
	DEFVAL { modulo8 }
	::= { x25ConfEntry 11 }
	
x25ConfNvc	OBJECT-TYPE
	SYNTAX	INTEGER (1..8)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议上能建立的虚电路的最大数。"
	::= { x25ConfEntry 12 }
	
x25ConfWin	OBJECT-TYPE
	SYNTAX	INTEGER (1..127)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议输入窗口大小。"
	DEFVAL { 2 }
	::= { x25ConfEntry 13 }
	
x25ConfWout	OBJECT-TYPE
	SYNTAX	INTEGER (1..127)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议输出窗口大小。"
	DEFVAL { 2 }
	::= { x25ConfEntry 14 }

x25ConfT20	OBJECT-TYPE
	SYNTAX	INTEGER (1..1000)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议DTE/DCE重新启动定时器时延值。"
	DEFVAL { 10 }
	::= { x25ConfEntry 15 }
	
x25ConfT21	OBJECT-TYPE
	SYNTAX	INTEGER (1..1000)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议DTE/DCE呼叫规程定时器时延值。"
	DEFVAL { 10 }
	::= { x25ConfEntry 16 }
	
x25ConfT22	OBJECT-TYPE
	SYNTAX	INTEGER (1..1000)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议DTE/DCE复原规程定时器时延值。"
	DEFVAL { 10 }
	::= { x25ConfEntry 17 }
	
x25ConfT23	OBJECT-TYPE
	SYNTAX	INTEGER (1..1000)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议DTE/DCE清除规程定时器时延值。"
	DEFVAL { 10 }
	::= { x25ConfEntry 18 }
	
x25ConfHic	OBJECT-TYPE
	SYNTAX	INTEGER (1..1024)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议最高呼入虚电路。"
	DEFVAL { 10 }
	::= { x25ConfEntry 19 }
	
x25ConfHoc	OBJECT-TYPE
	SYNTAX	INTEGER (1..1024)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议最高呼出虚电路。"
	DEFVAL { 10 }
	::= { x25ConfEntry 20 }
	
lapbConfTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LapbConfEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"LAPB协议配置表。"
	::= { x25IfMib 3 }
	
lapbConfEntry	OBJECT-TYPE
	SYNTAX	LapbConfEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"LAPB配置表条目。"
	INDEX { lapbIfIndex }
	::= { lapbConfTable 1 }
	
LapbConfEntry ::=
	SEQUENCE {
		lapbIfIndex
			INTEGER,
		lapbMode
			INTEGER,
		lapbState
			INTEGER,
		lapbModulo
			INTEGER,
		lapbN1
			INTEGER,
		lapbN2
			INTEGER,
		lapbT1
			INTEGER,
		lapbT2
			INTEGER,
		lapbT4
			INTEGER,
		lapbK
			INTEGER
				}
				
lapbIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"LAPB配置表索引，与ifIndex相同。"
	::= { lapbConfEntry 1 }
	
lapbMode	OBJECT-TYPE
	SYNTAX	INTEGER {
				dte(1),
				dce(2)
					}
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB的工作模式。"
	DEFVAL { dte }
	::= { lapbConfEntry 2 }

lapbState	OBJECT-TYPE
	SYNTAX	INTEGER {
				init(0),
				dm_sent(1),
				sabm_sent(2),
				abm(3),
				wait_sabm(4),
				wait_ua(5),
				disc_sent(6),
				disconnected(7),
				init_start(8)
					}
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"LAPB的状态。"
	DEFVAL { dte }
	::= { lapbConfEntry 3 }
	
lapbModulo	OBJECT-TYPE
	SYNTAX	INTEGER {
				modulo8(1),
				modulo128(2)
					}
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB协议的工作模式。"
	DEFVAL { modulo8 }
	::= { lapbConfEntry 4 }
	
lapbN1	OBJECT-TYPE
	SYNTAX	INTEGER (137..1513)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB的接收I帧的最大比特数。"
	::= { lapbConfEntry 5 }
	
lapbN2	OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB发送帧的最大尝试次数。"
	DEFVAL { 10 }
	::= { lapbConfEntry 6 }
	
lapbT1	OBJECT-TYPE
	SYNTAX	INTEGER (1..64000)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB的重发计时器。"
	DEFVAL { 3 }
	::= { lapbConfEntry 7 }
	
lapbT2	OBJECT-TYPE
	SYNTAX	INTEGER (1..32000)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB的接收计时器，到时就发送证实帧。"
	DEFVAL { 1 }
	::= { lapbConfEntry 8 }

lapbT4	OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB的保活计时器，到时就发送保活帧。"
	DEFVAL { 15 }
	::= { lapbConfEntry 9 }
	
lapbK	OBJECT-TYPE
	SYNTAX	INTEGER (1..127)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"LAPB的窗口大小。如果lapbModulo的值是modulo8则设置范围是1..7，如果是modulo128则设置范围是1..127。"
	DEFVAL { 7 }
	::= { lapbConfEntry 10 }
	
--//*********** map **********//
x25MapConfTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF X25MapConfEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射配置表。"
	::= { x25MapMib 1 }
	
x25MapConfEntry	OBJECT-TYPE
	SYNTAX	X25MapConfEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射配置表条目。"
	INDEX { x25MapConfIfIndex, x25MapConfMode, x25MapConfAddr }
	::= { x25MapConfTable 1 }
	
X25MapConfEntry ::=
	SEQUENCE {
		x25MapConfIfIndex
			INTEGER,
		x25MapConfMode
			INTEGER,
		x25MapConfAddr
			OCTET STRING,
		x25MapConfX121Addr
			OCTET STRING,
		x25MapConfLimite
			BITS,
		x25MapConfBridgeIfIndex
			INTEGER,
		x25MapConfBridgePort
			INTEGER,
		x25MapConfBridgeType
			INTEGER,
		x25MapConfType
			INTEGER,
		x25MapConfStatus
			RowStatus
				}
				
x25MapConfIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射的接口索引，与ifIndex相同。"
	::= { x25MapConfEntry 1 }
	
x25MapConfMode	OBJECT-TYPE
	SYNTAX	INTEGER {
				ip(1),
				compressedIp(2),
				qllc(3),
				bridge(4)
					}
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射的协议类型。"
	DEFVAL { ip }
	::= { x25MapConfEntry 2 }
	
x25MapConfAddr	OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1..64))
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.121地址映射的上层协议地址。"
	::= { x25MapConfEntry 3 }
	
x25MapConfX121Addr	OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1..14))
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射的X.121地址。"
	::= { x25MapConfEntry 4 }
	
x25MapConfLimite	OBJECT-TYPE
	SYNTAX	INTEGER
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射的限制，按位配置。"
	DEFVAL { {none} }
	::= { x25MapConfEntry 5 }
	
x25MapConfBridgeIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射桥接的接口索引，与ifIndex相同。"
	::= { x25MapConfEntry 6 }	
	
x25MapConfBridgePort	OBJECT-TYPE
	SYNTAX	INTEGER (0..65535)
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射桥接的端口号。"
	::= { x25MapConfEntry 7 }	
	
x25MapConfBridgeType	OBJECT-TYPE
	SYNTAX	INTEGER {
				tp-tcp(1),
				none(2)
					}
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射桥接的标志。"
	::= { x25MapConfEntry 8 }	
	
x25MapConfType	OBJECT-TYPE
	SYNTAX	INTEGER {
				static(1),
				pvc(2)
					}
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射的类型，一种是手工配置的，另一种是配置PVC时生成的。"
	::= { x25MapConfEntry 9 }
	
x25MapConfStatus	OBJECT-TYPE
	SYNTAX	RowStatus
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"X.25协议地址映射表的行状态，用于增加、删除或修改一行的内容。"
	::= { x25MapConfEntry 10 }
	

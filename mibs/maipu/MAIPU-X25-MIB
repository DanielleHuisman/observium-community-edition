	SYNTAX	INTEGER {
				close(1),
				open(2)
					}
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"X.25协议交换开关。"
	DEFVAL { open }
	::= { x25RouteMib 1 }
	
x25RouteConfTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF X25RouteConfEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"X.25协议路由配置表。"
	::= { x25RouteMib 2 }
	
x25RouteConfEntry	OBJECT-TYPE

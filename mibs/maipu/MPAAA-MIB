aaaAuthorListOption1	OBJECT-TYPE
                    		SYNTAX  INTEGER {
                    			         nomethod(0),
                    			          none(1),
                    			          local(2),
                    			          radius(3),
                    			          tacacs(5),
                    			          grpSrv(7),
                    			          ifAuthenticated(16)
                    			        }
                    		MAX-ACCESS  read-write
                    		STATUS  current
                    		DESCRIPTION
                    			"AAA授权列表选项：
                    			 和认证选项类似，如配置数组 3 4 16 0 0 0 则表示授权顺序为radius tacacs if-authenticated
                    			 1--none
                    			 2--Local
                    			 3--Radius
                    			 5--Tacacs
                    			 7--SrvGrp(服务器组)
                    			 16--if-authenticated(如果通过认证就通过授权)"
                    		::= { aaaAuthorListEntry 4 }


	::= { ddrDialerMapEntry 2 }
	
ddrDialerMapProtocol	OBJECT-TYPE
	SYNTAX	INTEGER {
				ip(1)
					}
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION
		"DDR拨号映射的协议方式."
	::= { ddrDialerMapEntry 3 }
					
ddrDialerMapIpAddr	OBJECT-TYPE
	SYNTAX	IpAddress
	MAX-ACCESS	read-create
	STATUS	current
	DESCRIPTION

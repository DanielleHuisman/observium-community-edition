dlswLocal	OBJECT-IDENTITY
	STATUS	current
	DESCRIPTION
		""
	::= { mpDLSwMib 3 }

localIpAddr	OBJECT-TYPE
	SYNTAX	OCTET STRING
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"localIpAddr"
	::= { dlswLocal 1 }


    staticUnicastPriority OBJECT-TYPE
        SYNTAX INTEGER {low(0),normal(1),medium(2),high(3)}
        MAX-ACCESS read-create              
        STATUS current
        DESCRIPTION
            "The priority of the static unicast address."
        ::= { staticUnicastEntry 3 }        

    staticUnicastStatus OBJECT-TYPE
        SYNTAX INTEGER {
            active(1),

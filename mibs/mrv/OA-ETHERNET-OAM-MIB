      nbEthOamDmLastRttSum2               Unsigned32,

      nbEthOamDmLastDSJittNumber          Unsigned32,
      nbEthOamDmLastDSJittMin             Unsigned32,
      nbEthOamDmLastDSJittMax             Unsigned32,
      nbEthOamDmLastDSJittAverage         Unsigned32,
      nbEthOamDmLastDSJittSum2            Unsigned32,
      nbEthOamDmLastDSJittIA              Unsigned32,

      nbEthOamDmLastSDJittNumber          Unsigned32,
      nbEthOamDmLastSDJittMin             Unsigned32,
      nbEthOamDmLastSDJittMax             Unsigned32,
      nbEthOamDmLastSDJittAverage         Unsigned32,
      nbEthOamDmLastSDJittSum2            Unsigned32,
      nbEthOamDmLastSDJittIA              Unsigned32,
      
      nbEthOamDmLastPcktLoss              Unsigned32,
      nbEthOamDmLastType                  TestRunMethod,
      nbEthOamDmLastPriority              Unsigned32,
      nbEthOamDmLastHistIndx              Unsigned32
    }



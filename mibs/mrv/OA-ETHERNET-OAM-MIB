    DESCRIPTION
       "Ethernet OAM Delay Measurement last completed test results Table."
    REFERENCE
       "."
    ::= { nbEthOamDm 3 }

nbEthOamDmLastEntry OBJECT-TYPE
    SYNTAX      NbEthOamDmLastEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "Contains results of Ethernet OAM Delay Measurement test."
    INDEX { nbEthOamMdIndex, nbEthOamMaIndex, nbEthOamMepIdentifier }
    ::= { nbEthOamDmLastTable 1 }

NbEthOamDmLastEntry ::= SEQUENCE {
      nbEthOamDmLastStarted               DateAndTime,
      nbEthOamDmLastDestMepId             NbEthOamMepIdOrZero,
      nbEthOamDmLastDestMepMac            MacAddress,
      nbEthOamDmLastMsgTransmitted        Unsigned32,
      nbEthOamDmLastMsgReceived           Unsigned32,

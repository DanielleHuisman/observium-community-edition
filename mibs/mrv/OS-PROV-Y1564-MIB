    -- AUGMENTS { osProvY1564RsltEntry }
    INDEX { osEthServId, osEthServY1564FlowId }
    ::= { osProvY1564HcRsltTable 1 }

OsProvY1564HcRsltEntry ::= SEQUENCE {
      osProvY1564HcRsltTransmitted           Counter64,
      osProvY1564HcRsltReceived              Counter64
}



    IMPORTS
        BridgeId, Timeout
            FROM BRIDGE-MIB
        InterfaceIndex
            FROM IF-MIB
        Counter32, enterprises, Integer32, IpAddress, NOTIFICATION-TYPE,
        OBJECT-TYPE, Opaque
            FROM SNMPv2-SMI
        DateAndTime, DisplayString, MacAddress, RowStatus, TEXTUAL-CONVENTION
            FROM SNMPv2-TC;


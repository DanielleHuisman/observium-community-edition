                         ipeStsBridgeFdbBridgeIndex,
                         ipeStsBridgeFdbIfIndex,
                         ipeStsBridgeFdbIndex
                     }
        ::= { ipeStsBridgeFdbTable 1 }

    IpeStsBridgeFdbEntry ::= SEQUENCE {

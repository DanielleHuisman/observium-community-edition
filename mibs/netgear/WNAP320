                VapStatusEntry ::=
                        SEQUENCE {
				vapStatIndex
					INTEGER,
                                txByteCount
                                        Counter32,
                                txFrameCount
		                        Counter32,
		                rxByteCount
                		        Counter32,
		                rxFrameCount
                		        Counter32,
            		        txPayloadBytes
                			Counter32,
                                rxPayloadBytes
			                Counter32,
 				numberOfClients
                                        INTEGER,
				clientsAuthRequestTotal 
					INTEGER,
				clientsAuthResponseTotal 
					Unsigned32,
				clientsSuccessConnectTotal 
					Unsigned32,
				clientsAbnormalDisconnectTotal
					Unsigned32,
				clientsNormalDisconnectTotal 
					Unsigned32
			}


                        SYNTAX SEQUENCE OF RadioStatusEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "List of virtual access point in a tabular form."
                        ::= { statistic 4 }
                        
                radioStatusEntry OBJECT-TYPE
                        SYNTAX RadioStatusEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "An entry in the radioStatusTable."
                        INDEX { wlanIndex }
                        ::= { radioStatusTable 1 }


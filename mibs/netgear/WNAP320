				wlanRxBytesTotal
					Counter64,
				wlanTxPacketsTotal
					Unsigned32,
				wlanTxBytesTotal
					Counter64,
				wlanDiscardPacketsTotal
					Unsigned32,
				wlanTxPacketFailTotal
					Unsigned32,
    				wlanTxRetryTotal 
					Unsigned32,
				wlanFcsFailCntTotal
					Unsigned32,
				wlanRtsFailCntTotal
					Unsigned32,
				wlanAckFailCntTotal
					Unsigned32,

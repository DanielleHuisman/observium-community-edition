    DEFVAL { on }
    ::= { ntcDevLogLocal 1 }
    
ntcDevLogRemEnable               OBJECT-TYPE
    SYNTAX      NtcEnable
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION

    ::= { ntcEtherIfRedMgmt 3 }
    
ntcEtherIfRedDataSwitchOrder     OBJECT-TYPE
    SYNTAX      INTEGER { none(0), data1or2(1), data1before2(2), data2before1(3), data1(4), data2(5) }
    MAX-ACCESS  read-write

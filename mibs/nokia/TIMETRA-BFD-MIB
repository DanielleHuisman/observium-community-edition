        tmnxBfdOnLspExtSessLclAddr,
        tmnxBfdOnLspExtSessPathId,
        tmnxBfdOnLspExtSessTunnelId,
        tmnxBfdOnLspExtSessInfoId
    }
    ::= { tmnxBfdOnLspExtSessTable 1 }

TmnxBfdOnLspExtSessEntry         ::= SEQUENCE
{

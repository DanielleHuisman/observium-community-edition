    tmnxIsisAdjSetActiveMembers      Unsigned32,
    tmnxIsisAdjSetUpTime             Integer32,
    tmnxIsisAdjSetStatus             INTEGER,
    tmnxIsisAdjSetMtu                Unsigned32
}


    eMplsTEPLblMldSnpgGrpAddress     InetAddress,
    eMplsTEPLblMldSnpgGrpType        TmnxIgmpSnpgGroupType,
    eMplsTEPLblMldSnpgGrpFilterMode  TmnxIgmpGroupFilterMode,
    eMplsTEPLblMldSnpgGrpUpTime      TimeTicks,
    eMplsTEPLblMldSnpgGrpExpiryTime  Unsigned32,
    eMplsTEPLblMldSnpgGrpCompatMode  Unsigned32,
    eMplsTEPLblMldSnpgGrpV1ExpTime   Unsigned32
}


    eMplsTEPLblMldRxSmetRoutes       Unsigned32,
    eMplsTEPLblMldDroppedSmetRoutes  Unsigned32,
    eMplsTEPLblMldOrigAddrType       InetAddressType,
    eMplsTEPLblMldOrigAddress        InetAddress,
    eMplsTEPLblMldEvpnProxySupport   TruthValue
}


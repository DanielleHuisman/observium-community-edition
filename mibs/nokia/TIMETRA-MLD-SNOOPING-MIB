{
    eVxlanMldSnpgGrpAddressType      InetAddressType,
    eVxlanMldSnpgGrpAddress          InetAddress,
    eVxlanMldSnpgGrpType             TmnxIgmpSnpgGroupType,
    eVxlanMldSnpgGrpFilterMode       TmnxMldGroupFilterMode,
    eVxlanMldSnpgGrpUpTime           TimeTicks,
    eVxlanMldSnpgGrpExpiryTime       Unsigned32,
    eVxlanMldSnpgGrpCompatMode       Unsigned32,
    eVxlanMldSnpgGrpV1HostExpTime    Unsigned32
}


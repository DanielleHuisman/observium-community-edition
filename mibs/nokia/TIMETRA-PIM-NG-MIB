    ::= { vRtrPimNgIfObjs 24 }

vRtrPimNgTSidIPmsiEntry          OBJECT-TYPE
    SYNTAX      VRtrPimNgTSidIPmsiEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the vRtrPimNgTSidIPmsiTable. This is a read-only table."
    INDEX       {
        vRtrID,
        vRtrIfIndex
    }
    ::= { vRtrPimNgTSidIPmsiTable 1 }

VRtrPimNgTSidIPmsiEntry          ::= SEQUENCE
{
    vRtrPimNgTSidIPmsiRootAddrType   InetAddressType,
    vRtrPimNgTSidIPmsiRootAddr       InetAddress,
    vRtrPimNgTSidIPmsiTreeIdx        Unsigned32,
    vRtrPimNgTSidIPmsiLspName        TNamedItem
}

vRtrPimNgTSidIPmsiRootAddrType   OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of vRtrPimNgTSidIPmsiRootAddrType indicates the address type
         of vRtrPimNgTSidIPmsiRootAddr."
    ::= { vRtrPimNgTSidIPmsiEntry 1 }

vRtrPimNgTSidIPmsiRootAddr       OBJECT-TYPE
    SYNTAX      InetAddress (SIZE (0|4|16))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of vRtrPimNgTSidIPmsiRootAddr indicates the root address for
         the IPMSI."
    ::= { vRtrPimNgTSidIPmsiEntry 2 }

vRtrPimNgTSidIPmsiTreeIdx        OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of vRtrPimNgTSidIPmsiTreeIdx indicates the lsp identifier
         associated with the IPMSI."
    ::= { vRtrPimNgTSidIPmsiEntry 3 }

vRtrPimNgTSidIPmsiLspName        OBJECT-TYPE
    SYNTAX      TNamedItem
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of vRtrPimNgTSidIPmsiLspName indicates the lsp name
         associated with the IPMSI.

         In case of terminating IPMSIs the value of vRtrPimNgTSidIPmsiLspName
         indicates the name of the tunnel interface."
    ::= { vRtrPimNgTSidIPmsiEntry 4 }

vRtrPimNgNotificationObjs        OBJECT IDENTIFIER ::= { tmnxPimNgObjs 3 }


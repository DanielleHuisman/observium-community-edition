        vRtrPimTSidPmsiCGrpSrcState,
        vRtrPimTSidPmsiCGrpSrcJoinTimer,
        vRtrPimTSidPmsiCGrpSrcHldDnTimer,
        vRtrPimTSidPmsiCGrpSrcExpTimer,
        vRtrPimTSidPmsiCGrpSrcUptime,
        vRtrPimTSidDataMtCGrpSrcDataTh,
        vRtrPimTSidPmsiCGrpSrcIfIndex,
        vRtrPimTSidPmsiCGrpSrcRxThEnbl,
        vRtrPimTSidPmsiCGrpSrcRxCount,
        vRtrPimNgTSidIPmsiRootAddrType,
        vRtrPimNgTSidIPmsiRootAddr,
        vRtrPimNgTSidIPmsiTreeIdx,
        vRtrPimNgTSidIPmsiLspName
    }
    STATUS      current
    DESCRIPTION
        "The group of objects supporting manangement of extended PIM general
         capabilities for TREE SID objects on Nokia SROS series 19.0 systems."
    ::= { tmnxPimNgGroups 66 }


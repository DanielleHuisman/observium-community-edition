    SYNTAX       INTEGER {
                     use-dot1p  (1),
                     use-dscp   (2),
                     all        (3)
                 }
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This object specifies system remark type, tNetworkPolicyEgressRemark
        will be set to true and tNetworkEgressRemarkType will specify the
        remark type."
    ::= { tNetworkPolicyExtnEntry 5 }
    


        tmnxVwmShelfAdminState          TmnxAdminState,
        tmnxVwmShelfOperState           TmnxAdminState,
        tmnxVwmShelfVwmType             INTEGER,
        tmnxVwmShelfEquippedType        INTEGER,
        tmnxVwmshelfTypeConnected       INTEGER
    }
    

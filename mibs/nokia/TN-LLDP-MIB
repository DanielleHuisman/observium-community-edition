tnLldpNotifications             OBJECT IDENTIFIER ::= { tnSRNotifyPrefix 59 }
tnLldpObjects                   OBJECT IDENTIFIER ::= { tnSRObjs         59 }
-- tnLldpConformance               OBJECT IDENTIFIER ::= { tnSRConfs        59 }

tnLldpConfiguration             OBJECT IDENTIFIER ::= { tnLldpObjects     1 }
tnLldpStatistics                OBJECT IDENTIFIER ::= { tnLldpObjects     2 }

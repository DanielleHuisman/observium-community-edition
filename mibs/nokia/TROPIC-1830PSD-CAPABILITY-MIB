    ::= { tnPsdAgentCapability 1 }

tn1830CapabilityR110 AGENT-CAPABILITIES
    PRODUCT-RELEASE "Release 1.1.0."
    STATUS          current
    DESCRIPTION     "Agent Capabilities for the 1830 PSD product."

--
-- Capabilities for SNMPv2-MIB
--

    SUPPORTS            SNMPv2-MIB
        INCLUDES        { systemGroup,
                          snmpBasicNotificationsGroup,
                          snmpWarmStartNotificationGroup }

--
-- Capabilities for IF-MIB
--

    SUPPORTS            IF-MIB
        INCLUDES        { ifGeneralInformationGroup,
                          ifVHCPacketGroup }

        VARIATION       ifIndex
            DESCRIPTION "In the value of this object, the shelf, slot and port
                         values are encoded as follows:
                         shelf           = (value & 0x7F000000) >> 24
                         slot            = (value & 0x00FF0000) >> 16
                         port            = (value & 0x0000FF00) >>  8
                         is logical port = (value & 0x00008000) >> 15
                         The logical port becomes functionally a LAG aggregator
                         in case the ifType equals ieee8023adLag(161) and as
                         soon as LAG is enabled for this port.
                         See description tnLagPortSubgroup."

        VARIATION       ifDescr
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       ifType
            SYNTAX      INTEGER { other(1),
                                  ethernetCsmacd(6),
                                  softwareLoopback(24),
                                  propVirtual(53),
                                  ieee8023adLag(161),
                                  opticalChannel(195) }
            DESCRIPTION "Limited set of values."

        VARIATION       ifAdminStatus
            DESCRIPTION "The only permitted value for interfaces of ifType
                         propVirtual(53) and ieee8023adLag(161) is up(1).
                         For the OTN NID system modes:
                         - value testing(3) is only supported for CLIENT1.
                         - CLIENT1, NETWORK1 and NETWORK2 can be set
                           to up(1) or down(2), the only permitted value for
                           CLIENT2 is down(2).
                         For the ETH NID system mode:
                         - value testing(3) is only supported for CLIENT1 and
                           CLIENT2.
                         - CLIENT1, CLIENT2, NETWORK1 and NETWORK2 can be set
                           to up(1) or down(2)."

        VARIATION       ifOperStatus
            SYNTAX      INTEGER { up(1), down(2), unknown(4) }
            DESCRIPTION "Limited set of values.

                         For the OTN NID system modes:
                         Interfaces of ifType propVirtual(53):
                         - ifOperStatus.CLIENT  = ifOperStatus.CLIENT1

                         For the ETH NID system mode:
                         Interfaces of ifType ieee8023adLag(161) in case
                         LAG is functionally disabled (see tnLagPortSubgroup):
                         - ifOperStatus.CLIENT  = ifOperStatus.CLIENT1
                         - ifOperStatus.NETWORK = ifOperStatus.NETWORK1
                         For interfaces of ifType ieee8023adLag(161) in case
                         LAG is functionally enabled (see tnLagPortSubgroup):
                         - down(2) in case lagLos(742) defect is active and
                           up(1) otherwise."

        VARIATION       ifLinkUpDownTrapEnable
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       ifConnectorPresent
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       ifAlias
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       ifHCInUcastPkts
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       ifHCInMulticastPkts
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       ifHCInBroadcastPkts
            ACCESS      not-implemented
            DESCRIPTION "This object is not supported."

        VARIATION       ifHCOutUcastPkts
            ACCESS      not-implemented

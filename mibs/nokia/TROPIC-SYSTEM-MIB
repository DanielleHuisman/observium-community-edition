                         level1Write(2),  -- nms
                         level2Read(3),   -- admin
                         level2Write(4),  -- admin
                         level3Read(5),   -- provisioner
                         level3Write(6),  -- provisioner
                         level4Read(7),   -- service
                         level4Write(8),  -- service
                         level5Read(9),   -- observer
                         level5Write(10), -- observer
                         level6Read(11),  -- crypto
                         level6Write(12)  -- crypto
                       }
        MAX-ACCESS     read-write
        STATUS         current

		rc2kDeviceGlobalUsbBytesFree 	Counter64,
		rc2kDeviceGlobalUsbNumFiles 	Integer32,
		rc2kDeviceGlobalExtflashBytesUsed 	Counter64,
		rc2kDeviceGlobalExtflashBytesFree 	Counter64,

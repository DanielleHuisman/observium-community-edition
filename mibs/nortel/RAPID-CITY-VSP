                      phbcs1(8),
		          val9(9),
                      phbaf11(10),
		          val11(11),
                      phbaf12(12),
		          val13(13),
                      phbaf13(14),
		          val15(15),
                      phbcs2(16),
		          val17(17),
                      phbaf21(18),

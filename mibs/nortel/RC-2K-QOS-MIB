
RC-2K-QOS-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Counter64
        FROM SNMPv2-SMI
    DisplayString, TruthValue, RowStatus
        FROM SNMPv2-TC
    InterfaceIndex 
        FROM IF-MIB
    rc2kQos
        FROM RC-2K-MIB
    EnableValue, PortSet
        FROM RAPID-CITY;


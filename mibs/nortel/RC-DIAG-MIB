
RC-DIAG-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, IpAddress, Counter32
        FROM SNMPv2-SMI
    DisplayString, TruthValue, MacAddress, RowStatus
        FROM SNMPv2-TC
    InterfaceIndex
        FROM IF-MIB
    rcDiag, rcMirror, EnableValue, PortSet
        FROM RAPID-CITY
    IsisPlsbNodeNickName
        FROM RC-ISIS-MIB;


rcDiagMib MODULE-IDENTITY

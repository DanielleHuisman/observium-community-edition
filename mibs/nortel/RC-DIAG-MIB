    SYNTAX       DisplayString
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  "User defined data to match with the packets received"
    DEFVAL       { "\0"  }
    ::= { rcDiagPcapAdvancedFilterEntry 8 }


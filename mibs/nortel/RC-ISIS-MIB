        SYNTAX        INTEGER  {
                        l2isid(1)
                        }
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "L2 isid redistribute type.
                       Configurable values:
                        l2isid(1) - L2 isid"
        ::= { rcIsisMultiAreaL2RedistributeIsidEntry 1 }


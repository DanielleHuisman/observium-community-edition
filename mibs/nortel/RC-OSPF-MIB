
RC-OSPF-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Counter32, IpAddress, TimeTicks
        FROM SNMPv2-SMI
    DisplayString, RowStatus, TruthValue
        FROM SNMPv2-TC
    InterfaceIndexOrZero
        FROM IF-MIB
    rcOspf, rcStat, EnableValue
        FROM RAPID-CITY
    rcIpConf
        FROM RC-IP-MIB;


rcOspfMib MODULE-IDENTITY
       LAST-UPDATED     "202101130000Z"       
       ORGANIZATION     "Bay Networks, Rapid-City Group"
       CONTACT-INFO     "Yu-Ten Lee
                         Postal:        Nortel Networks
                                        4655 Great America Parkway
                                        Santa Clara, CA 95052-8185

                         Tel:           408-495-2560
                         Fax:           408-495-6690
                         E-mail:        ylee@nortel.com"
       DESCRIPTION      "Enterprise MIB for the Accelar product family."

       REVISION "202101130000Z"  -- 13 Jan 2021
       DESCRIPTION "Version 26:  Added rcStatOspfIfClearStats"


       REVISION "201703130000Z"  -- 13 March 2017
       DESCRIPTION "Version 25:  Added rcIpConfOspfDefaultMetric2500MegPort"

       REVISION "201701200000Z"  -- 20 Jan 2017
       DESCRIPTION "Version 24:  Removed rcIpConfOspfIfTable because it had a mib object
	   			 (rcIpConfOspfIfType) that conflicts with the same named

-- Isid Endpoint Cvid Client Mep

rcIsidEndptCvidClientMepTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcIsidEndptCvidClientMepEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A table used to maintain CFM Client MEP 
                       assigned to the Isid Endpoint with Cvid"
        ::= { rc8021ah 5 }

rcIsidEndptCvidClientMepEntry OBJECT-TYPE
        SYNTAX        RcIsidEndptCvidClientMepEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "An entry in the rcIsidEndptCvidClientMep table."
        INDEX         { rcIsidEndptCvidClientMepIsidId, rcIsidEndptCvidClientMepTlsIndex, rcIsidEndptCvidClientMepCvid}
        ::= { rcIsidEndptCvidClientMepTable 1 }

RcIsidEndptCvidClientMepEntry ::= SEQUENCE {
        rcIsidEndptCvidClientMepIsidId    Integer32,
        rcIsidEndptCvidClientMepTlsIndex  Integer32,
        rcIsidEndptCvidClientMepCvid      Integer32,
        rcIsidEndptCvidClientMepMepList   OCTET STRING,
	rcIsidEndptCvidClientMepNumOfMep  Integer32,
	rcIsidEndptCvidClientMepRowStatus RowStatus,
	rcIsidEndptCvidClientMepPort      InterfaceIndex,
	rcIsidEndptCvidClientDownMepMepList     OCTET STRING,
	rcIsidEndptCvidClientDownMepNumOfMep    Integer32
        }

rcIsidEndptCvidClientMepIsidId OBJECT-TYPE
        SYNTAX        Integer32 (1..16777215) 
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Service Instance ID of the I component"
        ::= { rcIsidEndptCvidClientMepEntry 1 }

rcIsidEndptCvidClientMepTlsIndex OBJECT-TYPE
        SYNTAX        Integer32 (1..2147483647)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Tls Index of an Isid Endpoint."
        ::= { rcIsidEndptCvidClientMepEntry 2 }

rcIsidEndptCvidClientMepCvid OBJECT-TYPE
        SYNTAX        Integer32 (1..4096) 
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "C-Vlan Id associate with the Isid Endpoint"
        ::= { rcIsidEndptCvidClientMepEntry 3 }

rcIsidEndptCvidClientMepMepList OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(0..408))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "List of Client Meps assigned to this entry. 
		       A client mep is consist of <md>.<ma>.<mep_id>. Domain
		       Name, Association and Mep Id is separated by '.' 
		       A list of mep is consist of 
		       <md1>.<ma1>.<mepid1>,<md2>.<ma2>.<mepid2>,... 
		       Each mep is separated by ','. Maximum up 8 meps"
        ::= { rcIsidEndptCvidClientMepEntry 4 }

rcIsidEndptCvidClientMepNumOfMep OBJECT-TYPE
        SYNTAX        	Integer32
        MAX-ACCESS      read-only
        STATUS        	current
        DESCRIPTION   	"Used to indicate the number of Client Meps
                      	is assigned to this entry"
        ::= { rcIsidEndptCvidClientMepEntry 5 }

rcIsidEndptCvidClientMepRowStatus OBJECT-TYPE
        SYNTAX          RowStatus
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Row Create attibute. For perfomace and efficience, 
                         rcIsidEndptCvidClientMepMepList is required for creation.
			 If any Mip is added, entry would be created.
			 If all Mip are removed, entry would be deleted"
        ::= { rcIsidEndptCvidClientMepEntry 6 }

rcIsidEndptCvidClientMepPort  OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies a port."
        DEFVAL        { 1 }
        ::= { rcIsidEndptCvidClientMepEntry 7 }

rcIsidEndptCvidClientDownMepMepList OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(0..408))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "List of Client Down Meps assigned to this entry. 
		       A client mep is consist of <md>.<ma>.<mep_id>. Domain
		       Name, Association and Mep Id is separated by '.' 
		       A list of mep is consist of 
		       <md1>.<ma1>.<mepid1>,<md2>.<ma2>.<mepid2>,... 
		       Each mep is separated by ','. Maximum up 8 meps"
        ::= { rcIsidEndptCvidClientMepEntry 8 }

rcIsidEndptCvidClientDownMepNumOfMep OBJECT-TYPE
        SYNTAX        	Integer32
        MAX-ACCESS      read-only
        STATUS        	current
        DESCRIPTION   	"Used to indicate the number of Client Down Meps
                      	is assigned to this entry"
        ::= { rcIsidEndptCvidClientMepEntry 9 }


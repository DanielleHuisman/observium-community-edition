-- Pwe3 Endpoint Cvid Client Mep

rcPwEndptCvidClientMepTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcPwEndptCvidClientMepEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A table used to maintain CFM Client MEP 
                       assigned to the Isid Endpoint with Cvid"
        ::= { rcPwe3 3 }

rcPwEndptCvidClientMepEntry OBJECT-TYPE
        SYNTAX        RcPwEndptCvidClientMepEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "An entry in the rcIsidEndptCvidClientMep table."
        INDEX         { rcPwEndptCvidClientMepPwIndex, rcPwEndptCvidClientMepTlsIndex, rcPwEndptCvidClientMepCvid}
        ::= { rcPwEndptCvidClientMepTable 1 }

RcPwEndptCvidClientMepEntry ::= SEQUENCE {
        rcPwEndptCvidClientMepPwIndex   INTEGER,
        rcPwEndptCvidClientMepTlsIndex  INTEGER,
        rcPwEndptCvidClientMepCvid      INTEGER,
        rcPwEndptCvidClientMepMepList   OCTET STRING,
	rcPwEndptCvidClientMepNumOfMep  Integer32
        }

rcPwEndptCvidClientMepPwIndex OBJECT-TYPE
        SYNTAX        INTEGER (1..16777215) 
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Service Instance ID of the I component"
        ::= { rcPwEndptCvidClientMepEntry 1 }

rcPwEndptCvidClientMepTlsIndex OBJECT-TYPE
        SYNTAX        INTEGER (1..2147483647)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Tls Index of an PW Endpoint."
        ::= { rcPwEndptCvidClientMepEntry 2 }

rcPwEndptCvidClientMepCvid OBJECT-TYPE
        SYNTAX        INTEGER (1..4094) 
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "C-Vlan Id associate with the PW Endpoint"
        ::= { rcPwEndptCvidClientMepEntry 3 }

rcPwEndptCvidClientMepMepList OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(0..408))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "List of Client Meps assigned to this entry. 
		       A client mep is consist of <md>.<ma>.<mep_id>. Domain
		       Name, Association and Mep Id is separated by '.' 
		       A list of mep is consist of 
		       <md1>.<ma1>.<mepid1>,<md2>.<ma2>.<mepid2>,... 
		       Each mep is separated by ','. Maximum up 8 meps"
        ::= { rcPwEndptCvidClientMepEntry 4 }

rcPwEndptCvidClientMepNumOfMep OBJECT-TYPE
        SYNTAX        	Integer32
        MAX-ACCESS      read-only
        STATUS        	current
        DESCRIPTION   	"Used to indicate the number of Client Meps
                      	is assigned to this entry"
        ::= { rcPwEndptCvidClientMepEntry 5 }

END

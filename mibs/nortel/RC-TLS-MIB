	rcTlsUniIpServiceStatsStndTxEirOopFrames		Counter64,
	rcTlsUniIpServiceStatsStndTxEirDroppedFrames		Counter64,
	rcTlsUniIpServiceStatsStndTxPirFrames			Counter64,
	rcTlsUniIpServiceStatsStndTxPirOopFrames		Counter64,
	rcTlsUniIpServiceStatsStndTxGreenFrames		Counter64,
	rcTlsUniIpServiceStatsStndTxYellowFrames		Counter64,
	rcTlsUniIpServiceStatsStndTxRedFrames			Counter64,
	rcTlsUniIpServiceStatsMcastTxDroppedFrames		Counter64,
	rcTlsUniIpServiceStatsBcastUUcastTxTotalFrames		Counter64,
	rcTlsUniIpServiceStatsBcastUUcastTxDroppedFrames	Counter64,
	rcTlsUniIpServiceStatsDropClassRxDroppedOctets  	Counter64,
	rcTlsUniIpServiceStatsDropClassRxDroppedFrames  	Counter64,
	rcTlsUniIpServiceStatsDropClassTxDroppedOctets  		Counter64,
	rcTlsUniIpServiceStatsDropClassTxDroppedFrames  	Counter64
      }


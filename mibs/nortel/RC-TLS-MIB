-- rcBanRingStatsTable 

rcBanRingStatsTable OBJECT-TYPE

        SYNTAX        SEQUENCE OF RcBanRingStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Ban Ring stats table"
        ::= { rcBanRing 5 }
 
rcBanRingStatsEntry OBJECT-TYPE
        SYNTAX        RcBanRingStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "BAN ring table entry. Contain RRP and Rdp Message
                      sending/receiving and missing counter"
        INDEX         { rcBanRingStatsId }
        ::= { rcBanRingStatsTable 1 }
 
RcBanRingStatsEntry ::= 
        SEQUENCE {
                rcBanRingStatsId                            INTEGER,
                rcBanRingStatsRingOperState                 INTEGER,
                rcBanRingStatsRingName                      DisplayString,
                rcBanRingStatsPrimaryPortTxRrpHello         Counter32,
                rcBanRingStatsPrimaryPortRxRrpHello         Counter32,
                rcBanRingStatsSecondaryPortTxRrpHello       Counter32,  
                rcBanRingStatsSecondaryPortRxRrpHello       Counter32,
                rcBanRingStatsPrimaryPortMissingRrpHello    Counter32,
                rcBanRingStatsSecondaryPortMissingRrpHello  Counter32,
                rcBanRingStatsPrimaryPortTxRdp              Counter32,     
                rcBanRingStatsPrimaryPortRxRdp              Counter32,
                rcBanRingStatsSecondaryPortTxRdp            Counter32,     
                rcBanRingStatsSecondaryPortRxRdp            Counter32
       }

rcBanRingStatsId      OBJECT-TYPE
        SYNTAX        INTEGER (1..254)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "BAN Ring ID."
        ::= { rcBanRingStatsEntry 1 }

rcBanRingStatsRingOperState    OBJECT-TYPE
        SYNTAX        INTEGER { 
			  down(0),
                          normal(1), 
                          cut(2), 
                          recover(3) 
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "BAN Ring operation status."
        ::= { rcBanRingStatsEntry 2 }

rcBanRingStatsRingName   OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..32)) 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Name fo the Ban ring"
        ::= { rcBanRingStatsEntry 3 }


rcBanRingStatsPrimaryPortTxRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are 
                       transmitted by primary port"
        ::= { rcBanRingStatsEntry 4 }
                                            
rcBanRingStatsPrimaryPortRxRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are 
                       received on primary port. Every RRP hello message
                       is transmitted by Primary port, is expected to be 
                       responsed by a Connected Port (ESU Port). The
                       respose is a receiving count"
        ::= { rcBanRingStatsEntry 5 }
                          
rcBanRingStatsSecondaryPortTxRrpHello   OBJECT-TYPE 
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are 
                       transmitted by Seondary port"
        ::= { rcBanRingStatsEntry 6 }            
                                            
rcBanRingStatsSecondaryPortRxRrpHello   OBJECT-TYPE    
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are 
                       received on Secondary port. Every RRP hello message
                       is transmitted by Secondary port, is expected to be 
                       responsed by a Connected Port (ESU Port). The
                       respose is a receiving count"
        ::= { rcBanRingStatsEntry 7 }

rcBanRingStatsPrimaryPortMissingRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages responses
                       are missing on Primary port. Each RRP hello
                       message is transmitted by Primary port, 
                       is expected to be responded by a Connected Port
                       (ESU Port). Otherwise, it is a missing count"
        ::= { rcBanRingStatsEntry 8 }                                            
rcBanRingStatsSecondaryPortMissingRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages responses
                       are missing on Secondary port. Each RRP hello
                       message is transmitted by Secondary port, 
                       is expected to be respondsed by a Connected Port
                       (ESU Port). Otherwise, it is a missing count"
        ::= { rcBanRingStatsEntry 9 }
                     
rcBanRingStatsPrimaryPortTxRdp   OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RDP messages are transmitted 
                       by Primary Port." 
        ::= { rcBanRingStatsEntry 10 }

rcBanRingStatsPrimaryPortRxRdp     OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RDP messages are received 
                       on Primary Port. No RDP Message received on
                       Primary Port if rcBanRingStatsRingOperState is 
                       'normal'."
        ::= { rcBanRingStatsEntry 11 }

rcBanRingStatsSecondaryPortTxRdp   OBJECT-TYPE        
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RDP messages are transmitted 
                       by Secondary Port when rcBanRingStatsRingOperState
                       is in 'cut'. No RDP Tx when it is 'normal'" 
        ::= { rcBanRingStatsEntry 12 }

rcBanRingStatsSecondaryPortRxRdp  OBJECT-TYPE  
       SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RDP messages received 
                       on Secondary Port." 
        ::= { rcBanRingStatsEntry 13 }

-- rcBanDhRingStatsTable 
rcBanDhRingStatsTable OBJECT-TYPE

        SYNTAX        SEQUENCE OF RcBanDhRingStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Ban Ring stats table"
        ::= { rcBanRing 6  }

rcBanDhRingStatsEntry OBJECT-TYPE
        SYNTAX        RcBanDhRingStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "BAN ring table entry. Contain RRP and Rdp Message
                      sending/receiving and missing counter"
        INDEX         { rcBanDhRingStatsId }
        ::= { rcBanDhRingStatsTable 1 }

RcBanDhRingStatsEntry ::=
        SEQUENCE {
                rcBanDhRingStatsId                         INTEGER,
                rcBanDhRingStatsRingOperState              INTEGER,
                rcBanDhRingStatsRingName                   DisplayString,
                rcBanDhRingStatsRingPortTxRrpHello         Counter32,
                rcBanDhRingStatsRingPortRxRrpHello         Counter32,
                rcBanDhRingStatsIbtPortTxRrpHello          Counter32,
                rcBanDhRingStatsIbtPortRxRrpHello          Counter32,
                rcBanDhRingStatsRingPortMissingRrpHello    Counter32,
                rcBanDhRingStatsIbtPortMissingRrpHello     Counter32,
                rcBanDhRingStatsRingPortTxRdp              Counter32,
                rcBanDhRingStatsRingPortRxRdp              Counter32,
                rcBanDhRingStatsIbtPortRxRdp               Counter32

       }

rcBanDhRingStatsId      OBJECT-TYPE
        SYNTAX        INTEGER (1..254)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "BAN Ring ID."
        ::= { rcBanDhRingStatsEntry 1 }

rcBanDhRingStatsRingOperState    OBJECT-TYPE
        SYNTAX        INTEGER {
                          down(0),
                          normal(1),
                          cut(2),
                          recover(3)
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "BAN Ring operation status."
        ::= { rcBanDhRingStatsEntry 2 }

rcBanDhRingStatsRingName   OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..32))
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Name fo the Ban ring"
        ::= { rcBanDhRingStatsEntry 3 }


rcBanDhRingStatsRingPortTxRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are
                       transmitted by primary port"
        ::= { rcBanDhRingStatsEntry 4 }

rcBanDhRingStatsRingPortRxRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are
                       received on primary port. Every RRP hello message
                       is transmitted by Ring port, is expected to be
                       responsed by a Connected Port (ESU Port). The
                       respose is a receiving count"
        ::= { rcBanDhRingStatsEntry 5 }

rcBanDhRingStatsIbtPortTxRrpHello   OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are
                       transmitted by Seondary port"
        ::= { rcBanDhRingStatsEntry 6 }

rcBanDhRingStatsIbtPortRxRrpHello   OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages are
                       received on Ibt port. Every RRP hello message
                       is transmitted by Ibt port, is expected to be
                       responsed by a Connected Port (ESU Port). The
                       respose is a receiving count"
        ::= { rcBanDhRingStatsEntry 7 }

rcBanDhRingStatsRingPortMissingRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages responses
                       are missing on Ring port. Each RRP hello
                       message is transmitted by Ring port,
                       is expected to be responded by a Connected Port
                       (ESU Port). Otherwise, it is a missing count"
        ::= { rcBanDhRingStatsEntry 8 }

rcBanDhRingStatsIbtPortMissingRrpHello  OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RRP Hello messages responses
                       are missing on Ibt port. Each RRP hello
                       message is transmitted by Ibt port,
                       is expected to be respondsed by a Connected Port
                       (ESU Port). Otherwise, it is a missing count"
        ::= { rcBanDhRingStatsEntry 9 }

rcBanDhRingStatsRingPortTxRdp   OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RDP messages are transmitted
                       by Ring Port."
        ::= { rcBanDhRingStatsEntry 10 }

rcBanDhRingStatsRingPortRxRdp     OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RDP messages are received
                       on Ring Port."
        ::= { rcBanDhRingStatsEntry 11 }

rcBanDhRingStatsIbtPortRxRdp  OBJECT-TYPE
       SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total Number of RDP messages received
                       on Ibt Port."
        ::= { rcBanDhRingStatsEntry 12 }

rcBanRingTrapObjects OBJECT IDENTIFIER ::= { rcBanRing 7 }

rcBanRingTrapIndication OBJECT-TYPE
        SYNTAX        INTEGER  {
                                 normal(1),
                                 cut(2),
                                 down(3),
                                 adminDisable(4)
                                }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The type of trap send is represented"
        ::= { rcBanRingTrapObjects 1 }

rcBanRingTrapReason     OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE (256))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The Reason for a Ring Cut situation."
        ::= { rcBanRingTrapObjects 2 }

-- Standalone RBS Stats Table 

rcBanRingSaRbsStatsTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcBanRingSaRbsStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Standalone stats table"
        ::= { rcBanRing 8 }
 
rcBanRingSaRbsStatsEntry OBJECT-TYPE
        SYNTAX        RcBanRingSaRbsStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Standalone RBS statistics entry. Which contains
                       operational state and Rdp rx and tx counter"
        INDEX         { rcBanRingSaRbsStatsBanPort }
        ::= { rcBanRingSaRbsStatsTable 1 }
 
RcBanRingSaRbsStatsEntry ::=
        SEQUENCE {
                rcBanRingSaRbsStatsBanPort            InterfaceIndex,
                rcBanRingSaRbsStatsOperState          INTEGER,
                rcBanRingSaRbsStatsTxRdp              Counter32,     
                rcBanRingSaRbsStatsRxRdp              Counter32
          }

rcBanRingSaRbsStatsBanPort OBJECT-TYPE
        SYNTAX        InterfaceIndex(64..687)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Standalone RBS BAN port number."
        ::= { rcBanRingSaRbsStatsEntry 1 }
 
rcBanRingSaRbsStatsOperState OBJECT-TYPE
        SYNTAX        INTEGER {
			  down(1), 
                          normal(2), 
                          cut(3), 
                          recover(4) 
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Standalone RBS operation status."
        ::= { rcBanRingSaRbsStatsEntry 2 }

rcBanRingSaRbsStatsTxRdp   OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total number of RDP messages transmitted 
                       by standalone RBS BAN port." 
        ::= { rcBanRingSaRbsStatsEntry 3 }

rcBanRingSaRbsStatsRxRdp     OBJECT-TYPE
        SYNTAX         Counter32
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION    "The total number of RDP messages received 
                       on standalone RBS BAN port."
        ::= { rcBanRingSaRbsStatsEntry 4 }

-- Standalone ESU Topology Table
 
rcEsuStandaloneTopoTable OBJECT-TYPE

        SYNTAX        SEQUENCE OF RcEsuStandaloneTopoEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Table contains entrires of topology information
	              of ESU Standalone"
        ::= { rcBanRing 9 }
 
rcEsuStandaloneTopoEntry OBJECT-TYPE
        SYNTAX        RcEsuStandaloneTopoEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry contains ESU Standalone real time topology information. 
		       If Standalone interface is down or Standalone is disabled, 
		       Topology entry will not exist."
        INDEX         { rcEsuStandaloneTopoPort}
        ::= { rcEsuStandaloneTopoTable 1 }
 
RcEsuStandaloneTopoEntry ::=
        SEQUENCE {
                rcEsuStandaloneTopoPort               InterfaceIndex,
                rcEsuStandaloneTopoEsuId              Integer32,
                rcEsuStandaloneTopoEsuMac             MacAddress,
                rcEsuStandaloneTopoEsuIp              IpAddress,
                rcEsuStandaloneTopoEsuPortMask        Integer32,
                rcEsuStandaloneTopoEsuPortState       Integer32
       }

rcEsuStandaloneTopoPort OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "ESU Stanalone port interface, which is configured on the 
		       MERS8600"
        ::= { rcEsuStandaloneTopoEntry 1 }
 
rcEsuStandaloneTopoEsuId OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current

        DESCRIPTION   "Module ID of ESU device which is being connected to the 
		       Standalone interface."
        ::= { rcEsuStandaloneTopoEntry 2 }
 
rcEsuStandaloneTopoEsuMac OBJECT-TYPE
        SYNTAX        MacAddress
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Mgmt Mac address of ESU device which is being connected 
                       to the Standalone interface"
        ::= { rcEsuStandaloneTopoEntry 3 }
 
rcEsuStandaloneTopoEsuIp OBJECT-TYPE
        SYNTAX        IpAddress
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "IP address of ESU device which is being connected 
                       to the Standalone interface"
        ::= { rcEsuStandaloneTopoEntry 4 }

rcEsuStandaloneTopoEsuPortMask OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "ESU device's ports configured mask. Bit Set representation,
		       From less significant bit to the most significant bit.
		       Less significant bit 0 represents to the first port, Less 
		       significant bit 1 represents second port on module. 
		       So on so for ... 
		       Bit clear (0) means port is not configured, 
		       Bit Set(1) means port is configured "
        ::= { rcEsuStandaloneTopoEntry 5 }
 
rcEsuStandaloneTopoEsuPortState OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "ESU device's port state mask. Bit Set representation,
		       From less significant bit to the most significant bit.
		       Less significant bit 0 represents to the first port, Less 
		       significant bit 1 represents second port switch. 
		       So on so for ... 
		       Bit clear (0) means state down, Bit Set(1) means state up"
        ::= { rcEsuStandaloneTopoEntry 6 }

-- Ring module table

rcRingModuleTable OBJECT-TYPE

        SYNTAX        SEQUENCE OF RcRingModuleEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Ring module table"
        ::= { rcRing 1 }

rcRingModuleEntry     OBJECT-TYPE
        SYNTAX        RcRingModuleEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Ring module entry."
        INDEX         { rcRingModuleRingId, rcRingModuleModId }
        ::= { rcRingModuleTable 1 }
 
RcRingModuleEntry ::=
        SEQUENCE {
                rcRingModuleRingId                Integer32,
                rcRingModuleModId                 Integer32,
                rcRingModuleOperStatus            INTEGER,
                rcRingModuleAdminState            INTEGER,
		rcRingModuleRingName              DisplayString,
		rcRingModuleUniEndptAction        INTEGER
       }

rcRingModuleRingId    OBJECT-TYPE
        SYNTAX        Integer32 (0..254)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "BAN Ring ID."
        ::= { rcRingModuleEntry 1 }
 
rcRingModuleModId     OBJECT-TYPE
        SYNTAX        Integer32 (1..30)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "BAN Ring RBS ID."
        ::= { rcRingModuleEntry 2 }
 
rcRingModuleOperStatus  OBJECT-TYPE
        SYNTAX          INTEGER { 
                          up(1), 
                          disabled(2),
                          notpresent(3)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "BAN Ring RBS operation status."
        ::= { rcRingModuleEntry 3 }

rcRingModuleAdminState OBJECT-TYPE
        SYNTAX         INTEGER { 
			  enabled (1), 
			  disabled(0)
                       }
        MAX-ACCESS     read-write
        STATUS         current
        DESCRIPTION    "BAN Ring RBS administration status."
        ::= { rcRingModuleEntry 4 }

rcRingModuleRingName  OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (1..32)) 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Ring Name"
        ::= { rcRingModuleEntry 5 }

rcRingModuleUniEndptAction  OBJECT-TYPE
        SYNTAX              INTEGER {
			      disable (0),
		              enable  (1),
			      delete  (2)
			    }
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "Delete, disable or enable all of the UNIs and end-points on
                             the ESU"
        ::= { rcRingModuleEntry 6 }       


-- Bidirectional Ring based on R/RS module
rcRingTable OBJECT-TYPE

        SYNTAX        SEQUENCE OF RcRingEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Bidirectional ring table"
        ::= { rcRing 2 }

rcRingEntry OBJECT-TYPE
        SYNTAX        RcRingEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Bidirectional ring table entry."
        INDEX         { rcRingId }
        ::= { rcRingTable 1 }

RcRingEntry ::=
        SEQUENCE {
                rcRingId                    Integer32,
                rcRingPortType              INTEGER,
                rcRingPort                  InterfaceIndex,
                rcRingPartnerPortMac        MacAddress,
                rcRingRctType               INTEGER,
                rcRingRctPort               InterfaceIndex,
                rcRingRctVid                Integer32,
                rcRingRctMlt                Integer32,
                rcRingRctPbtTg              DisplayString,
                rcRingAdminState            EnableValue,
                rcRingRctHello              EnableValue,
                rcRingRctHelloTimer         Integer32,
                rcRingOperState             INTEGER,
                rcRingName                  DisplayString,
                rcRingDiscoverInterval      Integer32,
                rcRingRowStatus             RowStatus,
                rcRingNumVlanIds            Integer32,
                rcRingVlanIds               IdList,
                rcRingRrpHello              EnableValue, 
                rcRingRrpHelloInterval      Integer32,
                rcRingClearRrpStats         TruthValue,
                rcRingClearRdpStats         TruthValue,
                rcRingPrimaryEsuModIds      IdList,
                rcRingSecondaryEsuModIds    IdList,
                rcRingType                  INTEGER,
                rcRingRctOperState          INTEGER,
                rcRingCopyBtagPbits         EnableValue
       }

rcRingId OBJECT-TYPE
        SYNTAX        Integer32 (1..254)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Bidirectional Ring ID."
        ::= { rcRingEntry 1 }

rcRingPortType  OBJECT-TYPE
        SYNTAX          INTEGER {
            primary(1),
            secondary(2)
            }
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Primary/Secondary Ring Port"
        DEFVAL          { primary }
        ::= { rcRingEntry 2 }

rcRingPort OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Home Ring port number."
        ::= { rcRingEntry 3 }

rcRingPartnerPortMac OBJECT-TYPE
        SYNTAX        MacAddress
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Home Ring Partner's Ring port MAC address."
        ::= { rcRingEntry 4 }

rcRingRctType  OBJECT-TYPE
        SYNTAX          INTEGER {
            none(1),
            vlan(2),
            pbt(3),
            pbb(4)
            }
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Bidirectional Ring Ring Closing Trunk Type"
        DEFVAL          { none }
        ::= { rcRingEntry 5 }

rcRingRctPort OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Single link port number used for RCT."
        ::= { rcRingEntry 6 }

rcRingRctVid OBJECT-TYPE
        SYNTAX        Integer32 (0..4094)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Bidirectional Ring RCT Vlan Id."
        ::= { rcRingEntry 7 }

rcRingRctMlt OBJECT-TYPE
        SYNTAX        Integer32 (0..256)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Multi-link Trunk MLT Interface ID used for RCT."
        ::= { rcRingEntry 8 }

rcRingRctPbtTg OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..32))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "PBT Trunk group name that is used for RCT."
        ::= { rcRingEntry 9 }

rcRingAdminState OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Administratively enable or disable Bidirectional Ring."
        DEFVAL        { disable }
        ::= { rcRingEntry 10 }

rcRingRctHello OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "RCT hello enable or disable."
        DEFVAL        { disable }
        ::= { rcRingEntry 11 }

rcRingRctHelloTimer OBJECT-TYPE
        SYNTAX        Integer32 (1..255)
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Bidirectional Ring RCT Hello timer value in Seconds."
        DEFVAL          { 10 }
        ::= { rcRingEntry 12 }

rcRingOperState OBJECT-TYPE
        SYNTAX        INTEGER {
                          down(1),
                          normal(2),
                          cut(3),
                          rct-cut(4),
                          partner-ring-port-down(5),
                          partner-down(6),
                          recover(7)
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Bidirectional Ring operation status."
        DEFVAL        { down }
        ::= { rcRingEntry 13 }

rcRingName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..32))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Modification of this field is allowed only
                       when the admin state is disabled."
        ::= { rcRingEntry 14 }

rcRingDiscoverInterval OBJECT-TYPE
        SYNTAX          Integer32 (1..255)
        UNITS           "seconds"
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "The time interval between discovery messages that
                         are generated to check for ring module existence."
        DEFVAL          { 30 }
        ::= { rcRingEntry 15 }

rcRingRowStatus OBJECT-TYPE
        SYNTAX        RowStatus
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Uses for controlling the creation and
                       deletion of this row."
        ::= { rcRingEntry 16 }

rcRingNumVlanIds OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Used to indicate the number of VLAN IDs that
                        are stored in the rcBanRingVlanIds variable."
        ::= { rcRingEntry 17 }

rcRingVlanIds OBJECT-TYPE
        SYNTAX          IdList (SIZE (4096))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "An array used to identify which VLANs this
                         Ring is associated with.  Each VLAN ID is 
                         stored as a two octet value. The first octet
                         in the pair holds bits 15-8 of the VLAN ID,
                         while the second octet holds bits 7-0 of the
                         VLAN ID."
        ::= { rcRingEntry 18 }

rcRingRrpHello OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "RRP hello enable or disable."
        DEFVAL        { disable }
        ::= { rcRingEntry 19 }

rcRingRrpHelloInterval OBJECT-TYPE
        SYNTAX          Integer32 (1000..10000)
        UNITS           "milliseconds"
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "The time interval between hello messages that
                         are generated to the ESU."
        DEFVAL          { 1000 }
        ::= { rcRingEntry 20 }

rcRingClearRrpStats     OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "To clear the RRP sending/receiving and missing
                         hello message counter"
        DEFVAL          { false }
        ::= { rcRingEntry 21 }

rcRingClearRdpStats   OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "To clear the RDP sending/receiving message counter"
        DEFVAL          { false }
        ::= { rcRingEntry 22 }

rcRingPrimaryEsuModIds OBJECT-TYPE
        SYNTAX          IdList (SIZE (31))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Each octet represents the module id homed on primary "
        ::= { rcRingEntry 23 }

rcRingSecondaryEsuModIds OBJECT-TYPE
        SYNTAX          IdList (SIZE (31))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     " octet represents the module id homed on secondary "
        ::= { rcRingEntry 24 }

rcRingType  OBJECT-TYPE
        SYNTAX          INTEGER {
            single-home(1),
            dual-home(2)
            }
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Primary/Secondary Ring Port"
        DEFVAL          { single-home }
        ::= { rcRingEntry 25 }

rcRingRctOperState OBJECT-TYPE
        SYNTAX        INTEGER {
                          up(1),
                          down(2)
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Bidirectional Ring Ring Closing Trunk operation status."
        DEFVAL        { down }
        ::= { rcRingEntry 26 }

rcRingCopyBtagPbits OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Administratively enable or disable Copying of Btag Pbits on Ring."
        DEFVAL        { enable }
        ::= { rcRingEntry 27 }

rcRingStatsTable OBJECT-TYPE

        SYNTAX        SEQUENCE OF RcRingStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Bidirectional Ring stats table"
        ::= { rcRing 3 }

rcRingStatsEntry OBJECT-TYPE
        SYNTAX        RcRingStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Bidirectional Ring table entry. Contain RRP and Rdp Message
                      sending/receiving and missing counter"
        INDEX         { rcRingStatsId }
        ::= { rcRingStatsTable 1 }

RcRingStatsEntry ::=
        SEQUENCE {
                rcRingStatsId                         Integer32,
                rcRingStatsRingOperState              INTEGER,
                rcRingStatsRingName                   DisplayString,
                rcRingStatsRingPortTxRrpHello         Counter32, 
                rcRingStatsRingPortRxRrpHello         Counter32, 
                rcRingStatsRctTxHello                 Counter32,
                rcRingStatsRctRxHello                 Counter32,
                rcRingStatsRingPortMissingRrpHello    Counter32, 
                rcRingStatsRctMissingHello            Counter32,
                rcRingStatsRingPortTxRdp              Counter32,
                rcRingStatsRingPortRxRdp              Counter32,
                rcRingStatsRctRxRdp                   Counter32
       }

rcRingStatsId      OBJECT-TYPE
        SYNTAX        Integer32 (1..254)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Bidirectional Ring ID."
        ::= { rcRingStatsEntry 1 }

rcRingStatsRingOperState    OBJECT-TYPE
        SYNTAX        INTEGER {
                          down(1),
                          normal(2),
                          cut(3),
				  rct-cut(4),
		              partner-ring-port-down(5),
		              partner-down(6),
                          recover(7)
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Bidirectional Ring operation status."
        ::= { rcRingStatsEntry 2 }

rcRingStatsRingName   OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..32))
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Name of the Bidirectional ring"
        ::= { rcRingStatsEntry 3 }


rcRingStatsRingPortTxRrpHello  OBJECT-TYPE

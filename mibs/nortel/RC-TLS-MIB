        SEQUENCE {
                rcTlsTdiUserMacTdId       INTEGER,
                rcTlsTdiUserMacAddr       MacAddress,
                rcTlsTdiUserUniAddr       IpAddress,
                rcTlsTdiUserMacRowStatus  RowStatus
       }


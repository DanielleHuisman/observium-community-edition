       	 rcIsidEndptStatsCfmEgrRtLimitDroppedFrames             	Counter64,
	rcIsidEndptStatsDropClassRxDroppedOctets  		Counter64,
	rcIsidEndptStatsDropClassRxDroppedFrames  		Counter64,
	rcIsidEndptStatsDropClassTxDroppedOctets  		Counter64,
	rcIsidEndptStatsDropClassTxDroppedFrames  		Counter64
      }


-- Isid endpoint stats table

rcIsidEndptStatsTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcIsidEndptStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   ""
        ::= { rc8021ah 4 }
 
rcIsidEndptStatsEntry OBJECT-TYPE
        SYNTAX        RcIsidEndptStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A Isid End-Point Statistics  table entry."
        INDEX         { rcIsidEndptStatsIsidId, rcIsidEndptStatsTlsIndex }
        ::= { rcIsidEndptStatsTable 1 }
 
RcIsidEndptStatsEntry ::=
        SEQUENCE {
                rcIsidEndptStatsIsidId                       INTEGER,
                rcIsidEndptStatsTlsIndex                     INTEGER,
                rcIsidEndptStatsPort                         InterfaceIndex,
                rcIsidEndptStatsPremRxOctets                 Counter64, 
                rcIsidEndptStatsPremRxDroppedOctets          Counter64,
                rcIsidEndptStatsGoldRxOctets                 Counter64,       
                rcIsidEndptStatsGoldRxDroppedOctets          Counter64,
                rcIsidEndptStatsSilverRxOctets               Counter64,       
                rcIsidEndptStatsSilverRxDroppedOctets        Counter64,
                rcIsidEndptStatsStndRxOctets                 Counter64,      
                rcIsidEndptStatsStndRxDroppedOctets          Counter64,
                rcIsidEndptStatsPremTxOctets                 Counter64,      

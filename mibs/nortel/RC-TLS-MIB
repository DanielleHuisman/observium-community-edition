-- rcOePortTable

rcOePortTable OBJECT-TYPE
        SYNTAX	      SEQUENCE OF RcOePortEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Optical Ethernet Port table, contains OE supported 
		       port and OE Tls port information"
        ::= { rcTls 23 }

rcOePortEntry OBJECT-TYPE
        SYNTAX        RcOePortEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry containing port information for a particular 
		       OE supported port"
        INDEX         { rcOePortIndex }
        ::= { rcOePortTable 1 }

RcOePortEntry ::= SEQUENCE {
        rcOePortIndex                           InterfaceIndex,
	rcOePortType                            INTEGER,
        rcOePortServiceType                     INTEGER,
	rcOePortSwitchedUniCount  	        Counter32,
	rcOePortTransparentUniCount             Counter32,
	rcOePortQinQ1UniCount                   Counter32,
	rcOePortQinQ2UniCount                   Counter32,
	rcOePortQinQEtherType                   INTEGER,
	rcOePortMplsUniCount                    Counter32 
        }
	
rcOePortIndex OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies a OE 
	               supported port.
                       This value is similar to ifIndex in MIB2."
        ::= { rcOePortEntry 1 }

rcOePortType OBJECT-TYPE
	SYNTAX        INTEGER {
			 esm8x1000Base(1),
			 pr48x1000BaseTX(2),
                         pr30x1000BaseSXBB(3),
			 pr3x10GBaseGbic(4),
			 pr3x10GBaseXZW(5)
	              }
	MAX-ACCESS    read-only
	STATUS        current
	DESCRIPTION   "Port Hardware type, which include PowerRange and 
	               ESM port"
	::= { rcOePortEntry 2 }

rcOePortServiceType OBJECT-TYPE
	SYNTAX        INTEGER {
                         none(1),
			 server(2), 
		         ringPrimary(3),
			 ringSecondary(4),
			 standalone(5),
			 transparent(6),
			 multiService(7)
	              }
	MAX-ACCESS    read-only
	STATUS        current
	DESCRIPTION   "Port OE service type which is assigned"
	::= { rcOePortEntry 3 }

rcOePortSwitchedUniCount OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of Switched UNI this port belongs to"
        ::= { rcOePortEntry 4 }

rcOePortTransparentUniCount OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of Transparent UNI this port belongs to"
        ::= { rcOePortEntry 5 }

rcOePortQinQ1UniCount OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of QinQ1 UNI this port belongs to"
        ::= { rcOePortEntry 6 }

rcOePortQinQ2UniCount OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of QinQ2 UNI this port belongs to"
        ::= { rcOePortEntry 7 }

rcOePortQinQEtherType   OBJECT-TYPE
        SYNTAX        INTEGER (1..'FFFF'h) 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The QinQ ether type value. It is hex value. 
                       It should be displayed as Hex value. It is 
		       only be used when port is associated with 
		       QinQ service. Otherwise, it would be ignored"
        ::= { rcOePortEntry 8 }

rcOePortMplsUniCount OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of MPLS (vc-label)UNI  this port belongs to"
        ::= { rcOePortEntry 9 }     
-- OE MAC Count Table

rcOeRecordCountTable  OBJECT-TYPE
SYNTAX          SEQUENCE OF RcOeRecordCountEntry
MAX-ACCESS      not-accessible
STATUS          current
DESCRIPTION     "OE port Record Count (MAC/IP count) table."
        ::= { rcTls 24 }

rcOeRecordCountEntry   OBJECT-TYPE
SYNTAX          RcOeRecordCountEntry
MAX-ACCESS      not-accessible
STATUS          current
DESCRIPTION     "Entry containing information about MAC/IP entries learnt 
                 on each ESM port or R-Mod lane."
INDEX           {rcOeRecordCountSlot, rcOeRecordCountIndex}
::= { rcOeRecordCountTable  1 }

RcOeRecordCountEntry   ::= SEQUENCE {
                rcOeRecordCountSlot                 Integer32,
                rcOeRecordCountIndex               Integer32,
                rcOeRecordCountPort                DisplayString,
                rcOeRecordCountIngress             Integer32,
                rcOeRecordCountEgress              Integer32,
                rcOeRecordCountIp                  Integer32,
                rcOeRecordCountRecRemaining        Integer32,
                rcOeRecordCountRecTotal            Integer32
                }

rcOeRecordCountSlot      OBJECT-TYPE
        SYNTAX        Integer32 (1..10)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies a 
                       slot with an ESM/R-Mod card"
        ::= { rcOeRecordCountEntry   1}

rcOeRecordCountIndex     OBJECT-TYPE
        SYNTAX        Integer32 (0..7)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies an ESM
                       port or R-Mod lane. In the case of a slot which 
                       has ESM card, the index can go from 0..7. Index 0
                       corresponds to port 0, index 1 corresponds to port
                       1 and so on. For an R-Mod card, the index goes
                       from 0 to 2 or 0 to 1 depending on the card type.
                       Index 0 refers to left lane, Index 1 refers to
                       center lane and so on."
        ::= { rcOeRecordCountEntry   2}

rcOeRecordCountPort      OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (1..15))
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "ESM port or range of ports in an R-Mod lane"
        ::= { rcOeRecordCountEntry   3}

rcOeRecordCountIngress   OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of ingress MAC addresses learnt on an ESM
                       port or R-Mod lane."
        ::= { rcOeRecordCountEntry   4}

rcOeRecordCountEgress    OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of egress MAC addresses learnt on an ESM
                       port or R-Mod lane."
        ::= { rcOeRecordCountEntry   5}

rcOeRecordCountIp        OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of IP addresses learnt against Cust-IP-Vlans
                       on an ESM port or R-Mod lane."
        ::= { rcOeRecordCountEntry   6}

rcOeRecordCountRecRemaining    OBJECT-TYPE
        SYNTAX              Integer32
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION   "Number of MAC table records that are unused on an
                       ESM port or R-Mod lane. These records are used for
                       storing learnt MAC/IP addresses."
        ::= { rcOeRecordCountEntry   7}

rcOeRecordCountRecTotal  OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The total number of MAC table records that are available
                       on an ESM port or R-Mod lane. These records are used
                       for storing learnt MAC/IP addresses."
        ::= { rcOeRecordCountEntry   8}

-- rcTlsIpServiceRmodTable

rcTlsIpServiceRmodTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcTlsIpServiceRmodEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Table contains Rmod values for customer IP Vlan.Table entry
                       for cust-ip-vlan is created internally when the cust-ip-vlan
                       is configured on a port.The entry will be deleted only after
                       the last cust-ip-vlan end-point is deleted. "
        ::= { rcTls 25}

rcTlsIpServiceRmodEntry OBJECT-TYPE
        SYNTAX        RcTlsIpServiceRmodEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry contains Rmod values for customer IP Vlan."
        INDEX         { rcTlsIpServiceRmodCustIpVlan}
        ::= { rcTlsIpServiceRmodTable 1}

RcTlsIpServiceRmodEntry ::=
        SEQUENCE {
            rcTlsIpServiceRmodCustIpVlan                 INTEGER,
            rcTlsIpServiceRmodIpTblSize                  INTEGER,
            rcTlsIpServiceRmodIpTblWaterMk               INTEGER,
            rcTlsIpServiceRmodAgeTimer                   INTEGER
        }

rcTlsIpServiceRmodCustIpVlan OBJECT-TYPE
        SYNTAX        INTEGER (1..4096)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Customer IP Vlan for IP Mcast Vlan mapping"
        ::= { rcTlsIpServiceRmodEntry 1}


rcTlsIpServiceRmodIpTblSize OBJECT-TYPE
        SYNTAX        INTEGER (0..16384)
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "The size of the IP table. No table entries if
                       it's set to 0."
        DEFVAL       { 0 }
        ::= { rcTlsIpServiceRmodEntry 2 }

rcTlsIpServiceRmodIpTblWaterMk OBJECT-TYPE
        SYNTAX        INTEGER (0..90)
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "The Low Water Mark for Ip table size. Set to 90%
                       of the table size, can be over-written."
        DEFVAL       { 0 }
        ::= { rcTlsIpServiceRmodEntry 3}

rcTlsIpServiceRmodAgeTimer OBJECT-TYPE
        SYNTAX        INTEGER (1..1966020)
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION   "IP address ageout timer in seconds.
                       Valid only if rcTlsIpService is Enabled"
        DEFVAL        { 300 }
        ::= {  rcTlsIpServiceRmodEntry 4 }


-- rcTlsTdiBroadcastOptimizationStatsTable

rcTlsTdiBroadcastOptimizationStatsTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcTlsTdiBroadcastOptimizationStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Tls BroadcastOptimization statistics table."
        ::= { rcTls 26 }	
 
rcTlsTdiBroadcastOptimizationStatsEntry OBJECT-TYPE
        SYNTAX        RcTlsTdiBroadcastOptimizationStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A Tls Broadcast Optimization Statistics table entry, 
		      which is used for TDI Broadcast Optimization 
		      Statistics."
      	INDEX         { rcTlsTdiBroadcastOptimizationStatsTdiId, 
			rcTlsTdiBroadcastOptimizationStatsPortIndex}
        ::= { rcTlsTdiBroadcastOptimizationStatsTable 1 }
 

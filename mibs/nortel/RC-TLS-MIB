-- ESM Port Policer Stats

rcEsmPortPolicerStatsTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcEsmPortPolicerStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "ESM (Ethernet Service Module) port Policer Out of 
		       Profile dropped statistic Table. It is used for TLS 
		       feature. Only applicable to port interface which 
		       support TLS feature. The CoS Profile is defined by 
		       rcCosProfileTable."
        ::= { rcTls 19 }

rcEsmPortPolicerStatsEntry  OBJECT-TYPE
        SYNTAX        RcEsmPortPolicerStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry contains out-of-profile (dropped) octet counts"
        INDEX         { rcEsmPortPolicerStatsPort  }
        ::= { rcEsmPortPolicerStatsTable 1 }
 
RcEsmPortPolicerStatsEntry ::=
	SEQUENCE {	 
		rcEsmPortPolicerStatsPort               InterfaceIndex,
		rcEsmPortPolicerStatsTxPremDropOctets   Counter64,
		rcEsmPortPolicerStatsTxGoldDropOctets   Counter64,
		rcEsmPortPolicerStatsTxSilverDropOctets Counter64,	
      		rcEsmPortPolicerStatsTxStndDropOctets   Counter64
	}
rcEsmPortPolicerStatsPort OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Port interface Index"
        ::= { rcEsmPortPolicerStatsEntry 1}

rcEsmPortPolicerStatsTxPremDropOctets	OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Port Egress policer, count of premium octets dropped"
	::= { rcEsmPortPolicerStatsEntry 2}

rcEsmPortPolicerStatsTxGoldDropOctets	OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Port Egress policer, count of Gold octets dropped"
	::= { rcEsmPortPolicerStatsEntry 3}

rcEsmPortPolicerStatsTxSilverDropOctets	OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Port Egress policer, count of Silver octets dropped"
	::= { rcEsmPortPolicerStatsEntry 4}

rcEsmPortPolicerStatsTxStndDropOctets	OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Port Egress policer, count of Standard octets dropped"
	::= { rcEsmPortPolicerStatsEntry 5}


-- rcTlsIpServiceTable 

rcTlsIpServiceTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcTlsIpServiceEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Table contains a customer IP Vlan and the local IP 
		       Vlan for IP Mcast service"
        ::= { rcTls 20 }

rcTlsIpServiceEntry OBJECT-TYPE
        SYNTAX        RcTlsIpServiceEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry contains the customer IP Vlan and the Local 
		       Vlan for for the IP Mcast service"
        INDEX         { rcTlsIpServiceTlsIndex, rcTlsIpServiceCustIpVlan }
        ::= { rcTlsIpServiceTable 1 }

RcTlsIpServiceEntry ::=
        SEQUENCE {
                rcTlsIpServiceTlsIndex                   INTEGER,
                rcTlsIpServiceCustIpVlan                 INTEGER,
                rcTlsIpServicePort                       InterfaceIndex,
                rcTlsIpServiceClearStats                 TruthValue,
                rcTlsIpServiceServiceProviderIpVlan      INTEGER,
                rcTlsIpServiceRowStatus                  RowStatus,
		rcTlsIpServiceName                       DisplayString,
		rcTlsIpServiceAgeTimer                   INTEGER,
		rcTlsIpServiceIngressCosProfileName      DisplayString,
		rcTlsIpServiceIngressPolicer             EnableValue,
		rcTlsIpServiceEgressCosProfileName       DisplayString,
		rcTlsIpServiceEgressPolicer              EnableValue,
		rcTlsIpServiceAction                     INTEGER,
		rcTlsIpServiceAdminState                 EnableValue,
		rcTlsIpServiceOperState                  INTEGER,
	        rcTlsIpServicePriMapping                 OCTET STRING,
	        rcTlsIpServicePriOverride                TruthValue,
		rcTlsIpServiceIpTblSize                  INTEGER,	
		rcTlsIpServiceIpTblWaterMk               INTEGER,
		rcTlsIpServiceIpTblMltId                 INTEGER,
		rcTlsIpServicePriOverrideType            INTEGER,
                rcTlsIpServiceColorBasedRemarking        EnableValue,
                rcTlsIpServiceDropRedColorPackets        TruthValue,
		rcTlsIpServiceRingId	                 INTEGER,
		rcTlsIpServiceModId	        	 Integer32,
		rcTlsIpServiceEsuPort                    INTEGER
        }

rcTlsIpServiceTlsIndex OBJECT-TYPE
        SYNTAX        INTEGER (1..2147483647)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "TLS Index"
        ::= { rcTlsIpServiceEntry 1 }

rcTlsIpServiceCustIpVlan OBJECT-TYPE
        SYNTAX        INTEGER (1..4096)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Customer IP Vlan for IP Mcast Vlan mapping"
        ::= { rcTlsIpServiceEntry 2 }

rcTlsIpServicePort OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Customer IP Vlan for IP Mcast Vlan mapping"
        DEFVAL        { 1 }
        ::= { rcTlsIpServiceEntry 3 }

rcTlsIpServiceClearStats OBJECT-TYPE
        SYNTAX        TruthValue 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Clear Ip Service Statistics"
        ::= { rcTlsIpServiceEntry 4 }

rcTlsIpServiceServiceProviderIpVlan OBJECT-TYPE
        SYNTAX        INTEGER (1..4094)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Service Provider IP Vlan for UNI IP Mcast Vlan mapping"
        ::= { rcTlsIpServiceEntry 5 }

rcTlsIpServiceName  OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..30)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Name of IP Service Vlan Entry. By default, It is 
		       VLAN-<uni id>-<cust ip vlan>"
        ::= { rcTlsIpServiceEntry 6 } 

rcTlsIpServiceRowStatus  OBJECT-TYPE
        SYNTAX        RowStatus
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Used to create/delete entries"
        ::= { rcTlsIpServiceEntry 7 }

rcTlsIpServiceAgeTimer  OBJECT-TYPE
        SYNTAX        INTEGER (1..1966020)
        MAX-ACCESS    read-create
        STATUS        obsolete
        DESCRIPTION   "Mac address ageout timer in seconds.
		       Valid only if rcTlsIpService is Enabled"
	DEFVAL        { 300 }
        ::= { rcTlsIpServiceEntry 8 }

rcTlsIpServiceIngressCosProfileName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (1..20)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Ingress COS Profile Table Name for Customer Ip Service"
        DEFVAL       { "DEFAULT_MGT_PROFILE"  } 
        ::= { rcTlsIpServiceEntry 9 }

rcTlsIpServiceIngressPolicer OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Enable/disable to use Ingress Cos Profile table 
			(rcTlsIpServiceIngressCosProfileName) for this 
			customer IP service"
	DEFVAL        { enable }
        ::= { rcTlsIpServiceEntry 10 }

rcTlsIpServiceEgressCosProfileName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..20)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Egress COS Profile Table Name for this customer IP
			Service."
        DEFVAL       { "DEFAULT_MGT_PROFILE"  } 
        ::= { rcTlsIpServiceEntry 11 }

rcTlsIpServiceEgressPolicer OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Enable/disable to use Egress Cos Profile table 
			(rcTlsIpServiceEgressCosProfileName) for this 
			customer Ip Service"
	DEFVAL        { disable }
        ::= { rcTlsIpServiceEntry 12 }

rcTlsIpServiceAction  OBJECT-TYPE 
	SYNTAX        INTEGER {
                         none(1),       -- none of the following
                         flushIp(2)     -- flush Ip forwarding table
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Mcast related action- Flush learningMac addresse"
        DEFVAL        { none }
	::= { rcTlsIpServiceEntry 13 }

rcTlsIpServiceAdminState OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Value indicating whether the Ip Service is enabled"
        DEFVAL        { disable }
        ::= { rcTlsIpServiceEntry 14 }

rcTlsIpServiceOperState OBJECT-TYPE
        SYNTAX        INTEGER { 
                          up(1), 
                          down(2) 
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The current operational state of this Customer IP Vlan."
        ::= { rcTlsIpServiceEntry 15 }

rcTlsIpServicePriMapping OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(8))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Maps 802.1p priorities (0-7) to Optical Ethernet
                       Class of Service (COS) values (0-7) for packets.
                       The first bytes represent the COS for 802.1p
                       priority 0. Subsequent bytes represent the
                       mappings of the remaining 802.1p priorities."
        DEFVAL       { "00:00:02:04:00:06:06:07"  }
        ::= { rcTlsIpServiceEntry 16 }

rcTlsIpServicePriOverride  OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Enable or disable to override the Priority Mapping."
        DEFVAL          { false }
        ::= { rcTlsIpServiceEntry 17 }

rcTlsIpServiceIpTblSize OBJECT-TYPE
        SYNTAX        INTEGER (0..97000) 
        MAX-ACCESS    read-create
        STATUS        obsolete
        DESCRIPTION   "The maximum size of the IP table. No limit if
                       it's set to 0."
        DEFVAL       { 0 }
        ::= { rcTlsIpServiceEntry 18 }

rcTlsIpServiceIpTblWaterMk OBJECT-TYPE
        SYNTAX        INTEGER (0..90)
        MAX-ACCESS    read-create
        STATUS        obsolete
        DESCRIPTION   "The Low Water Mark for Ip table size. Set to 90%
                       of the table size, can be over-written."
        DEFVAL       { 0 }
        ::= { rcTlsIpServiceEntry 19 }

rcTlsIpServiceIpTblMltId OBJECT-TYPE
        SYNTAX        INTEGER (0..256)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Mlt Id of the Ip Table"
        DEFVAL       { 0 }
        ::= { rcTlsIpServiceEntry 20 }
rcTlsIpServicePriOverrideType OBJECT-TYPE
        SYNTAX        INTEGER
                      {
                         tos(1),
                         dscp(2)
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "This field indicated whether ToS or DSCP should be
                       used to override P bit when override is enabled."
        DEFVAL        { tos }
        ::= { rcTlsIpServiceEntry 21 }

rcTlsIpServiceColorBasedRemarking OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "disable/enable color-based egress mapping for this
                      end-point."
        DEFVAL        { disable }
        ::= { rcTlsIpServiceEntry 22 }

rcTlsIpServiceDropRedColorPackets OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "disable/enable dropping of red-colored packets for
                      this end-point if color-based mapping is used."
        DEFVAL        { true }
        ::= { rcTlsIpServiceEntry 23 }

rcTlsIpServiceRingId OBJECT-TYPE
        SYNTAX      	INTEGER (0..254)
        MAX-ACCESS    	read-create
        STATUS        	current
        DESCRIPTION   	"Ring Id of the Ip table."
        ::= { rcTlsIpServiceEntry 24 }		    

rcTlsIpServiceModId OBJECT-TYPE
        SYNTAX      	Integer32 (0..30)
        MAX-ACCESS    	read-create
        STATUS        	current
        DESCRIPTION   	"RBS Module ID of the Ip table."
        ::= { rcTlsIpServiceEntry 25 }

rcTlsIpServiceEsuPort OBJECT-TYPE
        SYNTAX    	INTEGER (0..28)
        MAX-ACCESS    	read-create
        STATUS        	current
        DESCRIPTION   	"RBS port number of the Ip table."
        ::= { rcTlsIpServiceEntry 26 }	





rcCosProfileCosMap  OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE (8))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "This indicates the priorit to CoS mapping.
		       The first byte represents the CoS for priority 
		       0 and subsequent bytes represent the CoS for 
		       the remaining 802.1p priorities. Format is 
		       a:b:c:d:e:f:g:h, where 0 <= a,b,c,d,e,f,g,h <= 4.
		       0 represents drop-class, 1 represents best-effort, 
		       2 represents silver, 3 represents gold and 4 
		       represents premium traffic."
        DEFVAL { "1:1:2:2:3:3:4:4" }
        ::= { rcCosProfileEntry 29 }

rcCosProfilePremiumEirUsage  OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Administratively enable or disable Premium EIR usage."
        DEFVAL { disable }
        ::= { rcCosProfileEntry 30 }

rcCosProfilePremiumEirUnits  OBJECT-TYPE
        SYNTAX        INTEGER { 
                          unitsOf64Kbps (1), 
                          unitsOf1Mbps (2),
			  unitsOf100Kbps (3)
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Indicates whether the Premium EIR value is measured 
		       in units of 64Kbps or 100Kbps or 1Mbps."
        DEFVAL { unitsOf64Kbps }
        ::= { rcCosProfileEntry 31 }

rcCosProfilePremiumEir OBJECT-TYPE
        SYNTAX        INTEGER (0..10000)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "The value of this object indicates the EIR rate
                       for traffic that has been classified as Premium,
                       a NNSC classification that is analogous to DSCP
                       EF, ToS Critical, or 802.1p user priority 6. This
                       attribute reflects the excess rate for Premium 
		       traffic. The excess rate provides per-class control 
		       over the volume of frames marked yellow. If eir 
		       usage is disabled, traffic that goes over the eir 
		       is immediately marked red and dropped. If eir 
		       usage is enabled, traffic that conforms to eir is
		       marked yellow and is passed through the pir token 
		       bucket and all traffic that is over the eir is 
		       marked red and dropped. If Premium EIR units is set 
		       to 64Kbps, this value cannot exceed 15. If it is 
		       set to 100Kbps, this value cannot exceed 200."
        DEFVAL { 0 }
        ::= { rcCosProfileEntry 32 }


rcCosProfilePremiumEirTBMaxCtl  OBJECT-TYPE
        SYNTAX        INTEGER { 
                          automatic (1), 
                          manual (2) 
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Specifies whether the Max. Token Bucket size 
		       attribute rcCosProfilePremiumEirTBMaxSize can 
		       be modified by the user or is set internally.
		       automatic -- set internally, manual -- modifiable" 
        DEFVAL { automatic }
	::= { rcCosProfileEntry 33 }

rcCosProfilePremiumEirTBMaxSize   OBJECT-TYPE
        SYNTAX        INTEGER (1..8000) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Specifies the Max. Token Bucket size limit for 
		       the Premium EIR bucket, in Kilobytes."  
        DEFVAL { 1 }
        ::= { rcCosProfileEntry 34 }


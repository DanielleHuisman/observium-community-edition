-- L2CP MIB section

rcL2cpMacBlockTable OBJECT-TYPE
		SYNTAX         SEQUENCE OF RcL2cpMacBlockEntry
		MAX-ACCESS     not-accessible
		STATUS         current
		DESCRIPTION    "L2CP MAC block table"
		::= { rc8021ah 11 }   

rcL2cpMacBlockEntry OBJECT-TYPE
		SYNTAX         RcL2cpMacBlockEntry
		MAX-ACCESS     not-accessible
		STATUS         current
		DESCRIPTION    "L2cp Mac Block Table Entry"
		INDEX          { rcL2cpMacBlockId }
		::= { rcL2cpMacBlockTable 1 }

RcL2cpMacBlockEntry ::=
        SEQUENCE {
                 rcL2cpMacBlockId               Integer32,
                 rcL2cpMacBlockName             DisplayString,
                 rcL2cpMacBlockStartMacAddr     MacAddress,
                 rcL2cpMacBlockEndMacAddr       MacAddress,
                 rcL2cpMacBlockRowStatus        RowStatus
                }

rcL2cpMacBlockId OBJECT-TYPE
		SYNTAX         Integer32 (1..8)
		MAX-ACCESS     not-accessible 
		STATUS         current
		DESCRIPTION    "L2cp Mac Block ID."
		::= { rcL2cpMacBlockEntry 1 }

rcL2cpMacBlockName  OBJECT-TYPE
                SYNTAX          DisplayString (SIZE (0..20))
                MAX-ACCESS      read-create
                STATUS          current
                DESCRIPTION     "L2CP MAC block name, by default it would
                                be 'MAC-BLOCK-<mac-block-ID>'"
                ::= { rcL2cpMacBlockEntry  2 }

rcL2cpMacBlockStartMacAddr OBJECT-TYPE
		SYNTAX          MacAddress
		MAX-ACCESS      read-create
		STATUS          current
		DESCRIPTION     "Start Mac Address of the L2cp Mac Block
				It must be a multicast MAC address."
		DEFVAL          { "00:00:00:00:00:00:00:00" }
		::= { rcL2cpMacBlockEntry  3 }

rcL2cpMacBlockEndMacAddr OBJECT-TYPE
		SYNTAX          MacAddress
		MAX-ACCESS      read-create
		STATUS          current
		DESCRIPTION     "End Mac Address of the L2cp Mac Block
				It must be a multicast MAC address."
		DEFVAL          { "00:00:00:00:00:00:00:00" }
		::= { rcL2cpMacBlockEntry  4 }

rcL2cpMacBlockRowStatus OBJECT-TYPE
                SYNTAX          RowStatus
                MAX-ACCESS      read-create
                STATUS          current
                DESCRIPTION     "Used for controlling the creation and
                                deletion of the MAC Block."
                ::= { rcL2cpMacBlockEntry 5 }

rcL2cpProfileTable OBJECT-TYPE       
		SYNTAX         SEQUENCE OF RcL2cpProfileEntry
		MAX-ACCESS     not-accessible
		STATUS         current
		DESCRIPTION    "L2CP Profile table."
		::= { rc8021ah 12 }
 
rcL2cpProfileEntry OBJECT-TYPE
		SYNTAX         RcL2cpProfileEntry
		MAX-ACCESS     not-accessible
		STATUS         current
		DESCRIPTION    "L2cp Profile Table Entry"
		INDEX          { rcL2cpProfileId }
		::= { rcL2cpProfileTable 1 }

RcL2cpProfileEntry ::=
        SEQUENCE {
                 rcL2cpProfileId               Integer32,
                 rcL2cpProfileName             DisplayString,
                 rcL2cpProfileApplyProfile     TruthValue, 
                 rcL2cpProfileForceDelete      TruthValue,
                 rcL2cpProfileRowStatus        RowStatus  
                }

rcL2cpProfileId OBJECT-TYPE
		SYNTAX         Integer32 (1..128)
		MAX-ACCESS     not-accessible
		STATUS         current
		DESCRIPTION    "L2CP Profile Id."
		::= { rcL2cpProfileEntry 1 }

rcL2cpProfileName OBJECT-TYPE
		SYNTAX         DisplayString (SIZE (0..20))
		MAX-ACCESS     read-create 
		STATUS         current
		DESCRIPTION    "L2CP Profile name."
		::= { rcL2cpProfileEntry 2 }

rcL2cpProfileApplyProfile OBJECT-TYPE
		SYNTAX         TruthValue
		MAX-ACCESS     read-create
		STATUS         current
		DESCRIPTION    " When apply is issued by the administrator, 
				all the changes made to the l2cp-profile will be
				reflected at the end-point and l2cp-profile 
				will be in Applied(true) state. Whenever any
				changes have been made to the profile, the state
				is changed to the Not Applied(false) state.If
				want these changes reflected at the end-point, 
				again issue the apply command."
		DEFVAL         { false }
		::= { rcL2cpProfileEntry 3 }

rcL2cpProfileForceDelete OBJECT-TYPE
		SYNTAX          TruthValue
		MAX-ACCESS      read-create
		STATUS          current
		DESCRIPTION     "Action to delete the L2cp-profile and change 
				the profile on all its associated end-points 
				to NULL_PROFILE"
		DEFVAL          {false}
		::= { rcL2cpProfileEntry 4 }

rcL2cpProfileRowStatus OBJECT-TYPE
                SYNTAX          RowStatus
                MAX-ACCESS      read-create
                STATUS          current
                DESCRIPTION     "Used for controlling the creation and deletion
                                of the row."
                ::= {  rcL2cpProfileEntry 5 }

rcL2cpProfileRuleTable OBJECT-TYPE  
		SYNTAX          SEQUENCE OF RcL2cpProfileRuleEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "L2CP profile rule table"
		::= { rc8021ah 13  }

rcL2cpProfileRuleEntry OBJECT-TYPE
		SYNTAX          RcL2cpProfileRuleEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "L2cp Profile Rule Table Entry"
		INDEX           { rcL2cpProfileRuleProfileId, rcL2cpProfileRuleId }
		::= { rcL2cpProfileRuleTable 1 }

RcL2cpProfileRuleEntry ::=
        SEQUENCE {
                 rcL2cpProfileRuleProfileId      Integer32,
                 rcL2cpProfileRuleId             Integer32,
                 rcL2cpProfileRuleProfileName    DisplayString,
                 rcL2cpProfileRuleMacBlockId     Integer32,
                 rcL2cpProfileRuleAction         INTEGER,
                 rcL2cpProfileRulePriority       Integer32,
                 rcL2cpProfileRuleRowStatus      RowStatus
                }

rcL2cpProfileRuleProfileId OBJECT-TYPE
		SYNTAX          Integer32 (1..128)
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "L2CP Profile ID."
		::=  { rcL2cpProfileRuleEntry 1 }

rcL2cpProfileRuleId  OBJECT-TYPE
		SYNTAX          Integer32 (1..8)
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Rule Id added to the profile."
		::= { rcL2cpProfileRuleEntry 2 }

rcL2cpProfileRuleProfileName OBJECT-TYPE
		SYNTAX          DisplayString (SIZE (0..20)) 
		MAX-ACCESS      read-create 
		STATUS          current
		DESCRIPTION     "L2CP Profile name."
		::=  { rcL2cpProfileRuleEntry 3 }

rcL2cpProfileRuleMacBlockId OBJECT-TYPE
		SYNTAX          Integer32 (1..8)
		MAX-ACCESS      read-create
		STATUS          current
		DESCRIPTION     "L2cp-Mac-Block Id added to the rule. 
				Rule will define what action has to be applied
				to the L2CP packet having MAC address in the
				MAC Block range."
		::= { rcL2cpProfileRuleEntry 4 }

rcL2cpProfileRuleAction OBJECT-TYPE
		SYNTAX          INTEGER{
                                  normal(1),
                                  expedite(2),
                                  drop(3)
                                }
		MAX-ACCESS      read-create
		STATUS          current
		DESCRIPTION     "Action applied to L2CP packet. One of the 
				defined actions normal, expedite or drop can
				can be applied to the L2CP packet."
		DEFVAL          { normal }
		::= { rcL2cpProfileRuleEntry 5 }

rcL2cpProfileRulePriority OBJECT-TYPE
		SYNTAX          Integer32 (0..7)
		MAX-ACCESS      read-create
		STATUS          current
		DESCRIPTION     "l2cp mac block priority can be set only incase
				of expedite action."
		::= { rcL2cpProfileRuleEntry 6 }
                 
rcL2cpProfileRuleRowStatus OBJECT-TYPE
                SYNTAX          RowStatus
                MAX-ACCESS      read-create
                STATUS          current
                DESCRIPTION     "Used for controlling the creation and
                                deletion of the row."
                ::= { rcL2cpProfileRuleEntry 7 }

-- Isid Management isid endpoint table

rcIsidMgmtEndptTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcIsidMgmtEndptEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Table of Management-endpoint entries."
        ::= { rc8021ah 14 }

rcIsidMgmtEndptEntry OBJECT-TYPE
        SYNTAX        RcIsidMgmtEndptEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Management Endpoint table entry."
        INDEX         { rcIsidMgmtEndptIsidId,rcIsidMgmtEndptTlsIndex }
        ::= { rcIsidMgmtEndptTable 1 }

RcIsidMgmtEndptEntry ::=
        SEQUENCE {
                  rcIsidMgmtEndptIsidId            Integer32,
                  rcIsidMgmtEndptTlsIndex          Integer32,
                  rcIsidMgmtEndptPort              InterfaceIndexOrZero,
                  rcIsidMgmtEndptMltId             Integer32,
                  rcIsidMgmtEndptServiceType       INTEGER,
                  rcIsidMgmtEndptOuterQtag         Integer32,
                  rcIsidMgmtEndptDataIsid          Integer32
                  }

rcIsidMgmtEndptIsidId OBJECT-TYPE
        SYNTAX        Integer32 (1..16777215)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "The management isid on which endpoint is configured"
        ::= { rcIsidMgmtEndptEntry 1 }


rcIsidMgmtEndptTlsIndex OBJECT-TYPE
        SYNTAX        Integer32 (1..2147483647)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "TLS Index for Isid Endpoint, the NID 
	               functionality is supported only on Port and MLT endpoints
		       The port and MLT-Id can be derived from 32 bits in TlsIndex"
        ::= { rcIsidMgmtEndptEntry 2 }

rcIsidMgmtEndptPort OBJECT-TYPE
        SYNTAX        InterfaceIndexOrZero
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The port on which management endpoint is configured. The value is zero for MLT based endpoint."
        DEFVAL        { 0 }
        ::= { rcIsidMgmtEndptEntry 3 }

rcIsidMgmtEndptMltId  OBJECT-TYPE
        SYNTAX      Integer32 (0..256)
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "MLT Id of the ISID, the value is zero for port based endpoint"
        DEFVAL        { 0 }
        ::= { rcIsidMgmtEndptEntry 4 }

rcIsidMgmtEndptOuterQtag OBJECT-TYPE
        SYNTAX        Integer32 (0..4094)
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "S-vlan (outer-qtag) of management endpoint configured in case of endpoint service type qnq1"
        ::= { rcIsidMgmtEndptEntry 5 }

rcIsidMgmtEndptServiceType OBJECT-TYPE
        SYNTAX        INTEGER {
                          transparent (1),
                          qnq1(2)
                          }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Service type of management isid endpoint"
        ::= { rcIsidMgmtEndptEntry 6 }


rcIsidMgmtEndptDataIsid OBJECT-TYPE
        SYNTAX        Integer32 (0..16777215)
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Data isid corresponding to management isid on which management isid endpoint is configured. Data Isid of value 0 indicates that there is no corresponding Data Isid present for management Isid."
        ::= { rcIsidMgmtEndptEntry 7 }

rcL2cpProfileTableSize OBJECT-TYPE
                SYNTAX        Integer32   
                MAX-ACCESS    read-only
                STATUS        current
                DESCRIPTION   "Used to indicate the number of entries in the
                               rcL2cpProfileTable"
                ::= { rc8021ah 15 }

-- Pwe3 endpoint table

rcPwEndptTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcPwEndptEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   ""
        ::= { rcPwe3 1 }
 
rcPwEndptEntry OBJECT-TYPE
        SYNTAX        RcPwEndptEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A Customer Endpoint table entry."
        INDEX         { rcPwEndptPwIndex, rcPwEndptTlsIndex }
        ::= { rcPwEndptTable 1 }
 
RcPwEndptEntry ::=
        SEQUENCE {
                 rcPwEndptPwIndex                  INTEGER,
                 rcPwEndptTlsIndex                 INTEGER,
                 rcPwEndptPort                     InterfaceIndex,
                 rcPwEndptQtags                    OCTET STRING,
                 rcPwEndptNumQtagIds               Integer32,      
                 rcPwEndptOuterQtag                INTEGER,
                 rcPwEndptConnectType              INTEGER,
                 rcPwEndptAdminState               EnableValue,
                 rcPwEndptRemoteMacAddr            MacAddress,
                 rcPwEndptIngressCosProfileName    DisplayString,
                 rcPwEndptEgressCosProfileName     DisplayString,
                 rcPwEndptIngressPolicer           EnableValue,
                 rcPwEndptEgressPolicer            EnableValue,
                 rcPwEndptPriMapping               OCTET STRING,
                 rcPwEndptName                     DisplayString,
                 rcPwEndptOperState                INTEGER,
                 rcPwEndptPriOverride              TruthValue,
                 rcPwEndptBpduTraffic              INTEGER,        
                 rcPwEndptRowStatus                RowStatus,
                 rcPwEndptClearStats               TruthValue,
                 rcPwEndptServiceType              INTEGER,
                 rcPwEndptMuxMode                  INTEGER,
                 rcPwEndptInVcLabel                INTEGER,
                 rcPwEndptOutVcLabel               INTEGER,
                 rcPwEndptCwPreference             TruthValue,
                 rcPwEndptNextHopMac               MacAddress,
                 rcPwEndptTrunkGroupName           DisplayString,
		 rcPwEndptMepList   		   OCTET STRING,
		 rcPwEndptNumOfMep  		   Integer32,
		 rcPwEndptClientMepList   	   OCTET STRING,
		 rcPwEndptNumOfClientMep  	   Integer32,
                 rcPwEndptCcmOperStatus            INTEGER,
		 rcPwEndptPolicingOption	   INTEGER,
                 rcPwEndptPriOverrideType          INTEGER,
                 rcPwEndptColorBasedRemarking      EnableValue,
                 rcPwEndptDropRedColorPackets      TruthValue,
		 rcPwEndptUserLabel1 	           OCTET STRING,
 		 rcPwEndptUserLabel2 	           OCTET STRING,
		 rcPwEndptIngressMirrorState       EnableValue,
                 rcPwEndptEgressMirrorState        EnableValue,
                 rcPwEndptMltId                    INTEGER
                 }

rcPwEndptPwIndex    OBJECT-TYPE 
        SYNTAX        INTEGER (1..16777215)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "PW table index number."
        ::= { rcPwEndptEntry 1 }

rcPwEndptTlsIndex OBJECT-TYPE
        SYNTAX        INTEGER (1..2147483647)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "TLS Index"
        ::= { rcPwEndptEntry 2 }

rcPwEndptPort  OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies a port."
        DEFVAL        { 1 }
        ::= { rcPwEndptEntry 3 }

rcPwEndptQtags  OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(512))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "This bitfield indicates which Qtags (in the range
		      1 to 4096) are associated with this end-point. 
		      The Most Significant (left-most) Bit represents Qtag 1.
		      The Least Significant Bit represents Qtag 4096."
        ::= { rcPwEndptEntry 4 }

rcPwEndptNumQtagIds  OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Used to indicate the number of Qtag IDs that
                        are stored in the rcPwEndptQtags variable."
        ::= { rcPwEndptEntry 5 }

rcPwEndptOuterQtag OBJECT-TYPE
        SYNTAX        INTEGER (0..4094) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Outer qtag for QinQ ISID endpoint"
        DEFVAL        { 0 }
        ::= { rcPwEndptEntry 6 }

rcPwEndptConnectType OBJECT-TYPE
        SYNTAX        INTEGER {
                          any2any (1),
                          hub     (2),
                          spoke   (3),
                          pt2pt   (4)
                         }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Type of end-point"
        DEFVAL       { pt2pt }
        ::= { rcPwEndptEntry 7 }

rcPwEndptAdminState OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Admin control of the end-point state."
        DEFVAL        { disable }
        ::= { rcPwEndptEntry 8 }

rcPwEndptRemoteMacAddr OBJECT-TYPE
        SYNTAX        MacAddress
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "A remote MAC address of other endpoint "
        ::= { rcPwEndptEntry 9 }

rcPwEndptIngressCosProfileName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..20)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "COS Profile Table Name which is used by
                       this endpoint as ingress policer"
        DEFVAL       { "DEFAULT_PROFILE"  } 
        ::= { rcPwEndptEntry 10 }

rcPwEndptEgressCosProfileName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..20)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "COS Profile Table Name which is used by 
                       this endpoint as egress policer"
        DEFVAL       { "DEFAULT_PROFILE"  } 
        ::= { rcPwEndptEntry 11 }

rcPwEndptIngressPolicer OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Enable/disable to use Ingress Cos Profile table 
                        (rcPwEndptCosProfileName) "
        DEFVAL        { enable }
        ::= { rcPwEndptEntry 12 }

rcPwEndptEgressPolicer OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Enable/disable to use Egress Cos Profile table 
                        (rcPwEndptEgressCosProfileName) "
        DEFVAL        { disable }
        ::= { rcPwEndptEntry 13 }

rcPwEndptPriMapping OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(8))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Maps 802.1p priorities (0-7) to Optical Ethernet
                       Class of Service (COS) values (0-7) for packets
                       across a Uni. The first bytes represent the COS
                       for 802.1p priority 0. Subsequent bytes represent
                       the mappings of the remaining 802.1p priorities."
        DEFVAL       { "00:00:02:04:00:06:06:07"  }
        ::= { rcPwEndptEntry 14 }

rcPwEndptName       OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..30)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Name of Endpoint, By default, it would be 
                       'EP-<sid> Port <PortNum>'.
                       Example: sid = 1, port = 1/2, rcPwEndptName would
                       be 'EP-1 PORT 1/2'"
        ::= { rcPwEndptEntry 15 }

rcPwEndptOperState  OBJECT-TYPE
        SYNTAX        INTEGER { 
                          up(1), 
                          down(2),
			  transport-down(3) 
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The current operational state of this ENDPOINT."
        ::= { rcPwEndptEntry 16 }

rcPwEndptPriOverride OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Enable or disable to override the Priority Mapping."
        DEFVAL          { false }
        ::= { rcPwEndptEntry 17 }

rcPwEndptBpduTraffic OBJECT-TYPE
        SYNTAX          INTEGER {
                        normal(1),
                        expedite(2),
                        drop(3)
                        }
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Bpdu traffic control."
        DEFVAL          { normal }
        ::= { rcPwEndptEntry 18 }

rcPwEndptRowStatus  OBJECT-TYPE
        SYNTAX        RowStatus
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Uses for controlling the creation and
                       deletion of this row."
        ::= { rcPwEndptEntry 19 }

rcPwEndptClearStats OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Clear Endpoint statistics."
        DEFVAL        { false }
        ::= { rcPwEndptEntry 20 } 

rcPwEndptServiceType OBJECT-TYPE
        SYNTAX         INTEGER {
                          transparent (1),
                          switched(2),
                          qnq1(3),
                          qnq2(4),
                          mpls(5)
                         } 
        MAX-ACCESS     read-create 
        STATUS         current
        DESCRIPTION    "service type of the end-point. It is also
                        releated with."
        DEFVAL         { switched }
        ::= { rcPwEndptEntry 21 }

rcPwEndptMuxMode  OBJECT-TYPE 
        SYNTAX      INTEGER {
                       oneToone(1),       -- Only one qtag as member
                       manyToOne(2)       -- Many qtags as member
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "An endpoint can have many qtags mapped to it
                       only if the MuxMode is set to manyToOne. For
                       a manyToOne i-sid there will not be any
                       remapping of qtags at the local egress."
        DEFVAL        { oneToone }
        ::= { rcPwEndptEntry 22 }

rcPwEndptInVcLabel  OBJECT-TYPE
        SYNTAX        INTEGER (0..1048575)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Incoming vc label."
        DEFVAL        { 0 }
        ::= { rcPwEndptEntry 23 }

rcPwEndptOutVcLabel OBJECT-TYPE
        SYNTAX        INTEGER (0..1048575)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Outgoing vc label."
        DEFVAL        { 0 }
        ::= { rcPwEndptEntry 24 }

rcPwEndptCwPreference OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Control word supported or unsupported."
        DEFVAL        { true }
        ::= { rcPwEndptEntry 25 }

rcPwEndptNextHopMac OBJECT-TYPE
        SYNTAX        MacAddress
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Nexthop Mac address."
        ::= { rcPwEndptEntry 26 }

rcPwEndptTrunkGroupName OBJECT-TYPE
        SYNTAX	      DisplayString (SIZE (0..32))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "PBT Trunk Group Name."
        ::= { rcPwEndptEntry 27 }

rcPwEndptMepList OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE (0..408))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "List of Meps assigned to this entry. 
                       A mep is consist of <md>.<ma>.<mep_id>. Domain
                       Name, Association and Mep Id is separated by '.' 
                       A list of mep is consist of 
                       <md1>.<ma1>.<mepid1>,<md2>.<ma2>.<mepid2>,... 
                       Each mep is separated by ','. Maximum up 8 meps"
        ::= { rcPwEndptEntry 28 }

rcPwEndptNumOfMep OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Used to indicate the number of Meps
                       is assigned to this entry"
        ::= { rcPwEndptEntry 29 }

rcPwEndptClientMepList OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE (0..408))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "List of Client Meps assigned to this entry. 
                       A client mep is consist of <md>.<ma>.<mep_id>. Domain
                       Name, Association and Mep Id is separated by '.' 
                       A list of client meps is consist of 
                       <md1>.<ma1>.<mepid1>,<md2>.<ma2>.<mepid2>,... 
                       Each mep is separated by ','. Maximum up 8 meps"
        ::= { rcPwEndptEntry 30 }

rcPwEndptNumOfClientMep OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Used to indicate the number of Client Meps
                       is assigned to this entry"
        ::= { rcPwEndptEntry 31 }

rcPwEndptCcmOperStatus OBJECT-TYPE
        SYNTAX        INTEGER { 
                          up(1), 
                          down(2)
                      }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The current CCM operational status of this endpoint."
        ::= { rcPwEndptEntry 32 }
rcPwEndptPolicingOption	OBJECT-TYPE
	SYNTAX	      INTEGER { 
			preEncap(1), 
			postEncap(2)
		      }
	MAX-ACCESS    read-create
	STATUS	      current
	DESCRIPTION   "Specify whether the policing is pre-encapsulation 
		       or post-encapsulation for Pseudo Wire Endpoints."
	::= { rcPwEndptEntry 33 }


rcPwEndptPriOverrideType OBJECT-TYPE
        SYNTAX        INTEGER
                      {
                         tos(1),
                         dscp(2),
			 mplsexp(3)
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "This field indicated whether ToS or DSCP should be
                       used to override P bit when override is enabled."
        DEFVAL        { tos }
        ::= { rcPwEndptEntry 34 }

rcPwEndptColorBasedRemarking OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "disable/enable color-based egress mapping for this
                      end-point."
        DEFVAL        { disable }
        ::= { rcPwEndptEntry 35 }

rcPwEndptDropRedColorPackets OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "disable/enable dropping of red-colored packets for
                      this end-point if color-based mapping is used."
        DEFVAL        { true }
        ::= { rcPwEndptEntry 36}

rcPwEndptUserLabel1 OBJECT-TYPE
 	SYNTAX          OCTET STRING (SIZE(0..89))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "User provided field. Used to give extra management information/data "
        ::= { rcPwEndptEntry 37 }


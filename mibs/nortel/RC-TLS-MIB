	rcEsmPortStatsLastSample       INTEGER,
	rcEsmPortStatsLastTimestamp    OCTET STRING,
	rcEsmPortStatsLastMin          Gauge32,
	rcEsmPortStatsLastMax          Gauge32,
	rcEsmPortStatsLastAve          Gauge32 	
	}


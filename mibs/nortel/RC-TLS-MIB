        DESCRIPTION  "Number of Multicast Tx Frames dropped."
        ::= { rcTlsUniIpServiceStatsEntry 274 }

rcTlsUniIpServiceStatsBcastUUcastTxTotalFrames     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Broadcast/Unknown Unicast Tx total Frames."
        ::= { rcTlsUniIpServiceStatsEntry 275 }

rcTlsUniIpServiceStatsBcastUUcastTxDroppedFrames     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Broadcast/Unknown Tx Frames dropped."
        ::= { rcTlsUniIpServiceStatsEntry 276 }


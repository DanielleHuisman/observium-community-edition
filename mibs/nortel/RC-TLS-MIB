-- ISID NNI RATE LIMIT TABLE
--

rcIsidNniRateLimiterTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcIsidNniRateLimiterEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "802.1ah Service Identifier NNI Rate Limiter table"
        ::= { rc8021ah 6}

rcIsidNniRateLimiterEntry OBJECT-TYPE
        SYNTAX        RcIsidNniRateLimiterEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   ""
        INDEX         { rcIsidNniRateLimiterIsidId, 
			rcIsidNniRateLimiterSlotNumber, 
			rcIsidNniRateLimiterLaneNumber }
        ::= { rcIsidNniRateLimiterTable 1 }

RcIsidNniRateLimiterEntry ::=
        SEQUENCE {
                    rcIsidNniRateLimiterIsidId		INTEGER,
                    rcIsidNniRateLimiterSlotNumber	INTEGER,
                    rcIsidNniRateLimiterLaneNumber	INTEGER,
                    rcIsidNniRateLimiterRateLimit	INTEGER,
                    rcIsidNniRateLimiterRowStatus	RowStatus,
		    rcIsidNniRateLimiterClearStats	TruthValue
                 }

rcIsidNniRateLimiterIsidId 	OBJECT-TYPE
        SYNTAX        INTEGER (1..16777215)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Service Instance ID of the I component"
        ::= { rcIsidNniRateLimiterEntry 1 }

rcIsidNniRateLimiterSlotNumber	OBJECT-TYPE
	SYNTAX		INTEGER (1..10)
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"Slot number."
	::= { rcIsidNniRateLimiterEntry 2 }

rcIsidNniRateLimiterLaneNumber	OBJECT-TYPE
	SYNTAX		INTEGER (1..3)
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"Lane number. On a 30 Port 1G card (8630GBR),
			 Ports 1 to 10 are grouped under Lane 1, Ports
			 11 to 20 are grouped under Lane 2 and Ports 21
			 to 30 are grouped under Lane 3. For a 3 Port
			 10G card (8683XLR), Port 1 comes under Lane 1,
			 Port 2 comes under Lane 2 and Port 3 comes 
			 under Lane 3. For a 48 Port 1G card (8648GTR),
			 Ports 1 to 24 are grouped under Lane 1 and Ports
			 25 to 48 are grouped under Lane 2."
	::= { rcIsidNniRateLimiterEntry 3 }

rcIsidNniRateLimiterRateLimit	OBJECT-TYPE
	SYNTAX		INTEGER (1..10000)
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"Rate Limit in Mbps."
        DEFVAL          { 1 }
	::= { rcIsidNniRateLimiterEntry 4 }

rcIsidNniRateLimiterRowStatus	OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"Create/delete operations for this entry."
	::= { rcIsidNniRateLimiterEntry 5 }

rcIsidNniRateLimiterClearStats OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Action to clear stats for
		      this Isid NNI Rate Limiter"
        DEFVAL        { false }
        ::= { rcIsidNniRateLimiterEntry 6 }


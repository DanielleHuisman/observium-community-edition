        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Disable/enable egress mirroring on this end-point."
        DEFVAL        { disable }
        ::= { rcPwEndptEntry 40 }

rcPwEndptMltId  OBJECT-TYPE
        SYNTAX      INTEGER (0..256)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "MLT Id of this endpoint"
        DEFVAL        { 0 }
        ::= { rcPwEndptEntry 41 }


-- Pwe3 endpoint stats table

rcPwEndptStatsTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcPwEndptStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   ""
        ::= { rcPwe3 2 }

rcPwEndptStatsEntry OBJECT-TYPE
        SYNTAX        RcPwEndptStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A Pw End-Point Statistics  table entry."
        INDEX         { rcPwEndptStatsPwIndex, rcPwEndptStatsTlsIndex }
        ::= { rcPwEndptStatsTable 1 }

RcPwEndptStatsEntry ::=
        SEQUENCE {
                rcPwEndptStatsPwIndex                      INTEGER,
                rcPwEndptStatsTlsIndex                     INTEGER,
                rcPwEndptStatsPort                         InterfaceIndex,
                rcPwEndptStatsPremRxOctets                 Counter64,
                rcPwEndptStatsPremRxDroppedOctets          Counter64,
                rcPwEndptStatsGoldRxOctets                 Counter64,
                rcPwEndptStatsGoldRxDroppedOctets          Counter64,
                rcPwEndptStatsSilverRxOctets               Counter64,
                rcPwEndptStatsSilverRxDroppedOctets        Counter64,
                rcPwEndptStatsStndRxOctets                 Counter64,
                rcPwEndptStatsStndRxDroppedOctets          Counter64,
                rcPwEndptStatsPremTxOctets                 Counter64,
                rcPwEndptStatsGoldTxOctets                 Counter64,
                rcPwEndptStatsSilverTxOctets               Counter64,
                rcPwEndptStatsStndTxOctets                 Counter64,
                rcPwEndptStatsPremTxMcastOctets            Counter64,
                rcPwEndptStatsGoldTxMcastOctets            Counter64,
                rcPwEndptStatsSilverTxMcastOctets          Counter64,
                rcPwEndptStatsStndTxMcastOctets            Counter64,
                rcPwEndptStatsPremCongestionDropOctets     Counter64,
                rcPwEndptStatsGoldCongestionDropOctets     Counter64,
                rcPwEndptStatsSilverCongestionDropOctets   Counter64,
                rcPwEndptStatsStndCongestionDropOctets     Counter64,
                rcPwEndptStatsPremCirDroppedOctets         Counter64,
                rcPwEndptStatsPremPirDroppedOctets         Counter64,
                rcPwEndptStatsGoldCirDroppedOctets         Counter64,
                rcPwEndptStatsGoldPirDroppedOctets         Counter64,
                rcPwEndptStatsSilverCirDroppedOctets       Counter64,
                rcPwEndptStatsSilverPirDroppedOctets       Counter64,
                rcPwEndptStatsStndCirDroppedOctets         Counter64,
                rcPwEndptStatsStndPirDroppedOctets         Counter64,
                rcPwEndptStatsName                         DisplayString,
                rcPwEndptStatsPremTxDroppedOctets          Counter64,
                rcPwEndptStatsGoldTxDroppedOctets          Counter64,
                rcPwEndptStatsSilverTxDroppedOctets        Counter64,
                rcPwEndptStatsStndTxDroppedOctets          Counter64,
                rcPwEndptStatsPremTxCongestionDropOctets   Counter64,
                rcPwEndptStatsGoldTxCongestionDropOctets   Counter64,
                rcPwEndptStatsSilverTxCongestionDropOctets Counter64,
                rcPwEndptStatsStndTxCongestionDropOctets   Counter64,
                rcPwEndptStatsPremTxCirDroppedOctets       Counter64,
                rcPwEndptStatsPremTxPirDroppedOctets       Counter64,
                rcPwEndptStatsGoldTxCirDroppedOctets       Counter64,
                rcPwEndptStatsGoldTxPirDroppedOctets       Counter64,
                rcPwEndptStatsSilverTxCirDroppedOctets     Counter64,
                rcPwEndptStatsSilverTxPirDroppedOctets     Counter64,
                rcPwEndptStatsStndTxCirDroppedOctets       Counter64,
                rcPwEndptStatsStndTxPirDroppedOctets       Counter64,
                rcPwEndptStatsRxMcastTotalOctets           Counter64,
                rcPwEndptStatsTxMcastTotalOctets           Counter64,
                rcPwEndptStatsPremRxMcastDroppedOctets     Counter64,
                rcPwEndptStatsGoldRxMcastDroppedOctets     Counter64,
                rcPwEndptStatsSilverRxMcastDroppedOctets   Counter64,
                rcPwEndptStatsStndRxMcastDroppedOctets     Counter64,
rcPwEndptStatsPremRxFrames                 Counter64,
                rcPwEndptStatsPremRxDroppedFrames          Counter64,
                rcPwEndptStatsGoldRxFrames                 Counter64,
                rcPwEndptStatsGoldRxDroppedFrames          Counter64,
                rcPwEndptStatsSilverRxFrames               Counter64,
                rcPwEndptStatsSilverRxDroppedFrames        Counter64,
                rcPwEndptStatsStndRxFrames                 Counter64,
                rcPwEndptStatsStndRxDroppedFrames          Counter64,
                rcPwEndptStatsPremTxFrames                 Counter64,
                rcPwEndptStatsGoldTxFrames                 Counter64,
                rcPwEndptStatsSilverTxFrames               Counter64,
                rcPwEndptStatsStndTxFrames                 Counter64,
                rcPwEndptStatsPremTxMcastFrames            Counter64,
                rcPwEndptStatsGoldTxMcastFrames            Counter64,
                rcPwEndptStatsSilverTxMcastFrames          Counter64,
                rcPwEndptStatsStndTxMcastFrames            Counter64,
                rcPwEndptStatsPremCongestionDropFrames     Counter64,
                rcPwEndptStatsGoldCongestionDropFrames     Counter64,
                rcPwEndptStatsSilverCongestionDropFrames   Counter64,
                rcPwEndptStatsStndCongestionDropFrames     Counter64,
                rcPwEndptStatsPremCirDroppedFrames         Counter64,
                rcPwEndptStatsPremPirDroppedFrames         Counter64,
                rcPwEndptStatsGoldCirDroppedFrames         Counter64,
                rcPwEndptStatsGoldPirDroppedFrames         Counter64,
                rcPwEndptStatsSilverCirDroppedFrames       Counter64,
                rcPwEndptStatsSilverPirDroppedFrames       Counter64,
                rcPwEndptStatsStndCirDroppedFrames         Counter64,
                rcPwEndptStatsStndPirDroppedFrames         Counter64,
                rcPwEndptStatsPremTxDroppedFrames          Counter64,
                rcPwEndptStatsGoldTxDroppedFrames          Counter64,
                rcPwEndptStatsSilverTxDroppedFrames        Counter64,
                rcPwEndptStatsStndTxDroppedFrames          Counter64,
                rcPwEndptStatsPremTxCongestionDropFrames   Counter64,
                rcPwEndptStatsGoldTxCongestionDropFrames   Counter64,
                rcPwEndptStatsSilverTxCongestionDropFrames Counter64,
                rcPwEndptStatsStndTxCongestionDropFrames   Counter64,
                rcPwEndptStatsPremTxCirDroppedFrames       Counter64,
                rcPwEndptStatsPremTxPirDroppedFrames       Counter64,
                rcPwEndptStatsGoldTxCirDroppedFrames       Counter64,
                rcPwEndptStatsGoldTxPirDroppedFrames       Counter64,
                rcPwEndptStatsSilverTxCirDroppedFrames     Counter64,
                rcPwEndptStatsSilverTxPirDroppedFrames     Counter64,
                rcPwEndptStatsStndTxCirDroppedFrames       Counter64,
                rcPwEndptStatsStndTxPirDroppedFrames       Counter64,
                rcPwEndptStatsRxMcastTotalFrames           Counter64,
                rcPwEndptStatsTxMcastTotalFrames           Counter64,
                rcPwEndptStatsPremRxMcastDroppedFrames     Counter64,
                rcPwEndptStatsGoldRxMcastDroppedFrames     Counter64,
                rcPwEndptStatsSilverRxMcastDroppedFrames   Counter64,
                rcPwEndptStatsStndRxMcastDroppedFrames     Counter64
      }

rcPwEndptStatsPwIndex OBJECT-TYPE
        SYNTAX        INTEGER (1..16777215)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Pw that the end-point is a member of."
        ::= { rcPwEndptStatsEntry 1 }


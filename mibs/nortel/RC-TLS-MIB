        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Drop Class Rx Frames dropped."
        ::= { rcTlsUniIpServiceStatsEntry 278 }

rcTlsUniIpServiceStatsDropClassTxDroppedOctets	OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Drop Class Tx Octets dropped."
        ::= { rcTlsUniIpServiceStatsEntry 279 }


-- Isid endpoint table

rcIsidEndptTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcIsidEndptEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   ""
        ::= { rc8021ah 2 }
 
rcIsidEndptEntry OBJECT-TYPE
        SYNTAX        RcIsidEndptEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A Customer Endpoint table entry."
        INDEX         { rcIsidEndptIsidId, rcIsidEndptTlsIndex }
        ::= { rcIsidEndptTable 1 }
 
RcIsidEndptEntry ::=
        SEQUENCE {
                 rcIsidEndptIsidId                   Integer32,
                 rcIsidEndptTlsIndex                 Integer32,
                 rcIsidEndptPort                     InterfaceIndex,
                 rcIsidEndptQtags                    OCTET STRING,
                 rcIsidEndptNumQtagIds               Integer32,      
                 rcIsidEndptOuterQtag                Integer32,
                 rcIsidEndptConnectType              INTEGER,
                 rcIsidEndptAdminState               EnableValue,
                 rcIsidEndptRemoteMacAddr            MacAddress,
                 rcIsidEndptIngressCosProfileName    DisplayString,
                 rcIsidEndptEgressCosProfileName     DisplayString,
                 rcIsidEndptIngressPolicer           EnableValue,
                 rcIsidEndptEgressPolicer            EnableValue,
                 rcIsidEndptPriMapping               OCTET STRING,
                 rcIsidEndptName                     DisplayString,
                 rcIsidEndptOperState                INTEGER,
                 rcIsidEndptPriOverride              TruthValue,
                 rcIsidEndptBpduTraffic              INTEGER,        
                 rcIsidEndptRowStatus                RowStatus,
                 rcIsidEndptClearStats               TruthValue,
                 rcIsidEndptServiceType              INTEGER,
                 rcIsidEndptMuxMode                  INTEGER,
                 rcIsidEndptMltId                    Integer32,
                 rcIsidEndptTrunkGroup               DisplayString,
		 rcIsidEndptMepList   		     OCTET STRING,
		 rcIsidEndptNumOfMep  		     Integer32,
		 rcIsidEndptClientMepList   	     OCTET STRING,
		 rcIsidEndptNumOfClientMep  	     Integer32,
                 rcIsidEndptRingId		     Integer32,
		 rcIsidEndptModId		     Integer32,
		 rcIsidEndptEsuPort                  Integer32,
		 rcIsidEndptPolicingOption	     INTEGER,
                 rcIsidEndptPriOverrideType          INTEGER,
                 rcIsidEndptColorBasedRemarking      EnableValue,
                 rcIsidEndptDropRedColorPackets      TruthValue,
 	         rcIsidEndptUserLabel1	             OCTET STRING,
	         rcIsidEndptUserLabel2	             OCTET STRING,
		 rcIsidEndptIngressMirrorState       EnableValue,
		 rcIsidEndptEgressMirrorState        EnableValue,
                 rcIsidEndptDefaultIsid              EnableValue,
                 rcIsidEndptL2ptMacSwapCdp           EnableValue,
                 rcIsidEndptL2ptMacSwapVtp           EnableValue,
                 rcIsidEndptL2ptMacSwapStp           EnableValue,
		 rcIsidEndptEndPointId		     Integer32,
                 rcIsidEndptDirectedMcast            EnableValue,
		 rcIsidEndptDEDecodeScheme	     INTEGER,
		 rcIsidEndptPcpDecodeMap	     OCTET STRING,
		 rcIsidEndptCopyItagPcpDeiToStag     EnableValue,
		 rcIsidEndptDhcpL2Relay              EnableValue,
		 rcIsidEndptIgmpAclName              DisplayString,
		 rcIsidEndptIgmpStaticEntriesName    DisplayString,
                 rcIsidEndptL2cpProfile              DisplayString,
                 rcIsidEndptNidMgmtSrcMacState       EnableValue,
                 rcIsidEndptNidMgmtSrcMac            MacAddress,
                 rcIsidEndptNidMgmtSrcIpAddress      IpAddress,
                 rcIsidEndptNidMgmtSrcIpMask         IpAddress,
                 rcIsidEndptCfmIngRateLimitSt        	EnableValue, 
                 rcIsidEndptCfmIngRateLimit          	INTEGER,
                 rcIsidEndptCfmEgrRateLimitSt       	EnableValue,
                 rcIsidEndptCfmEgrRateLimit          	INTEGER,
                 rcIsidEndptDHUId                    		INTEGER,
                 rcIsidEndptDHUOperState            		INTEGER,
                 rcIsidEndptDHUHomeOnMe            	TruthValue,
                 rcIsidEndptCfmIngRateLmtAction      INTEGER,
                 rcIsidEndptCfmEgrRateLmtAction      INTEGER
                 }

rcIsidEndptIsidId OBJECT-TYPE
        SYNTAX        Integer32 (1..16777215)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Service Instance ID of the I component"
        ::= { rcIsidEndptEntry 1 }

rcIsidEndptTlsIndex OBJECT-TYPE
        SYNTAX        Integer32 (1..2147483647)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "TLS Index"
        ::= { rcIsidEndptEntry 2 }

rcIsidEndptPort  OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "An index value that uniquely identifies a port."
        DEFVAL        { 1 }
        ::= { rcIsidEndptEntry 3 }

rcIsidEndptQtags  OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(512))
        MAX-ACCESS    read-create  
        STATUS        current
        DESCRIPTION   "This bitfield indicates which Qtags (in the range
		      1 to 4096) are associated with this end-point. 
		      The Most Significant (left-most) Bit represents Qtag 1.
		      The Least Significant Bit represents Qtag 4096."
        ::= { rcIsidEndptEntry 4 }

rcIsidEndptNumQtagIds  OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Used to indicate the number of Qtag IDs that
                        are stored in the rcIsidEndptQtags variable."
        ::= { rcIsidEndptEntry 5 }

rcIsidEndptOuterQtag OBJECT-TYPE
        SYNTAX        Integer32 (0..4094) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Outer qtag for QinQ ISID endpoint"
        DEFVAL        { 0 }
        ::= { rcIsidEndptEntry 6 }

rcIsidEndptConnectType OBJECT-TYPE
        SYNTAX        INTEGER {
                          any2any (1),
                          pt2pt   (4)
                         } 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Type of end-point"
        DEFVAL       { any2any }
        ::= { rcIsidEndptEntry 7 }

rcIsidEndptAdminState OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Admin control of the end-point state."
        DEFVAL        { disable }
        ::= { rcIsidEndptEntry 8 }

rcIsidEndptRemoteMacAddr OBJECT-TYPE
        SYNTAX        MacAddress
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "A remote MAC address of other endpoint "
        ::= { rcIsidEndptEntry 9 }

rcIsidEndptIngressCosProfileName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..20)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "COS Profile Table Name which is used by
                       this endpoint as ingress policer"
        DEFVAL       { "DEFAULT_PROFILE"  } 
        ::= { rcIsidEndptEntry 10 }

rcIsidEndptEgressCosProfileName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..20)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "COS Profile Table Name which is used by 
                       this endpoint as egress policer"
        DEFVAL       { "DEFAULT_PROFILE"  } 
        ::= { rcIsidEndptEntry 11 }

rcIsidEndptIngressPolicer OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Enable/disable to use Ingress Cos Profile table 
                        (rcIsidEndptCosProfileName) "
        DEFVAL        { enable }
        ::= { rcIsidEndptEntry 12 }

rcIsidEndptEgressPolicer OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Enable/disable to use Egress Cos Profile table 
                        (rcIsidEndptEgressCosProfileName) "
        DEFVAL        { disable }
        ::= { rcIsidEndptEntry 13 }

rcIsidEndptPriMapping OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(8))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Maps 802.1p priorities (0-7) to Optical Ethernet
                       Class of Service (COS) values (0-7) for packets
                       across a Uni. The first bytes represent the COS
                       for 802.1p priority 0. Subsequent bytes represent
                       the mappings of the remaining 802.1p priorities."
        DEFVAL       { "00:00:02:04:00:06:06:07"  }
        ::= { rcIsidEndptEntry 14 }

rcIsidEndptName       OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..30)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Name of Endpoint, By default, it would be 
                       'EP-<sid> Port <PortNum>'.
                       Example: sid = 1, port = 1/2, rcIsidEndptName would
                       be 'EP-1 PORT 1/2'"
        ::= { rcIsidEndptEntry 15 }

rcIsidEndptOperState  OBJECT-TYPE
        SYNTAX        INTEGER { 
                          up(1), 
                          down(2),
			  transport-down(3)   
                       }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The current operational state of this ENDPOINT."
        ::= { rcIsidEndptEntry 16 }

rcIsidEndptPriOverride OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Enable or disable to override the Priority Mapping."
        DEFVAL          { false }
        ::= { rcIsidEndptEntry 17 }

rcIsidEndptBpduTraffic OBJECT-TYPE
        SYNTAX          INTEGER {
                        normal(1),
                        expedite(2),
                        drop(3)
                        }
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Bpdu traffic control."
        DEFVAL          { normal }
        ::= { rcIsidEndptEntry 18 }

rcIsidEndptRowStatus  OBJECT-TYPE
        SYNTAX        RowStatus
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Uses for controlling the creation and
                       deletion of this row."
        ::= { rcIsidEndptEntry 19 }

rcIsidEndptClearStats OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Clear Endpoint statistics."
        DEFVAL        { false }
        ::= { rcIsidEndptEntry 20 } 

rcIsidEndptServiceType OBJECT-TYPE
        SYNTAX         INTEGER {
                          transparent (1),
                          switched(2),
                          qnq1(3),
                          qnq2(4)
                         } 
        MAX-ACCESS     read-create 
        STATUS         current
        DESCRIPTION    "service type of the end-point. It is also
                        releated with."
        DEFVAL         { switched }
        ::= { rcIsidEndptEntry 21 }

rcIsidEndptMuxMode  OBJECT-TYPE 
        SYNTAX      INTEGER {
                       oneToone(1),       -- Only one qtag as member
                       manyToOne(2)       -- Many qtags as member
                      }
        MAX-ACCESS    read-create
        STATUS        obsolete
        DESCRIPTION   "An endpoint can have many qtags mapped to it
                       only if the MuxMode is set to manyToOne. For
                       a manyToOne i-sid there will not be any
                       remapping of qtags at the local egress."
        DEFVAL        { oneToone }
        ::= { rcIsidEndptEntry 22 }

rcIsidEndptMltId  OBJECT-TYPE
        SYNTAX      Integer32 (0..256)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "MLT Id of the ISID"
        DEFVAL        { 0 }
        ::= { rcIsidEndptEntry 23 }

rcIsidEndptTrunkGroup OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..32)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "trunk group name"
        ::= { rcIsidEndptEntry 24 }


rcIsidEndptMepList OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(0..408))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "List of Meps assigned to this entry. 
		       A mep is consist of <md>.<ma>.<mep_id>. Domain
		       Name, Association and Mep Id is separated by '.' 
		       A list of mep is consist of 
		       <md1>.<ma1>.<mepid1>,<md2>.<ma2>.<mepid2>,... 
		       Each mep is separated by ','. Maximum up 8 meps"
        ::= { rcIsidEndptEntry 25 }

rcIsidEndptNumOfMep OBJECT-TYPE
        SYNTAX        	Integer32
        MAX-ACCESS      read-only
        STATUS        	current
        DESCRIPTION   	"Used to indicate the number of Meps
                      	is assigned to this entry"
        ::= { rcIsidEndptEntry 26 }

rcIsidEndptClientMepList OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(0..408))
        MAX-ACCESS    read-create
        STATUS        deprecated
        DESCRIPTION   "List of Client Meps assigned to this entry. 
		       A client mep is consist of <md>.<ma>.<mep_id>. Domain
		       Name, Association and Mep Id is separated by '.' 
		       A list of client meps is consist of 
		       <md1>.<ma1>.<mepid1>,<md2>.<ma2>.<mepid2>,... 
		       Each mep is separated by ','. Maximum up 8 meps"
        ::= { rcIsidEndptEntry 27 }

rcIsidEndptNumOfClientMep OBJECT-TYPE
        SYNTAX        	Integer32
        MAX-ACCESS      read-only
        STATUS        	deprecated
        DESCRIPTION   	"Used to indicate the number of Client Meps
                      	is assigned to this entry"
        ::= { rcIsidEndptEntry 28 }

rcIsidEndptRingId	OBJECT-TYPE
        SYNTAX      	Integer32 (0..254)
        MAX-ACCESS    	read-create
        STATUS        	current
        DESCRIPTION   	"Ring Id where the end-point is a member."
        ::= { rcIsidEndptEntry 29 }		    

rcIsidEndptModId	OBJECT-TYPE
        SYNTAX      	Integer32 (0..30)
        MAX-ACCESS    	read-create
        STATUS        	current
        DESCRIPTION   	"RBS Module ID where the end-point is a member."
        ::= { rcIsidEndptEntry 30 }

rcIsidEndptEsuPort	OBJECT-TYPE
        SYNTAX    	Integer32 (0..28)
        MAX-ACCESS    	read-create
        STATUS        	current
        DESCRIPTION   	"RBS port number where the end-point is a member."
        ::= { rcIsidEndptEntry 31 }		  

rcIsidEndptPolicingOption OBJECT-TYPE
	SYNTAX	INTEGER { 
		  preEncap(1), 
		  postEncap(2)
		}
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"Specify whether the policing is 
			 pre-encapsulation or post-encapsulation for 
			 Isid Endpoints."
	::= { rcIsidEndptEntry 32 }


rcIsidEndptPriOverrideType OBJECT-TYPE
        SYNTAX        INTEGER
                      {
                         tos(1),
                         dscp(2),
			 mplsexp(3)
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "This field indicated whether ToS or DSCP should be
                       used to override P bit when override is enabled."
        DEFVAL        { tos }
        ::= { rcIsidEndptEntry 33 }

rcIsidEndptColorBasedRemarking OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "disable/enable color-based egress mapping for this
                      end-point."
        DEFVAL        { disable }
        ::= { rcIsidEndptEntry 34 }

rcIsidEndptDropRedColorPackets OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "disable/enable dropping of red-colored packets for
                      this end-point if color-based mapping is used."
        DEFVAL        { true }
        ::= { rcIsidEndptEntry 35 }		  


rcIsidEndptUserLabel1  OBJECT-TYPE
 	SYNTAX          OCTET STRING (SIZE(0..89))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "User provided field. Used to give extra management information/data "
        ::= { rcIsidEndptEntry 36 }

rcIsidEndptUserLabel2  OBJECT-TYPE
 	SYNTAX          OCTET STRING (SIZE(0..89))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "User provided additional field. Used to give extra management information/data "
        ::= { rcIsidEndptEntry 37 }

rcIsidEndptIngressMirrorState OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Disable/Enable Ingress Mirroring on this endpoint."
        DEFVAL        { disable }
        ::= { rcIsidEndptEntry 38 }

rcIsidEndptEgressMirrorState OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Disable/Enable Egress Mirroring on this endpoint."
        DEFVAL        { disable }
        ::= { rcIsidEndptEntry 39 }

rcIsidEndptDefaultIsid OBJECT-TYPE
        SYNTAX      	EnableValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "disable/enable default Isid."
        DEFVAL  { disable }
        ::= { rcIsidEndptEntry 40 }

rcIsidEndptL2ptMacSwapCdp OBJECT-TYPE
        SYNTAX          EnableValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "disable/enable mac swapping on isid end-point for CDP protocol"
        DEFVAL          { disable }
        ::= { rcIsidEndptEntry 42 }

rcIsidEndptL2ptMacSwapVtp OBJECT-TYPE
        SYNTAX          EnableValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "disable/enable mac swapping on isid end-point for VTP protocol"
        DEFVAL          { disable }
        ::= { rcIsidEndptEntry 43 }

rcIsidEndptL2ptMacSwapStp OBJECT-TYPE
        SYNTAX          EnableValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "disable/enable mac swapping on isid end-point for STP protocol"
        DEFVAL          { disable }
        ::= { rcIsidEndptEntry 44 }

rcIsidEndptEndPointId  OBJECT-TYPE
	SYNTAX		Integer32 (0..49)
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION 	"End-point ID where the end-point is a member. The maximum 
			number of endpoints per port on an I-SID is 10 and per MLT 
			per I-SID is 10 on R-Module. On RC-Modules, the maximum 
                        number of port based endpoints supported per port per I-SID                                 is 50 and the of maximum number of MLT Based end-points 
                        supported per MLT per I-SID is 16"
	DEFVAL		{ 0 }
	::= { rcIsidEndptEntry 45 }

rcIsidEndptDirectedMcast       OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Disable/Enable directed multicasting on this endpoint."
        DEFVAL        { disable }
        ::= { rcIsidEndptEntry 46 }

rcIsidEndptDEDecodeScheme   OBJECT-TYPE
        SYNTAX        INTEGER
		      {
			ad8p0d(1),
			ad7p1d(2),
			ad6p2d(3),
			m7p1d(4),
			m6p2d(5),
			m5p3d(6),
			customPcp(7),
			dei(8)
		      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "This indicated the DE decoding 
		       scheme that should be used"
        DEFVAL        { ad8p0d }
        ::= { rcIsidEndptEntry 47 }

rcIsidEndptPcpDecodeMap   OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE (8))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "This indicated the Drop Eligibility 
		      bit for each priority. The first byte 
		      represents the drop eligibility for 
		      priority 0. Subsequent bytes represent 
		      the drop eligibility of the remaining 
		      802.1p priorities. Format is a:b:c:d:e:f:g:h, 
		      where 0 <= a, b, c, d, e, f, g, h <=1"
        DEFVAL        { "00:00:00:00:00:00:00:00" }
        ::= { rcIsidEndptEntry 48 }

rcIsidEndptCopyItagPcpDeiToStag OBJECT-TYPE
        SYNTAX          EnableValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Disable/Enable copying of I-Tag PCP/DEI bits to S-Tag p-bits and DEI at the egress"
        DEFVAL          { disable }
        ::= { rcIsidEndptEntry 49 }

rcIsidEndptDhcpL2Relay      OBJECT-TYPE
        SYNTAX        EnableValue
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Disable/Enable DhcpL2Relay on this endpoint."
        DEFVAL        { disable }
        ::= { rcIsidEndptEntry 50 }

rcIsidEndptIgmpAclName      OBJECT-TYPE
        SYNTAX        DisplayString (SIZE(0..64))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Name of the IP prefix-list containing allowed multicast
                       groups for this end-point"
        ::= { rcIsidEndptEntry 51 }

rcIsidEndptIgmpStaticEntriesName     OBJECT-TYPE
        SYNTAX        DisplayString (SIZE(0..64))
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Name of the IP prefix-list containing statically 
                       forwarded multicast groups for this end-point"
        ::= { rcIsidEndptEntry 52 }

rcIsidEndptL2cpProfile OBJECT-TYPE
        SYNTAX          DisplayString (SIZE (0..20))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "l2cp-profile associated with this end-point.
                         By default NULL L2CP profile will be associated."
        DEFVAL          { "NULL_L2CP_PROFILE" }
        ::= { rcIsidEndptEntry 53 }

rcIsidEndptNidMgmtSrcMacState OBJECT-TYPE
         SYNTAX          EnableValue
         MAX-ACCESS      read-create
         STATUS          current
         DESCRIPTION     "Disable/Enable configuration of management 
	                  source C-MAC on I-SID"
         DEFVAL          { disable }
         ::= { rcIsidEndptEntry 54 }
 
rcIsidEndptNidMgmtSrcMac OBJECT-TYPE
         SYNTAX        MacAddress
         MAX-ACCESS    read-create
         STATUS        current
         DESCRIPTION   "The customer source MAC address that has to 
	                go over the management I-SID"
         ::= { rcIsidEndptEntry 55 }
 
rcIsidEndptNidMgmtSrcIpAddress      OBJECT-TYPE
         SYNTAX        IpAddress
         MAX-ACCESS    read-create
         STATUS        current
         DESCRIPTION   "NID IP address that has to go over management I-SID"
         ::= { rcIsidEndptEntry 56 }
 
rcIsidEndptNidMgmtSrcIpMask  OBJECT-TYPE
         SYNTAX        IpAddress
         MAX-ACCESS    read-create
         STATUS        current
         DESCRIPTION   "The NID management IP address mask"
         ::= { rcIsidEndptEntry 57 }

rcIsidEndptCfmIngRateLimitSt OBJECT-TYPE
        SYNTAX          EnableValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Disable/enable the end-point Rate Limiter 
                         for CFM frames, in the ingress direction."
        DEFVAL          { disable }
        ::= { rcIsidEndptEntry 58 }

rcIsidEndptCfmIngRateLimit OBJECT-TYPE
        SYNTAX          INTEGER (0 | 20..1000)
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Rate Limit value for CFM ingress rate limiter
                         on this end-point.(in packets per second)"
        DEFVAL          { 20 }
        ::= { rcIsidEndptEntry 59 }

rcIsidEndptCfmEgrRateLimitSt OBJECT-TYPE
        SYNTAX          EnableValue
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Disable/enable the end-point Rate Limiter
                         for CFM frames, in the egress direction."
        DEFVAL          { disable }
        ::= { rcIsidEndptEntry 60 }

rcIsidEndptCfmEgrRateLimit OBJECT-TYPE
        SYNTAX          INTEGER (0 | 20..1000)
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "Rate Limit value for CFM egress rate limiter
                         on this end-point.(in packets per second)"
        DEFVAL          { 20 }
        ::= { rcIsidEndptEntry 61 }

rcIsidEndptDHUId OBJECT-TYPE
        SYNTAX          INTEGER (0..254)
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     "DHU Id where the end-point is a member."
        ::= { rcIsidEndptEntry 62 }

rcIsidEndptDHUOperState OBJECT-TYPE
        SYNTAX        INTEGER {
                          down(1),
                          normal(2),
                          partnerportdown(3),
                          partnernodedown(4)
                      }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "The current operational state of DHU
                       where the end-point is a member."
        ::= { rcIsidEndptEntry 63 }

rcIsidEndptDHUHomeOnMe OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Home direction to the DHU where the end-point is a member."
        DEFVAL        { false }
        ::= { rcIsidEndptEntry 64 }

rcIsidEndptCfmIngRateLmtAction OBJECT-TYPE
        SYNTAX        INTEGER {
                         drop(1),
                         downgrade(2)
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "On ingress, to mark the rate limited packets to yellow using 
                      downgrade option or to drop them using drop option."
        DEFVAL        { drop }
        ::= { rcIsidEndptEntry 65 }

rcIsidEndptCfmEgrRateLmtAction OBJECT-TYPE
        SYNTAX        INTEGER {
                         drop(1),
                         downgrade(2)
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "On egress, to mark the rate limited packets to yellow using 
                      downgrade option or to drop them using drop option."
        DEFVAL        { drop }
        ::= { rcIsidEndptEntry 66 }



-- Isid stats table

rcIsidStatsTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF RcIsidStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   ""
        ::= { rc8021ah 3 }
 
rcIsidStatsEntry OBJECT-TYPE
        SYNTAX        RcIsidStatsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A Isid Statistics table entry."
        INDEX         {  rcIsidStatsId }
        ::= { rcIsidStatsTable 1 }
 
RcIsidStatsEntry ::=
        SEQUENCE {
                rcIsidStatsId                            INTEGER,
                rcIsidStatsName                          DisplayString,
                rcIsidStatsPremRxOctets                  Counter64,     
                rcIsidStatsPremRxDroppedOctets           Counter64,
                rcIsidStatsGoldRxOctets                  Counter64,       
                rcIsidStatsGoldRxDroppedOctets           Counter64,
                rcIsidStatsSilverRxOctets                Counter64,       
                rcIsidStatsSilverRxDroppedOctets         Counter64,
                rcIsidStatsStndRxOctets                  Counter64,       
                rcIsidStatsStndRxDroppedOctets           Counter64,

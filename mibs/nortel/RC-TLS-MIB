		rcTlsTdiBroadcastOptimizationStatsFrames           Counter64,
		rcTlsTdiBroadcastOptimizationStatsOctets           Counter64,
		rcTlsTdiBroadcastOptimizationStatsName             DisplayString,	
		rcTlsTdiBroadcastOptimizationStatsPortType         INTEGER, 
		rcTlsTdiBroadcastOptimizationStatsRingId           INTEGER
	}	


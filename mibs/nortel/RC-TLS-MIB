        SYNTAX        DisplayString (SIZE (0..25)) 
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Name of Service Instance of the I component "
        ::= { rcIsidEntry 2 }

rcIsidBackboneVlan OBJECT-TYPE
        SYNTAX        INTEGER (1..4094)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Backbone Vlan which is used to carry Service 
                       Instance Over a Provider Backbone network"
        DEFVAL        { 1 }     
        ::= { rcIsidEntry 3 }

rcIsidAdminState OBJECT-TYPE
        SYNTAX        INTEGER {
                         enable(1),
                         disable(2),
                         forceDisable(3)
                      }
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION   "Administratively enable or disable this I Service 
                       Identifier entry."
        DEFVAL        { disable }
        ::= { rcIsidEntry 4 } 


        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Gold Service Octets dropped."
        ::= { rcPwEndptStatsEntry 7 }

rcPwEndptStatsSilverRxOctets OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Silver Service Octets received."

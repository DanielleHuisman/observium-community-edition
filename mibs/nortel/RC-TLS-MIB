                rcBanRingSaRbsRowStatus             RowStatus,
		rcBanRingSaRbsClearRdpStats         TruthValue,
		rcBanRingSaRbsCopyBtagPbits         EnableValue,
		rcBanRingSaRbsEsuIp                 IpAddress  
          }


                rcIsidNniRateLimiterStatsTotalRxDroppedOctets	Counter64,
                rcIsidNniRateLimiterStatsTotalRxPackets		Counter64,
                rcIsidNniRateLimiterStatsTotalRxDroppedPackets	Counter64
        }


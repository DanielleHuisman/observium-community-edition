        DESCRIPTION  "Number of Standard Service Tx Frames received that 
		      were marked 'Green' by the policer."
        ::= { rcTlsIpServiceStatsEntry 271 }

rcTlsIpServiceStatsStndTxYellowFrames     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Standard Service Tx Frames received that 
		      were marked 'Yellow' by the policer."
        ::= { rcTlsIpServiceStatsEntry 272 }

rcTlsIpServiceStatsStndTxRedFrames     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Standard Service Tx Frames received that 
		      were marked 'Red' by the policer."
        ::= { rcTlsIpServiceStatsEntry 273 }

rcTlsIpServiceStatsMcastTxDroppedFrames     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Multicast Tx Frames dropped."
        ::= { rcTlsIpServiceStatsEntry 274 }

rcTlsIpServiceStatsBcastUUcastTxTotalFrames     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Broadcast/Unknown Unicast Tx total Frames."
        ::= { rcTlsIpServiceStatsEntry 275 }

rcTlsIpServiceStatsBcastUUcastTxDroppedFrames     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Broadcast/Unknown Tx Frames dropped."
        ::= { rcTlsIpServiceStatsEntry 276 }

rcTlsIpServiceStatsDropClassRxDroppedOctets     OBJECT-TYPE
        SYNTAX        Counter64
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION  "Number of Drop Class Rx Octets dropped."
        ::= { rcTlsIpServiceStatsEntry 277 }


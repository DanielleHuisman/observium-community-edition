rcIsidStatsId OBJECT-TYPE
        SYNTAX        INTEGER (1..16777215)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "ISID Identifier."
        ::= { rcIsidStatsEntry 1 }
 
rcIsidStatsName OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..25)) 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Name of ISID."
        ::= { rcIsidStatsEntry 2 }
 

	boardNeMap	OBJECT-TYPE
		SYNTAX			OCTET STRING
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"It describes the map which contains the current NE Name."
		::=  {  boardEntry  11  }

	boardState      OBJECT-TYPE
		SYNTAX			INTEGER { operation ( 1 ) , test ( 2 ) }
		MAX-ACCESS		read-only
		STATUS			current

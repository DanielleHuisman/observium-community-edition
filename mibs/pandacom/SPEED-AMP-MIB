		DESCRIPTION
				"Module SFP Temperature is out of specified Range"
		::= { speedAmpModuleSFPAlarmEntry 7 }

	speedAmpMSFPDWDMAlarm OBJECT-TYPE
		SYNTAX INTEGER
		{
			notAvailiable(0),
			noAlarm(1),
			activeAlarm(2),
			unknown(255)
		}
		MAX-ACCESS  read-only
		STATUS  current
		DESCRIPTION
				"DWDM SFP - DWDM Alarm"
		::= { speedAmpModuleSFPAlarmEntry 8 }


END

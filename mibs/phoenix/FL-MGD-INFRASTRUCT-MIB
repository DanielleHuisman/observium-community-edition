        ::={ flWorkFWCtrlDLRDomainEntry 8 } 

    
     flWorkFWCtrlDLRRingStatus 
        OBJECT-TYPE
        SYNTAX INTEGER {
                            normal(0),
                            ringFault(1),
                            loopDetect(2),
                            partialFault(3),
                            rapidFault(4)
                            }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "Current status of the DLR ring."
        ::={ flWorkFWCtrlDLRDomainEntry 9 } 

     flWorkFWCtrlDLRDeviceStatus 

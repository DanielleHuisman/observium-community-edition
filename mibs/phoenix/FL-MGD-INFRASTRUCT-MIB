    FlWorkFWCtrlUserConfigEntry ::= SEQUENCE {
                 flWorkFWCtrlUserIndex
                     Integer32,
                 flWorkFWCtrlUserName
                     DisplayString,
                 flWorkFWCtrlUserPassword
                     DisplayString,
                 flWorkFWCtrlUserAccessMode

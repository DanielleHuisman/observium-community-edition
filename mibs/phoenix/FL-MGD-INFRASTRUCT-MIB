    		::= { flWorkWlanIf1VisibleAccessPointEntry 5 }

   flWorkWlanIf1VisibleAccessPointEntrySecurity 
   			OBJECT-TYPE
    			SYNTAX     OCTET STRING
    			MAX-ACCESS     read-only
    			STATUS     current
    			DESCRIPTION

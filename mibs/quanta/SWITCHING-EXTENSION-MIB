   -- 
   -- ***********************************************************
   -- 
   -- Textual Conventions
   -- 
   -- ***********************************************************
   
	BridgeEvbTLVTC ::= TEXTUAL-CONVENTION
	    STATUS     current
	    DESCRIPTION
	           "EVB TLV structure."
	    SYNTAX   BITS {
	                vdpCapable        (0),
	                ecpCapable        (1),
	                ecpRTE            (2),
	                vdpTimerCapable   (3),
	                reflectiveRelayCap(14),
	                stdForwardingCap  (15)
	                }


    agentPortMirroringGroup                     OBJECT IDENTIFIER ::= { agentConfigGroup 10 }

--     agentMirroredPortIfIndex OBJECT-TYPE
--         SYNTAX      Integer32
--         MAX-ACCESS  read-write
--         STATUS      obsolete

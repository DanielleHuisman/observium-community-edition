    rlPolicyGalileoDebugVlanId           TruthValue,
    rlPolicyGalileoDebugInport           TruthValue,
    rlPolicyGalileoDebugIpxDstNet        TruthValue,
    rlPolicyGalileoDebugIpxDstNode       TruthValue,
    rlPolicyGalileoDebugIpSrcAddr        TruthValue,
    rlPolicyGalileoDebugIpDstAddr        TruthValue,
    rlPolicyGalileoDebugIpProtocol       TruthValue,
    rlPolicyGalileoDebugIpSrcPort        TruthValue,
    rlPolicyGalileoDebugIpDstPort        TruthValue,
    rlPolicyGalileoDebugStatus           RowStatus
}


  SYNTAX INTEGER {
    any(1),
    udp(2),
    tcp(3),
    sctp(4),
    tls(5)
  }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    "Transport of received request."
  DEFVAL { any }
  ::=  {adSIPForwardingClassEntry 6}


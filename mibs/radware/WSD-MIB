    SYNTAX INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
     	 "Application port for TCP secondary server connectivity check."
    DEFVAL  { 80 }
    ::=  {rsWSDSSFEntry 6}


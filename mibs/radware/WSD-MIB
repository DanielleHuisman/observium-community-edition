    SYNTAX  INTEGER  (1..3600)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Polling interval in seconds for the secondary servers."
   DEFVAL  { 10 }
    ::=  {rsWSDSSFEntry 4}

rsWSDSSFCheckRetries OBJECT-TYPE
    SYNTAX  INTEGER  (1..20)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Minimum number of unsuccessful polling attempts before a server is
deemed
      disconnected."
    DEFVAL  { 5 }
    ::=  {rsWSDSSFEntry 5}


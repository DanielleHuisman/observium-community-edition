      disable(2),
      general(3),
      health(4)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Statistics Reporting Mode: reporting enable / disable, full statistics
      report or partial"
        ::=  {rsNWSD 12}


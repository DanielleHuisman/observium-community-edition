  SYNTAX DisplayString (SIZE (1..50))
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    "The Client SSL Policy name."
  DEFVAL { "None" }
  ::=  {rsWSDL4PolicyEntry 22}


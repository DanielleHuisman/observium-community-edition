    ACCESS read-write
   	STATUS mandatory
   	DESCRIPTION
     	 "The farm name."
    DEFVAL { "None" }
	::=  {rsWSDL4PolicyEntry 8}

rsWSDL4PolicyApplication OBJECT-TYPE
    SYNTAX  INTEGER  {
        na (1),
        http(2),
        ftpcontrol(3),
        ftpdata(4),
        https(5),
        rtsp(6),
        rsh(7),
        rexec(8),
        tftp(9),
        sip(10),
        tsCookie(11),
        radius(12),
        fix(13),
        virtualIPInterface(14),
        ping(15),
        tcp(16),
        udp(17),
        sctp(18), 
        mh-sctp(19),
        restricted-icap(20), -- Added in 1.03.06DL
        sips(21),
        generic-ssl(22)
--		diameter(24)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Treat as given port number. Tells how to treat the traffic that matches 
       the L4 policy."
    DEFVAL { na }
   ::=  {rsWSDL4PolicyEntry 9}


  SYNTAX INTEGER {
    any(1),
    sip(2),
    sips(3)
  }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    "Received request scheme."
  DEFVAL { any }
  ::=  {adSIPForwardingClassEntry 7}

adSIPFrwrdClassDestNet OBJECT-TYPE
  SYNTAX DisplayString (SIZE(0..50))
  ACCESS read-write

    raisecomUserAclIpPrecedence  OBJECT-TYPE    
        SYNTAX  INTEGER {
                    routine(0),
                    priority(1),
                    immediate(2),
                    flash(3),
                    flash-override(4),
                    critical(5),
                    internet(6),
                    network(7)  }
		ACCESS  read-create
		STATUS  current
		DESCRIPTION
			"The value of the IP precedence in the IP protocol. 
			It conflicts with the Tos and Dscp when setting."
		::=	{ raisecomUserAclEntry 16 }      
		
	raisecomUserAclIpTos   OBJECT-TYPE    
        SYNTAX  INTEGER (0..15)
        ACCESS  read-create
		STATUS  current
		DESCRIPTION

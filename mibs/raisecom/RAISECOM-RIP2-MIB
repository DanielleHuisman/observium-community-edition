       SYNTAX  INTEGER {    
            local(2),      
            netmgmt(3),     
            ospf(13) }
       MAX-ACCESS   read-only
       STATUS   current
       DESCRIPTION
          "Imported route protocol, only support static, connect, rip and ospf now."  
       ::= { raisecomRip2DistributeListOutProtocolEntry 1 } 


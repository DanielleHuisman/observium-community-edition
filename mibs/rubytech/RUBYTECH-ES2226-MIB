    SYNTAX         INTEGER(0|1)
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "The vlan type of high-acl entry.
         0 : Any,
         1 : Tag-Based Vlan.
        "
    ::= { es2226HighAclEntry 6 }

es2226HighAclTagVid OBJECT-TYPE
    SYNTAX         INTEGER(0..4094)
    MAX-ACCESS     read-write
    STATUS         current

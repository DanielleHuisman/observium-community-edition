    SYNTAX         INTEGER(0|1)
    MAX-ACCESS     read-write
    STATUS         current
    DESCRIPTION
        "Set mvr tagging.
         0 : untag-out,
         1 : tag-out.
        "
    ::= { es2410cMvrSettingEntry 3 }


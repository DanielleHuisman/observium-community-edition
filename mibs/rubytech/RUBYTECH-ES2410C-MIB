    SYNTAX      INTEGER(1..3)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The row status of igmp vlan entry.
         1 : active,
         2 : not-inService,
         3 : delete
        "
    ::= { es2410cIgmpVlanEntry 3 }


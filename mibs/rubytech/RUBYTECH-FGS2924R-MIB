    fgs2924rDot1XBackendOtherRequestsToSupplicant       Counter32,
    fgs2924rDot1XBackendAuthSuccesses                   Counter32,
    fgs2924rDot1XBackendAuthFails                       Counter32,
    fgs2924rDot1XAuthEapolFramesRx                      Counter32,

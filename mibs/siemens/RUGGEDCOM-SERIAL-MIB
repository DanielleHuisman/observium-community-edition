	SYNTAX	INTEGER {
			static(1),
			dynamic(2),
			staticAndDynamic(3)
		}
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION
		"The device address table in which addresses will be found for unicast 
		messages."
	::= { rcTinAndWin 9}

rcTinAndWinLinkStats 	OBJECT-TYPE
	SYNTAX				EnabledStatus
	MAX-ACCESS			read-write
	STATUS				current
	DESCRIPTION
		"Enables links statistics collection for TIN and WIN protocols."
     ::= { rcTinAndWin 10}


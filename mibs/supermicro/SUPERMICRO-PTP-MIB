    SYNTAX  	INTEGER {
                hmacSha196 (1),
                hmacSha256128 (2)
                }  
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
	" This indicates the algorithm to be used with the
          security key"
    DEFVAL { 1 }
    ::= { fsPtpSecKeyDataSetEntry 2 }

fsPtpSecKeyLength OBJECT-TYPE
    SYNTAX  	Integer32
    MAX-ACCESS  read-write
    STATUS      current

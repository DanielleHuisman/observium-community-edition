	    OCTET STRING,
        fsPtpSecKeyStartTime 
	    TimeStamp, 
        fsPtpSecKeyExpirationTime  
	    TimeStamp,
        fsPtpSecKeyValid   
	    TruthValue,
        fsPtpSecKeyRowStatus  
	    RowStatus
	}


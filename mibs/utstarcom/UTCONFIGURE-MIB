	    onu204i(21),
	    onu208i(22),
	    onu204h(23),
	    onu408t(24),
	    onu2024i(25),
	    onub210(26),
	    onu1001h(27),
	    onub220(28),
	    onu804i(29),
	    onu101(30),
	    onu1001b(31),
	    onu2004h(32),
	    onu204d(33),
	    onu404t(34),
	    onu4024t(35),
	    onu4024i(36),
        onu101i(38),
        onub1205y(39),
        onub1205d(40),
        onub130(41),
	    tkOnu(100),
	    pmcOnu(101),    
	    unknow(200)
 }
   MAX-ACCESS  read-only
   STATUS  current
   DESCRIPTION

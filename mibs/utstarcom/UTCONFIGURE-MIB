    SYNTAX INTEGER(0..15)
    MAX-ACCESS    read-write
	STATUS       current
    DESCRIPTION
        "The MLS Qos Cos queue schedule entry's weight. Only apply when mode is
		set to WRR"
    ::= { utsMLSQosExtCosQueueSchedEntry  6 }
	

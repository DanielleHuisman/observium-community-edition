        ::= { utsSyslogGroup 1 }        

   utsSyslogConsoleSeverity OBJECT-TYPE
      SYNTAX  UtsSyslogServerityLevelType
		MAX-ACCESS  read-write
		STATUS  current
		DESCRIPTION
			"Syslog console severity level."
        ::= { utsSyslogGroup 2 }        


   utsSyslogFlashSeverity OBJECT-TYPE
		SYNTAX  UtsSyslogServerityLevelType
		MAX-ACCESS  read-write
		STATUS  current
		DESCRIPTION
			"Syslog flash severity level."
        ::= { utsSyslogGroup 3 }        


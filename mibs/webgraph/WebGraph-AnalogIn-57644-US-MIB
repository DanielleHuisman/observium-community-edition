    SYNTAX OCTET STRING 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Description of the location of the Webio, which is user definable."
    ::= { wtWebGraphAnalogIn57644Text 3 }

wtWebGraphAnalogIn57644DeviceContact OBJECT-TYPE
    SYNTAX OCTET STRING 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Contact address of the system operator of the Webio, which is user definable."
    ::= { wtWebGraphAnalogIn57644Text 4 }

-- wtWebGraphAnalogIn57644TimeZone Group
----------------------------

wtWebGraphAnalogIn57644TzOffsetHrs OBJECT-TYPE
    SYNTAX INTEGER 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Offset hours for the time zone (winter time) with respect to UTC."
    ::= { wtWebGraphAnalogIn57644TimeZone 1 }

wtWebGraphAnalogIn57644TzOffsetMin OBJECT-TYPE
    SYNTAX INTEGER 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Offset minutes for the time zone (winter time) with respect to UTC."
    ::= { wtWebGraphAnalogIn57644TimeZone 2 }

wtWebGraphAnalogIn57644TzEnable OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(4))
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Binary coded conditions:
        Octet 1: unused
        Octet 2: unused
        Octet 3: unused
        Octet 4:
        	Bit 1-7: unused
	        Bit 0  : Apply Timezone"
    ::= { wtWebGraphAnalogIn57644TimeZone 3 }    
    
wtWebGraphAnalogIn57644StTzOffsetHrs OBJECT-TYPE
    SYNTAX INTEGER 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Offset hours of the time zone (summer time) with respect to UTC."
    ::= { wtWebGraphAnalogIn57644TimeZone 4 }

wtWebGraphAnalogIn57644StTzOffsetMin OBJECT-TYPE
    SYNTAX INTEGER 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Offset minutes of the time zone (summer time) with respect to UTC."
    ::= { wtWebGraphAnalogIn57644TimeZone 5 }
    
wtWebGraphAnalogIn57644StTzEnable OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(4))
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Binary coded conditions:
        Octet 1: unused
        Octet 2: unused
        Octet 3: unused
        Octet 4:
        	Bit 1-7: unused
	        Bit 0  : Apply Summertime"
    ::= { wtWebGraphAnalogIn57644TimeZone 6 }       
    
wtWebGraphAnalogIn57644StTzStartMonth OBJECT-TYPE
    SYNTAX INTEGER {
    	wtWebGraphAnalogIn57644StartMonth-January(1),
    	wtWebGraphAnalogIn57644StartMonth-February(2),
    	wtWebGraphAnalogIn57644StartMonth-March(3),
    	wtWebGraphAnalogIn57644StartMonth-April(4),
    	wtWebGraphAnalogIn57644StartMonth-May(5),
    	wtWebGraphAnalogIn57644StartMonth-June(6),
    	wtWebGraphAnalogIn57644StartMonth-July(7),
    	wtWebGraphAnalogIn57644StartMonth-August(8),
    	wtWebGraphAnalogIn57644StartMonth-September(9),
    	wtWebGraphAnalogIn57644StartMonth-October(10),
    	wtWebGraphAnalogIn57644StartMonth-November(11),
    	wtWebGraphAnalogIn57644StartMonth-December(12)
    	}
    	
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Month in which the change is made from winter time to summer time."
    ::= { wtWebGraphAnalogIn57644TimeZone 7 }

wtWebGraphAnalogIn57644StTzStartMode OBJECT-TYPE

    IMPORTS

    OBJECT-TYPE                                        FROM RFC-1212
    TRAP-TYPE                                          FROM RFC-1215
    DisplayString, PhysAddress                         FROM RFC1213-MIB
    enterprises, TimeTicks, IpAddress, NetworkAddress  FROM RFC1155-SMI;

 
--    IpAddress ::=
--        [APPLICATION 0]
--            IMPLICIT OCTET STRING (SIZE (4))
--    Counter ::=
--        [APPLICATION 1]
--            IMPLICIT INTEGER (0..4294967295)
--    Gauge ::=
--        [APPLICATION 2]
--            IMPLICIT INTEGER (0..4294967295)
--    TimeTicks ::=
--        [APPLICATION 3]
--            IMPLICIT INTEGER (0..4294967295)


--    DisplayString ::= OCTET STRING
--    PhysAddress   ::= OCTET STRING

--    internet         OBJECT IDENTIFIER ::= { iso org(3) dod(6) 1 }
--    privat           OBJECT IDENTIFIER ::= { internet 4 }
--    enterprises      OBJECT IDENTIFIER ::= { privat 1 }



--  Wiesemann & Theis Com-Server MIB

    wut                				OBJECT IDENTIFIER ::= { enterprises 5040 }
    wtComServer        				OBJECT IDENTIFIER ::= { wut 1 }
    wtWebio            				OBJECT IDENTIFIER ::= { wtComServer 2 }

--  wtWebGraph Tree

    wtWebGraphAnalogIn57644  			OBJECT IDENTIFIER ::= { wtWebio 12 }
    
--  wtWebGraphAnalogIn57644 Tree

    wtWebGraphAnalogIn57644Inventory    	OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644 1 }
    wtWebGraphAnalogIn57644SessCntrl 		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644 2 }
    wtWebGraphAnalogIn57644Config    		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644 3 }
    wtWebGraphAnalogIn57644Diag    		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644 4 }
    
--  wtWebGraphAnalogIn57644Config Tree

    wtWebGraphAnalogIn57644Device    		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Config 1 }
    wtWebGraphAnalogIn57644Ports     		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Config 2 }	
    wtWebGraphAnalogIn57644Manufact  		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Config 3 }	

--  wtWebGraphAnalogIn57644Device Tree

    wtWebGraphAnalogIn57644Text			OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Device 1 }	
    wtWebGraphAnalogIn57644TimeDate 		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Device 2 }
    wtWebGraphAnalogIn57644Basic		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Device 3 }	
    wtWebGraphAnalogIn57644Datalogger		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Device 4 }	
    wtWebGraphAnalogIn57644Alarm		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Device 5 }
    wtWebGraphAnalogIn57644Graphics 		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Device 6 }	
    
--  wtWebGraphAnalogIn57644TimeDate Tree

    wtWebGraphAnalogIn57644TimeZone		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644TimeDate 1 }
    wtWebGraphAnalogIn57644TimeServer		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644TimeDate 2 }	
    wtWebGraphAnalogIn57644DeviceClock		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644TimeDate 3 }

--  wtWebGraphAnalogIn57644Basic Tree
    
    wtWebGraphAnalogIn57644Network		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Basic 1 }
    wtWebGraphAnalogIn57644HTTP			OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Basic 2 }
    wtWebGraphAnalogIn57644Mail			OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Basic 3 }	
    wtWebGraphAnalogIn57644SNMP			OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Basic 4 }
    wtWebGraphAnalogIn57644UDP			OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Basic 5 }
    wtWebGraphAnalogIn57644Syslog		OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Basic 6 }
    wtWebGraphAnalogIn57644FTP			OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Basic 7 }

--  wtWebGraphAnalogIn57644Graphics Tree
    
    wtWebGraphAnalogIn57644GraphicsBase 	OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Graphics 1 }
    wtWebGraphAnalogIn57644GraphicsSelect 	OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Graphics 2 }
    wtWebGraphAnalogIn57644GraphicsScale 	OBJECT IDENTIFIER ::= { wtWebGraphAnalogIn57644Graphics 3 }
       
-- wtWebGraphAnalogIn57644Inventory Group
------------------------

wtWebGraphAnalogIn57644Sensors OBJECT-TYPE
    SYNTAX INTEGER (1..2)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of sensors"
    ::= { wtWebGraphAnalogIn57644Inventory 1 }

-- Interface Table

wtWebGraphAnalogIn57644SensorTable OBJECT-TYPE
    SYNTAX SEQUENCE OF WtWebGraphAnalogIn57644SensorEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "List of sensors."
    ::= { wtWebGraphAnalogIn57644Inventory 2 }

wtWebGraphAnalogIn57644SensorEntry OBJECT-TYPE
    SYNTAX WtWebGraphAnalogIn57644SensorEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "List of sensor numbers."
    INDEX  { wtWebGraphAnalogIn57644SensorNo }
    ::= { wtWebGraphAnalogIn57644SensorTable 1 }

WtWebGraphAnalogIn57644SensorEntry ::= SEQUENCE {
    wtWebGraphAnalogIn57644SensorNo INTEGER
}

wtWebGraphAnalogIn57644SensorNo OBJECT-TYPE
    SYNTAX INTEGER (1..2)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Sensor number."
    ::= { wtWebGraphAnalogIn57644SensorEntry 1 }


-- wtWebGraphAnalogIn57644Values Table

wtWebGraphAnalogIn57644ValuesTable OBJECT-TYPE
    SYNTAX SEQUENCE OF WtWebGraphAnalogIn57644ValuesEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "List of measurement values corresponding to the channel numbers."
    ::= { wtWebGraphAnalogIn57644Inventory 3 }

wtWebGraphAnalogIn57644ValuesEntry OBJECT-TYPE
    SYNTAX WtWebGraphAnalogIn57644ValuesEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Measurement values of the sensors."
    INDEX  { wtWebGraphAnalogIn57644SensorNo }
    ::= { wtWebGraphAnalogIn57644ValuesTable 1 }

WtWebGraphAnalogIn57644ValuesEntry ::= SEQUENCE{
    wtWebGraphAnalogIn57644Values OCTET STRING
}

wtWebGraphAnalogIn57644Values OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE (5))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Measurement value of the sensor, ASCII, 7 characters in length."
    ::= { wtWebGraphAnalogIn57644ValuesEntry 1 }

-- wtWebGraphAnalogIn57644BinaryValues Table

wtWebGraphAnalogIn57644BinaryValuesTable OBJECT-TYPE
    SYNTAX SEQUENCE OF WtWebGraphAnalogIn57644BinaryValuesEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "List of binary measurement values corresponding to the channel numbers."
    ::= { wtWebGraphAnalogIn57644Inventory 4 }

wtWebGraphAnalogIn57644BinaryValuesEntry OBJECT-TYPE
    SYNTAX WtWebGraphAnalogIn57644BinaryValuesEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Binary measurement values of the sensors."
    INDEX  { wtWebGraphAnalogIn57644SensorNo }
    ::= { wtWebGraphAnalogIn57644BinaryValuesTable 1 }

WtWebGraphAnalogIn57644BinaryValuesEntry ::= SEQUENCE{
    wtWebGraphAnalogIn57644BinaryValues INTEGER
}    

wtWebGraphAnalogIn57644BinaryValues OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Binary measurement value of the sensor in 1/10 degrees Celsius or 1/10 per cent,
        signed number, 0x4FFF6 indicates an 
        invalid measured value."
    ::= { wtWebGraphAnalogIn57644BinaryValuesEntry 1 }

-- wtWebGraphAnalogIn57644SessCntrl Group
--------------------------------

wtWebGraphAnalogIn57644SessCntrlPassword OBJECT-TYPE
    SYNTAX OCTET STRING 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Entering this password opens a session which is ended by writing a 1 to 
        wtWebGraphAnalogIn57644SessCntrlLogout. When read this variable provides a string of
        length zero."
    ::= { wtWebGraphAnalogIn57644SessCntrl 1 }

wtWebGraphAnalogIn57644SessCntrlConfigMode OBJECT-TYPE
    SYNTAX INTEGER {
    wtWebGraphAnalogIn57644SessCntrl-NoSession(0),
    wtWebGraphAnalogIn57644SessCntrl-Session(1)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "When the device is in SNMP configuration mode, the value 1 is
        returned, otherwise 0. If the read value is 0 even though the
        password was written, the device is already in config mode and the
        variables can only be read."
    ::= { wtWebGraphAnalogIn57644SessCntrl 2 }

wtWebGraphAnalogIn57644SessCntrlLogout OBJECT-TYPE
    SYNTAX INTEGER 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "If the device is in configuration mode and the session was opened by
        this SNMP Manager, writing a 1 to this variable will end the session 
        and all parameters are stored.
        Writing a 2 discards all entries again.
        When read this variable provides the value 0."
    ::= { wtWebGraphAnalogIn57644SessCntrl 3 }

wtWebGraphAnalogIn57644SessCntrlAdminPassword OBJECT-TYPE
    SYNTAX OCTET STRING 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "This password protects access to the variables with administrator rights.
        It can only be changed by the administrator. If no password was assigned,
        the valid password is public."
    ::= { wtWebGraphAnalogIn57644SessCntrl 4 }

wtWebGraphAnalogIn57644SessCntrlConfigPassword OBJECT-TYPE
    SYNTAX OCTET STRING 
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "This password protects access to the variables with configurator rights. It can
        be changed by the administrator and by the configurator. If no password was assigned, 
        the valid password is public. When read this variable provides a null-pointer."
    ::= { wtWebGraphAnalogIn57644SessCntrl 5 }
    


    wtWebGraphAnalogIn57644Alarm2Min			OCTET STRING,
    wtWebGraphAnalogIn57644Alarm2Max			OCTET STRING,
    wtWebGraphAnalogIn57644Alarm2Hysteresis		OCTET STRING,
    wtWebGraphAnalogIn57644AlarmSyslogIpAddr		OCTET STRING,
    wtWebGraphAnalogIn57644AlarmSyslogPort		INTEGER,
    wtWebGraphAnalogIn57644AlarmSyslogText		OCTET STRING,

    SYNTAX INTEGER {
    	wtTrapReceiver2x2StopMonth-January(1),
    	wtTrapReceiver2x2StopMonth-February(2),
    	wtTrapReceiver2x2StopMonth-March(3),
    	wtTrapReceiver2x2StopMonth-April(4),

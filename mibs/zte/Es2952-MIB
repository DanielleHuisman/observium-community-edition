		vctPair2Result  INTEGER,
		vctPair2Lenth  Integer32,
		vctPair3Result  INTEGER,
		vctPair3Lenth  Integer32,		
		vctPair4Result  INTEGER,
		vctPair4Lenth  Integer32		
		}



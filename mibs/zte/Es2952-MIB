        DESCRIPTION
            "Physical Ethnet interface ifName"
        ::= { opticalInfoEntry 2 } 
    opticalInfoOnline  OBJECT-TYPE
        SYNTAX      INTEGER  { online ( 1 ) , offline ( 2 ) } 
        MAX-ACCESS  read-only 
        STATUS  current
        DESCRIPTION
            "Optical Module Info Online"
        ::= { opticalInfoEntry 3 } 


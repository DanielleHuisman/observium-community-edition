    MplsL2vpnType ::= TEXTUAL-CONVENTION
        DISPLAY-HINT "d"
        STATUS       current
        DESCRIPTION
            "Mpls L2vpn type"
        SYNTAX  INTEGER
        {
            l2vpn-UNKNOWN(0),
            l2vpn-VPLS(1),
            l2vpn-VLL(2),
            l2vpn-MSPW(3),
            l2vpn-vls(4),
            l2vpn-VLANVLL(5)
        }


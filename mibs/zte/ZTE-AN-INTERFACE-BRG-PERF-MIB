        ::= { zxAnBrgIfAlmProfileApplyTable 1 }

    ZxAnBrgIfAlmProfileApplyEntry ::= SEQUENCE {
        zxAnBrgIfAlmPrf                DisplayString,
        zxAnBrgIfAlmPrfApplyRowStatus      RowStatus
        }
        

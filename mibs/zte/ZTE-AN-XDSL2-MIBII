        SYNTAX      DisplayString (SIZE(1..32))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This object identifies a row in this table."
        ::= { zxAnXdsl2ExtConfPrfEntry 1 }
             
    zxAnXdsl2LConfInpMinEtrUs OBJECT-TYPE
      SYNTAX      Unsigned32(0..200000)
      UNITS       "kbps"

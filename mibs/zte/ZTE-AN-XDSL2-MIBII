        DESCRIPTION
           "Foreign AC root mean square (RMS) voltage between tip and ring."
        ::= { zxAnXdsl2MeltStatusEntry 16 }
        
    zxAnXdsl2MeltAcVtg  OBJECT-TYPE
        SYNTAX      Integer32
        UNITS       "100 mV"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
           "Foreign AC root mean square (RMS) voltage between tip and ground."

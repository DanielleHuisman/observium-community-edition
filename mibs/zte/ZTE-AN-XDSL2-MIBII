        SYNTAX      INTEGER
            {
                all         (1), 
                resistance  (2),
                capacitance (3),
                voltage     (4),
                current     (5),
                frequency   (6),
                admittance  (7),
                ppa         (8)
            }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
           "This object indicates which kind of measurements is to be done."
        DEFVAL { all }
        ::= { zxAnXdsl2MeltConfEntry 1 }
        

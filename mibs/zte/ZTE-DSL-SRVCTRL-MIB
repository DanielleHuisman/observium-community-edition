    ZxDslVmacBrgPortEntry  ::=  SEQUENCE {
        zxDslVmacBrgPortId       Integer32,
        zxDslVmacTranslateMode   INTEGER,
        zxDslVmacTranslateLimit  INTEGER        
        }        
    

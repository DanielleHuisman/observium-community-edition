                zxAnEponOnuPonMacMaxDsBufferSize        INTEGER,
                zxAnEponOnuPonMacMinUsBufferSize        INTEGER,
                zxAnEponOnuPonMacMaxUsBufferSize        INTEGER
                }           
                

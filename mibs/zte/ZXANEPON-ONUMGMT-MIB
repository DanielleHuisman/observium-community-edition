        SYNTAX      INTEGER{
                      enable(1),
                      disable(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "Tk ONU optical administrative status."
        DEFVAL { disable }
        ::= { zxAnEponOnuTkOpticalCtrlEntry 1 } 
        

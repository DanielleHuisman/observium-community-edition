        SYNTAX      INTEGER
                        {
                        fiberBroken(1),
                        otherReasons(2),
                        allOnuDyingGasp(3),
                        allOnuNeverOnline(4)
                        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "OLT LOS Reason  
                 fiberBroken:trunk or branch fiber is broken.              
                 allOnuDyingGasp:all ONUs under the PON port are 
                                 power off.
                 allOnuNeverOnline:all ONUs under the PON port are 
                                   never online.
                 otherReasons:other reasons."

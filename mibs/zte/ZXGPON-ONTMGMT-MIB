        SYNTAX      INTEGER (0..8)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Outer VLAN priority for Vlan translate,'8' indicates copy priority from inner VLAN priority."    
        ::= { zxGponVlanTranslateEntry 6 }    


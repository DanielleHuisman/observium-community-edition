        }

    zxGponVoIPLineStatusCodecUsed OBJECT-TYPE
        SYNTAX      INTEGER{
                    auto-select(1),
                    g711-a-law(2),
                    g711-mu-law(3),
                    g723(4),
                    g722-1(5),

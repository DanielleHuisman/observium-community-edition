            zxGponGemPPPMHDInfoMeIdIdx                     INTEGER,
            zxGponGemPPPMHDInfoIntervalEndTime             INTEGER,
            zxGponGemPPPMHDInfoThresholdDataID             INTEGER,
            zxGponGemPPPMHDInfoLostPackets                 INTEGER,
            zxGponGemPPPMHDInfoMisinsertedPackets          INTEGER,

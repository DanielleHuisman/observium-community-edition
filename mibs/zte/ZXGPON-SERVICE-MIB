            zxGponGemPPPMHDAlarmCfgMisInsertPkgs     TruthValue,
            zxGponGemPPPMHDAlarmCfgMisInsertPkgsMask TruthValue,
            zxGponGemPPPMHDAlarmCfgImpairBlks        TruthValue,
            zxGponGemPPPMHDAlarmCfgImpairBlksMask    TruthValue            
        }

    zxGponGemPPPMHDAlarmCfgMeId OBJECT-TYPE
        SYNTAX      INTEGER
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "An arbitrary(unique) integer for identifiying an entry
             in the ONT equipment, from 1 to M."
        ::= { zxGponGemPPPMHDAlarmCfgEntry 1}

    zxGponGemPPPMHDAlarmCfgLosPkgs OBJECT-TYPE

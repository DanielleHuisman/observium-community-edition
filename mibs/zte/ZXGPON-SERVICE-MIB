        DESCRIPTION
            ""
        INDEX   { zxGponMgmtPonOltId }
        ::= { zxGponPonFloodBandwidthInfoTable 1 }

    ZxGponPonFloodBandwidthInfoEntry ::=
        SEQUENCE {
            zxGponPonFloodBandwidth     Unsigned32
        }

    zxGponPonFloodBandwidth       OBJECT-TYPE
      SYNTAX      Unsigned32
      MAX-ACCESS  read-write
      STATUS      current 
      DESCRIPTION 
        "64-2609192 kbps"
      REFERENCE   "N/A"
      ::= { zxGponPonFloodBandwidthInfoEntry 1 }


        ::= { zxGponGemPortStaticsTable 1 }

    ZxGponGemPortStaticsEntry ::=
        SEQUENCE {
            zxGponGemPortUpUcastPkts              Unsigned32,
            zxGponGemPortUpMcastPkts              Unsigned32,

    ZxGponOnuAlarmStatusEntry ::=
        SEQUENCE {
            zxGponOnuAlarmLOSd      TruthValue,
            zxGponOnuAlarmLOSdMask  TruthValue,
            zxGponOnuAlarmLOFd      TruthValue,

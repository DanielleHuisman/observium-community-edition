        DESCRIPTION
            "Rtm L2vpn Vfi Interface Vpn Type"
        ::= { rtmL2vpnIfEntry 11 }

    rtmL2vpnIfVlanRangeBot1  OBJECT-TYPE
        SYNTAX  INTEGER
        MAX-ACCESS  read-only

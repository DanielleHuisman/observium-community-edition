-- Acl bind to interface   
zxr10ACLBoundIfTable  OBJECT-TYPE
        SYNTAX  SEQUENCE OF Zxr10ACLBoundIfEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "Bind Acl to Interface"
        ::= { zxr10ACL 5 }

    zxr10ACLBoundIfEntry  OBJECT-TYPE
        SYNTAX  Zxr10ACLBoundIfEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "Bind Acl to Interface"
        INDEX  { zxr10ACLBoundIf }
        ::= { zxr10ACLBoundIfTable 1 }
        
    Zxr10ACLBoundIfEntry ::=
        SEQUENCE {
            zxr10ACLBoundIf
               Integer32,
            zxr10ACLNo 
               Integer32, 
            zxr10ACLName
               DisplayString,
            zxr10ACLBoundIfDirection
               INTEGER,      
            zxr10ACLRowStatus
                RowStatus
    }  
    
    zxr10ACLBoundIf  OBJECT-TYPE
        SYNTAX   Integer32
        MAX-ACCESS  read-create
        STATUS  current
        DESCRIPTION
            "The ifindex of physical interface that bind to acl"
        ::= { zxr10ACLBoundIfEntry 1 }  
      
    zxr10ACLNo  OBJECT-TYPE
        SYNTAX   Integer32
        MAX-ACCESS  read-create
        STATUS  current
        DESCRIPTION
            "ACL number
             <1-349>      IPv4 ACL number                                                                                                      
             <1000-1999>  IPv4 ACL number(expanded range)                                                                                      
             <2000-2499>  IPv6 ACL number    "
        ::= { zxr10ACLBoundIfEntry 2 }     
        
    zxr10ACLName  OBJECT-TYPE
        SYNTAX   DisplayString
        MAX-ACCESS  read-create
        STATUS  current
        DESCRIPTION
            "ACL name (1-31 characters)"
        ::= { zxr10ACLBoundIfEntry 3 }     
               
    zxr10ACLBoundIfDirection   OBJECT-TYPE  
        SYNTAX   INTEGER{in(0),out(1)}
        MAX-ACCESS  read-create
        STATUS  current
        DESCRIPTION
            "in    Inbound packets
             out   outbound packets"
        ::= { zxr10ACLBoundIfEntry 4 }
                
   zxr10ACLRowStatus   OBJECT-TYPE
        SYNTAX   RowStatus
        MAX-ACCESS  read-create
        STATUS  current
        DESCRIPTION
        "createAndGo(4)  Bind Acl to Interface
         destroy(6)      Undo Bind Acl to Interface"
        ::= { zxr10ACLBoundIfEntry 5 }                   

        END 
              
                        

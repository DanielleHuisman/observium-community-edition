                Integer32,
            zxr10LinkACLDinVlanID
                Integer32, 
            zxr10LinkACLDinVlanIDRight
                Integer32,
            zxr10LinkACLDoutVlanID
                Integer32, 
            zxr10LinkACLDoutVlanIDRight
                Integer32,    
            zxr10LinkACLInMAC
                MacAddress,
            zxr10LinkACLInMACWildcard
                MacAddress,        
            zxr10LinkACLInMACAny
                INTEGER,  
            zxr10LinkACLOutMAC 
                MacAddress,
            zxr10LinkACLOutMACWildCard
                MacAddress,         
            zxr10LinkACLOutMACAny
                INTEGER,                 
            zxr10LinkACLFlag
                INTEGER,
            zxr10LinkACLTimeRangeName
                DisplayString, 

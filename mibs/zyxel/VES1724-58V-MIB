	SYNTAX DisplayString
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The start time of recording of this entry in mm/dd/yyyy hh/mm/ss format
	  mm: 1 ~ 12
 	  dd: 1 ~ 31
 	  yyyy: 1970 ~ 2037		
		hh: 0 ~ 23
		mm: 0 ~ 59
		ss: 0 ~ 59"
	::= { dslPmHist15minEntry 3 }
	
	dslPmHist15minEndTime OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION

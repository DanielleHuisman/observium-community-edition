xdsl2PMExtended  		OBJECT IDENTIFIER ::= { vdsl2Stats 2 }

xdsl2PMLineExtended  		OBJECT IDENTIFIER ::= { xdsl2PMExtended 1 }
xdsl2PMChannelExtended 		OBJECT IDENTIFIER ::= { xdsl2PMExtended 2 }


        SYNTAX INTEGER  {
        							critical(1),
        							major(2),
        							minor(3),
        							warning(4),
        							event(5)
        				}
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
                "alarm severity.
                event(5): No permit editing the severity of alarm with event severity.
                          Read-only. No permit editing the alarm severity to be event.
                "
	::= { alarmConfEntry 2 }


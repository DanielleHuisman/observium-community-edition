----------------------------------------
-- loopguardStats
----------------------------------------
	loopguardSysStatus OBJECT-TYPE
	SYNTAX INTEGER {
		enable(1),
		disable(2)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Displays if any DSL/Enet port on the device enables loopguard."
	::= { loopguardStats 1 }

	loopguardPortStatsTable OBJECT-TYPE
	SYNTAX SEQUENCE OF LoopguardPortStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Loop guard DSL/Enet port statistics table"
	::= { loopguardStats 2 }

	loopguardPortStatsEntry OBJECT-TYPE
	SYNTAX LoopguardPortStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		""
	INDEX { ifIndex }
	::= { loopguardPortStatsTable 1 }

	LoopguardPortStatsEntry ::=
		SEQUENCE {
			loopguardPortStatsLoopDetected			INTEGER,
			loopguardPortStatsLinkedState			INTEGER,
			loopguardPortStatsTxPkts				Counter32,
			loopguardPortStatsRxPkts				Counter32,
			loopguardPortStatsBadPkts				Counter32,
			loopguardPortStatsShutdownTime          DisplayString,
			loopguardPortStatsOps				INTEGER
		}

	loopguardPortStatsLoopDetected OBJECT-TYPE
	SYNTAX INTEGER {
		loop(1),
		noLoop(2)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Displays if loop is detected on specific DSL/Enet port."
	::= { loopguardPortStatsEntry 1 }

	loopguardPortStatsLinkedState OBJECT-TYPE
	SYNTAX INTEGER {
		active(1),
		inactive(2)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Displays linked state on specific DSL/Enet port."
	::= { loopguardPortStatsEntry 2 }

	loopguardPortStatsTxPkts OBJECT-TYPE
	SYNTAX Counter32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Displays counter of transmited loopguard probe packet on specific DSL/Enet port."
	::= { loopguardPortStatsEntry 3 }

	loopguardPortStatsRxPkts OBJECT-TYPE
	SYNTAX Counter32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Displays counter of received loopguard probe packet on specific DSL/Enet port."
	::= { loopguardPortStatsEntry 4 }

	loopguardPortStatsBadPkts OBJECT-TYPE
	SYNTAX Counter32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Displays counter of received loopguard probe packet with incorrect checksum on specific DSL/Enet port."
	::= { loopguardPortStatsEntry 5 }

	loopguardPortStatsShutdownTime OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Displays port shutdown time for loopguard detection on specific DSL/Enet port."
	::= { loopguardPortStatsEntry 6 }

	loopguardPortStatsOps OBJECT-TYPE
	SYNTAX INTEGER {
		clearLoopguardStatistics(1)
	}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"1 : Clears loopguard statistics (packet counter) on specific DSL/Enet port."
	::= { loopguardPortStatsEntry 7 }

----------------------------------------
-- clear
----------------------------------------

       	counterClearTarget OBJECT-TYPE
        SYNTAX OCTET STRING
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
		"This managed object is used for identify the target on which
		the counter would be cleared. The target is encoded as:
		Byte 1:   reserved
		Byte 2~3: reserved
		Byte 4:   reserved
		Byte 5~11:each octet specifies a set of eight ports, with the first
		          octet specifying ports 1 through 8, etc."
        ::= { clear 1 }

        counterClearOps OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
		"The corresponding bit is set to 1 to start the operation.
		The various bit positions are:
		BIT 1: clear interface counters
		BIT 2: clear pvc packet counters
		BIT 3: clear port ATM counters
		BIT 4: clear pvc ATM counters
		BIT 5: clear DHCP counters
		BIT 6: clear IGMP counters
		BIT 7: clear IGMP port counters"
        ::= { clear 2 }

        counterClearVpi OBJECT-TYPE
        SYNTAX INTEGER (0..255)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
                "VPI of the pvc whose counters would be cleared."
        ::= { clear 3 }

        counterClearVci OBJECT-TYPE
        SYNTAX INTEGER (1..65535)
        MAX-ACCESS read-write
       	STATUS current
        DESCRIPTION
                "VCI of the pvc whose counters would be cleared."
        ::= { clear 4 }
                          
                          
END
        	pppoeIARemoteIDData     	DisplayString,
        	pppoeIARowStatus		RowStatus,
        	pppoeIARemoteIDType   INTEGER,
        	pppoeIACircuitIDType   INTEGER,
					pppoeIAOption		   INTEGER,
			pppoeIADelim		   INTEGER,
					pppoeIARemoteIDDelim	DisplayString
           }
        

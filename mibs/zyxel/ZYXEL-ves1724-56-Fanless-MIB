        SYNTAX  INTEGER
        {   
        	no_change(1),
        	discard_the_packet(2),
        	do_not_drop_the_matching_frame_previously_marked_for_dopping(3)
        }
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
                      	"Forwarding Action for policy rule"
        ::= { policyEntry 12 } 
   
        policyPriorityAction OBJECT-TYPE

       	::= { vturClassificationEntry 23 } 

      vturClassificationRowStatus OBJECT-TYPE
        SYNTAX  RowStatus
        MAX-ACCESS  read-create
        STATUS  current
        DESCRIPTION
                      	""
        ::= { vturClassificationEntry 24 }
           
      vturClassificationBandwidth OBJECT-TYPE

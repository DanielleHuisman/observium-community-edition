		SYNTAX	BITS {
			send_the_packet_to_the_mirror_port(1),
			send_the_packet_to_the_egress_port(2),
			send_the_matching_frames_to_the_egress_port(3),
			set_the_packets_VLAN_ID(4) 
		}
		MAX-ACCESS  read-create
		STATUS      current
		DESCRIPTION
	        "Outgoing Action for policy rule"
		::= { policyEntry 15 }


        ::= { vlanCounterEntry 22 }
        
        vlanCounterTxPktsGiant  OBJECT-TYPE
        SYNTAX  Counter64
        UNITS	"Packets"
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
        "The total number of packets received that were 
         greater than 1518 octets in length."
        ::= { vlanCounterEntry 23 }
                
        vlanCounterRxPktsGiant  OBJECT-TYPE
        SYNTAX  Counter64
        UNITS	"Packets"
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
        "The total number of packets received that were 
         greater than 1518 octets in length."
        ::= { vlanCounterEntry 24 }



